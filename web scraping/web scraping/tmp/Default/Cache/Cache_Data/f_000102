n    SalaryRange.DAILY,\n    SalaryRange.MONTHLY,\n    SalaryRange.YEARLY\n];\nconst APPLICABLE_SALARY_TYPE_MAP: { [key: string]: ReadonlyArray<DisplaySalaryRange> } = {\n    [JOB_TYPE_PART_TIME]: [SalaryRange.HOURLY, SalaryRange.DAILY, SalaryRange.MONTHLY],\n    [JOB_TYPE_FULL_TIME]: [SalaryRange.MONTHLY, SalaryRange.YEARLY]\n};\n\nconst EMPTY_SEARCH_INPUT_QUERY_PARAMETER = {\n    query: '',\n    value: ''\n};\n\nconst isSalaryTypeApplicable = (\n    type: SalaryRange,\n    types: ReadonlyArray<DisplaySalaryRange>\n): type is DisplaySalaryRange => {\n    const salaryTypes: ReadonlyArray<SalaryRange> = types;\n\n    return salaryTypes.indexOf(type) >= 0;\n};\n\nconst isTagApplicable = (tag: JobTag, jobType: string) =>\n    !((jobType === JOB_TYPE_PART_TIME && !tag.pt) || (jobType === JOB_TYPE_FULL_TIME && !tag.ft));\n\nconst getAppliedValue = <T extends string>(\n    selectedItem: T,\n    items: ReadonlyArray<FilterItem> | undefined,\n    defaultValue: T\n) => (items && items.some(({ id }) => id === selectedItem) ? selectedItem : defaultValue);\n\nconst mapPrimitiveToQuery = (query: string, value: string | number) => ({\n    query,\n    value: `${value}`\n});\n\nconst mapArrayToQuery = (query: string, value: ReadonlyArray<string>) => ({ query, value });\n\nconst mapSalaryToQuery = (value: { type: SalaryRange; min: number; max: number }) =>\n    [\n        { query: 'rbsaltype', value: value.type },\n        value.min >= 0 && { query: 'rbsalmin', value: `${value.min}` },\n        value.max >= 0 && { query: 'rbsalmax', value: `${value.max}` }\n    ].filter((v): v is SearchInputQueryParameter => !!v);\n\nconst areLatLongsEquals = (a: LatLong | null, b: LatLong | null): boolean => {\n    if (a === null && b === null) {\n        return true;\n    }\n\n    if (a !== null && b !== null) {\n        return a.latitude === b.latitude && a.longitude === b.longitude;\n    }\n\n    return false;\n};\n\nconst getScForLocationOptions = (\n    value: LocationOptionValue | null,\n    builder: ICompositeFilterEncoder | undefined\n) => {\n    const result: Array<SearchCriteriaKeywordFilter | SearchCriteriaCompositeFilter> = [];\n    if (value) {\n        const { areas, transitLines, transitStations } = value;\n        if (areas.length > 0) {\n            result.push({\n                type: 'keyword',\n                field: 'mloc',\n                keys: areas\n            });\n        }\n        if (builder) {\n            const encodedValue = builder.encodeTransitFilter(transitLines, transitStations);\n            if (encodedValue) {\n                result.push({\n                    type: 'composite',\n                    value: encodedValue\n                });\n            }\n        }\n    }\n    return result;\n};\n\nconst filterTruthySearchInputValue = (v: SearchInputValue | null): v is SearchInputValue => !!v;\n\nexport default class SearchInput {\n    public readonly rootStore: RootStore;\n\n    // TODO: possible refactor: remove standard fields?\n    @observable\n    public keyword = '';\n\n    @observable\n    public location = '';\n\n    @observable\n    public latLong: LatLong | null = null;\n\n    @observable\n    public radius = -1;\n\n    @observable\n    public type = '';\n\n    @observable\n    public age = '';\n\n    @observable\n    public indeedApply = '';\n\n    @observable\n    public salary = {\n        type: SalaryRange.NONE,\n        ranges: {\n            [SalaryRange.HOURLY]: {\n                min: -1,\n                max: -1\n            },\n            [SalaryRange.DAILY]: {\n                min: -1,\n                max: -1\n            },\n            [SalaryRange.MONTHLY]: {\n                min: -1,\n                max: -1\n            },\n            [SalaryRange.YEARLY]: {\n                min: -1,\n                max: -1\n            }\n        }\n    };\n\n    @observable\n    public tags: string[] = [];\n\n    @observable\n    public occupation = '';\n\n    @observable\n    public searchCriteria = '';\n\n    @observable\n    public regionId = '';\n\n    @observable\n    public areas: string[] = [];\n\n    @observable\n    public transitLines: string[] = [];\n\n    @observable\n    public transitStations: string[] = [];\n\n    @observable\n    public encryptedFccId = '';\n\n    @observable\n    public cjsSubQuery = '';\n\n    @observable\n    public customFields: Record<string, string[]> = {};\n\n    @observable\n    public additionalQueryParams: Record<string, string[]> = {};\n\n    @observable\n    public sourceTypes: string[] = [];\n\n    @computed\n    public get appliedRadius(): number {\n        // Radius is meaningless if location is empty\n        if (this.location) {\n            return this.radius;\n        }\n\n        return -1;\n    }\n\n    /**\n     * Will get the location only if there is not a custom lat long location provided. This can be used\n     * for determining the query parameter value for this field.\n     */\n    @computed\n    public get appliedLocation(): string | null {\n        if (this.latLong === null) {\n            return this.location;\n        } else {\n            return null;\n        }\n    }\n\n    @computed\n    public get appliedType(): string {\n        // We use it here to determine if this is one of the set values, we could change this to use\n        // the dynamic filters instead\n        return getAppliedValue(\n            this.type,\n            this.rootStore.config.richSearchComponentModel.filterSettingModel.types,\n            ''\n        );\n    }\n\n    @computed\n    public get selectedType(): string {\n        return this.type;\n    }\n\n    @computed\n    public get appliedAge(): string {\n        // we could migrate this to use the dynamic filters\n        return getAppliedValue(\n            this.age,\n            this.rootStore.config.richSearchComponentModel.filterSettingModel.ages,\n            ''\n        );\n    }\n\n    @computed\n    public get appliedIndeedApply(): string {\n        // we could migrate this to use the dynamic filters\n        return getAppliedValue(\n            this.indeedApply,\n            this.rootStore.config.richSearchComponentModel.filterSettingModel.indeedApplyItems,\n            ''\n        );\n    }\n\n    @computed\n    public get applicableSalaryTypes(): ReadonlyArray<DisplaySalaryRange> {\n        return APPLICABLE_SALARY_TYPE_MAP[this.type] || DISPLAY_SALARY_TYPE_LIST;\n    }\n\n    @computed\n    public get displaySalary(): DisplaySalaryFilter {\n        const applicableSalaryTypes = this.applicableSalaryTypes;\n        let type: DisplaySalaryRange;\n        if (isSalaryTypeApplicable(this.salary.type, applicableSalaryTypes)) {\n            type = this.salary.type;\n        } else {\n            type = applicableSalaryTypes[0];\n        }\n\n        return {\n            type,\n            ...this.salary.ranges[type]\n        };\n    }\n\n    @computed\n    public get isSalaryRangeValid(): boolean {\n        const { min, max } = this.displaySalary;\n\n        return min < 0 || max < 0 || min < max;\n    }\n\n    @computed\n    public get selectedSalary(): AppliedSalaryFilter | null {\n        const type = this.salary.type;\n\n        if (type === SalaryRange.NONE) {\n            return null;\n        }\n\n        const range = this.salary.ranges[type];\n\n        if (range.min < 0) {\n            return null;\n        }\n\n        return { type, ...range };\n    }\n\n    @computed\n    public get appliedSalary(): AppliedSalaryFilter {\n        const { type: displaySalaryType, min, max } = this.displaySalary;\n        // not used by global it seems?\n        const salarySettings = this.rootStore.config.richSearchComponentModel.filterSettingModel\n            .salarySettings;\n        const type = getAppliedValue(\n            displaySalaryType,\n            salarySettings && salarySettings.map((v) => v.salaryType),\n            SalaryRange.NONE\n        );\n\n        if (!this.isSalaryRangeValid || type === SalaryRange.NONE || (min < 0 && max < 0)) {\n            return {\n                type: SalaryRange.NONE,\n                min: -1,\n                max: -1\n            };\n        }\n\n        return { type, min, max: max === -2 ? -1 : max };\n    }\n\n    @computed\n    public get applicableTags(): ReadonlyArray<string> {\n        const tagSettings = this.rootStore.config.richSearchComponentModel.filterSettingModel\n            .jobTagsSettingModel;\n        if (!tagSettings) {\n            return [];\n        }\n\n        const jobType = this.appliedType;\n\n        return this.tags.filter(\n            (tag) =>\n                tagSettings.tags.hasOwnProperty(tag) &&\n                (!tagSettings.isTagsDynamic || isTagApplicable(tagSettings.tags[tag], jobType))\n        );\n    }\n\n    @computed\n    public get inapplicableTags(): ReadonlyArray<string> {\n        const tagSettings = this.rootStore.config.richSearchComponentModel.filterSettingModel\n            .jobTagsSettingModel;\n        if (!tagSettings || !tagSettings.isTagsDynamic) {\n            return [];\n        }\n\n        const jobType = this.appliedType;\n\n        return this.tags.filter(\n            (tag) =>\n                tagSettings.tags.hasOwnProperty(tag) &&\n                !isTagApplicable(tagSettings.tags[tag], jobType)\n        );\n    }\n\n    @computed\n    public get appliedOccupation(): string {\n        // probably not used by global\n        return this.rootStore.config.richSearchComponentModel.filterSettingModel\n            .occupationSettings &&\n            this.rootStore.config.richSearchComponentModel.filterSettingModel.occupationSettings.hasOwnProperty(\n                this.occupation\n            )\n            ? this.occupation\n            : '';\n    }\n\n    @computed\n    public get appliedLocationOptions(): LocationOptionValue | null {\n        if (this.appliedLocation) {\n            return null;\n        } else if (!this.regionId) {\n            return null;\n        } else {\n            return {\n                regionId: this.regionId,\n                areas: this.areas,\n                transitLines: this.transitLines.slice(),\n                transitStations: this.transitStations.slice()\n            };\n        }\n    }\n\n    @computed\n    private get customFilters(): ReadonlyArray<DynamicFilter> {\n        if (\n            this.rootStore.initialSearchInput &&\n            this.rootStore.initialSearchInput.dynFiltersModel\n        ) {\n            return this.rootStore.initialSearchInput.dynFiltersModel.filters;\n        }\n\n        return [];\n    }\n\n    @computed\n    public get customFieldNames(): ReadonlyArray<string> {\n        return this.customFilters.map(getFilterUrlParams).reduce((acc, val) => acc.concat(val), []);\n    }\n\n    public getCustomField(field: string): ReadonlyArray<string> {\n        return [...(get(this.customFields, field) || [])];\n    }\n\n    @computed\n    private get isRadiusChanged(): boolean {\n        if (!this.rootStore.initialSearchInput) {\n            return false;\n        }\n\n        if (!this.rootStore.initialSearchInput.dynFiltersModel) {\n            return this.appliedRadius !== this.rootStore.initialSearchInput.radius;\n        }\n\n        const radiusFilter = this.rootStore.initialSearchInput.dynFiltersModel.filters.find(\n            (filter) => getFilterUrlParam(filter) === 'radius'\n        );\n        if (!radiusFilter) {\n            return false;\n        }\n\n        const newRadius = this.getCustomField('radius');\n        const selectedRadius = radiusFilter.options.find((option) => option.selected);\n        if (!selectedRadius) {\n            return false;\n        }\n        const selectedRadiusValue = selectedRadius?.paramValMap?.radius;\n\n        return (\n            newRadius[0] !== undefined &&\n            selectedRadiusValue !== undefined &&\n            newRadius[0] !== `${selectedRadiusValue}`\n        );\n    }\n\n    @computed\n    private get searchHrefBase() {\n        return this.rootStore.config.isDesktop ? '/jobs' : '/m/jobs';\n    }\n\n    @computed\n    public get searchHref() {\n        return `${this.searchHrefBase}?${this.searchHrefQueryString}`;\n    }\n\n    @computed\n    public get searchHrefQueryString() {\n        return this.buildQueryStringFromParams(this.getSearchParams());\n    }\n\n    private readonly buildQueryStringFromParams = (\n        params: (SearchInputQueryParameter | SearchInputListQueryParameter)[]\n    ) => {\n        const defaultParameters: Record<string, string> = {\n            q: '',\n            ...(this.latLong === null ? { l: '' } : undefined)\n        };\n        const queryParameters = params.reduce<\n            Record<string, undefined | string | ReadonlyArray<string>>\n        >((prev, curr) => ({ ...prev, [curr.query]: curr.value }), defaultParameters);\n\n        if (this.rootStore.search.from) {\n            queryParameters.from = this.rootStore.search.from;\n        }\n\n        if (this.rootStore.search.cjsFrom) {\n            queryParameters.cjsFrom = this.rootStore.search.cjsFrom;\n        }\n\n        if (\n            this.rootStore.config.richSearchComponentModel.isGlobalVersion &&\n            this.rootStore.initialSearchInput &&\n            isBrowser()\n        ) {\n            // TODO redefine the behavior before starting SPA i18n test.\n            const currentQueryParams = parse(window.location.search.substr(1));\n\n            // FIXME: MOBILE does not provider information about whether \"radius\" is set by the user previously\n            // and we need to check this to exclude radius parameter.\n            if (typeof currentQueryParams.radius === 'undefined' && !this.isRadiusChanged) {\n                queryParameters.radius = undefined;\n            }\n\n            if (queryParameters.from === 'serpso') {\n                // maintaining params from current url, only if it is not to be set\n                if (typeof queryParameters.rq === 'undefined') {\n                    queryParameters.rq = currentQueryParams.rq;\n                }\n                if (typeof queryParameters.rsIdx === 'undefined') {\n                    queryParameters.rsIdx = currentQueryParams.rsIdx;\n                }\n\n                // FIXME special treatment for Remote search, this is really bad.\n                if (\n                    queryParameters.remotejob &&\n                    queryParameters.remotejob[0] === '1' &&\n                    typeof currentQueryParams.forceLocation === 'undefined'\n                ) {\n                    queryParameters.forceLocation = '0';\n                } else {\n                    queryParameters.forceLocation = currentQueryParams.forceLocation;\n                }\n            }\n        }\n\n        // for fromage=last, we need to extract pts and ts from recent saarch to make it work properly\n        const fromageTimestamps: { ts?: string; pts?: string } = {};\n        if (this.rootStore.recentSearch.currentRecentSearch && this.age === JobAgeValue.NEW_JOBS) {\n            fromageTimestamps.pts = this.rootStore.recentSearch.currentRecentSearch.pts;\n            fromageTimestamps.ts = this.rootStore.recentSearch.currentRecentSearch.ts;\n        }\n        return stringify(\n            { ...queryParameters, ...toJS(this.additionalQueryParams), ...fromageTimestamps },\n            { arrayFormat: 'repeat' }\n        );\n    };\n\n    @computed\n    public get isSearchEnabled(): boolean {\n        if (this.rootStore.config.richSearchComponentModel.allowFieldSearch) {\n            return this.getSearchParams().length > 0;\n        } else {\n            return (\n                this.getAppliedValues([\n                    SearchInputField.KEYWORD,\n                    SearchInputField.LOCATION,\n                    SearchInputField.LAT_LONG\n                ]).length > 0 || this.rootStore.config.allowEmptyWhatWhereSearch\n            );\n        }\n    }\n\n    @computed\n    public get isEqualToInitialSearchInput(): boolean {\n        // Always true when initial search input is empty\n        if (!this.rootStore.initialSearchInput) {\n            return true;\n        }\n\n        if (this.keyword !== this.rootStore.initialSearchInput.keyword) {\n            return false;\n        }\n        if (this.location !== this.rootStore.initialSearchInput.location) {\n            return false;\n        }\n        if (!areLatLongsEquals(this.latLong, this.rootStore.initialSearchInput.latLong)) {\n            return false;\n        }\n        if (this.appliedRadius !== this.rootStore.initialSearchInput.radius) {\n            return false;\n        }\n        if (this.appliedType !== this.rootStore.initialSearchInput.type) {\n            return false;\n        }\n        if (this.appliedAge !== this.rootStore.initialSearchInput.age) {\n            return false;\n        }\n        if (this.appliedIndeedApply !== this.rootStore.initialSearchInput.indeedApply) {\n            return false;\n        }\n        if (this.searchCriteria !== this.rootStore.initialSearchInput.searchCriteria) {\n            return false;\n        }\n        if (this.rootStore.config.richSearchComponentModel.filterSettingModel.salarySettings) {\n            if (this.appliedSalary.type !== this.rootStore.initialSearchInput.salaryType) {\n                return false;\n            }\n            if (this.appliedSalary.type !== SalaryRange.NONE) {\n                if (\n                    this.appliedSalary.min !== this.rootStore.initialSearchInput.salaryMin ||\n                    this.appliedSalary.max !== this.rootStore.initialSearchInput.salaryMax\n                ) {\n                    return false;\n                }\n            }\n        }\n        if (this.rootStore.config.richSearchComponentModel.filterSettingModel.jobTagsSettingModel) {\n            const tags = [...this.applicableTags].sort();\n            if (tags.length !== this.rootStore.initialSearchInput.tags.length) {\n                return false;\n            }\n            if (!this.rootStore.initialSearchInput.tags.every((tag, i) => tag === tags[i])) {\n                return false;\n            }\n        }\n        if (\n            this.rootStore.config.richSearchComponentModel.filterSettingModel.occupationSettings &&\n            this.appliedOccupation !== this.rootStore.initialSearchInput.occupation\n        ) {\n            return false;\n        }\n\n        return true;\n    }\n\n    public constructor(rootStore: RootStore) {\n        this.rootStore = rootStore;\n    }\n\n    public getSearchParams() {\n        const useScForDynamicFilters = this.rootStore.config.shouldUseScFilters;\n        const removeLegacyParamsForSc = this.rootStore.config.shouldRemoveLegacyParams;\n        const existingQueryParams: Record<string, boolean> = {};\n\n        // Custom fields override standard fields with same query parameter name\n        const appliedValues = this.getAppliedValues(\n            [...ORDERED_ALL_SEARCH_INPUT_FIELDS, ...keys(this.customFields)],\n            true\n        ).reverse();\n\n        let searchCriteria: SearchCriteria = { params: {} };\n        const scFilters: SearchCriteriaFilter[] = [];\n\n        // Convert to pure JS object as Mobx array is not working correctly with concat\n        const scEnabledFilters = toJS(\n            this.rootStore.initialSearchInput?.dynFiltersModel?.filters ?? []\n        ).filter((filter) => filter.scEnabled);\n\n        const params = appliedValues\n            .map((value) => {\n                // Process \"sc\" later\n                if (value.field === SearchInputField.SEARCH_CRITERIA) {\n                    const deserializedSc = deserializeSc(value.value || '');\n                    if (deserializedSc) {\n                        searchCriteria = deserializedSc;\n                    }\n                    return [];\n                } else if (\n                    'scFilter' in value &&\n                    typeof value.scFilter !== 'undefined' &&\n                    !value.empty\n                ) {\n                    scFilters.push(...[value.scFilter].flat());\n                }\n\n                const queryParams = ([] as Array<\n                    SearchInputQueryParameter | SearchInputListQueryParameter\n                >).concat(value.query);\n                const newQueryParams = queryParams.filter(\n                    (q) => !existingQueryParams[q.query] && q.query\n                );\n                queryParams.forEach((q) => {\n                    if (this.rootStore.config.isEuHomePage && 'l' === q.query) {\n                        existingQueryParams[q.query] = !value.empty;\n                    } else {\n                        existingQueryParams[q.query] = true;\n                    }\n                });\n\n                return !value.empty ? newQueryParams : [];\n            })\n            .reduceRight((prev, curr) => prev.concat(curr), [])\n            // The following filter is necessary to filter out legacy params supported in \"sc\"\n            .filter(\n                (param) =>\n                    !(\n                        useScForDynamicFilters &&\n                        removeLegacyParamsForSc &&\n                        scEnabledFilters.find((filter) => param.query === getFilterUrlParam(filter))\n                    )\n            );\n\n        this.removeAllFiltersFromSc(searchCriteria);\n        if (useScForDynamicFilters) {\n            // Converts (duplicates) scEnabled dynamic filters to \"sc\" (if they are not in sc)\n            this.addSelectedFilters(searchCriteria, scEnabledFilters);\n        }\n\n        // Converts (duplicates) standard filters to \"sc\"\n        if (scFilters.length > 0) {\n            addFiltersToSc(searchCriteria, scFilters);\n        }\n\n        if (!isSearchCriteriaEmpty(searchCriteria)) {\n            params.push({\n                query: SearchInputQuery.SEARCH_CRITERIA,\n                value: serializeSearchCriteria(searchCriteria)\n            });\n        }\n        return params;\n    }\n\n    public getAppliedValue(field: string): SearchInputCustomFieldValue | null;\n    public getAppliedValue(field: SearchInputField): SearchInputStandardFieldValue;\n    public getAppliedValue(field: SearchInputField | string): SearchInputValue | null;\n    public getAppliedValue(field: SearchInputField | string): SearchInputValue | null {\n        if (typeof field === 'string') {\n            const value = get(this.customFields, field) as undefined | string[];\n            if (value) {\n                const filteredValue = value.filter((v: string) => !!v);\n                let matchedFilter = this.customFilters.find(\n                    (filter) => getFilterUrlParam(filter) === field\n                );\n                if (!matchedFilter) {\n                    matchedFilter = this.customFilters.find((filter) =>\n                        filter.options.some(\n                            (option) => option.paramValMap && field in option.paramValMap\n                        )\n                    );\n                }\n\n                return {\n                    field: SearchInputField.NONE,\n                    name: SearchInputName.NONE,\n                    value: filteredValue,\n                    empty: filteredValue.length === 0,\n                    query: [mapArrayToQuery(field, toJS(filteredValue))],\n                    getLabel: localizedCustomFields(filteredValue, matchedFilter)\n                };\n            }\n        } else {\n            switch (field) {\n                case SearchInputField.KEYWORD:\n                    return {\n                        field: SearchInputField.KEYWORD,\n                        name: SearchInputName.KEYWORD,\n                        value: this.keyword,\n                        empty: !this.keyword,\n                        query: mapPrimitiveToQuery(SearchInputQuery.KEYWORD, this.keyword),\n                        getLabel: plainText(this.keyword)\n                    };\n                case SearchInputField.LOCATION:\n                    return {\n                        field: SearchInputField.LOCATION,\n                        name: SearchInputName.LOCATION,\n                        value: this.appliedLocation,\n                        empty: !this.appliedLocation,\n                        query: mapPrimitiveToQuery(\n                            SearchInputQuery.LOCATION,\n                            this.appliedLocation ?? ''\n                        ),\n                        getLabel: plainText(this.appliedLocation ?? '')\n                    };\n                case SearchInputField.LAT_LONG:\n                    return {\n                        field: SearchInputField.LAT_LONG,\n                        name: SearchInputName.LAT_LONG,\n                        value: this.latLong,\n                        empty: !this.latLong,\n                        query: mapPrimitiveToQuery(\n                            SearchInputQuery.LAT_LONG,\n                            `${this.latLong?.latitude.toFixed(5)},${this.latLong?.longitude.toFixed(\n                                5\n                            )}`\n                        ),\n                        getLabel: () => 'blah'\n                    };\n                case SearchInputField.RADIUS:\n                    return {\n                        field: SearchInputField.RADIUS,\n                        name: SearchInputName.RADIUS,\n                        value: this.appliedRadius,\n                        empty: this.appliedRadius < 0,\n                        query: mapPrimitiveToQuery(SearchInputQuery.RADIUS, this.appliedRadius),\n                        getLabel: localizedRadius(this.appliedRadius)\n                    };\n                case SearchInputField.TYPE:\n                    return {\n                        field: SearchInputField.TYPE,\n                        name: SearchInputName.TYPE,\n                        value: this.appliedType,\n                        empty: !this.appliedType,\n                        query: mapPrimitiveToQuery(SearchInputQuery.TYPE, this.appliedType),\n                        getLabel: localizedFilterItem(\n                            this.appliedType,\n                            this.rootStore.config.richSearchComponentModel.filterSettingModel.types\n                        )\n                    };\n                case SearchInputField.AGE:\n                    return {\n                        field: SearchInputField.AGE,\n                        name: SearchInputName.AGE,\n                        value: this.appliedAge,\n                        empty: !this.appliedAge,\n                        query: mapPrimitiveToQuery(SearchInputQuery.AGE, this.appliedAge),\n                        getLabel: localizedFilterItem(\n                            this.appliedAge,\n                            this.rootStore.config.richSearchComponentModel.filterSettingModel.ages\n                        )\n                    };\n                case SearchInputField.INDEED_APPLY:\n                    return {\n                        field: SearchInputField.INDEED_APPLY,\n                        name: SearchInputName.INDEED_APPLY,\n                        value: this.appliedIndeedApply,\n                        empty: !this.appliedIndeedApply,\n                        query: mapPrimitiveToQuery(\n                            SearchInputQuery.INDEED_APPLY,\n                            this.appliedIndeedApply\n                        ),\n                        getLabel: localizedFilterItem(\n                            this.appliedIndeedApply,\n                            this.rootStore.config.richSearchComponentModel.filterSettingModel\n                                .indeedApplyItems\n                        )\n                    };\n                case SearchInputField.SALARY:\n                    return {\n                        field: SearchInputField.SALARY,\n                        name: SearchInputName.SALARY,\n                        value: this.appliedSalary,\n                        empty: this.appliedSalary.type === SalaryRange.NONE,\n                        query: mapSalaryToQuery(this.appliedSalary),\n                        scFilter:\n                            this.rootStore.config.scFilters.indexOf('salary') > -1\n                                ? {\n                                      type: 'salary',\n                                      min:\n                                          this.appliedSalary.min > -1\n                                              ? this.appliedSalary.min.toString(10)\n                                              : undefined,\n                                      max:\n                                          this.appliedSalary.max > -1\n                                              ? this.appliedSalary.max.toString(10)\n                                              : undefined,\n                                      salaryType: this.appliedSalary.type\n                                  }\n                                : undefined,\n                        getLabel: localizedSalary(\n                            this.appliedSalary,\n                            this.rootStore.config.richSearchComponentModel.filterSettingModel\n                                .salarySettings\n                        )\n                    };\n                case SearchInputField.TAGS:\n                    return {\n                        field: SearchInputField.TAGS,\n                        name: SearchInputName.TAGS,\n                        value: this.applicableTags,\n                        empty: this.applicableTags.length === 0,\n                        query: mapArrayToQuery(SearchInputQuery.TAGS, this.applicableTags),\n                        getLabel: localizedTags(\n                            this.applicableTags,\n                            this.rootStore.config.richSearchComponentModel.filterSettingModel\n                                .jobTagsSettingModel\n                        ),\n                        scFilter:\n                            this.rootStore.config.scFilters.indexOf('tags') > -1\n                                ? {\n                                      type: 'keyword',\n                                      field: 'attr',\n                                      keys: this.applicableTags\n                                  }\n                                : undefined\n                    };\n                case SearchInputField.OCCUPATION:\n                    return {\n                        field: SearchInputField.OCCUPATION,\n                        name: SearchInputName.OCCUPATION,\n                        value: this.appliedOccupation,\n                        empty: !this.appliedOccupation,\n                        query: mapPrimitiveToQuery(\n                            SearchInputQuery.OCCUPATION,\n                            this.appliedOccupation\n                        ),\n                        scFilter:\n                            this.rootStore.config.scFilters.indexOf('jobcat1') > -1\n                                ? {\n                                      type: 'keyword',\n                                      field: 'occ',\n                                      keys: [this.appliedOccupation]\n                                  }\n                                : undefined,\n                        getLabel: localizedOccupation(\n                            this.appliedOccupation,\n                            this.rootStore.config.richSearchComponentModel.filterSettingModel\n                                .occupationSettings\n                        )\n                    };\n                case SearchInputField.LOCATION_OPTION:\n                    return {\n                        field: SearchInputField.LOCATION_OPTION,\n                        name: SearchInputName.LOCATION_OPTION,\n                        query: [],\n                        value: this.appliedLocationOptions,\n                        empty: !this.appliedLocationOptions,\n                        scFilter: getScForLocationOptions(\n                            this.appliedLocationOptions,\n                            this.rootStore.compositeFilterEncoder\n                        ),\n                        // Given the data for display labels for stations, etc. come from React hooks,\n                        // we are not using `getLabel()` here.\n                        // Use `UIStore.prototype.locationOptionLabel` instead.\n                        getLabel: () => ''\n                    };\n                case SearchInputField.SEARCH_CRITERIA:\n                    const searchCriteriaObject = deserializeSc(this.searchCriteria);\n                    const searchCriteria = isSearchCriteriaEmpty(searchCriteriaObject)\n                        ? ''\n                        : this.searchCriteria;\n                    return {\n                        field: SearchInputField.SEARCH_CRITERIA,\n                        name: SearchInputName.SEARCH_CRITERIA,\n                        value: searchCriteria,\n                        empty: !searchCriteria,\n                        query: mapPrimitiveToQuery(\n                            SearchInputQuery.SEARCH_CRITERIA,\n                            searchCriteria\n                        ),\n                        getLabel: plainText(searchCriteria)\n                    };\n                case SearchInputField.SOURCE_TYPES:\n                    return {\n                        field: SearchInputField.SOURCE_TYPES,\n                        name: SearchInputName.SOURCE_TYPES,\n                        value: this.sourceTypes,\n                        empty: this.sourceTypes.length === 0,\n                        query: EMPTY_SEARCH_INPUT_QUERY_PARAMETER,\n                        getLabel: localizedSourceTypes(this.sourceTypes),\n                        scFilter:\n                            this.sourceTypes.length > 0\n                                ? {\n                                      type: 'keyword',\n                                      field: 'st',\n                                      keys: this.sourceTypes\n                                  }\n                                : undefined\n                    };\n            }\n        }\n\n        return null;\n    }\n\n    public getAppliedValues(\n        fields: ReadonlyArray<SearchInputField | string>,\n        // eslint-disable-next-line @typescript-eslint/typedef\n        retainEmptyFields = false\n    ): SearchInputValue[] {\n        const values = fields\n            .map((field) => this.getAppliedValue(field))\n            .filter(filterTruthySearchInputValue);\n        if (retainEmptyFields) {\n            return values;\n        }\n\n        return values.filter((v) => !v.empty);\n    }\n\n    public getAppliedValuesForDisplay(\n        gt: GettextInterface,\n        fields: ReadonlyArray<SearchInputField | string>,\n        hideRadiusDefault = false,\n        showIndeedApplyDefaultLabel = false\n    ): string {\n        const values = fields\n            .map((field) => this.getAppliedValue(field))\n            .filter(filterTruthySearchInputValue);\n        // YOSEGI-3829: Show only the label for default value of IndeedApply when showIndeedApplyDefaultLabel is true\n        const searchInputValues = showIndeedApplyDefaultLabel\n            ? values.filter(\n                  (v) =>\n                      (v.field !== SearchInputField.INDEED_APPLY && !v.empty && !!v.value) ||\n                      (v.field === SearchInputField.INDEED_APPLY &&\n                          v.value === IndeedApplyValue.DEFAULT)\n              )\n            : values.filter((v) => !v.empty && !!v.value);\n\n        return getLocalizedSearchInputLabel(\n            gt,\n            searchInputValues,\n            this.rootStore.config.richSearchComponentModel.filterSettingModel,\n            hideRadiusDefault\n        );\n    }\n\n    private readonly resetJobAge = () => {\n        if (this.age === JobAgeValue.NEW_JOBS) {\n            this.age = JobAgeValue.ALL;\n        }\n    };\n\n    /**\n     * Removes all filters configuration from searchCriteria\n     */\n    private readonly removeAllFiltersFromSc = (searchCriteria: SearchCriteria) => {\n        searchCriteria.keywordFilters = [];\n        searchCriteria.booleanFilters = [];\n        searchCriteria.salaryFilter = undefined;\n        searchCriteria.params.cf = undefined;\n\n        // Remove non-dynamic filters\n        const staticFilters: SearchCriteriaFilter[] = [];\n        const filterSettingModel = this.rootStore.config.richSearchComponentModel\n            .filterSettingModel;\n        if (filterSettingModel.salarySettings) {\n            staticFilters.push({ type: 'salary', salaryType: 'YEARLY' }); // salaryType is arbitrarily chosen only to satisfy the type\n        }\n        if (filterSettingModel.occupationSettings) {\n            staticFilters.push({\n                type: 'keyword',\n                field: 'occ',\n                keys: Object.keys(filterSettingModel.occupationSettings)\n            });\n        }\n        if (filterSettingModel.jobTagsSettingModel) {\n            staticFilters.push({\n                type: 'keyword',\n                field: 'attr',\n                keys: Object.keys(filterSettingModel.jobTagsSettingModel.tags)\n            });\n        }\n        removeFiltersFromSc(searchCriteria, staticFilters);\n    };\n\n    private readonly addSelectedFilters = (\n        searchCriteria: SearchCriteria,\n        scEnabledFilters: DynamicFilter[]\n    ) => {\n        for (const filter of scEnabledFilters) {\n            const filterUrlParam = getFilterUrlParam(filter);\n            const selectedValues = this.getCustomField(filterUrlParam);\n\n            SearchInput.addSelectedScFilters(searchCriteria, filter, selectedValues);\n        }\n    };\n\n    public static addSelectedScFilters = (\n        searchCriteria: SearchCriteria,\n        filter: DynamicFilter,\n        selectedValues: readonly string[]\n    ) => {\n        const selectedScFilters = filter.options\n            .filter((option) => selectedValues.includes(getFilterOptionValue(option)))\n            .map((option) => {\n                if (\n                    option.type === 'bucket' ||\n                    option.type === 'noCountBucket' ||\n                    option.type === 'range'\n                ) {\n                    return option.scFilters ?? [];\n                } else {\n                    return [];\n                }\n            })\n            .reduce((a, b) => [...a, ...b], []);\n\n        if (filter.presentation.componentType === 'multiSelect') {\n            addMultiSelectFilters(searchCriteria, selectedScFilters);\n        } else {\n            addFiltersToSc(searchCriteria, selectedScFilters);\n        }\n    };\n\n    @action\n    public setKeyword = (value: string) => {\n        this.keyword = value;\n        this.resetJobAge();\n    };\n\n    @action\n    public setLocation = (value: string) => {\n        this.location = value;\n        // If we are changing the location, we want to clear the custom location here as it shouldn't be used\n        this.latLong = null;\n        this.resetJobAge();\n    };\n\n    @action\n    public setLatLong = (value: LatLong | null) => {\n        this.latLong = value;\n        this.resetJobAge();\n    };\n\n    @action\n    public setRadius = (value: number) => {\n        this.radius = value;\n        this.resetJobAge();\n    };\n\n    @action\n    public setType = (value: string) => {\n        this.type = value;\n        this.resetJobAge();\n    };\n\n    @action\n    public setSelectedType = (value: string) => {\n        this.setType(value);\n    };\n\n    @action\n    public setAge = (value: string) => {\n        this.age = value;\n    };\n\n    @action\n    public setIndeedApply = (value: string) => {\n        this.indeedApply = value;\n    };\n\n    @action\n    public setSalaryType = (value: SalaryRange) => {\n        this.salary.type = value;\n        this.resetJobAge();\n    };\n\n    @action\n    public setSalaryMin = (value: number) => {\n        this.salary.ranges[this.displaySalary.type].min = value;\n        this.resetJobAge();\n    };\n\n    @action\n    public setSalaryMax = (value: number) => {\n        this.salary.ranges[this.displaySalary.type].max = value;\n        this.resetJobAge();\n    };\n\n    @action\n    public setTags = (...value: ReadonlyArray<string>) => {\n        this.tags.splice(0, this.tags.length, ...value);\n        this.resetJobAge();\n    };\n\n    @action\n    public removeTag = (value: string) => {\n        const index = this.tags.indexOf(value);\n        if (index >= 0) {\n            this.tags.splice(index, 1);\n            this.resetJobAge();\n        }\n    };\n\n    @action\n    public setOccupation = (value: string) => {\n        this.occupation = value;\n        this.resetJobAge();\n    };\n\n    @action\n    public setSearchCriteria = (value: string) => {\n        this.searchCriteria = value;\n        this.resetJobAge();\n    };\n\n    @action\n    public setRegionId = (value: string) => {\n        this.regionId = value;\n    };\n\n    @action\n    public setAreas = (...value: ReadonlyArray<string>) => {\n        const sortedValues = [...value];\n        sortedValues.sort();\n        this.areas.splice(0, this.areas.length, ...sortedValues);\n    };\n\n    @action\n    public resetArea = () => {\n        this.areas.splice(0, this.areas.length);\n    };\n\n    @action\n    public setTransitLines = (...value: ReadonlyArray<string>) => {\n        const sortedValues = [...value];\n        sortedValues.sort();\n        this.transitLines.splice(0, this.transitLines.length, ...sortedValues);\n    };\n\n    @action\n    public setTransitStations = (...value: ReadonlyArray<string>) => {\n        const sortedValues = [...value];\n        sortedValues.sort();\n        this.transitStations.splice(0, this.transitStations.length, ...sortedValues);\n    };\n\n    @action\n    public setEncryptedFccId = (value: string) => {\n        this.encryptedFccId = value;\n        this.resetJobAge();\n    };\n\n    @action\n    public setCJSSubQuery = (value: string) => {\n        this.cjsSubQuery = value;\n        this.resetJobAge();\n    };\n\n    @action\n    public setCustomField = (field: string, ...value: ReadonlyArray<string>) => {\n        set(this.customFields, field, [...value]);\n    };\n\n    @action\n    public setAdditionalQueryParam = (field: string, ...value: ReadonlyArray<string>) => {\n        set(this.additionalQueryParams, field, [...value]);\n    };\n\n    @action\n    public resetAdditionalQueryParams = () => {\n        keys(this.additionalQueryParams).forEach((key) => remove(this.additionalQueryParams, key));\n    };\n\n    @action\n    public reset = (\n        fields: SearchInputField = SearchInputField.ALL,\n        customFields?: ReadonlyArray<string>,\n        resetIndeedApplyToZero = false\n    ) => {\n        if (fields & SearchInputField.KEYWORD) {\n            this.keyword = '';\n        }\n        if (fields & SearchInputField.LOCATION) {\n            this.location = '';\n        }\n        if (fields & SearchInputField.LAT_LONG) {\n            this.latLong = null;\n        }\n        if (fields & SearchInputField.RADIUS) {\n            this.radius = -1;\n        }\n        if (fields & SearchInputField.TYPE) {\n            this.type = '';\n        }\n        if (fields & SearchInputField.AGE) {\n            this.age = '';\n        }\n        if (fields & SearchInputField.INDEED_APPLY) {\n            if (resetIndeedApplyToZero) {\n                this.indeedApply = '0';\n            } else {\n                this.indeedApply = '';\n            }\n        }\n        if (fields & SearchInputField.SOURCE_TYPES) {\n            this.rootStore.config.richSearchComponentModel.japanSourceTypeFilterGroup === 3\n                ? this.sourceTypes.splice(\n                      0,\n                      this.sourceTypes.length,\n                      ...[\n                          SourceTypeValue.EMPLOYER,\n                          SourceTypeValue.RECRUITER,\n                          SourceTypeValue.JOB_BOARD,\n                          SourceTypeValue.OTHER,\n                          SourceTypeValue.ASSOCIATION\n                      ]\n                  )\n                : this.sourceTypes.splice(0);\n        }\n        if (fields & SearchInputField.SALARY) {\n            this.salary.type = SalaryRange.NONE;\n            DISPLAY_SALARY_TYPE_LIST.forEach((key) => {\n                this.salary.ranges[key].min = -1;\n                this.salary.ranges[key].max = -1;\n            });\n        }\n        if (fields & SearchInputField.TAGS) {\n            this.tags.splice(0);\n        }\n        if (fields & SearchInputField.OCCUPATION) {\n            this.occupation = '';\n        }\n        if (fields & SearchInputField.LOCATION_OPTION) {\n            this.regionId = '';\n            this.areas.splice(0);\n            this.transitLines.splice(0);\n            this.transitStations.splice(0);\n        }\n        if (fields & SearchInputField.SEARCH_CRITERIA) {\n            this.searchCriteria = '';\n        }\n        (customFields || this.customFieldNames).forEach((fieldName) => {\n            const matchedFilter = this.customFilters.find(\n                (filter) => getFilterUrlParam(filter) === fieldName\n            );\n            if (matchedFilter) {\n                const defaultOption = matchedFilter.options.find((option: any) => option.default);\n                if (defaultOption) {\n                    this.setCustomField(fieldName, getFilterOptionValue(defaultOption));\n                } else {\n                    this.setCustomField(fieldName, matchedFilter.presentation.noOptionValue);\n                }\n\n                return;\n            }\n            this.setCustomField(fieldName);\n        });\n        this.resetAdditionalQueryParams();\n\n        this.rootStore.ui.updateDisplayedJobsCount();\n    };\n\n    @action\n    public apply = (\n        draft: SearchInput,\n        fields: SearchInputField = SearchInputField.ALL,\n        customFields?: ReadonlyArray<string>,\n        applyAdditionalQueryParams = false\n    ) => {\n        if (fields & SearchInputField.KEYWORD) {\n            this.keyword = draft.keyword;\n        }\n        if (fields & SearchInputField.LOCATION) {\n            this.location = draft.location;\n        }\n        if (fields & SearchInputField.LAT_LONG) {\n            this.latLong = draft.latLong;\n        }\n        if (fields & SearchInputField.RADIUS) {\n            this.radius = draft.radius;\n        }\n        if (fields & SearchInputField.TYPE) {\n            this.type = draft.type;\n        }\n        if (fields & SearchInputField.SALARY) {\n            this.salary.type = draft.salary.type;\n            DISPLAY_SALARY_TYPE_LIST.forEach((key) => {\n                this.salary.ranges[key].min = draft.salary.ranges[key].min;\n                this.salary.ranges[key].max = draft.salary.ranges[key].max;\n            });\n        }\n        if (fields & SearchInputField.AGE) {\n            this.age = draft.age;\n        }\n        if (fields & SearchInputField.INDEED_APPLY) {\n            this.indeedApply = draft.indeedApply;\n        }\n        if (fields & SearchInputField.SOURCE_TYPES) {\n            this.sourceTypes.splice(0, this.sourceTypes.length, ...draft.sourceTypes);\n        }\n        if (fields & SearchInputField.TAGS) {\n            this.tags.splice(0, this.tags.length, ...draft.tags);\n        }\n        if (fields & SearchInputField.OCCUPATION) {\n            this.occupation = draft.occupation;\n        }\n        if (fields & SearchInputField.LOCATION_OPTION) {\n            this.regionId = draft.regionId;\n            this.areas.splice(0, this.areas.length, ...draft.areas);\n            this.transitLines.splice(0, this.transitLines.length, ...draft.transitLines);\n            this.transitStations.splice(0, this.transitStations.length, ...draft.transitStations);\n        }\n        if (fields & SearchInputField.SEARCH_CRITERIA) {\n            this.searchCriteria = draft.searchCriteria;\n        }\n        (customFields || this.customFieldNames).forEach((fieldName) => {\n            const value = get(draft.customFields, fieldName) as undefined | string[];\n            if (value) {\n                this.setCustomField(fieldName, ...value);\n            }\n        });\n        if (applyAdditionalQueryParams) {\n            this.resetAdditionalQueryParams();\n            keys(draft.additionalQueryParams).forEach((key) =>\n                this.setAdditionalQueryParam(key, ...draft.additionalQueryParams[key])\n            );\n        }\n    };\n\n    @action\n    public applyInitialSearchInput = (\n        initialSearchInput: InitialSearchInput,\n        isOnDesktopSERP: boolean,\n        isDesktopMultiSelectFilter: boolean,\n        hasUnifiedFilters?: boolean\n    ) => {\n        this.keyword = initialSearchInput.keyword;\n        this.location = initialSearchInput.location;\n        this.latLong = initialSearchInput.latLong;\n        this.radius = initialSearchInput.radius;\n        this.type = initialSearchInput.type;\n        this.age = initialSearchInput.age;\n        this.indeedApply = initialSearchInput.indeedApply;\n        this.applySourceTypes(initialSearchInput.searchCriteria);\n        this.salary.type = initialSearchInput.salaryType;\n        DISPLAY_SALARY_TYPE_LIST.forEach((key) => {\n            if (this.salary.type === key) {\n                this.salary.ranges[key].min = initialSearchInput.salaryMin;\n                this.salary.ranges[key].max = initialSearchInput.salaryMax;\n            } else {\n                thi