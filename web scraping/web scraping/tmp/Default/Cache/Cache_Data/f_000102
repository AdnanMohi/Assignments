=> {\n    return !['input', 'button'].includes(element);\n};\n\nconst useDesktopTstInlineWhatEvents = (props: UseDesktopTstInlineWhatEventsProps) => {\n    const { search, recentSearch, config } = useStore();\n    const {\n        onChange,\n        handleNavigation,\n        openSearchOverlay,\n        closeSearchOverlay,\n        unfocusSuggestion,\n        focusNextSuggestion,\n        focusPreviousSuggestion,\n        focusedSuggestion\n    } = props;\n    const { draft } = search;\n    const defaultInputRef = useRef<HTMLInputElement | null>(null);\n    const inputRef = useMemo(() => {\n        return props.inputRef ?? defaultInputRef;\n    }, [defaultInputRef, props.inputRef]);\n    const [showDropdown, setShowDropdown] = useState(false);\n\n    const [draftWhatValueHasChanged, setDraftWhatValueHasChanged] = useState(false);\n\n    const handleBlur = (e?: React.FocusEvent<HTMLElement>) => {\n        unfocusSuggestion();\n        setShowDropdown(false);\n\n        // close overlay if focus is not on sibling input/button\n        const shouldCloseOverlay =\n            !e?.relatedTarget ||\n            (typeof e?.relatedTarget === 'object' &&\n                isInvalidElement(Object.values(e.relatedTarget)[0].elementType));\n\n        if (shouldCloseOverlay) {\n            closeSearchOverlay?.();\n        }\n    };\n\n    const onKeyDown = (event: KeyboardEvent<HTMLInputElement>) => {\n        // Close if shift+tab to leave input what/where inputs\n        if (event.shiftKey && event.key === 'Tab') {\n            closeSearchOverlay?.();\n            return;\n        }\n\n        const keyActions = {\n            ArrowDown: () => {\n                event.preventDefault();\n                focusNextSuggestion();\n            },\n            ArrowUp: () => {\n                event.preventDefault();\n                focusPreviousSuggestion();\n            },\n            Enter: () => {\n                if (focusedSuggestion?.element) {\n                    event.preventDefault();\n                }\n                if (focusedSuggestion?.element && 'suggestion' in focusedSuggestion.element) {\n                    handleWhatSelect(focusedSuggestion.element);\n                } else if (focusedSuggestion?.element && 'href' in focusedSuggestion.element) {\n                    handleSplitSearchClickRecentSearch(focusedSuggestion.element);\n                }\n            }\n        };\n\n        const action = keyActions[event.key as keyof typeof keyActions];\n        action?.();\n    };\n\n    const handleWhatInputChangeForDesktopYosegi = (event: React.ChangeEvent<HTMLInputElement>) => {\n        onChange('input', event.target.value);\n        setDraftWhatValueHasChanged(true);\n        openDropdown();\n        unfocusSuggestion();\n    };\n\n    const focusWhereInput = () => props.whereInputRef?.current?.click();\n\n    const handleWhatSelect = (\n        suggestion: ExtendedAutocompleteItem,\n        _index?: number,\n        _acmeToggleCjsTstGrp?: number\n    ) => {\n        onChange('autoComplete', suggestion.suggestion);\n        unfocusSuggestion();\n        setShowDropdown(false);\n        setTimeout(focusWhereInput);\n    };\n\n    const handleCustomWhatSelect = (suggestion: string, _index: number, _queryFrom: string) => {\n        onChange('input', suggestion);\n    };\n\n    const handleSplitSearchClickRecentSearch = (rc: RecentSearch) => {\n        handleBlur();\n        draft.setKeyword(rc.query ?? '');\n        draft.setLocation(rc.location ?? '');\n        search.commitDraft(SearchInputField.KEYWORD | SearchInputField.LOCATION);\n        handleNavigation?.(rc.href);\n    };\n\n    const openDropdown = () => {\n        openSearchOverlay?.();\n        setShowDropdown(true);\n        if (config.isGlobalHomePage || config.richSearchComponentModel.userLastVisitTime > 0) {\n            recentSearch.fetchNewJobsCountForRecentSearch?.();\n        }\n    };\n\n    const handleClear = () => {\n        inputRef.current?.focus();\n    };\n\n    return {\n        handleBlur,\n        handleWhatSelect,\n        handleSplitSearchClickRecentSearch,\n        onKeyDown,\n        draftWhatValueHasChanged,\n        handleWhatInputChangeForDesktopYosegi,\n        handleCustomWhatSelect,\n        handleClear,\n        showDropdown,\n        openDropdown\n    };\n};\n\nexport { useDesktopTstInlineWhatEvents };\n","import React from 'react';\n\nimport { AutocompleteProps } from '@indeed/autocomplete-ui';\nimport { observer } from 'mobx-react-lite';\n\nimport { useStore } from '../../../context';\nimport { CompanySuggestionPayload } from '../../../util/companyUtils';\n\nimport DesktopTstInlineWhat from './DesktopTstInlineWhat';\n\nexport interface InlineWhatProps extends AutocompleteProps {\n    endpoint: string;\n    onEmployerKeyChange: (employerKey: string | undefined) => void;\n    isCompanySERPEnabled: boolean;\n    handleNavigation?: (href: string, replace?: boolean) => void;\n    openSearchOverlay?: () => void;\n    closeSearchOverlay?: () => void;\n    placeholder: string;\n}\n\n/**\n * Inline version of what form; used on desktop.\n */\nconst InlineWhat = (props: InlineWhatProps) => {\n    const { search, logger, config } = useStore();\n    const { draft } = search;\n\n    const {\n        config: autoCompleteConfig,\n        onEmployerKeyChange,\n        isCompanySERPEnabled,\n        handleNavigation,\n        openSearchOverlay,\n        closeSearchOverlay,\n        placeholder\n    } = props;\n    const { richSearchComponentModel } = config;\n    const { employer, isDsanr } = richSearchComponentModel;\n\n    // What change handlers\n    const handleWhatChange = (\n        source: string,\n        value: string,\n        payload?: CompanySuggestionPayload\n    ) => {\n        if (isCompanySERPEnabled) {\n            if (payload && payload.employerKey) {\n                draft.setEncryptedFccId(payload.employerKey);\n                draft.setCJSSubQuery(payload.subQuery ? payload.subQuery : ''