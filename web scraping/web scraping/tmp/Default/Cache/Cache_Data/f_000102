export const MapOptionsToCheckbox = ({\n    option,\n    index,\n    optionRefs,\n    selections,\n    setSelections,\n    id\n}: MapOptionsToCheckboxProps) => {\n    const ref: React.RefObject<HTMLInputElement> = React.createRef();\n    optionRefs.push(ref);\n\n    const handleChange = (e: React.ChangeEvent<HTMLInputElement>) => {\n        const value = e.target.value;\n        const newSelections = new Set(selections);\n        if (e.target.checked) {\n            newSelections.add(value);\n        } else {\n            newSelections.delete(value);\n        }\n        setSelections(newSelections);\n    };\n\n    return (\n        <Checkbox\n            key={index}\n            id={`${id}-${index}`}\n            ref={ref}\n            label={option.label}\n            name={`${id}-menu`}\n            data-index={index}\n            checked={selections.has(option.value)}\n            value={option.value}\n            onChange={handleChange}\n        />\n    );\n};\n","import React from 'react';\n\nimport { MapOptionsToRadioProps } from '../../../../../../types/OverlayPillOverlaySheet';\nimport { Radio } from '../../../../../shared/ifl-v5-components/Radio/Radio';\n\nexport const MapOptionsToRadio = ({\n    id,\n    index,\n    option,\n    selections,\n    setSelections,\n    toggleOption\n}: MapOptionsToRadioProps) => {\n    return (\n        <Radio\n            key={option.value}\n            id={`${id}-${index}`}\n            label={option.label}\n            name={`${id}-menu`}\n            value={option.value}\n            checked={selections.has(option.value)}\n            onChange={(e) => {\n                toggleOption && selections.has(toggleOption.value)\n                    ? setSelections(new Set([e.target.value, toggleOption.value]))\n                    : setSelections(new Set([e.target.value]));\n            }}\n        />\n    );\n};\n","import React from 'react';\n\nimport { Switch } from '@indeed/ifl-components';\n\nimport { MapToggleToSwitchProps }