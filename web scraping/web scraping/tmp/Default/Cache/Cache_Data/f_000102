atwhere/panes/what/useSuggestions/useSuggestions';\nimport { ExtendedAutocompleteItem } from '../splitwhatwhere/panes/what/WhatOverlayPaneUtils';\nimport { RecentSearch } from '../splitwhatwhere/recentsearch/RecentSearchList';\n\nimport {\n    dropdownSuggestionsWrapperSx,\n    inlineIconSx,\n    inlineWhatInputContainerSx\n} from './InlineWhatWhere.jss';\n\nconst DesktopTstInlineWhat = (props: DesktopTstInlineWhatProps) => {\n    const { search, recentSearch, ui } = useStore();\n    const { pgettext } = useI18n();\n    const { config: autoCompleteConfig, employer, openSearchOverlay, placeholder } = props;\n    const { draft } = search;\n    const { jsj_hp_recentsearchesage_tst, jsj_yosegi_reselect_v2_tst } = useProctorGroups();\n\n    const defaultInputRef = useRef<HTMLInputElement | null>(null);\n    const inputRef = useMemo(() => {\n        return props.inputRef ?? defaultInputRef;\n    }, [defaultInputRef, props.inputRef]);\n    const isRecentSearchesAgeValid = (jsj_hp_recentsearchesage_tst?.value ?? -1) < 1;\n\n    const recentSearches = getRecentSearches(recentSearch, search, isRecentSearchesAgeValid);\n\n    const { refinedSuggestions, sessionId } = useWhatOverlayAutocomplete(\n        draft.keyword,\n        false,\n        false,\n        autoCompleteConfig\n    );\n    const {\n        focusedSuggestion,\n        focusNextSuggestion,\n        focusPreviousSuggestion,\n        unfocusSuggestion\n    } = useSuggestionsFocus<ExtendedAutocompleteItem | RecentSearch>(\n        draft.keyword,\n        jsj_yosegi_reselect_v2_tst.value > 0 ? [] : refinedSuggestions\n    );\n\n    const {\n        handleBlur,\n        handleWhatSelect,\n        handleSplitSearchClickRecentSearch,\n        draftWhatValueHasChanged,\n        handleWhatInputChangeForDesktopYosegi,\n        handleCustomWhatSelect,\n        handleClear,\n        showDropdown,\n        openDropdown,\n        onKeyDown\n    } = useDesktopTstInlineWhatEvents({\n        ...props,\n        focusedSuggestion,