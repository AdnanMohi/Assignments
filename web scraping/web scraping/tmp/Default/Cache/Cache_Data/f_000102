    // ifl/Input uses `ref` property for the Refs for input element\n        setTextInputRefFallback(ref) {\n            if (ref && !this.inputElement.current) {\n                // @ts-ignore\n                this.inputElement.current = ref;\n            }\n            if (this.props.ref) {\n                this.props.ref(ref);\n            }\n        }\n        renderInputAndAutocomplete(input, menu, leftChildren, rightChildren) {\n            if (uncontrolled) {\n                return (React.createElement(React.Fragment, null,\n                    React.createElement(\"div\", { className: 'autocomplete-input-container' },\n                        leftChildren,\n                        input,\n                        rightChildren),\n                    menu));\n            }\n            else {\n                const autocompleteOpenClass = this.state.isAutocompleteOpen\n                    ? { className: 'is-autocomplete-open' }\n                    : '';\n                return (React.createElement(\"div\", Object.assign({}, autocompleteOpenClass),\n                    React.createElement(\"div\", { className: 'autocomplete-input-container' },\n                        leftChildren,\n                        input,\n                        rightChildren),\n                    React.createElement(\"div\", { className: 'autocomplete-container', style: this.props.disableDefaultStyle ||\n                            this.ac.shouldDisableDefaultStyle()\n                            ? {}\n                            : {\n                                zIndex: 2,\n                                position: 'relative',\n                                margin: '0px',\n                                padding: '0px',\n                            } }, menu)));\n            }\n        }\n        /* eslint-disable @typescript-eslint/no-unused-vars */\n        render() {\n            const _a = this.props, { container, config, suggestionsAriaLabel, setCombinedSuggestions, setSel