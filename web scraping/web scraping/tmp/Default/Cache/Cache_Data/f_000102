        state: { state, [this.levelKey]: level }\n        });\n    }\n\n    // eslint-disable-next-line @typescript-eslint/typedef\n    public pop(level = 1) {\n        if (this.isOwnState()) {\n            this.history.go(-1 * level);\n        }\n    }\n\n    /*\n     * This method traverse back all the history stack for the same levelKey\n     */\n    public popAll() {\n        if (this.isOwnState()) {\n            const level = this.history.location.state[this.levelKey];\n            this.history.go(-1 * level);\n        }\n    }\n}\n\nconst createHistoryStack = (props: Props): HistoryStack => {\n    if (props.history) {\n        return new BrowserHistoryStack(props);\n    } else {\n        return new DummyHistoryStack();\n    }\n};\n\nexport default createHistoryStack;\n","export const getMinMaxOfSameDigits = (num: number) => {\n    const isNegative = num < 0;\n    const numDigits = Math.abs(num).toString().length;\n\n    if (numDigits === 1) {\n        return isNegative ? [-9, 0] : [0, 9];\n    }\n\n    const min = 10 ** (numDigits - 1);\n    const max = 10 ** numDigits - 1;\n    return isNegative ? [-max, -min] : [min, max];\n};\n\nexport const randInt = (min: number, max: number) => {\n    if (min > max) {\n        throw RangeError('min should be less than or equal to max');\n    }\n    return Math.floor(Math.random() * (max - min + 1) + min);\n};\n","import noScroll from '@indeed/cmi-jp-common-components/lib/components/util/noscroll';\nimport {\n    fetchNewJobsCount,\n    hasIdenticalSearchParams\n} from '@indeed/cmi-jp-common-components/lib/transport/newJobCount';\nimport debounce from 'lodash.debounce';\nimport { action, autorun, computed, observable } from 'mobx';\n\nimport RichSearchTrackEventLogger from '../logging/RichSearchTrackEventLogger';\nimport { BundleType } from '../types/bundleType';\nimport { Employer } from '../types/employer';\nimport { HostLocation } from '../types/HostLocation';\nimport { FirstClickLogType } from '../types/RichSearchComponentModel';\nimport { getScrollY, isBrowser } from '../util/browser';\nimport { FOREVER, getCookie, setCookie } from '../util/cookie';\nimport createHistoryStack, { HistoryStack } from '../util/historyStack';\nimport localStorage from '../util/localStorage';\nimport { hasNewJobs } from '../util/recentSearch';\nimport { isDisclaimerSkipped, setDisclaimerSkipped } from '../util/savedSearch';\nimport { getMinMaxOfSameDigits, randInt } from '../util/utils';\n\nimport RootStore from './RootStore';\n\nexport const POSTING_FILTERS_CONTAINER_HEIGHT = 48;\n\nexport const GNAV_HEIGHT = 56;\n\nexport const FETCH_DEBOUNCE_RATE = 50;\n\nexport enum PaneType {\n    NONE = '',\n    // Panes for the Rich search component\n    LOCATION_OPTIONS = 'location_options',\n    LOCATION = 'location',\n    CRITERIA = 'criteria',\n    RADIUS = 'radius',\n    // Panes for global search component\n    SPLIT_SEARCH_WHAT = 'split_search_what',\n    SPLIT_SEARCH_WHERE = 'split_search_where',\n    SPLIT_SEARCH_AREA = 'split_search_area',\n    SPLIT_SEARCH_WHERE_FROM_AREA = 'split_search_where_from_area',\n    SPLIT_SEARCH_WHERE_FROM_WHAT = 'split_search_where_from_what',\n    SPLIT_SEARCH_WHERE_SINGLE_LINE = 'split_search_where_single_line',\n    SPLIT_SEARCH_CITY_FROM_WHAT = 'split_search_city_from_what',\n\n    SPLIT_SEARCH_WHAT_FROM_WHERE = 'split_search_what_from_where',\n    SPLIT_SEARCH_WHAT_FROM_SUBURB = 'split_search_what_from_suburb',\n    ALL_FILTERS = 'allFilters',\n    VISUAL_FILTERS_PRIMARY = 'visual_filters_primary',\n    VISUAL_FILTERS_ALL_INDUSTRIES = 'visual_filters_all_industries',\n    VISUAL_FILTERS_CUSTOM_LOCATION = 'visual_filters_custom_location',\n    SPLIT_SEARCH_WHERE_FROM_LOCATION_NUDGE = 'split_search_where_from_location_nudge',\n    SPLIT_SEARCH_CITY_FROM_LOCATION_NUDGE = 'split_search_city_from_location_nudge',\n    LOCATION_NUDGE = 'location_nudge',\n    SPLIT_SEARCH_CITY = 'split_search_city',\n    SPLIT_SEARCH_SUBURB = 'split_search_suburb',\n    SPLIT_SEARCH_SUBUR