Pane: LocationOptionSubPaneType = LocationOptionSubPaneType.NONE;\n\n    @observable\n    public openedCriteriaSubPane: CriteriaSubPaneType = CriteriaSubPaneType.NONE;\n\n    @observable\n    public actualJobsCount: number | null = null;\n\n    @observable\n    public displayedJobsCount: number | null = null;\n\n    /**\n     * Control the display of pane:\n     * (1) when it's empty: show region pane;\n     * (2) when it has 1 element: show prefecture pane, and the element indicates index of region;\n     * (3) when it has 2 elements: show city pane, and these 2 elements indicate index of region and prefecture.\n     */\n    @observable\n    public locationIndices: number[] = [];\n\n    @observable\n    public locationDepth = 1;\n\n    @observable\n    public keywordHasFocus = false;\n\n    @observable\n    public locationHasFocus = false;\n\n    @observable\n    public pendingRadiusRequestId = 0;\n\n    /**\n     * Scroll position of the browser.\n     *\n     * @note This variable is initialized to -Infinity so that any update later would trigger sticky behavior update later.\n     */\n    @observable\n    public browserScrollY = -Infinity;\n\n    public prevBrowserScrollY = -Infinity;\n\n    public scrollEnabled = true;\n\n    @observable\n    public isStickyView = false;\n\n    @observable\n    public isScrolledView = false;\n\n    @observable\n    public isClickedView = false;\n\n    @observable\n    public shrinkedSummaryExpanded = false;\n\n    @observable\n    public recentSearchForceClose = false;\n\n    @observable\n    public recentSearchEditMode = false;\n\n    @observable\n    public isExternalReferer = false;\n\n    @observable\n    public shouldHideRichSearch = false;\n\n    @observable\n    public totalJobCount = 0;\n\n    @observable\n    public locationOptionIntent: LocationOptionIntent = '';\n\n    @observable\n    public locationOptionLabel = '';\n\n    @observable\n    public selectedRegionId = '';\n\n    @computed\n    public get regionIdPresent() {\n        return this.selectedRegionId.split('|').length >= 2;\n    }\n\n    @computed\n    public get hasPendingRadiusRequest() {\n        return this.pendingRadiusRequestId !== 0;\n    }\n\n    @computed\n    public get isRadiusVisible() {\n        const showRadius = this.rootStore.search.draft.appliedRadius >= 0;\n\n        return !this.locationHasFocus && !this.hasPendingRadiusRequest && showRadius;\n    }\n\n    @computed\n    public get isRadiusDisabled() {\n        return this.rootStore.search.draft.appliedRadius < 0;\n    }\n\n    @observable\n    public saveSearchState = SaveSearchState.NONE;\n\n    @observable\n    public saveSearchDisclaimerSkipped = isDisclaimerSkipped();\n\n    @observable\n    public stickyPostingFiltersOffset = -POSTING_FILTERS_CONTAINER_HEIGHT;\n\n    @observable\n    public showFilterHint = false;\n\n    @observable\n    public isShowTransitSearchFeatureGuideTooltip = false;\n\n    @observable\n    public isForceLocationOptionPane = false;\n\n    /**\n     * A snapshot of `isForceLocationOptionPane`, of when we choose what main pane to open.\n     */\n    @observable\n    public shouldAdvertiseMultiLocations = false;\n\n    @observable\n    public isWhatWhereAlertShown = false;\n\n    @observable\n    public isScrolling = false;\n\n    @observable\n    public encryptedFccId: string | undefined;\n\n    @observable\n    public companyJobSearchQuery: string | undefined;\n\n    @observable\n    public companyNameInQuery: string | undefined;\n\n    @observable\n    public employer: Employer | undefined;\n\n    @observable\n    public isShowRemoteSearchCallout = false;\n\n    @observable\n    public isReadyToShowToast = false;\n\n    @observable\n    public isTriggerUseSafariFocusHack = false;\n\n    @computed\n    public get locationPaneType() {\n        switch (this.locationIndices.length) {\n            case 0:\n                return LocationPaneType.REGION_PANE;\n            case 1:\n                return LocationPaneType.PREFECTURE_PANE;\n            default:\n                return LocationPaneType.CITY_PANE;\n        }\n    }\n\n    @computed\n    public get isSaveSearchActionDisabled(): boolean {\n        return (\n            !this.enabled ||\n            !this.rootStore.saveSearch.saveSearchData ||\n            this.rootStore.saveSearch.isProcessing ||\n            !this.rootStore.saveSearch.isInitialized\n        );\n    }\n\n    @computed\n    public get isShowRecentSearch(): boolean {\n        if (this.rootStore.config.bundleType === BundleType.JP) {\n            const {\n                config: {\n                    hostLocation,\n                    richSearchComponentModel: { isLoggedIn, saveSearchModel }\n                }\n            } = this.rootStore;\n\n            if (hostLocation === HostLocation.MOBILE) {\n                return true;\n            }\n\n            // We assume that hostLocation === HostLocation.HOME_PAGE\n            const hasRecentSearch = this.rootStore.recentSearch\n                ? this.rootStore.recentSearch.hasRecentSearch\n                : this.rootStore.config.richSearchComponentModel.hasRecentSearch;\n            return hasRecentSearch || (isLoggedIn && !!saveSearchModel);\n        }\n\n        return this.rootStore.config.richSearchComponentModel.isShowRecentSearch;\n    }\n\n    public constructor(rootStore: RootStore) {\n        this.rootStore = rootStore;\n        this.historyStack = null;\n        this.isScrolledView = false;\n        this.isClickedView = false;\n\n        const {\n            richSearchComponentModel: { mobtk, isShowFilterHint }\n        } = this.rootStore.config;\n\n        this.mobtk = mobtk;\n        this.showFilterHint = isShowFilterHint;\n        this.recentSearchEditMode = true;\n\n        autorun(() => {\n            if (this.openedPane === PaneType.LOCATION_OPTIONS) {\n                this.evaluateAdvertiseMultiLocations();\n            }\n        });\n\n        // istanbul ignore else\n        if (isBrowser()) {\n            window.addEventListener('scroll', this.scrollListener);\n        }\n    }\n\n    public dispose() {\n        window.removeEventListener('scroll', this.scrollListener);\n    }\n\n    public initHistoryStack(paneHistory: any) {\n        this.historyStack = createHistoryStack({\n            levelKey: 'RichSearch',\n            history: paneHistory\n        });\n    }\n\n    @action\n    public setOpenedPane = (paneType: PaneType) => {\n        this.openedPane = paneType;\n    };\n\n    @action\n    public setSelectedRegionId = (id: string) => {\n        this.selectedRegionId = id;\n    };\n\n    @action\n    public setLocationOptionIntent = (intent: LocationOptionIntent) => {\n        this.locationOptionIntent = intent;\n    };\n\n    @action\n    public setLocationOptionLabel = (label: string) => {\n        this.locationOptionLabel = label;\n    };\n\n    @action\n    public setOpenedLocationOptionSubPane = (paneType: LocationOptionSubPaneType) => {\n        this.openedLocationOptionSubPane = paneType;\n    };\n\n    @action\n    public setOpenedCriteriaSubPane = (paneType: CriteriaSubPaneType) => {\n        this.openedCriteriaSubPane = paneType;\n    };\n\n    @action\n    public setActualJobsCount(value: number | null) {\n        this.actualJobsCount = value;\n    }\n\n    @action\n    public setDisplayedJobsCount(value: number | null) {\n        this.displayedJobsCount = value;\n    }\n\n    @action\n    public pushLocationIndex = (...value: number[]) => {\n        this.locationIndices.push(...value);\n        this.locationDepth++;\n    };\n\n    @action\n    public popLocationIndex = (count: number) => {\n        this.locationIndices.splice(Math.max(0, this.locationIndices.length - count));\n        this.locationDepth--;\n    };\n\n    @action\n    public clearLocationIndex = () => {\n        this.locationIndices.splice(0);\n        this.locationDepth = 1;\n    };\n\n    @action\n    public setKeywordHasFocus = (focused: boolean) => {\n        this.keywordHasFocus = focused;\n    };\n\n    @action\n    public setLocationHasFocus = (focused: boolean) => {\n        this.locationHasFocus = focused;\n    };\n\n    @action\n    public setShrinkedSummaryExpanded = (value: boolean) => {\n        this.shrinkedSummaryExpanded = value;\n    };\n\n    @action\n    public updateBrowserScrollPosition = () => {\n        // Disregard any scroll event when noscroll is enabled, so that it does not affect any state based on scroll position.\n        // As a matter of fact, the event is not triggered in iOS 13 or later.\n        if (isBrowser() && !this.scrollEnabled) {\n            return;\n        }\n\n        this.prevBrowserScrollY = this.browserScrollY;\n        this.browserScrollY = getScrollY();\n    };\n\n    @action\n    public forceCloseRecentSearch = () => {\n        this.recentSearchForceClose = true;\n    };\n\n    @action\n    public unforceRecentSearch = () => {\n        this.recentSearchForceClose = false;\n    };\n\n    @action\n    public showEmptyWhatWhereAlert = () => {\n        this.isWhatWhereAlertShown = true;\n    };\n\n    @action\n    public hideEmptyWhatWhereAlert = () => {\n        this.isWhatWhereAlertShown = false;\n    };\n\n    @action\n    public initializeFilterHint = () => {\n        const val = getCookie('noFilterHint');\n        if (val === '1') {\n            this.showFilterHint = false;\n        }\n    };\n\n    @action\n    public initializeTransitSearchFeatureGuideTooltip = () => {\n        const { transitSearchPayload } = this.rootStore.config.richSearchComponentModel;\n        if (\n            transitSearchPayload?.supportMLoc &&\n            localStorage.get(FEATURE_GUIDE_TOOLTIP_DISMISSED_KEY) !== '1'\n        ) {\n            this.isShowTransitSearchFeatureGuideTooltip = true;\n        }\n    };\n\n    @action\n    public initializeForceLocationOptionPane = () => {\n        const { transitSearchPayload } = this.rootStore.config.richSearchComponentModel;\n        if (\n            transitSearchPayload?.supportMLoc &&\n            localStorage.get(FORCED_LOCATION_OPTION_PANE_STOPPED_KEY) !== '1'\n        ) {\n            this.isForceLocationOptionPane = true;\n        }\n    };\n\n    @action\n    public evaluateAdvertiseMultiLocations = () => {\n        // Takes a snapshot of `isForceLocationOptionPane`.\n        // Should be called when `PaneType.LOCATION_OPTIONS` opens.\n        this.shouldAdvertiseMultiLocations = this.isForceLocationOptionPane;\n    };\n\n    @action\n    public dismissFilterHint = () => {\n        this.showFilterHint = false;\n        setCookie('noFilterHint', '1', FOREVER);\n    };\n\n    @action\n    public dismissTransitSearchFeatureGuideTooltip = () => {\n        this.isShowTransitSearchFeatureGuideTooltip = false;\n        if (localStorage.isAvailable()) {\n            localStorage.set(FEATURE_GUIDE_TOOLTIP_DISMISSED_KEY, '1');\n        }\n    };\n\n    @action\n    public stopForceLocationOptionPane = () => {\n        this.isForceLocationOptionPane = false;\n        if (localStorage.isAvailable()) {\n            localStorage.set(FORCED_LOCATION_OPTION_PANE_STOPPED_KEY, '1');\n        }\n    };\n\n    @action\n    public updateDisplayedJobsCount = debounce(() => {\n        if (this.rootStore.search.draft.isSearchEnabled) {\n            this._updateDisplayedJobsCount();\n        }\n    }, FETCH_DEBOUNCE_RATE);\n\n    @action\n    public renderDisplayedJobsCount = () => {\n        if (this.rootStore.search.draft.isSearchEnabled) {\n            this.updateDisplayedJobsCount();\n        } else {\n            this.stopDisplayedJobsCountCycling();\n            this.resetDisplayedJobsCount();\n        }\n    };\n\n    private readonly scrollListener = debounce(\n        this.updateBrowserScrollPosition,\n        SCROLL_DEBOUNCE_RATE\n    );\n\n    @computed\n    public get asState() {\n        return {\n            openedPane: this.openedPane,\n            selectedRegionId: this.selectedRegionId,\n            locationOptionIntent: this.locationOptionIntent,\n            openedLocationOptionSubPane: this.openedLocationOptionSubPane,\n            openedCriteriaSubPane: this.openedCriteriaSubPane,\n            locationIndices: [...this.locationIndices],\n            locationDepth: this.locationDepth,\n            shrinkedSummaryExpanded: this.shrinkedSummaryExpanded\n        };\n    }\n\n    @action\n    public restoreFromState = (state: this) => {\n        this.openedPane = state.openedPane;\n        this.selectedRegionId = state.selectedRegionId;\n        this.locationOptionIntent = state.locationOptionIntent;\n        this.openedLocationOptionSubPane = state.openedLocationOptionSubPane;\n        this.openedCriteriaSubPane = state.openedCriteriaSubPane;\n        this.openedLocationOptionSubPane = state.openedLocationOptionSubPane;\n        this.locationOptionIntent = state.locationOptionIntent;\n        this.locationIndices.splice(0, this.locationIndices.length, ...state.locationIndices);\n        this.locationDepth = state.locationDepth;\n        this.shrinkedSummaryExpanded = state.shrinkedSummaryExpanded;\n    };\n\n    @action\n    public openSaveSearchLoginModal = () => {\n        this.saveSearchState = SaveSearchState.LOGIN_MODAL;\n    };\n\n    @action\n    public openSaveSearchDisclaimer = () => {\n        this.saveSearchState = SaveSearchState.DISCLAIMER_MODAL;\n    };\n\n    @action\n    public displaySaveSearchToast = () => {\n        this.saveSearchState = SaveSearchState.SAVE_TOAST;\n    };\n\n    @action\n    public displayUnsaveSearchToast = () => {\n        this.saveSearchState = SaveSearchState.UNSAVE_TOAST;\n    };\n\n    @action\n    public resetSaveSearchState = () => {\n        this.saveSearchState = SaveSearchState.NONE;\n    };\n\n    @action\n    public resetDisplayedJobsCount = () => {\n        this.setDisplayedJobsCount(null);\n    };\n\n    @action\n    public skipSaveSearchDisclaimer = (disclaimerSkipped: boolean) => {\n        this.saveSearchDisclaimerSkipped = disclaimerSkipped;\n        setDisclaimerSkipped(this.saveSearchDisclaimerSkipped);\n    };\n\n    @action\n    public setStickyPostingFiltersOffset = (stickyPostingFiltersOffset: number) => {\n        this.stickyPostingFiltersOffset = stickyPostingFiltersOffset;\n    };\n\n    @action\n    public disableScroll = () => {\n        noScroll.on();\n        this.scrollEnabled = false;\n    };\n\n    @action\n    public enableScroll = (scrollY?: number) => {\n        noScroll.off();\n        // We need to wait to ignore scroll events triggered by noScroll library\n        const conScrollComplete = () => {\n            this.updateBrowserScrollPosition();\n            this.scrollEnabled = true;\n            this.isScrolling = false;\n        };\n\n        if (scrollY !== undefined) {\n            window.setTimeout(() => {\n                window.scrollTo(0, scrollY);\n                this.isScrolling = true;\n                window.setTimeout(conScrollComplete, 0);\n            });\n        } else {\n            window.setTimeout(conScrollComplete, 0);\n        }\n    };\n\n    @action\n    public setStickyView = (isStickyView: boolean) => {\n        this.isStickyView = isStickyView;\n    };\n\n    @action\n    public setScrolledView = (isScrolledView: boolean) => {\n        this.isScrolledView = isScrolledView;\n    };\n\n    @action\n    public setClickedView = (isClickedView: boolean) => {\n        this.isClickedView = isClickedView;\n    };\n\n    @action\n    public toggleRecentSearchEditMode = () => {\n        this.recentSearchEditMode = !this.recentSearchEditMode;\n    };\n\n    @action\n    public setRecentSearchEditMode = (editMode: boolean) => {\n        this.recentSearchEditMode = editMode;\n    };\n\n    @action setReadyToShowToast = (ready: boolean) => {\n        this.isReadyToShowToast = ready;\n    };\n\n    @action\n    public setTriggerUseSafariFocusHack = (isSafariTriggered: boolean) => {\n        this.isTriggerUseSafariFocusHack = isSafariTriggered;\n    };\n\n    @computed\n    public get hasNewArrivalJobs(): boolean {\n        const currentRecentSearch = this.rootStore.recentSearch.currentRecentSearch;\n        if (!currentRecentSearch) {\n            return false;\n        }\n\n        return hasNewJobs(currentRecentSearch.ts, currentRecentSearch.pts);\n    }\n\n    public restoreStateFromHistory() {\n        if (!this.historyStack) {\n            return;\n        }\n\n        const state = this.historyStack.getState();\n        if (state !== null) {\n            if (state.ui) {\n                this.restoreFromState(state.ui);\n            }\n        } else {\n            this.setOpenedPane(PaneType.NONE);\n            this.setSelectedRegionId('');\n            this.setLocationOptionIntent('');\n            this.setOpenedLocationOptionSubPane(LocationOptionSubPaneType.NONE);\n            this.setOpenedCriteriaSubPane(CriteriaSubPaneType.NONE);\n            this.setLocationOptionIntent('');\n        }\n\n        this.updateBrowserScrollPosition();\n    }\n\n    public pushHistory() {\n        if (!this.historyStack) {\n            return;\n        }\n\n        const state = {\n            ui: this.asState\n        };\n        this.historyStack.push(state);\n    }\n\n    public replaceHistory() {\n        if (!this.historyStack) {\n            return;\n        }\n\n        const state = {\n            ui: this.asState\n        };\n        this.historyStack.replace(state);\n    }\n\n    public popHistory(level?: number) {\n        if (!this.historyStack) {\n            return;\n        }\n\n        this.historyStack.pop(level);\n    }\n\n    public popAllHistory() {\n        if (!this.historyStack) {\n            return;\n        }\n\n        this.historyStack.popAll();\n    }\n\n    public resetLogSource = () => {\n        this.setClickedView(false);\n        this.setScrolledView(false);\n    };\n\n    public logFirstClick(logger: RichSearchTrackEventLogger, value: FirstClickLogType) {\n        if (!this.enabled) {\n            return;\n        }\n\n        if (this.isClickedView === false) {\n            const source = (() => {\n                if (this.isStickyView) {\n                    return 'stickyView';\n                }\n                return this.isScrolledView ? 'scrolledView' : 'initialLoadView';\n            })();\n            logger.log(source, 'firstClick', value);\n            this.setClickedView(true);\n        }\n    }\n\n    public startDisplayedJobsCountCycling() {\n        const startTime = new Date().getTime();\n\n        // before setting a new interval, clear the existing one first and reset the count\n        if (this.displayedJobsCountCyclingInterval) {\n            clearInterval(this.displayedJobsCountCyclingInterval);\n            this.setDisplayedJobsCount(this.actualJobsCount);\n        }\n\n        this.displayedJobsCountCyclingInterval = window.setInterval(() => {\n            // no response received after FILTERED_JOBS_COUNT_ANIMATION_TIME_LIMIT seconds\n            if (\n                new Date().getTime() - startTime > FILTERED_JOBS_COUNT_ANIMATION_TIME_LIMIT &&\n                this.displayedJobsCountCyclingInterval\n            ) {\n                clearInterval(this.displayedJobsCountCyclingInterval);\n                this.setDisplayedJobsCount(this.actualJobsCount);\n                return;\n            }\n\n            // start changing displayedJobsCount, randomly\n            if (this.displayedJobsCount === null || this.displayedJobsCount === undefined) {\n                this.setDisplayedJobsCount(\n                    randInt(DEFAULT_FILTERED_JOBS_COUNT_MIN, DEFAULT_FILTERED_JOBS_COUNT_MAX)\n                );\n            } else {\n                const [min, max] = getMinMaxOfSameDigits(this.displayedJobsCount);\n                this.setDisplayedJobsCount(randInt(min, max));\n            }\n        }, FILTERED_JOBS_COUNT_ANIMATION_INTERVAL);\n    }\n\n    public stopDisplayedJobsCountCycling() {\n        if (this.displayedJobsCountCyclingInterval) {\n            clearInterval(this.displayedJobsCountCyclingInterval);\n        }\n    }\n\n    private _updateDisplayedJobsCount() {\n        const requestTimestamp = Date.now();\n        const clientUrlParams = this.rootStore.search.draft.searchHrefQueryString;\n        const url = `/m/newjobs?${clientUrlParams}`;\n        this.startDisplayedJobsCountCycling();\n\n        fetchNewJobsCount(url)\n            .then((newJobsCount) => {\n                if (\n                    requestTimestamp > (this.lastCompletedJobCountRequestTimestamp || 0) &&\n                    hasIdenticalSearchParams(newJobsCount.key, clientUrlParams)\n                ) {\n                    this.setActualJobsCount(newJobsCount.newCount);\n                    if (this.rootStore.search.draft.isSearchEnabled) {\n                        this.setDisplayedJobsCount(newJobsCount.newCount);\n                    }\n\n                    this.lastCompletedJobCountRequestTimestamp = requestTimestamp;\n                    if (this.displayedJobsCountCyclingInterval) {\n                        clearInterval(this.displayedJobsCountCyclingInterval);\n                    }\n                }\n            })\n            .catch((err) => {\n                // non 200 OK response received, or when client goes offline during animation\n                if (this.displayedJobsCountCyclingInterval) {\n                    clearInterval(this.displayedJobsCountCyclingInterval);\n                    this.setDisplayedJobsCount(null);\n                }\n            });\n    }\n}\n","export enum JsonResultStatus {\n    SUCCESS = 'success',\n    FAILURE = 'failure',\n    FATAL_ERROR = 'fatal error'\n}\n\nexport interface JsonResult<T> {\n    status: JsonResultStatus;\n    message?: string;\n    body?: T;\n}\n","import RichSearchTrackEventLogger from '../logging/RichSearchTrackEventLogger';\nimport { JsonResult, JsonResultStatus } from '../types/api';\nimport {\n    GET_REQUEST_OPTIONS,\n    LineAlertFormData,\n    UpdateLineAlertRequestData\n} from '../types/savedSearch';\n\nconst tryToJSON = (response: Response) => {\n    const getText = response.text();\n    try {\n        return getText.then((text) => {\n            try {\n                return JSON.parse(text);\n            } catch (e) {\n                return getText;\n            }\n        });\n    } catch (e) {\n        return getText;\n    }\n};\n\nconst handleRequest = <T>(url: string, method = 'GET', options = {}): Promise<T> =>\n    fetch(url, { method, ...options })\n        .then((response) => {\n            const contentType = response.headers.get('content-type');\n            if (contentType && contentType.indexOf('application/json') !== -1) {\n                return response.json();\n            }\n            return tryToJSON(response);\n        })\n        .catch((e) => {\n            // TODO: global fetch error handling\n            throw e;\n        });\n\nexport const apiPath = (pathName: string, apiPaths?: Record<string, string>) =>\n    apiPaths ? apiPaths[pathName] : undefined;\n\nexport const getJsonResultBody = <T>(response: JsonResult<T>) => {\n    if (response.status !== JsonResultStatus.SUCCESS) {\n        throw new Error(`status: ${response.status}`);\n    }\n    if (!response.body) {\n        throw new Error('no body defined');\n    }\n    return response.body;\n};\n\n/**\n * Try to access to the URL with GET method and parse JSON response.\n *\n * Be careful that this function ignores some HTTP status codes\n * and may just return a string if it's not JSON format.\n * For example, if you call this function with a wrong URL,\n * then the returned Promise is not rejected but resolved\n * with a text that represents 404 message HTML.\n *\n * @param {url}\n * @param {options} additional options to pass to fetch()\n * @returns a Promise with a parsed object or something else\n */\nexport const fetchJson = <T>(url: string, options = {}): Promise<T> =>\n    handleRequest(url, 'GET', { credentials: 'same-origin', ...options });\n\nexport const postJson = <T>(url: string, options = {}): Promise<T> =>\n    handleRequest(url, 'POST', { credentials: 'same-origin', ...options });\n\n/**\n * @returns a Promise of the response. In case of timeout, the response will throw an error with name 'AbortError'\n */\nexport const fetchWithTimeout = async (\n    url: string,\n    options = {},\n    timeoutInMs = 5000\n): Promise<Response> => {\n    const abortController = new AbortController();\n    const id = setTimeout(() => abortController.abort(), timeoutInMs);\n    const response = await fetch(url, { ...options, signal: abortController.signal });\n    clearTimeout(id);\n    return response;\n};\n\nexport const fetchLineUser = async (\n    lineAlertsApiUrl: string,\n    logger: RichSearchTrackEventLogger\n): Promise<string> => {\n    try {\n        const response = await fetchWithTimeout(\n            `${lineAlertsApiUrl}/line/encrypt`,\n            GET_REQUEST_OPTIONS,\n            10000\n        );\n        if (response.ok) {\n            const userId = await response.text();\n            logger.log('JobAlertToggles', 'fetchLineUser', userId ? 'fetched' : 'fetched-empty');\n            return userId;\n        }\n        logger.log('JobAlertToggles', 'fetchLineUser', response.status);\n    } catch (err) {\n        // @ts-ignore\n        logger.log('JobAlertToggles', 'fetchLineUser', err.name);\n    }\n    return '';\n};\n\nexport const fetchLineAlerts = async (\n    lineAlertsApiUrl: string,\n    method: 'POST' | 'PATCH',\n    payload: LineAlertFormData | UpdateLineAlertRequestData,\n    mode: RequestMode = 'cors'\n): Promise<Response> =>\n    fetch(lineAlertsApiUrl, {\n        method,\n        mode,\n        headers: {\n            'Content-type': 'application/json'\n        },\n        body: JSON.stringify(payload)\n    });\n","export enum HostLocation {\n    HOME_PAGE = 'HOME_PAGE',\n    MOBILE = 'MOBILE',\n    DESKTOP = 'DESKTOP'\n}\n","import