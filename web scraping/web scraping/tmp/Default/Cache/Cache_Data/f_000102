te } from 'react';\n\nimport { UseSuggestionFocus } from '../types/UseSuggestionFocus';\nimport { focusSuggestionIndex } from '../util/focusSuggestionIndex';\nimport { getFocusedSuggestion } from '../util/getFocusedSuggestion';\n\n/**\n * React Hook for handling the state of focusing a suggestion.\n *\n * <p>Whenever the text value changes, the focus will be removed.\n *\n * @param input the input for searching against\n * @param suggestions the suggestions to focus on\n */\nexport const useSuggestionsFocus = <T>(input: string, suggestions?: T[]): UseSuggestionFocus<T> => {\n    const [selectedSuggestionIndex, setSelectedSuggestionIndex] = useState<number | null>(null);\n\n    useEffect(() => {\n        // Any changes to the input should un-focus the suggestion\n        setSelectedSuggestionIndex(null);\n    }, [input]);\n\n    const focusNextSuggestion = useCallback(() => {\n        const nextIndex = focusSuggestionIndex('next', selectedSuggestionIndex, suggestions);\n        if (typeof nextIndex === 'number') {\n            setSelectedSuggestionIndex(nextIndex);\n        }\n    }, [suggestions, selectedSuggestionIndex, setSelectedSuggestionIndex]);\n\n    const focusPreviousSuggestion = useCallback(() => {\n        const prevIndex = focusSuggestionIndex('previous', selectedSuggestionIndex, suggestions);\n        if (typeof prevIndex === 'number') {\n            setSelectedSuggestionIndex(prevIndex);\n        }\n    }, [suggestions, selectedSuggestionIndex, setSelectedSuggestionIndex]);\n\n    const unselectSuggestion = useCallback(() => setSelectedSuggestionIndex(null), [\n        setSelectedSuggestionIndex\n    ]);\n\n    const focusSuggestionAtIndex = useCallback(\n        (index) => {\n            setSelectedSuggestionIndex(index);\n        },\n        [setSelectedSuggestionIndex]\n    );\n\n    return {\n        focusedSuggestion: getFocusedSuggestion(selectedSuggestionIndex, suggestions),\n        focusNextSuggestion,\n        focusPreviousSuggestion,\n      