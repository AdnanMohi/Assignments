ants';\nimport {\n    createSavedSearch,\n    deleteSavedSearch,\n    existsSavedSearch,\n    getSavedSearches,\n    getSavedSearch,\n    getSavedSearchesCount,\n    updateSaveSearchTimestamp,\n    clearCachedSavedSearches\n} from '@indeed/cmi-jp-common-components/lib/util/savedSearches/savedSearches';\nimport { getSavedSearchJobCountUrl } from '@indeed/cmi-jp-common-components/lib/util/savedSearches/urls';\nimport {\n    compareSavedSearchData,\n    isEmailAlertAction,\n    isLineAlertAction\n} from '@indeed/cmi-jp-common-components/lib/util/savedSearches/utils';\nimport { action, computed, IReactionDisposer, observable, reaction, toJS } from 'mobx';\n\nimport { fetchLineUser } from '../transport/fetch';\nimport {\n    createJobAlert,\n    createLineJobAlert,\n    deleteJobAlert,\n    deleteLineJobAlert,\n    generateAlertFormData,\n    generateLineAlertFormData,\n    generateLineNewUserJobAlertCreationUrl,\n    redirectToLineNewUserJobAlertCreationPage\n} from '../transport/jobAlert';\nimport { LineAlertTestType } from '../types/RichSearchComponentModel';\nimport {\n    AlertFormData,\n    LineAlertFormData,\n    SavedSearchInState,\n    JobAlertType,\n    SaveSearchStatus,\n    JobAlert,\n    DeletedJobAlert\n} from '../types/savedSearch';\nimport { generateSaveSearchData, getDeleteLineAlertRequestData } from '../util/savedSearch';\n\nimport RootStore from './RootStore';\n\nfunction matchesSavedSearch(\n    savedSearchInState: SavedSearchInState,\n    {\n        ts,\n        data\n    }: {\n        ts: number;\n        data: SavedSearchData;\n    }\n) {\n    const tsInState = savedSearchInState.search.ts\n        ? savedSearchInState.search.ts\n        : savedSearchInState.search.date;\n\n    return tsInState === ts && compareSavedSearchData(savedSearchInState.search.data, data);\n}\n\nexport default class SaveSearchStore {\n    private findIndex(ts: number, data: SavedSearchData) {\n        return this.savedSearches.findIndex((savedSearchInState) =>\n            matchesSavedSearch(savedSearchInState, { ts, data })\n        );\n    }\n\n    public readonly rootStore: RootStore;\n\n    @observable\n    public saved: boolean;\n\n    @observable\n    public isProcessing: boolean;\n\n    @observable\n    public saveSearchEmailStatus: SaveSearchStatus;\n\n    @observable\n    public saveSearchLineStatus: SaveSearchStatus;\n\n    @observable\n    public isInitialized: boolean;\n\n    @observable\n    public savedSearches: SavedSearchInState[];\n\n    @observable\n    public isSaveEmailJobAlert: boolean;\n\n    @observable\n    public isSaveLineJobAlert: boolean;\n\n    @observable\n    public alertActionType: JobAlertActionType;\n\n    @observable\n    public encryptedLineUserId: string;\n\n    public isLineUserFetched: boolean;\n    private readonly disposers: IReactionDisposer[] = [];\n\n    public constructor(rootStore: RootStore) {\n        this.rootStore = rootStore;\n        this.saved = false;\n        this.isProcessing = false;\n        this.isInitialized = false;\n        this.savedSearches = [];\n        this.isLineUserFetched = false;\n        this.encryptedLineUserId = '';\n        this.isSaveEmailJobAlert = true;\n        this.saveSearchEmailStatus = SaveSearchStatus.NONE;\n        this.saveSearchLineStatus = SaveSearchStatus.NONE;\n        const saveSearchModel = this.rootStore.config.richSearchComponentModel.saveSearchModel;\n        this.isSaveLineJobAlert =\n            LineAlertTestType.BELOW_SELECTED ===\n            (saveSearchModel ? saveSearchModel.lineAlertTestType : undefined);\n        this.alertActionType = JobAlertActionType.EMAIL_ONLY;\n    }\n\n    public initReactions() {\n        clearCachedSavedSearches();\n        this.updateSaveSearchTimestamp();\n        this.updateSavedSearches();\n        void this.fetchEncryptedLineUserId();\n\n        const saveSearchDataReactionDisposer = reaction(\n            () => this.saveSearchData,\n            (saveSearchData) => {\n                this.isInitialized = true;\n                this.saved =\n                    this.rootStore.config.richSearchComponentModel.isLoggedIn &&\n                    existsSavedSearch(saveSearchData);\n            },\n            { fireImmediately: true }\n        );\n        const savedReactionDisposer = reaction(\n            () => this.saved,\n            (saved) => {\n                this.rootStore.externalStore.searchSaved = saved;\n            }\n        );\n        const isProcessingReactionDisposer = reaction(\n            () => this.isProcessing,\n            (isProcessing) => {\n                this.rootStore.externalStore.isSaveSearchProcessing = isProcessing;\n            }\n        );\n\n        const saveSearchEmailStatusReactionDisposer = reaction(\n            () => this.saveSearchEmailStatus,\n            (status) => {\n                this.rootStore.externalStore.saveSearchEmailStatus = status;\n            }\n        );\n\n        const saveSearchLineStatusReactionDisposer = reaction(\n            () => this.saveSearchLineStatus,\n            (status) => {\n                this.rootStore.externalStore.saveSearchLineStatus = status;\n            }\n        );\n\n        this.disposers.push(\n            saveSearchDataReactionDisposer,\n            savedReactionDisposer,\n            isProcessingReactionDisposer,\n            saveSearchEmailStatusReactionDisposer,\n            saveSearchLineStatusReactionDisposer\n        );\n    }\n\n    logSave() {\n        this.rootStore.logger.log('SaveSearchStore', 'save', getSavedSearchesCount());\n    }\n\n    logUnsave() {\n        this.rootStore.logger.log('SaveSearchStore', 'unsave', getSavedSearchesCount());\n    }\n\n    @computed\n    public get hasSavedSearch(): boolean {\n        return this.savedSearches.length > 0;\n    }\n\n    @computed\n    public get saveSearchData(): SavedSearchData | null {\n        if (!this.rootStore.initialSearchInput) {\n            return null;\n        }\n\n        const {\n            keyword,\n            location,\n            radius,\n            type,\n            salaryType,\n            salaryMin,\n            salaryMax,\n            tags,\n            occupation,\n            searchCriteria\n        } = this.rootStore.initialSearchInput;\n\n        return generateSaveSearchData({\n            keyword,\n            location,\n            radius,\n            type,\n            salaryType,\n            salaryMin,\n            salaryMax,\n            tags,\n            occupation,\n            searchCriteria\n        });\n    }\n\n    public createAlertFormData(pos?: string): AlertFormData {\n        if (!this.saveSearchData) {\n            return {};\n        }\n        return generateAlertFormData(\n            this.rootStore.config.richSearchComponentModel.mobtk,\n            this.saveSearchData,\n            pos\n        );\n    }\n\n    @computed\n    public get lineAlertFormData(): LineAlertFormData | null {\n        const saveSearchModel = this.rootStore.config.richSearchComponentModel.saveSearchModel;\n        if (!saveSearchModel) {\n            return null;\n        }\n\n        const { country, language, mobtk } = this.rootStore.config.richSearchComponentModel;\n        return generateLineAlertFormData(\n            this.saveSearchData,\n            this.encryptedLineUserId,\n            language,\n            country,\n            saveSearchModel.lineAlertTestType,\n            mobtk\n        );\n    }\n\n    @computed\n    public get lineNewUserJobAlertCreationUrl(): string | null {\n        return generateLineNewUserJobAlertCreationUrl(\n            this.rootStore.config.richSearchComponentModel.saveSearchModel,\n            this.saveSearchData,\n            this.rootStore.config.richSearchComponentModel.mobtk\n        );\n    }\n\n    @computed\n    public get recentSearchTimestamp() {\n        if (!this.rootStore.recentSearch.currentRecentSearch) {\n            return {\n                ts: 0,\n                pts: 0\n            };\n        }\n        const currentRecentSearch = this.rootStore.recentSearch.currentRecentSearch;\n\n        return {\n            ts: currentRecentSearch.ts ? parseInt(currentRecentSearch.ts, 10) : 0,\n            pts: currentRecentSearch.pts ? parseInt(currentRecentSearch.pts, 10) : 0\n        };\n    }\n\n    @action\n    public fetchEncryptedLineUserId = (): Promise<string> => {\n        if (this.isLineUserFetched) {\n            return Promise.resolve(this.encryptedLineUserId);\n        } else {\n            const lineAlertsApiUrl = this.rootStore.config.richSearchComponentModel.saveSearchModel\n                ?.lineAlertsApiUrl;\n            if (!lineAlertsApiUrl) {\n                return Promise.resolve('');\n            }\n\n            return fetchLineUser(lineAlertsApiUrl, this.rootStore.logger)\n                .then((result) => {\n                    this.fetchLineUserSuccess(result);\n                    return result;\n                })\n                .catch(() => '');\n        }\n    };\n\n    @action\n    private readonly fetchLineUserSuccess = (result: string) => {\n        this.encryptedLineUserId = result;\n        this.isLineUserFetched = true;\n    };\n\n    @action\n    private readonly setIsProcessing = (value: boolean) => {\n        this.isProcessing = value;\n    };\n\n    @action\n    public setSaveSearchStatusByType(type: JobAlertType, status: SaveSearchStatus) {\n        if (type === JobAlertType.EMAIL) {\n            this.saveSearchEmailStatus = status;\n        } else if (type === JobAlertType.LINE) {\n            this.saveSearchLineStatus = status;\n        }\n    }\n\n    @action\n    public setSaveSearchStatusByAction = (\n        actionType: JobAlertActionType,\n        status: SaveSearchStatus\n    ) => {\n        if (isLineAlertAction(actionType)) {\n            this.saveSearchEmailStatus = status;\n        }\n        if (isEmailAlertAction(actionType)) {\n            this.saveSearchLineStatus = status;\n        }\n    };\n\n    @action\n    public setIsSaveLineAlert = (value: boolean) => {\n        this.isSaveLineJobAlert = value;\n    };\n\n    @action\n    public setIsSaveEmailJobAlert = (value: boolean) => {\n        this.isSaveEmailJobAlert = value;\n    };\n\n    /*\n     * If savedSearchData is given, job alert and saved search will be created based on it (the case of VJ)\n     * otherwise, they will be created based on internal state of YOSEGI (the case of SERP)\n     */\n    @action\n    public saveSearch = (pos?: string) => {\n        const { saveSearchModel } = this.rootStore.config.richSearchComponentModel;\n        if (!saveSearchModel || !this.saveSearchData) {\n            return;\n        }\n\n        let alertType = JobAlertActionType.NONE;\n        if (this.isSaveEmailJobAlert && this.isSaveLineJobAlert) {\n            alertType = JobAlertActionType.EMAIL_AND_LINE;\n        } else if (this.isSaveEmailJobAlert) {\n            alertType = JobAlertActionType.EMAIL_ONLY;\n        } else if (this.isSaveLineJobAlert) {\n            alertType = JobAlertActionType.LINE_ONLY;\n        }\n\n        if (alertType !== JobAlertActionType.NONE) {\n            this.createJobAlerts(alertType, pos);\n        } else {\n            // eslint-disable-next-line @typescript-eslint/no-floating-promises\n            createSavedSearch(this.saveSearchData, this.recentSearchTimestamp, null, null);\n            this.saved = true;\n            this.rootStore.ui.displaySaveSearchToast();\n            this.updateSavedSearches();\n            this.logSave();\n        }\n    };\n\n    @action\n    public createJobAlerts = (alertActionType: JobAlertActionType, pos?: string) => {\n        if (alertActionType === JobAlertActionType.NONE) return;\n\n        const { saveSearchModel } = this.rootStore.config.richSearchComponentModel;\n        if (!saveSearchModel) {\n            return;\n        }\n\n        const saveAlertPromises = [];\n        this.isProcessing = true;\n        this.alertActionType = alertActionType;\n        if (isEmailAlertAction(alertActionType)) {\n            saveAlertPromises.push(createJobAlert(this.createAlertFormData(pos)));\n            this.saveSearchEmailStatus = SaveSearchStatus.PROCESSING;\n            this.isSaveEmailJobAlert = true;\n        }\n\n        if (isLineAlertAction(alertActionType) && !!this.encryptedLineUserId) {\n            saveAlertPromises.push(\n                createLineJobAlert(\n                    saveSearchModel.lineAlertsApiUrl,\n                    this.lineAlertFormData,\n                    this.rootStore.logger\n                )\n            );\n            this.saveSearchLineStatus = SaveSearchStatus.PROCESSING;\n            this.isSaveLineJobAlert = true;\n        }\n\n        Promise.all(saveAlertPromises)\n            .then((responses) => {\n                this.createJobAlertSuccess(responses);\n                if (this.isSaveLineJobAlert && !this.encryptedLineUserId) {\n                    redirectToLineNewUserJobAlertCreationPage(this.lineNewUserJobAlertCreationUrl);\n                }\n                responses.map((jobAlert) => {\n                    this.setSaveSearchStatusByType(\n                        jobAlert.alertType,\n                        jobAlert.subscriptionId ? SaveSearchStatus.SUCCESS : SaveSearchStatus.ERROR\n                    );\n                });\n                this.setIsProcessing(false);\n            })\n            .catch(() => {\n                this.setIsProcessing(false);\n                this.setSaveSearchStatusByAction(alertActionType, SaveSearchStatus.ERROR);\n            });\n    };\n\n    @action\n    public deleteJobAlerts = (alertActionType: JobAlertActionType): boolean => {\n        if (alertActionType === JobAlertActionType.NONE) return false;\n\n        const { saveSearchModel } = this.rootStore.config.richSearchComponentModel;\n        if (!saveSearchModel || !this.saveSearchData) {\n            return false;\n        }\n\n        const deleteAlertPromises = [];\n        this.isProcessing = true;\n        if (isEmailAlertAction(alertActionType)) {\n            const jobAlertId = getSavedSearch(this.saveSearchData)?.jobAlertId;\n            if (jobAlertId) {\n                deleteAlertPromises.push(deleteJobAlert(jobAlertId, this.createAlertFormData()));\n                this.saveSearchEmailStatus = SaveSearchStatus.PROCESSING;\n            }\n        }\n\n        if (isLineAlertAction(alertActionType) && this.encryptedLineUserId) {\n            const lineJobAlertId = getSavedSearch(this.saveSearchData)?.lineAlertId;\n            if (lineJobAlertId) {\n                deleteAlertPromises.push(\n                    deleteLineJobAlert(\n                        saveSearchModel.lineAlertsApiUrl,\n                        getDeleteLineAlertRequestData(lineJobAlertId)\n                    )\n                );\n                this.saveSearchLineStatus = SaveSearchStatus.PROCESSING;\n            }\n        }\n\n        Promise.all(deleteAlertPromises)\n            .then((results) => {\n                this.deleteJobAlertSuccess(results);\n\n                this.setIsProcessing(false);\n                results.map((result) =>\n                    this.setSaveSearchStatusByType(\n                        result.alertType,\n                        result.success ? SaveSearchStatus.SUCCESS : SaveSearchStatus.ERROR\n                    )\n                );\n            })\n            .catch(() => {\n                this.setIsProcessing(false);\n                this.setSaveSearchStatusByAction(alertActionType, SaveSearchStatus.ERROR);\n            });\n\n        return deleteAlertPromises.length > 0;\n    };\n\n    @action\n    public unsaveSearch = () => {\n        const { saveSearchModel } = this.rootStore.config.richSearchComponentModel;\n        if (!saveSearchModel || !this.saveSearchData) {\n            return;\n        }\n\n        if (this.deleteJobAlerts(this.alertActionType)) {\n            return;\n        }\n\n        // eslint-disable-next-line @typescript-eslint/no-floating-promises\n        deleteSavedSearch(this.saveSearchData, this.alertActionType);\n        this.saved = false;\n        this.rootStore.ui.displayUnsaveSearchToast();\n        this.updateSavedSearches();\n        this.logUnsave();\n    };\n\n    @action\n    public unsaveAllSearches = () => {\n        const { saveSearchModel } = this.rootStore.config.richSearchComponentModel;\n        if (!saveSearchModel || !this.saveSearchData) {\n            return;\n        }\n\n        if (this.deleteJobAlerts(JobAlertActionType.EMAIL_AND_LINE)) {\n            return;\n        }\n\n        // eslint-disable-next-line @typescript-eslint/no-floating-promises\n        deleteSavedSearch(this.saveSearchData, JobAlertActionType.EMAIL_AND_LINE);\n        this.saved = false;\n        this.rootStore.ui.displayUnsaveSearchToast();\n        this.updateSavedSearches();\n        this.logUnsave();\n    };\n\n    @action\n    private readonly createJobAlertSuccess = (jobAlerts: JobAlert[]) => {\n        if (!this.saveSearchData) {\n            return;\n        }\n\n        const emailResult = jobAlerts.filter(\n            (jobAlert) => jobAlert.alertType === JobAlertType.EMAIL\n        );\n        const lineResult = jobAlerts.filter((jobAlert) => jobAlert.alertType === JobAlertType.LINE);\n        // eslint-disable-next-line @typescript-eslint/no-floating-promises\n        createSavedSearch(\n            this.saveSearchData!,\n            this.recentSearchTimestamp,\n            emailResult.length > 0 ? emailResult[0].subscriptionId : null,\n            lineResult.length > 0 ? lineResult[0].subscriptionId : null\n        );\n\n        this.saved = true;\n        this.rootStore.ui.displaySaveSearchToast();\n        this.updateSavedSearches();\n        this.logSave();\n    };\n\n    @action\n    private readonly deleteJobAlertSuccess = (jobAlerts: DeletedJobAlert[]) => {\n        if (!this.saveSearchData) {\n            return;\n        }\n\n        const emailResult = jobAlerts.filter(\n            (jobAlert) => jobAlert.alertType === JobAlertType.EMAIL && jobAlert.success\n        );\n        const lineResult = jobAlerts.filter(\n            (jobAlert) => jobAlert.alertType === JobAlertType.LINE && jobAlert.success\n        );\n\n        let effectiveType;\n        if (emailResult.length > 0 && lineResult.length > 0) {\n            effectiveType = JobAlertActionType.EMAIL_AND_LINE;\n        } else if (emailResult.length > 0) {\n            effectiveType = JobAlertActionType.EMAIL_ONLY;\n        } else if (lineResult.length > 0) {\n            effectiveType = JobAlertActionType.LINE_ONLY;\n        }\n\n        if (effectiveType) {\n            // eslint-disable-next-line @typescript-eslint/no-floating-promises\n            deleteSavedSearch(this.saveSearchData, effectiveType);\n            this.saved = false;\n            this.rootStore.ui.displayUnsaveSearchToast();\n            this.updateSavedSearches();\n            this.logUnsave();\n        }\n    };\n\n    @computed\n    public get asState() {\n        return toJS(this);\n    }\n\n    // VisibleForTesting\n    public updateSaveSearchTimestamp = () => {\n        if (!this.rootStore.initialSearchInput) {\n            return;\n        }\n\n        const {\n            keyword,\n            location,\n            radius,\n            type,\n            salaryType,\n            salaryMin,\n            salaryMax,\n            tags,\n            occupation,\n            searchCriteria\n        } = this.rootStore.initialSearchInput;\n\n        const saveSearchData = generateSaveSearchData({\n            keyword,\n            location,\n            radius,\n            type,\n            salaryType,\n            salaryMin,\n            salaryMax,\n            tags,\n            occupation,\n            searchCriteria\n        });\n\n        if (saveSearchData) {\n            // Timestamp used for saved search is generated here on client side.\n            // This is different from recent searches wihth timestamp generated on server side.\n            // eslint-disable-next-line @typescript-eslint/no-floating-promises\n            updateSaveSearchTimestamp(saveSearchData, Date.now());\n        }\n    };\n\n    // TODO remove merging logic as it might result in inaccurate job count\n    @action\n    public updateSavedSearches() {\n        const { isLoggedIn } = this.rootStore.config.richSearchComponentModel;\n        const { isShowRecentSearch } = this.rootStore.ui;\n        if (!isLoggedIn || !isShowRecentSearch) {\n            this.savedSearches = [];\n\n            return;\n        }\n\n        const savedSearches = getSavedSearches();\n        if (!savedSearches) {\n            this.savedSearches = [];\n\n            return;\n        }\n\n        const prevSavedSearchesInState = this.savedSearches || [];\n        const newSavedSearchesInState: SavedSearchInState[] = [];\n\n        // Merge saved search in local storage and in state\n        // to avoid requesting job count multiple times\n        savedSearches.forEach((savedSearch) => {\n            const prevSavedSearchInState = prevSavedSearchesInState.find(\n                (savedSearchInState) =>\n                    savedSearchInState.search.date === savedSearch.date &&\n                    compareSavedSearchData(savedSearchInState.search.data, savedSearch.data)\n            );\n\n            if (prevSavedSearchInState && prevSavedSearchInState.search.ts === savedSearch.ts) {\n                newSavedSearchesInState.push(prevSavedSearchInState);\n            } else {\n                newSavedSearchesInState.push({\n                    search: savedSearch,\n                    jobCountLoading: false\n                });\n            }\n        });\n\n        newSavedSearchesInState.forEach((savedSearchInState) => {\n            if (savedSearchInState.newJobCount) {\n                return;\n            }\n\n            this.fetchSavedSearch(savedSearchInState.search);\n        });\n\n        this.savedSearches = newSavedSearchesInState;\n    }\n\n    public fetchSavedSearch = (savedSearch: SavedSearch) => {\n        const url = getSavedSearchJobCountUrl(savedSearch);\n        const ts = savedSearch.ts ? savedSearch.ts : savedSearch.date;\n        const data = savedSearch.data;\n\n        const index = this.findIndex(ts, data);\n        if (index >= 0) {\n            this.savedSearches[index].jobCountLoading = true;\n        }\n\n        void fetchNewJobsCount(url)\n            .then((newJobCount) => {\n                this.savedSearches = this.savedSearches.map((savedSearchInState) => {\n                    if (\n                        matchesSavedSearch(savedSearchInState, { ts, data }) &&\n                        newJobCount.newCount > 0\n                    ) {\n                        return {\n                            ...savedSearchInState,\n                            newJobCount\n                        };\n                    } else {\n                        return savedSearchInState;\n                    }\n                });\n            })\n            .finally(() => {\n                const indexAfterFinish = this.findIndex(ts, data);\n                if (indexAfterFinish >= 0) {\n                    this.savedSearches[indexAfterFinish].jobCountLoading = false;\n                }\n            });\n    };\n\n    public dispose(): void {\n        this.disposers.forEach((dispose) => dispose());\n    }\n}\n","import { action } from 'mobx';\n\nimport { SearchInputField } from '../types/SearchInput';\n\nimport RootStore from './RootStore';\nimport SearchInput from './SearchInput';\n\nexport default class SearchStore {\n    public readonly rootStore: RootStore;\n\n    public readonly canonical: SearchInput;\n\n    public readonly draft: SearchInput;\n\n    public from: string;\n\n    public cjsFrom: string | undefined;\n\n    public constructor(rootStore: RootStore) {\n        this.rootStore = rootStore;\n        this.canonical = new SearchInput(rootStore);\n        this.draft = new SearchInput(rootStore);\n        this.from = rootStore.config.searchFrom;\n    }\n\n    @action\n    public resetDraft = (fields: SearchInputField, customFields: ReadonlyArray<string> = []) => {\n        this.draft.reset(fields, customFields);\n    };\n\n    @action\n    public revertDraft = (fields: SearchInputField, customFields: ReadonlyArray<string> = []) => {\n        this.draft.apply(this.canonical, fields, customFields, true);\n    };\n\n    @action\n    public commitDraft = (fields: SearchInputField, customFields: ReadonlyArray<string> = []) => {\n        this.canonical.apply(this.draft, fields, customFields, true);\n    };\n\n    @action\n    public setFrom = (from: string) => {\n        this.from = from;\n    };\n\n    @action\n    public setCjsFrom = (cjsFrom?: string) => {\n        this.cjsFrom = cjsFrom;\n    };\n}\n","import { SavedSearchData } from '@indeed/cmi-jp-common-components/lib/types/savedSearch';\nimport { getSavedSearchesCount } from '@indeed/cmi-jp-common-components/lib/util/savedSearches/savedSearches';\nimport { MosaicStateManager } from '@indeed/mosaic-provider-utils/dist/web/state/MosaicStateManager';\nimport { action, reaction, IReactionDisposer, observable, get, keys, toJS } from 'mobx';\n\nimport RichSearchTrackEventLogger from '../logging/RichSearchTrackEventLogger';\nimport { getLocationRadius } from '../transport/locationRadius';\nimport { SalaryRange } from '../types/RichSearchComponentModel';\nimport RichSearchHostState from '../types/RichSearchHostState';\nimport { isBrowser } from '../util/browser';\n\nimport Configuration, { ConfigurationInput } from './Configuration';\nimport ExternalStore from './ExternalStore';\nimport { InitialSearchInput } from './InitialSearchInput';\nimport RecentSearchStore from './RecentSearchStore';\nimport SaveSearchStore from './SaveSearchStore';\nimport SearchStore from './SearchStore';\nimport UiStore, { LocationOptionSubPaneType, PaneType } from './UiStore';\n\nexport default class RootStore {\n    @observable\n    public initialSearchInput: InitialSearchInput | undefined;\n    public readonly config: Configuration;\n    public readonly search: SearchStore;\n    public readonly ui: UiStore;\n    public readonly saveSearch: SaveSearchStore;\n    public readonly recentSearch: RecentSearchStore;\n    public readonly externalStore: ExternalStore;\n    public readonly logger: RichSearchTrackEventLogger;\n    public readonly compositeFilterEncoder?: ICompositeFilterEncoder;\n    private readonly disposers: IReactionDisposer[] = [];\n\n    /**\n     * the order of sub-store construction has to be maintained as there are implicit dependencies\n     */\n    public constructor(\n        input: ConfigurationInput,\n        externalStore: ExternalStore,\n        logger: RichSearchTrackEventLogger,\n        compositeFilterEncoder?: ICompositeFilterEncoder\n    ) {\n        this.logger = logger;\n        this.config = new Configuration(input);\n        this.externalStore = externalStore;\n        this.search = new SearchStore(this);\n        this.ui = new UiStore(this);\n        this.saveSearch = new SaveSearchStore(this);\n        this.recentSearch = new RecentSearchStore(this);\n        this.compositeFilterEncoder = compositeFilterEncoder;\n\n        this.externalStore.setRootStore(this);\n        this.initDataWithoutUpdater();\n    }\n\n    /**\n     * Initialize all watchers.\n     *\n     * @note Order matters.\n     * @note We do it here instead of in constructor because we need the same data (in config) when hydrating.\n     */\n    public initReactions() {\n        this.saveSearch.initReactions();\n        this.disposers.push(\n            ...this.initDraftUpdater(),\n            ...this.initCanonicalUpdater(),\n            ...this.initInitialSearchInputUpdater(),\n            ...this.initRadiusUpdater(),\n            ...this.initJobCountUpdater()\n        );\n    }\n\n    private initDataWithoutUpdater() {\n        this.updateInitialSearch({ richSearchHostState: undefined });\n        if (this.initialSearchInput) {\n            this.search.canonical.applyInitialSearchInput(\n                this.initialSearchInput,\n                this.config.isOnDesktopSERP,\n                this.config.richSearchComponentModel.isDesktopMultiSelectFilterTst,\n                this.config.isUnifiedFilters\n            );\n            this.search.draft.apply(this.search.canonical);\n        }\n    }\n\n    /**\n     * Initialize watchers for updating draft search input when canonical one changes\n     */\n    private initDraftUpdater(): ReadonlyArray<IReactionDisposer> {\n        return [\n            reaction(\n                () => this.search.canonical.keyword,\n                (keyword) => {\n                    this.search.draft.keyword = keyword;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.location,\n                (location) => {\n                    this.search.draft.location = location;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.radius,\n                (radius) => {\n                    this.search.draft.radius = radius;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.type,\n                (type) => {\n                    this.search.draft.type = type;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.age,\n                (age) => {\n                    this.search.draft.age = age;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.indeedApply,\n                (indeedApply) => {\n                    this.search.draft.indeedApply = indeedApply;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.sourceTypes.slice(),\n                (sourceTypes) => {\n                    this.search.draft.sourceTypes.splice(\n                        0,\n                        this.search.draft.sourceTypes.length,\n                        ...sourceTypes\n                    );\n                }\n            ),\n            reaction(\n                () => this.search.canonical.salary.type,\n                (salaryType) => {\n                    this.search.draft.salary.type = salaryType;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.salary.ranges[SalaryRange.HOURLY].min,\n                (min) => {\n                    this.search.draft.salary.ranges[SalaryRange.HOURLY].min = min;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.salary.ranges[SalaryRange.HOURLY].max,\n                (max) => {\n                    this.search.draft.salary.ranges[SalaryRange.HOURLY].max = max;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.salary.ranges[SalaryRange.DAILY].min,\n                (min) => {\n                    this.search.draft.salary.ranges[SalaryRange.DAILY].min = min;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.salary.ranges[SalaryRange.DAILY].max,\n                (max) => {\n                    this.search.draft.salary.ranges[SalaryRange.DAILY].max = max;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.salary.ranges[SalaryRange.MONTHLY].min,\n                (min) => {\n                    this.search.draft.salary.ranges[SalaryRange.MONTHLY].min = min;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.salary.ranges[SalaryRange.MONTHLY].max,\n                (max) => {\n                    this.search.draft.salary.ranges[SalaryRange.MONTHLY].max = max;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.salary.ranges[SalaryRange.YEARLY].min,\n                (min) => {\n                    this.search.draft.salary.ranges[SalaryRange.YEARLY].min = min;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.salary.ranges[SalaryRange.YEARLY].max,\n                (max) => {\n                    this.search.draft.salary.ranges[SalaryRange.YEARLY].max = max;\n                }\n            ),\n            reaction(\n                // Need to call slice to watch changes\n                // See more at https://github.com/mobxjs/mobx/issues/385#issuecomment-230467701\n                () => this.search.canonical.tags.slice(),\n                (tags) => {\n                    this.search.draft.tags.splice(0, this.search.draft.tags.length, ...tags);\n                }\n            ),\n            reaction(\n                () => this.search.canonical.occupation,\n                (occupation) => {\n                    this.search.draft.occupation = occupation;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.regionId,\n                (regionId) => {\n                    this.search.draft.regionId = regionId;\n                }\n            ),\n            reaction(\n                () => this.search.canonical.areas.slice(),\n                (areas) => {\n                    this.search.draft.areas.splice(0, this.search.draft.areas.length, ...areas);\n                }\n            ),\n            reaction(\n                () => this.search.canonical.transitLines.slice(),\n                (transitLines) => {\n                    this.search.draft.transitLines.splice(\n                        0,\n                        this.search.draft.transitLines.length,\n                        ...transitLines\n                    );\n                }\n            ),\n            reaction(\n                () => this.search.canonical.transitStations.slice(),\n                (transitStations) => {\n                    this.search.draft.transitStations.splice(\n                        0,\n                        this.search.draft.transitStations.length,\n                        ...transitStations\n                    );\n                }\n            ),\n            reaction(\n                () => this.search.canonical.searchCriteria,\n                (searchCriteria) => {\n                    this.search.draft.searchCriteria = searchCriteria;\n                }\n            ),\n            reaction(\n                () => JSON.stringify(toJS(this.search.canonical.customFields)),\n                () => {\n                    keys(this.search.canonical.customFields).forEach((fieldName) => {\n                        const data = get(this.search.canonical.customFields, fieldName);\n                        if (data) {\n                            const [label, ...value] = data;\n                            this.search.draft.setCustomField(fieldName, label, ...value);\n                        }\n                    });\n                }\n            )\n        ];\n    }\n\n    /**\n     * Initialize watchers for updating canonical search input when initial search input changes\n     */\n    private initCanonicalUpdater(): ReadonlyArray<IReactionDisposer> {\n        return [\n            reaction(\n                () => this.initialSearchInput,\n                (initialSearchInput) => {\n                    // istanbul ignore else\n                    if (initialSearchInput) {\n                        // In reality, this normally does not triggered radius fetching as location keeps same.\n                        this.search.canonical.applyInitialSearchInput(\n                            initialSearchInput,\n                            this.config.isOnDesktopSERP,\n                            this.config.richSearchComponentModel.isDesktopMultiSelectFilterTst,\n                            this.config.isUnifiedFilters\n                        );\n                    }\n                }\n            )\n        ];\n    }\n\n    /**\n     * Initialize watchers for updating initial search input upon load and when data passed from SPA side\n     */\n    private initInitialSearchInputUpdater(): ReadonlyArray<IReactionDisposer> {\n        return [\n            reaction(\n                (): {\n                    richSearchHostState: RichSearchHostState | undefined;\n                } => ({\n                    richSearchHostState: MosaicStateManager.mosaicState.hostState\n                        ? MosaicStateManager.mosaicState.hostState.richSearchComponentParams\n                        : undefined\n                }),\n                this.updateInitialSearch,\n                { fireImmediately: true }\n            )\n        ];\n    }\n\n    private initRadiusUpdater(): ReadonlyArray<IReactionDisposer> {\n        // We don't need reaction for radius in global version\n        if (this.config.richSearchComponentModel.isGlobalVersion) {\n            return [];\n        }\n\n        return [\n            reaction(\n                () => ({\n                    focused: this.ui.locationHasFocus,\n                    canonicalLocation: this.search.canonical.location,\n                    draftLocation: this.search.draft.location\n                }),\n                ({ focused, canonicalLocation, draftLocation }) => {\n                    if (!focused) {\n                        // Tapping on autocomplete item triggers 2 changes sometimes: text in the location box and autocomplete item.\n                        // e.g. type \"東京都 shi\" in the location box, and tap on \"東京都 新宿区\" in the autocomplete.\n                        if (this.ui.pendingRadiusRequestId) {\n                            clearTimeout(this.ui.pendingRadiusRequestId);\n                        }\n\n                        if (\n                            this.config.richSearchComponentModel.transitSearchPayload?.supportMLoc\n                        ) {\n                            this.ui.pendingRadiusRequestId = window.setTimeout(() => {\n                                void getLocationRadius(draftLocation).then(this.updateDraftRadius);\n                            }, 0);\n                        } else {\n                            this.ui.pendingRadiusRequestId = window.setTimeout(() => {\n                                void getLocationRadius(canonicalLocation).then(\n                                    this.updateCanonicalRadius\n                                );\n                            }, 0);\n                        }\n                    }\n                }\n            )\n        ];\n    }\n\n    private initJobCountUpdater(): ReadonlyArray<IReactionDisposer> {\n        const { transitSearchPayload } = this.config.richSearchComponentModel;\n        if (!transitSearchPayload?.showJobCounts) {\n            return [];\n        }\n        return [\n            reaction(\n                () => ({\n                    isSearchEnabled: this.search.draft.isSearchEnabled,\n                    location: this.search.draft.location,\n                    locationHasFocus: this.ui.locationHasFocus,\n                    radius: this.search.draft.radius,\n                    keyword: this.search.draft.keyword,\n                    keywordHasFocus: this.ui.keywordHasFocus,\n                    type: this.search.draft.type,\n                    age: this.search.draft.age,\n                    indeedApply: this.search.draft.indeedApply,\n                    sourceTypes: this.search.draft.sourceTypes.slice(),\n                    appliedSalary: this.search.draft.appliedSalary,\n                    // Need to call slice to watch changes\n                    // See more at https://github.com/mobxjs/mobx/issues/385#issuecomment-230467701\n                    tags: this.search.draft.tags.slice(),\n                    occupation: this.search.draft.occupation,\n                    areas: this.search.draft.areas.slice(),\n                    transitLines: this.search.draft.transitLines.slice(),\n                    transitStations: this.search.draft.transitStations.slice(),\n                    customFields: this.search.draft.customFields,\n                    additionalQueryParams: this.search.draft.additionalQueryParams\n                }),\n                ({ locationHasFocus, keywordHasFocus, ...otherParams }) => {\n                    if (\n                        !locationHasFocus &&\n                        !keywordHasFocus &&\n                        (this.ui.openedPane === PaneType.CRITERIA ||\n                            this.ui.openedLocationOptionSubPane ===\n                                LocationOptionSubPaneType.LOCATION ||\n                            this.ui.openedLocationOptionSubPane ===\n                                LocationOptionSubPaneType.STATION ||\n                            this.ui.openedLocationOptionSubPane ===\n                                LocationOptionSubPaneType.LOCATION_INPUT)\n                    ) {\n                        this.ui.renderDisplayedJobsCount();\n                    }\n                }\n            )\n        ];\n    }\n\n    @action\n    public readonly updateInitialSearchWithSavedSearch = (input: SavedSearchData) => {\n        this.initialSearchInput = InitialSearchInput.fromSavedSearch(input);\n    };\n\n    private readonly updateInitialSearch = ({\n        richSearchHostState\n    }: {\n        richSearchHostState: RichSearchHostState | undefined;\n    }) => {\n        if (richSearchHostState) {\n            if (richSearchHostState.enabled) {\n                this.initialSearchInput = InitialSearchInput.fromHostState(richSearchHostState);\n                this.ui.mobtk = richSearchHostState.mobtk;\n                // Update the label only when we have read-to-use data from the host\n                this.ui.locationOptionLabel =\n                    richSearchHostState.locationOptionModel?.locationOptionLabel ?? '';\n                this.recentSearch.fetchRecentSearches();\n                this.saveSearch.updateSaveSearchTimestamp();\n                this.saveSearch.updateSavedSearches();\n                this.logger.logSqCnt(getSavedSearchesCount());\n            }\n            this.ui.enabled = richSearchHostState.enabled;\n            this.ui.isExternalReferer = richSearchHostState.isExternalReferer;\n            this.ui.totalJobCount = richSearchHostState.totalJobCount;\n            this.ui.displayedJobsCount = richSearchHostState.totalJobCount;\n            this.ui.shouldHideRichSearch = !!richSearchHostState.shouldHideRichSearch;\n            this.ui.encryptedFccId = richSearchHostState.encryptedFccId;\n            this.ui.companyJobSearchQuery = richSearchHostState.companyJobSearchQuery;\n            this.ui.companyNameInQuery = richSearchHostState.companyNameInQuery;\n            this.ui.employer = richSearchHostState.employer;\n            this.ui.isShowRemoteSearchCallout = richSearchHostState.isShowRemoteSearchCallout;\n        } else {\n            this.initialSearchInput = InitialSearchInput.fromConfig(this.config);\n            this.ui.mobtk = this.config.richSearchComponentModel.mobtk;\n            this.ui.enabled = true;\n            this.ui.isExternalReferer = this.config.richSearchComponentModel.isExternalReferer;\n            this.ui.totalJobCount = this.config.richSearchComponentModel.totalJobCount;\n            this.ui.displayedJobsCount = this.config.richSearchComponentModel.totalJobCount;\n            this.ui.shouldHideRichSearch = !!this.config.shouldHideRichSearch;\n            this.ui.encryptedFccId = this.config.richSearchComponentModel.encryptedFccId;\n            this.ui.companyJobSearchQuery = this.config.richSearchComponentModel.companyJobSearchQuery;\n            this.ui.companyNameInQuery = this.config.richSearchComponentModel.companyNameInQuery;\n            this.ui.employer = this.config.richSearchComponentModel.employer;\n            this.ui.isShowRemoteSearchCallout = this.config.isShowRemoteSearchCallout;\n            this.ui.locationOptionLabel =\n                this.config.richSearchComponentModel.locationOptionModel?.locationOptionLabel ?? '';\n            if (isBrowser()) {\n                this.logger.logSqCnt(getSavedSearchesCount());\n            }\n        }\n\n        this.ui.resetLogSource();\n\n        if (this.initialSearchInput && this.config.prefillExtendedRecentSearchParams) {\n            InitialSearchInput.applyExtendedRecentSearches(\n                this.initialSearchInput,\n                this.recentSearch.recentSearches\n            );\n        }\n    };\n\n    @action\n    private readonly updateCanonicalRadius = (radius: number) => {\n        const targetRadius =\n            this.search.canonical.radius >= 0 !== radius >= 0\n                ? radius\n                : this.search.canonical.radius;\n        this.search.canonical.radius = targetRadius;\n        this.ui.pendingRadiusRequestId = 0;\n    };\n\n    @action\n    private readonly updateDraftRadius = (radius: number) => {\n        const targetRadius =\n            this.search.draft.radius >= 0 !== radius >= 0 ? radius : this.search.draft.radius;\n        this.search.draft.radius = targetRadius;\n        this.ui.pendingRadiusRequestId = 0;\n    };\n\n    public dispose(): void {\n        this.disposers.forEach((dispose) => dispose());\n        this.ui.dispose();\n        this.saveSearch.dispose();\n        this.recentSearch.dispose();\n    }\n\n    @action\n    public setRegionId = (regionId: string) => {\n        if (this.initialSearchInput) {\n            this.initialSearchInput.regionId = regionId;\n        }\n    };\n}\n","import {\n    SearchCriteriaCompositeFilter,\n    SearchCriteriaFilter,\n    SearchCriteriaKeywordFilter\n} from '@indeed/cmi-jp-common-components/src/types/searchCriteria';\nimport {\n    addFiltersToSc,\n    addMultiS