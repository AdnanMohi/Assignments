{\n    const searchCriteria = data.searchCriteria ? deserializeSc(data.searchCriteria) : undefined;\n    if (searchCriteria?.keywordFilters) {\n        const attrs = searchCriteria.keywordFilters.filter((filter) => filter.field === 'attr');\n        const tags = attrs.reduce((set, attr) => {\n            attr.keys.map((key) => set.add(key));\n            return set;\n        }, new Set<string>());\n        if (data.tags) data.tags.forEach(tags.add, tags);\n        if (tags.size > 0) data.tags = Array.from(tags);\n\n        // Currently we do not support multiple occupations\n        const occupationFilter = searchCriteria.keywordFilters.find(\n            (filter) => filter.field === 'occ'\n        );\n        if (occupationFilter && occupationFilter.keys.length > 0) {\n            data.occupation = occupationFilter.keys[0];\n        }\n    }\n\n    if (searchCriteria?.salaryFilter) {\n        const salaryFilter = searchCriteria?.salaryFilter;\n        const salaryType = getValidSalaryType(salaryFilter.type.toString());\n        if (salaryType) {\n            data.salaryType = salaryType;\n            data.salaryMin = salaryFilter?.min ? parseInt(salaryFilter.min, 10) : -1;\n            data.salaryMax = salaryFilter?.max ? parseInt(salaryFilter.max, 10) : -1;\n        }\n    }\n};\n\nexport class InitialSearchInput {\n    public keyword = '';\n    public location = '';\n    public latLong: LatLong | null = null;\n    public radius = -1;\n    public type = '';\n    public age = '';\n    public indeedApply = '';\n    public salaryType: SalaryRange = SalaryRange.NONE;\n    public salaryMin = -1;\n    public salaryMax = -1;\n    public tags: string[] = [];\n    public occupation = '';\n    public lastVisitTime: number | undefined = undefined;\n    public dynFiltersModel: DynamicFiltersViewModel | undefined = undefined;\n    public searchCriteria = '';\n    public regionId = '';\n\n    public static fromHostState(richSearchHostState: RichSearchHostState) {\n        const searchInput = new InitialSearchInput();\n        searchInput.keyword = richSearchHostState.keyword || '';\n        searchInput.location = richSearchHostState.location || '';\n        searchInput.latLong = richSearchHostState.latLong || null;\n        searchInput.radius = richSearchHostState.radius;\n        searchInput.type = richSearchHostState.jobType || '';\n        searchInput.age = richSearchHostState.jobAge || '';\n        searchInput.indeedApply = richSearchHostState.indeedApplyFilter || '';\n        searchInput.salaryType = getValidSalaryType(richSearchHostState.salaryType);\n        searchInput.salaryMin = richSearchHostState.salaryMin || -1;\n        searchInput.salaryMax = richSearchHostState.salaryMax || -1;\n        searchInput.tags = richSearchHostState.jobTags?.sort() ?? [];\n        searchInput.occupation = richSearchHostState.occupation || '';\n        searchInput.lastVisitTime = richSearchHostState.lastVisitTime;\n        searchInput.dynFiltersModel = richSearchHostState.dynFiltersModel;\n        searchInput.searchCriteria = richSearchHostState.searchCriteria;\n        searchInput.regionId = richSearchHostState.locationOptionModel?.locationId ?? '';\n        updateInputsFromSearchCriteria(searchInput);\n        return searchInput;\n    }\n\n    public static fromConfig(config: Configuration) {\n        const searchInput = new InitialSearchInput();\n        searchInput.keyword = config.richSearchComponentModel.keyword || '';\n        searchInput.location = config.richSearchComponentModel.location || '';\n        searchInput.latLong = config.richSearchComponentModel.latLong || null;\n        searchInput.radius = config.richSearchComponentModel.radius;\n        searchInput.type = config.richSearchComponentModel.jobType || '';\n        searchInput.age = config.richSearchComponentModel.jobAge || '';\n        searchInput.indeedApply = config.richSearchComponentModel.indeedApplyFilter || '';\n        searchInput.salaryType = getValidSalaryType(config.richSearchComponentModel.salaryType);\n        searchInput.salaryMin = config.richSearchComponentModel.salaryMin;\n        searchInput.salaryMax = config.richSearchComponentModel.salaryMax;\n        searchInput.tags = [...(config.richSearchComponentModel.selectedJobTags || [])].sort();\n        searchInput.occupation = config.richSearchComponentModel.selectedOccupation || '';\n        searchInput.lastVisitTime = config.richSearchComponentModel.lastVisitTime;\n        searchInput.dynFiltersModel = config.richSearchComponentModel.dynFiltersModel;\n        searchInput.searchCriteria = config.richSearchComponentModel.searchCriteria || '';\n        searchInput.regionId =\n            config.richSearchComponentModel.locationOptionModel?.locationId ?? '';\n        updateInputsFromSearchCriteria(searchInput);\n\n        return searchInput;\n    }\n\n    public static fromSavedSearch(input: SavedSearchData) {\n        const searchInput = new InitialSearchInput();\n        searchInput.keyword = input.what || '';\n        searchInput.location = input.where || '';\n        searchInput.latLong = null; // currently don't support custom locations in latLong\n        searchInput.radius = input.radius ? parseInt(input.radius, 10) : -1;\n        searchInput.type = input.type || '';\n        const salaryType = input.salarytype ? input.salarytype.toUpperCase() : null;\n        searchInput.salaryType = getValidSalaryType(salaryType);\n        searchInput.salaryMin = input.salarymin ? parseInt(input.salarymin, 10) : -1;\n        searchInput.salaryMax = input.salarymax ? parseInt(input.salarymax, 10) : -1;\n        searchInput.tags = input.tags ? input.tags.split(',') : [];\n        searchInput.occupation = input.occupation || '';\n        searchInput.searchCriteria = input.searchCriteria || '';\n        updateInputsFromSearchCriteria(searchInput);\n\n        return searchInput;\n    }\n\n    public static applyExtendedRecentSearches(\n        searchInput: InitialSearchInput,\n        recentSearches: RecentSearchInState[]\n    ) {\n        if (recentSearches.length === 0) {\n            return;\n        }\n\n        const mostRecentSearch = recentSearches.sort(\n            (r1, r2) => parseInt(r2.query.ts, 10) - parseInt(r1.query.ts, 10)\n        )[0];\n\n        searchInput.tags = mostRecentSearch.query.tags\n            ? mostRecentSearch.query.tags\n            : searchInput.tags;\n        searchInput.occupation = mostRecentSearch.query.jobcat1\n            ? mostRecentSearch.query.jobcat1\n            : searchInput.occupation;\n        return searchInput;\n    }\n\n    public static equals(query: InitialSearchInput, other: InitialSearchInput): boolean {\n        return (\n            query.keyword === other.keyword &&\n            query.location === other.location &&\n            // TODO: use default radius for checking\n            (query.radius === other.radius ||\n                (query.radius === -1 && other.radius === 25) ||\n                (query.radius === 25 && other.radius === -1)) &&\n            query.type === other.type &&\n            query.salaryType === other.salaryType &&\n            query.salaryMin === other.salaryMin &&\n            query.salaryMax === other.salaryMax &&\n            query.searchCriteria === other.searchCriteria &&\n            isArrayEqual(query.tags, other.tags) &&\n            isIdEqual(query.occupation, other.occupation)\n        );\n    }\n}\n","export const isSuid = (id: string | undefined): boolean => {\n    return !!id && /^[A-Z0-9]{5,6}$/i.test(id);\n};\n\nexport const areIdsSuids = (tags: string[] | undefined): boolean => {\n    if (!tags || tags.length === 0) {\n        return false;\n    } else {\n        return tags.every(isSuid);\n    }\n};\n\nexport const isIdEqual = (id: string | undefined, otherId: string | undefined): boolean => {\n    return id === otherId;\n};\n\nexport const isArrayEqual = (arr1: string[] | undefined, arr2: string[] | undefined): boolean => {\n    if (!arr1 && !arr2) {\n        return true;\n    } else if (!arr1 || !arr2) {\n        return false;\n    } else if (arr1.length !== arr2.length) {\n        return false;\n    }\n\n    const arr1s = arr1.sort();\n    const arr2s = arr2.sort();\n\n    for (let i = 0; i < arr1s.length; i++) {\n        if (arr1s[i] !== arr2s[i]) return false;\n    }\n\n    return true;\n};\n",null,"import { stringify } from 'qs';\n\nimport { JsonResult } from '../types/api';\n\nimport { fetchJson, getJsonResultBody } from './fetch';\n\nconst urlGetLocationRadius = (params: Record<string, string>) =>\n    `/m/rpc/locationradius?${stringify(params)}`;\n\nexport const getLocationRadius = (location: string): Promise<number> =>\n    fetchJson<JsonResult<number>>(urlGetLocationRadius({ l: location })).then(getJsonResultBody);\n","import { checkIsSearchStartPage } from '../components/searchstart/SearchStartPageBody';\nimport { BundleType } from '../types/bundleType';\nimport { HostLocation } from '../types/HostLocation';\nimport { RemoteSearchCalloutModel } from '../types/RemoteSearchCalloutModel';\nimport { RichSearchComponentModel } from '../types/RichSearchComponentModel';\nimport {\n    ALL_BUT_LOCATION_AND_RADIUS,\n    ALL_BUT_KEYWORD_AND_LOCATION_AND_RADIUS,\n    SearchInputField\n} from '../types/SearchInput';\n\nexport interface ConfigurationInput {\n    bundleType: BundleType;\n    hostLocation: string;\n    isDesktop: boolean;\n    isUseToast: boolean;\n    autoCompleteFrom: string;\n    searchFrom: string;\n    isShowRemoteSearchCallout: boolean;\n    remoteSearchCalloutModel?: RemoteSearchCalloutModel;\n    richSearchComponentModel: RichSearchComponentModel;\n    shouldHideRichSearch?: boolean;\n    prefillExtendedRecentSearchParams: boolean;\n    allowEmptyWhatWhereSearch?: boolean;\n    showMultiSelectPill?: boolean;\n    shouldUsePanelForSingleSelect?: boolean;\n    shouldUseScFilters?: boolean;\n    shouldRemoveLegacyParams?: boolean;\n    shouldUseIFLToast?: boolean;\n    isUnifiedFilters: boolean;\n}