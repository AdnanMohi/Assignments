                          this.setCustomField(Object.keys(p)[0], Object.values(p)[0])\n                        );\n                    }\n                } else {\n                    this.setCustomField(\n                        getFilterUrlParam(filter),\n                        ...selectedOptions.map((option) => `${getFilterOptionValue(option)}`)\n                    );\n                }\n            }\n        }\n    }\n\n    private applyLocationOptions(regionId: string, searchCriteria: string) {\n        this.regionId = '';\n        this.areas.splice(0, this.areas.length);\n        this.transitLines.splice(0, this.transitLines.length);\n        this.transitStations.splice(0, this.transitStations.length);\n\n        if (regionId && this.rootStore.compositeFilterEncoder) {\n            const options = parseLocationOptionsFromSc(\n                searchCriteria,\n                this.rootStore.compositeFilterEncoder\n            );\n            this.regionId = regionId;\n            this.areas.push(...options.areas);\n            this.transitLines.push(...options.transitLines);\n            this.transitStations.push(...options.transitStations);\n        }\n    }\n\n    private applySourceTypes(searchCriteria: string) {\n        this.sourceTypes.splice(0, this.sourceTypes.length);\n        const sc = deserializeSc(searchCriteria);\n        if (sc && sc.keywordFilters) {\n            const sourceTypes = sc.keywordFilters.find((v) => v.field === 'st');\n            if (sourceTypes) {\n                this.sourceTypes.push(...sourceTypes.keys);\n            }\n        }\n\n        // for cmi_jp_sourcetype_filter_tst3, force the initial sourceTypes to be ['e'].\n        if (\n            this.rootStore.config.richSearchComponentModel.japanSourceTypeFilterGroup === 3 &&\n            this.sourceTypes.length === 0\n        ) {\n            this.sourceTypes.splice(0, this.sourceTypes.length, ...[SourceTypeValue.EMPLOYER]);\n        }\n    }\n}\n","import { isBrowser } fr