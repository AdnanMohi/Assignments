: [\n            ...(canRequestGeolocation ? ['current location' as const] : []),\n            ...(showSuggestions ? suggestions : [])\n        ],\n        logAutocompleteSelection: logSelection,\n        sessionId\n    };\n};\n","import { createContext } from 'react';\n\nimport Gettext, { GettextInterface } from '@indeed/indeed-gettext';\n\n/**\n * Context for containing the {@link GettextInterface} for doing UI translations.\n *\n * See {@link useI18n} for consuming this in a react hook.\n */\nexport const I18nContext = createContext<GettextInterface>(Gettext({ '': {} }));\n","import { SavedSearchData } from '@indeed/cmi-jp-common-components/lib/types/savedSearch';\nimport { splitSavedSearchTag } from '@indeed/cmi-jp-common-components/lib/util/savedSearches/utils';\nimport { deserializeSc } from '@indeed/cmi-jp-common-components/src/util/savedSearches/searchCriteria';\nimport { GettextInterface, ListFormat } from '@indeed/indeed-gettext';\nimport { SearchCriteria } from '@indeed/jobsearch-be-serialized-search-criteria';\n\nimport { getFilterOptionValue } from '../components/global/filter/utils/filterModelMigrationUtil';\nimport { LocalizedSearchData } from '../types/LocalizedSearchData';\nimport {\n    DynamicFilter,\n    FilterItem,\n    FilterSettingModel,\n    JobAgeValue,\n    JobTagsSettingModel,\n    OccupationSettings,\n    RichSearchComponentModel,\n    SalaryRange,\n    SalarySetting\n} from '../types/RichSearchComponentModel';\nimport { AppliedSalaryFilter, SearchInputField, SearchInputValue } from '../types/SearchInput';\nimport { hasSalaryRange } from '../util/savedSearchSalary';\nimport { RADIUS_DEFAULT } from '../util/searchConfig';\n\nimport {\n    LIST_FORMAT_COMMA_SPLICE,\n    LIST_FORMAT_CUSTOM_FIELDS,\n    LIST_FORMAT_WHERE_FIELD\n} from './listFormat';\n\nexport type LocalizedLabelGetter = (gt: GettextInterface) => string;\n\nexport const plainText = (value: string): LocalizedLabelGetter => () => value;\n\nexport const localizedRadius = (value: number): LocalizedLabelGetter => (gt: GettextInterface) => {\n    if (value < 0) {\n        return '';\n    }\n\n    return value === 0\n        ? gt.pgettext('japan search component', 'Exact Location')\n        : gt.npgettext('japan search component', '{0}km', '{0}km', value, [`${value}`]);\n};\n\nexport const localizedFilterItem = (\n    value: string,\n    filterItems?: ReadonlyArray<FilterItem>\n): LocalizedLabelGetter => () => {\n    if (filterItems) {\n        const item = filterItems.find((it) => it.id === value);\n        if (item) {\n            return item.label;\n        }\n    }\n\n    return '';\n};\n\nexport const localizedSourceTypes = (sourceTypes: ReadonlyArray<string>): LocalizedLabelGetter => (\n    gt: GettextInterface\n) => {\n    /*\n        cmi_jp_sourcetype_filter_tst:\n        For grp1 and grp2:\n            - when unchecked, sourceTypes should be [];\n            - when checked, sourceTypes should be ['e','j'] and ['e'], respectively.\n        For grp3:\n            - when unchecked, sourceTypes should be ['e','r','j','o','a'];\n            - when checked, sourceTypes should be ['e'].\n     */\n    if (sourceTypes.length === 0 || sourceTypes.length === 5) {\n        return '';\n    }\n    return gt.pgettext('SourceType filter', 'Direct employment only');\n};\n\n// TODO: translation\nexport const localizedCurrencyValue = (\n    value: number,\n    salarySetting: SalarySetting\n): LocalizedLabelGetter => (gt: GettextInterface) => {\n    if (value < 0) {\n        return '';\n    }\n    const salarySettingValue = salarySetting.values.find(\n        (settingValue) => settingValue.value === value\n    );\n\n    return salarySettingValue\n        ? salarySettingValue.label\n        : gt.pgettext('japan rich search salary', '${0}', [value]);\n};\n\nexport const localizedSalary = (\n    value: AppliedSalaryFilter,\n    salarySettings?: ReadonlyArray<SalarySetting>\n): LocalizedLabelGetter => (gt: GettextInterface) => {\n    if (salarySettings) {\n        const salarySetting = salarySettings.find(\n            (setting) => setting.salaryType.id === value.type\n        );\n        if (salarySetting) {\n            const salaryMinLabel = localizedCurrencyValue(value.min, salarySetting)(gt);\n            const salaryMaxLabel = localizedCurrencyValue(value.max, salarySetting)(gt);\n\n            if (salaryMinLabel && salaryMaxLabel) {\n                return gt.pgettext('japan rich search salary', '{0} - {1} {2}', [\n                    salaryMinLabel,\n                    salaryMaxLabel,\n                    salarySetting.salaryType.label\n                ]);\n            } else if (salaryMinLabel) {\n                return gt.pgettext('japan rich search salary', 'above {0} {1}', [\n                    salaryMinLabel,\n                    salarySetting.salaryType.label\n                ]);\n            } else if (salaryMaxLabel) {\n                return gt.pgettext('japan rich search salary', 'below {0} {1}', [\n                    salaryMaxLabel,\n                    salarySetting.salaryType.label\n                ]);\n            }\n        }\n    }\n\n    return '';\n};\n\nconst localizedTagsAsArray = (values: ReadonlyArray<string>, tags?: JobTagsSettingModel) => {\n    if (tags) {\n        return values\n            .filter((value) => tags.tags.hasOwnProperty(value))\n            .sort((left, right) => tags.tags[left].order - tags.tags[right].order)\n            .map((value) => tags.tags[value].label);\n    }\n\n    return [];\n};\n\nexport const localizedTags = (\n    values: ReadonlyArray<string>,\n    tags?: JobTagsSettingModel\n): LocalizedLabelGetter => (gt: GettextInterface) => {\n    const localizedValues = localizedTagsAsArray(values, tags);\n\n    return gt.formatlist(LIST_FORMAT_COMMA_SPLICE, localizedValues);\n};\n\nexport const localizedOccupation = (\n    value: string,\n    occupations?: OccupationSettings\n): LocalizedLabelGetter => () => {\n    if (occupations) {\n        const item = occupations[value];\n        if (item) {\n            return item.label;\n        }\n    }\n\n    return '';\n};\n\nexport const getLocalizeList = (\n    list: ReadonlyArray<string | undefined>,\n    listFormat: ListFormat = LIST_FORMAT_COMMA_SPLICE\n): LocalizedLabelGetter => (gt: GettextInterface) => {\n    return gt.formatlist(\n        listFormat,\n        list.filter((v?: string): v is string => !!v)\n    );\n};\n\nexport const localizedCustomFields = (\n    filteredValue: string[],\n    matchedFilter?: DynamicFilter\n): LocalizedLabelGetter => (gt: GettextInterface) => {\n    if (matchedFilter) {\n        return getLocalizeList(\n            filteredValue.map((v) => {\n                const matchedOption = matchedFilter.options.find(\n                    (option) => `${getFilterOptionValue(option)}` === v\n                );\n                if (matchedOption) {\n                    return matchedOption.label;\n                }\n                if (v === matchedFilter.presentation.noOptionValue) {\n                    return matchedFilter.presentation.noOptionPlaceholder;\n                }\n\n                return '';\n            }),\n            LIST_FORMAT_CUSTOM_FIELDS\n        )(gt);\n    }\n\n    return '';\n};\n\nexport const getLocalizedSearchLabel = (\n    gt: GettextInterface,\n    searchData: LocalizedSearchData,\n    filterSettingModel: FilterSettingModel,\n    shouldHideLocation: boolean\n): string => {\n    return getLocalizeList([\n        ...(shouldHideLocation\n            ? []\n            : [\n                  getLocalizeList(\n                      [\n                          searchData.where,\n                          searchData.radius ? localizedRadius(searchData.radius)(gt) : ''\n                      ],\n                      LIST_FORMAT_WHERE_FIELD\n                  )(gt)\n              ]),\n        searchData.what,\n        searchData.type ? localizedFilterItem(searchData.type, filterSettingModel.types)(gt) : '',\n        searchData.occupation\n            ? localizedOccupation(searchData.occupation, filterSettingModel.occupationSettings)(gt)\n            : '',\n        ...(searchData.tags\n            ? localizedTagsAsArray(searchData.tags, filterSettingModel.jobTagsSettingModel)\n            : []),\n        hasSalaryRange(searchData.salaryType, searchData.salaryMin, searchData.salaryMax)\n            ? localizedSalary(\n                  {\n                      type: searchData.salaryType,\n                      min: searchData.salaryMin,\n                      max: searchData.salaryMax\n                  },\n                  filterSettingModel.salarySettings\n              )(gt)\n            : '',\n        searchData.age ? localizedFilterItem(searchData.age, filterSettingModel.ages)(gt) : '',\n        searchData.indeedApply !== undefined\n            ? localizedFilterItem(searchData.indeedApply, filterSettingModel.indeedApplyItems)(gt)\n            : '',\n        searchData.sourceTypes ? localizedSourceTypes(searchData.sourceTypes)(gt) : ''\n    ])(gt);\n};\n\nexport const getLocalizedSearchInputLabel = (\n    gt: GettextInterface,\n    searchInputValues: SearchInputValue[],\n    filterSettingModel: FilterSettingModel,\n    hideRadiusDefault = false\n) => {\n    const defaultLocalizedSearchData: LocalizedSearchData = { salaryMin: -1, salaryMax: -1 };\n    const localizedSearchData: LocalizedSearchData = searchInputValues.reduce(\n        (data, appliedValue) => {\n            switch (appliedValue.field) {\n                case SearchInputField.KEYWORD:\n                    data.what = appliedValue.value || undefined;\n                    break;\n                case SearchInputField.LOCATION:\n                    data.where = appliedValue.value || undefined;\n                    break;\n                case SearchInputField.RADIUS:\n                    data.radius =\n                        hideRadiusDefault &&\n                        (appliedValue.value === 0 || appliedValue.value === RADIUS_DEFAULT)\n                            ? undefined\n                            : appliedValue.value;\n                    break;\n                case SearchInputField.TYPE:\n                    data.type = appliedValue.value;\n                    break;\n                case SearchInputField.AGE:\n                    data.age = appliedValue.value;\n                    break;\n                case SearchInputField.INDEED_APPLY:\n                    data.indeedApply = appliedValue.value;\n                    break;\n                case SearchInputField.SALARY:\n                    const salary: AppliedSalaryFilter = appliedValue.value;\n                    data.salaryType = salary.type;\n                    data.salaryMin = salary.min;\n                    data.salaryMax = salary.max;\n                    break;\n                case SearchInputField.TAGS:\n                    data.tags = appliedValue.value;\n                    break;\n                case SearchInputField.OCCUPATION:\n                    data.occupation = appliedValue.value;\n                    break;\n                case SearchInputField.SOURCE_TYPES:\n                    data.sourceTypes = appliedValue.value;\n                    break;\n            }\n            return data;\n        },\n        defaultLocalizedSearchData\n    );\n\n    return getLocalizedSearchLabel(gt, localizedSearchData, filterSettingModel, false);\n};\n\nexport interface LabelsForSearchData {\n    location: string;\n    keyword?: string;\n    type: string;\n    occupation: string;\n    salary: string;\n    tags: string;\n}\n\nexport const getLabelsForSearchData = (\n    gt: GettextInterface,\n    searchData: SavedSearchData,\n    filterSettingModel: FilterSettingModel\n): LabelsForSearchData => {\n    const localizedSearchData = getLocalizedSavedSearch(gt, searchData);\n    return {\n        location: getLocalizeList(\n            [\n                localizedSearchData.where,\n                localizedSearchData.rad