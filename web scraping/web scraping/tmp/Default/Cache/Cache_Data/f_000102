Props = {\n    id: undefined,\n    isActive: false,\n    hasDropdown: false,\n    isNew: false,\n    newIndicatorAriaLabel: ''\n};\n\nexport default TogglePill;\n","import { DynamicFilter } from '../../../../../types/RichSearchComponentModel';\nimport { FILTER_URL_PARAM_MAPPING } from '../../data/container';\nimport { getFilterUrlParam } from '../filterModelMigrationUtil';\n\n// XXX: See related files in MOBILE: FilterOrderModelFactory.java, pillGroup.soy\nexport const getId = (filter: DynamicFilter, isSelection: boolean, isDesktop: boolean): string => {\n    if (isDesktop) {\n        // Salary mosaic provider expects 'filter-salary-estimate' as ID\n        if (filter.filterName === 'salest') {\n            return 'filter-salary-estimate';\n        }\n        return `filter-${filter.filterName}`;\n    }\n\n    const name = FILTER_URL_PARAM_MAPPING[getFilterUrlParam(filter)];\n    if (!name) {\n        return '';\n    }\n\n    if (isSelection) {\n        return `${name}_filter_button`;\n    }\n    return `${name}_option_${name}`;\n};\n","import { DynamicFilter } from '../../../../../types/RichSearchComponentModel';\nimport { getFilterUrlParam } from '../filterModelMigrationUtil';\n\nexport const getKey = (filter: DynamicFilter) => {\n    return filter.filterName ?? getFilterUrlParam(filter);\n};\n","import { DynamicFilter } from '../../../../../types/RichSearchComponentModel';\n\n// filter IDs are different between desktop and mobile\n// this ensures filterNames are consistent for logging\nexport const getFilterName = (filter: DynamicFilter): string => {\n    switch (filter.filterName) {\n        case 'salest':\n            return 'salary-estimate';\n        case 'education':\n            return 'edulvl';\n        case 'jobtype1':\n            return 'jobtype';\n        case 'Location':\n            return 'location';\n        case 'jobcat1':\n            return 'jobcat';\n        default:\n            return filter.filterName ?? '';\n    }\n};\n","/**\n * Params for a multiselect url containing both legacy parameters and search criteria parameters, we return a url that uses them both correctly\n */\n\nimport { removeKeywordFilterWithValue } from '@indeed/cmi-jp-common-components/src/util/savedSearches/searchCriteria';\nimport {\n    deserializeSearchCriteria,\n    SearchCriteria,\n    serializeSearchCriteria\n} from '@indeed/jobsearch-be-serialized-search-criteria';\n\nimport SearchInput from '../stores/SearchInput';\nimport { DynamicFilter, DynamicFilterOption } from '../types/RichSearchComponentModel';\nimport { SearchInputQuery } from '../types/SearchInput';\n\nexport const createMultiselectUrl = (\n    baseUrl: string,\n    prevSelectedOption: DynamicFilterOption | undefined,\n    selectedValues: ReadonlyArray<string>,\n    paramKey: string,\n    filter: DynamicFilter\n) => {\n    const urlDirectory = baseUrl.substring(0, baseUrl.indexOf('?') + 1);\n    const urlSearchParams = new URLSearchParams(baseUrl.substring(baseUrl.indexOf('?')));\n    const searchCriteria: SearchCriteria = getScFromUrl(baseUrl);\n\n    urlSearchParams.delete(paramKey); // remove legacy parameter from url\n\n    if (prevSelectedOption?.paramValMap?.[paramKey]) {\n        removeKeywordFilterWithValue(searchCriteria, prevSelectedOption?.paramValMap?.[paramKey]); // remove old sc filter\n    }\n\n    SearchInput.addSelectedScFilters(searchCriteria, filter, selectedValues);\n\n    urlSearchParams.set(SearchInputQuery.SEARCH_CRITERIA, serializeSearchCriteria(searchCriteria));\n\n    return urlDirectory.concat(urlSearchParams.toString());\n};\n\nexport const getScFromUrl = (url: string) => {\n    const urlSearchParams = new URLSearchParams(url.substring(url.indexOf('?')));\n    const searchCriteriaResponse =\n        urlSearchParams.get('sc') && deserializeSearchCriteria(urlSearchParams.get('sc')!);\n\n    return searchCriteriaResponse && searchCriteriaResponse.status === 'SUCCESS'\n        ? searchCriteriaResponse.searchCriteria\n        : { params: {} };\n};\n","import SearchStore from '../../../../../stores/SearchStore';\nimport { PillComponentType } from '../../../../../types/container';\nimport { DynamicFilter, DynamicFilterOption } from '../../../../../types/RichSearchComponentModel';\nimport { createMultiselectUrl } from '../../../../../util/desktopMultiselect';\n\nexport const handleDesktopMultiSelectTstRedirect = (\n    filter: DynamicFilter,\n    clickedOption: DynamicFilterOption | undefined,\n    urlParam: string,\n    urlParamValue: string | null,\n    search: SearchStore\n) => {\n    if (filter.presentation.componentType !== PillComponentType.MultiSelect) {\n        const selectedOption = filter.options.find((option) => option.label === urlParamValue);\n        const selectedUrl = selectedOption?.url ?? search.canonical.searchHref;\n        return selectedUrl;\n    }\n    const prevSelectedOption: DynamicFilterOption | undefined = filter.options.find(\n        (option) => option.selected\n    );\n\n    if (!clickedOption && !prevSelectedOption) {\n        return search.canonical.searchHref;\n    }\n    const urlToModify =\n        clickedOption?.url ?? prevSelectedOption?.url ?? search.canonical.searchHref;\n\n    const redirectUrl = createMultiselectUrl(\n        urlToModify,\n        prevSelectedOption,\n        search.canonical.getCustomField(urlParam),\n        urlParam,\n        filter\n    );\n\n    return redirectUrl;\n};\n","import { DynamicFilter, DynamicFilterDataType } from '../../../../types/RichSearchComponentModel';\n\n/**\n * Temporary wrapper around DynamicFilter.showSelectedFilterAsToggle that provides default behavior when not set.\n */\nconst showSelectedFilterAsToggle = (filter: DynamicFilter, isDesktop: boolean): boolean => {\n    if (filter.presentation.showSelectedFilterAsToggle !== undefined) {\n        return filter.presentation.showSelectedFilterAsToggle;\n    }\n\n    // TODO: Remove hard-coded conditions once showSelectedAsToggle is set by clients\n    return (\n        (isDesktop && filter.filterName !== 'radius') ||\n        (!isDesktop && filter.dataType !== DynamicFilterDataType.STATIC)\n    );\n};\n\nexport default showSelectedFilterAsToggle;\n","import React from 'react';\n\nimport { Box, Heading, VisuallyHidden } from '@indeed/ifl-components';\nimport ScreenReaderAnnouncement, { Urgency } from '@indeed/ipl-screen-reader-announcement';\nimport classnames from 'classnames';\n\nimport { StoreContextData } from '../../../context';\nimport observing from '../../../hoc/observing';\nimport { withProctor } from '../../../proctor/withProctor';\nimport { ContainerProps, PillComponentType, State } from '../../../types/container';\nimport { HostLocation } from '../../../types/HostLocation';\nimport {\n    DynamicFilter,\n    DynamicFilterBucketOption,\n    DynamicFilterDataType,\n    DynamicFilterOption\n} from '../../../types/RichSearchComponentModel';\nimport { setPtkCookie } from '../../../util/ptkCookie';\n\nimport { EXCLUDE_MOVE_SELECTED_FILTERS_TO_BEGINNING } from './data/container';\nimport { ChipPill, ChipPillSelectionOption } from './pill/ChipPill';\nimport DropdownPill from './pill/DropdownPill';\nimport { FilterIcon } from './pill/Icons';\nimport { OverlayPill } from './pill/overlay/OverlayPill';\nimport SelectionPill from './pill/SelectionPill';\nimport TogglePill from './pill/TogglePill';\nimport {\n    getFilterName,\n    getId,\n    getKey,\n    handleDesktopMultiSelectTstRedirect,\n    setOptionToToggle\n} from './utils/container';\nimport {\n    getFilterOptionValue,\n    getFilterUrlParam,\n    getFilterUrlParams\n} from './utils/filterModelMigrationUtil';\nimport { createToggleOption, SALARY_FILTER_TOGGLE_PARAM } from './utils/salaryFilterUtil';\nimport showSelectedFilterAsToggle from './utils/showSelectedFilterAsToggle';\n\nclass Container extends React.Component<ContainerProps & StoreContextData, State> {\n    constructor(props: ContainerProps & StoreContextData) {\n        super(props);\n        this.state = {\n            screenReaderAnnounceMessage: '',\n            srCounter: 0\n        };\n    }\n\n    private readonly handleChange = (filter: DynamicFilter, ...value: Array<string>) => {\n        const { search, isOnDesktopSERP, config, onChange } = this.props;\n        if (\n            !search.rootStore.initialSearchInput?.dynFiltersModel ||\n            (isOnDesktopSERP && !config.richSearchComponentModel.isDesktopMultiSelectFilterTst) ||\n            (isOnDesktopSERP && !config.isUnifiedFilters)\n        ) {\n            return;\n        }\n\n        // Get the option that corresponds to the value passed in\n        const key = getFilterUrlParam(filter);\n        const val = value.length > 0 ? value[0] : null;\n\n        const clickedOption = filter?.options?.find(\n            (opt: any) => opt.paramValMap && opt.paramValMap[key] === val\n        );\n        const toggleOption = filter?.options.find((opt) => opt.type === 'toggle');\n\n        if (toggleOption) {\n            setOptionToToggle(toggleOption, filter, value, search.canonical);\n        }\n\n        if (\n            clickedOption?.paramValMap &&\n            config?.hostLocation === HostLocation.MOBILE &&\n            filter?.presentation?.componentType !== PillComponentType.MultiSelect\n        ) {\n            /*\n             * Allow multiple param map per filter option on MOBILE for non multi-select filters\n             * Multi-select filters use a different value format\n             */\n            for (const k in clickedOption.paramValMap) {\n                if (clickedOption.paramValMap.hasOwnProperty(k)) {\n                    search.canonical.setCustomField(k, clickedOption.paramValMap[k]);\n                }\n            }\n        } else {\n            // For non-MOBILE or multi-select filters, or no param map passed\n            search.canonical.setCustomField(key, ...value);\n        }\n\n        if (isOnDesktopSERP && config.richSearchComponentModel.isDesktopMultiSelectFilterTst) {\n            const selectedUrl = handleDesktopMultiSelectTstRedirect(\n                filter,\n                clickedOption,\n                key,\n                val,\n                search\n            );\n            onChange(undefined, selectedUrl);\n            return;\n        }\n\n        search.setFrom('serpso');\n        onChange();\n    };\n\n    private readonly moveSelectedFiltersToBeginning = (isDesktop: boolean, country: string) => {\n        return !isDesktop && !EXCLUDE_MOVE_SELECTED_FILTERS_TO_BEGINNING.includes(country);\n    };\n\n    private readonly getSelectedValues = (\n        filter: DynamicFilter\n    ): [string, ReadonlyArray<string>] => {\n        const isDesktopMultiSelectFilterTst: boolean =\n            (this.props.proctorGroups?.desktop_multiselect_filter_tst?.value ?? -1) > 0;\n        const isMultiSelectFilter: boolean =\n            filter.presentation.componentType === PillComponentType.MultiSelect;\n        const isUnifiedDesktopActiveAndMultiSelect: boolean =\n            this.props.config.isUnifiedFilters && isMultiSelectFilter;\n\n        if (\n            this.props.isOnDesktopSERP &&\n            !(isDesktopMultiSelectFilterTst && isMultiSelectFilter) &&\n            !isUnifiedDesktopActiveAndMultiSelect\n        ) {\n            const selected = filter.options.find((o) => o.selected);\n            if (selected) {\n                return [selected.label, [selected.label]];\n            }\n        } else {\n            const allFilterUrlParams = getFilterUrlParams(filter);\n            const appliedValues = allFilterUrlParams\n                .map((filterUrlParam) =>\n                    this.props.search.canonical.getAppliedValue(filterUrlParam)\n                )\n                .filter((v) => !!v);\n            if (appliedValues.length > 0) {\n                return [\n                    appliedValues[0]?.getLabel(this.props.gt) ?? '',\n                    appliedValues\n                        .map((appliedval) => (appliedval && appliedval.value) ?? '')\n                        .filter((v) => v !== '')\n                        .flat()\n                ];\n            }\n        }\n\n        return [filter.presentation.noOptionPlaceholder, []];\n    };\n\n    private readonly getSortedFilters = () => {\n        const filters = this.props.search.rootStore.initialSearchInput?.dynFiltersModel?.filters;\n\n        const moveSelectedFiltersToBeginning = this.moveSelectedFiltersToBeginning(\n            this.props.config.isDesktop,\n            this.props.config.richSearchComponentModel.country\n        );\n\n        if (!filters) {\n            return undefined;\n        }\n        // Check for the radius slider in the map where box\n        const hasRadiusSliderWidget = !this.props.config.isDesktop;\n\n        // Check for the presence of the global education filter\n        const hasYosegiEducationFilter = filters.some(\n            (obj: DynamicFilter) => getFilterUrlParam(obj) === 'education'\n        );\n\n        const removeExtraFilters = (filter: DynamicFilter) => {\n            // Hide unneeded filters\n            if (!hasYosegiEducationFilter && !hasRadiusSliderWidget) {\n                return true;\n            }\n            // Remove the Segment eduction level filter when the global education filter is present\n            if (hasYosegiEducationFilter && filter.presentation.displayName === 'Education Level') {\n                return false;\n            }\n            // Remove the radius filter if the radius slider is show in the where pane\n            return !(\n                hasRadiusSliderWidget &&\n                (filter.urlParam === 'radius' || filter.filterName === 'radius')\n            );\n        };\n\n        const updatePostedByFilter = (filter: DynamicFilter) => {\n            const isSrcType = filter.filterName === 'srctype';\n            const companyFilterOrder = filters.find(\n                (item) =>\n                    item.filterName === 'company' || item.presentation.displayName === 'Company'\n            )?.presentation.order;\n            const isLocaleCorrect =\n                this.props.config.richSearchComponentModel.country === 'US' &&\n                this.props.config.richSearchComponentModel.language === 'en';\n\n            if (!isSrcType || !this.props.config.isDesktop || !isLocaleCorrect) {\n                return filter;\n            }\n\n            if (companyFilterOrder) {\n                filter.presentation.order = companyFilterOrder + 1;\n            }\n\n            filter.options.forEach((option) => {\n                if (option.label === 'Recruiter') {\n                    option.label = 'Staffing agency';\n                }\n            });\n\n            return filter;\n        };\n\n        if (this.props.config.isDesktop && !moveSelectedFiltersToBeginning) {\n            return filters.filter(removeExtraFilters).filter(updatePostedByFilter);\n        }\n\n        const updateSalaryOptions = (filter: DynamicFilter) => {\n            if (filter.filterName !== 'salest') {\n                return filter;\n            }\n            filter.options?.forEach((option, i, arr) => {\n                if (SALARY_FILTER_TOGGLE_PARAM in option.paramValMap!) {\n                    filter.options[i] = createToggleOption(option);\n                }\n            });\n            return filter;\n        };\n\n        const isPrioritizedGroup = (filter: DynamicFilter, index: number) => {\n            if (moveSelectedFiltersToBeginning) {\n                return this.getSelectedValues(filter)[1].length > 0;\n            }\n\n            // Keep remote filter at the beginning if it is already positioned there.\n            if (index === 0 && getFilterUrlParam(filter) === 'remotejob') {\n                return true;\n            }\n\n            return (\n                filter.dataType !== DynamicFilterDataType.STATIC &&\n                (filter.presentation.componentType === PillComponentType.Button ||\n                    filter.presentation.componentType === PillComponentType.Select) &&\n                this.getSelectedValues(filter)[1].length > 0\n            );\n        };\n\n        if (moveSelectedFiltersToBeginning) {\n            return filters\n                .filter(isPrioritizedGroup)\n                .filter(removeExtraFilters)\n                .filter(updatePostedByFilter)\n                .filter(updateSalaryOptions)\n                .concat(\n                    filters\n                        .filter((filter, index) => !isPrioritizedGroup(filter, index))\n                        .filter(removeExtraFilters)\n                        .filter(updatePostedByFilter)\n                        .filter(updateSalaryOptions)\n                        .sort((f1, f2) => f1.presentation.order - f2.presentation.order)\n                );\n        }\n\n        return filters\n            .filter(isPrioritizedGroup)\n            .filter(removeExtraFilters)\n            .filter(updatePostedByFilter)\n            .filter(updateSalaryOptions)\n            .concat(\n                filters\n                    .filter((filter, index) => !isPrioritizedGroup(filter, index))\n                    .filter(removeExtraFilters)\n                    .filter(updatePostedByFilter)\n                    .filter(updateSalaryOptions)\n            );\n    };\n\n    private renderButtonPill(filter: DynamicFilter) {\n        const { gt } = this.props;\n        const isNew = !!(\n            filter.presentation.newLabelModel && filter.presentation.newLabelModel.labelType\n        );\n        if (filter.options[0].url) {\n            return (\n                <Box as=\"li\">\n                    <TogglePill\n                        id={getId(filter, false, true)}\n                        key={getKey(filter)}\n                        label={filter.presentation.displayName}\n                        isActive={filter.options[0].selected}\n                        url={filter.options[0].url}\n                        ariaLabel={\n                            filter.options[0].selected\n                                ? gt.gettext('Remove {0} filter', [filter.presentation.displayName])\n                                : gt.gettext('{0} filter', [filter.presentation.displayName])\n                        }\n                        disabled={!this.props.ui.enabled}\n                        isNew={isNew}\n                    />\n                </Box>\n            );\n        } else {\n            const [, values] = this.getSelectedValues(filter);\n            const screenReaderAnnounceMessage = gt.gettext(\n                // # Job Search Filter removed\n                'Filter {0} has been removed',\n                [filter.presentation.displayName]\n            );\n            return (\n                <Box as=\"li\">\n                    <TogglePill\n                        id={getId(filter, false, false)}\n                        key={getKey(filter)}\n                        label={filter.presentation.displayName}\n                        isActive={values.length > 0}\n                        onClick={() => {\n                            if (values.length > 0) {\n                                this.setState({\n                                    screenReaderAnnounceMessage,\n                                    srCounter: this.state.srCounter + 1\n                                });\n                            }\n                            this.handleChange(\n                                filter,\n                                values.length > 0\n                                    ? filter.presentation.noOptionValue\n                                    : getFilterOptionValue(filter.options[0])\n                            );\n                        }}\n                        ariaLabel={\n                            values.length > 0\n                                ? gt.gettext('Remove {0} filter', [filter.presentation.displayName])\n                                : gt.gettext('{0} filter', [filter.presentation.displayName])\n                        }\n                        disabled={!this.props.ui.enabled}\n                        isNew={isNew}\n                    />\n                </Box>\n            );\n        }\n    }\n\n    private getCount(filter: DynamicFilter, option: DynamicFilterOption): number | undefined {\n        if (\n            filter.presentation.countVisible &&\n            (option.type === 'bucket' || option.type === 'range')\n        ) {\n            return option.count;\n        }\n\n        return undefined;\n    }\n\n    private getDefaultValueForFilter(filter: DynamicFilter, hasDefault: boolean) {\n        if (hasDefault) {\n            return filter.presentation.noOptionValue;\n        } else {\n            const defaultOption = filter.options.find((option: any) => option.default);\n            if (defaultOption) {\n                return `${getFilterOptionValue(defaultOption)}`;\n            }\n            return filter.presentation.noOptionValue;\n        }\n    }\n\n    private getDefaultValueForMultiSelectFilter(filter: DynamicFilter, hasDefault: boolean) {\n        const defaultValue = this.getDefaultValueForFilter(filter, hasDefault);\n        return !!defaultValue ? [defaultValue] : [];\n    }\n\n    private renderOverlayPill(filter: DynamicFilter, isLast?: boolean) {\n        const [label, values] = this.getSelectedValues(filter);\n        const isNew = !!(\n            filter.presentation.newLabelModel && filter.presentation.newLabelModel.labelType\n        );\n        const options = [];\n        const hasDefault = filter.options.some((option: any) => option.default);\n        const hasNoOption = filter.options.some(\n            (option) => getFilterOptionValue(option) === filter.presentation.noOptionValue\n        );\n        const isMultiSelectFilter =\n            filter.presentation.componentType === PillComponentType.MultiSelect;\n\n        const isDesktopMultiSelectFilterTstGrp2: boolean =\n            (this.props.proctorGroups?.desktop_multiselect_filter_tst.value ?? -1) === 2;\n\n        if (!hasDefault && !hasNoOption && filter.presentation.noOptionPlaceholder) {\n            options.push({\n                label: filter.presentation.noOptionPlaceholder,\n                value: filter.presentation.noOptionValue\n            });\n        }\n\n        const mapFilterOptions = (option: DynamicFilterOption) => ({\n            label: option.label,\n            value:\n                isDesktopMultiSelectFilterTstGrp2 && !isMultiSelectFilter\n                    ? option.label\n                    : getFilterOptionValue(option),\n            count: this.getCount(filter, option)\n        });\n\n        options.push(...filter.options.map(mapFilterOptions));\n        const toggleOption = filter.options.find((opt) => opt.type === 'toggle');\n\n        // For salary, data fetched from JobService would have selected option at the end, with count = -1.\n        // This selected option has the lowest salary, thus should be at the front of the array.\n        if (filter.filterName === 'salest' && options.length > 1) {\n            const selectedOption = filter.options.find(\n                (opt) => (opt as DynamicFilterBucketOption).count === -1\n            );\n            if (selectedOption) {\n                const selectedOptionIndex = options.findIndex(\n                    (opt) => opt.label === selectedOption.label\n                );\n                options.unshift(options.splice(selectedOptionIndex, 1)[0]);\n            }\n        }\n\n        const newIndicatorAriaLabel =\n            filter.presentation.newLabelModel && filter.presentation.newLabelModel.labelText\n                ? filter.presentation.newLabelModel.labelText\n                : this.props.gt.gettext('New');\n\n        const isDesktopOrUnified = this.props.isOnDesktopSERP || this.props.config.isUnifiedFilters;\n\n        return (\n            <Box as=\"li\">\n                <OverlayPill\n                    id={getId(filter, false, isDesktopOrUnified)}\n                    isLast={isLast}\n                    key={getKey(filter)}\n                    label={values.length > 0 ? label : filter.presentation.displayName}\n                    displayName={filter.presentation.displayName}\n                    isActive={values.length > 0}\n                    isNew={isNew}\n                    isMultiSelect={isMultiSelectFilter}\n                    toggleOption={toggleOption ? mapFilterOptions(toggleOption) : undefined}\n                    toggleOnTop={\n                        filter.filterName === 'salest' &&\n                        this.props.proctorGroups?.mob_serp_filter_job_without_extracted_pay_tst\n                            ?.value === 2\n                    }\n                    shouldCloseOnSubmit={this.props.config.richSearchComponentModel.isSPA}\n                    selectedValue={\n                        new Set(\n                            values.length > 0\n                                ? values\n                                : this.getDefaultValueForMultiSelectFilter(filter, hasDefault)\n                        )\n                    }\n                    newIndicatorAriaLabel={newIndicatorAriaLabel}\n                    options={options.filter(\n                        (option) =>\n                            !!option.value &&\n                            (toggleOption ? option.label !== toggleOption.label : true)\n                    )}\n                    onChange={(value) => this.handleChange(filter, ...value)}\n                    isDesktop={this.props.config.isDesktop}\n                    gt={this.props.gt}\n                    filterName={getFilterName(filter)}\n                />\n            </Box>\n        );\n    }\n\n    private renderSelectPill(filter: DynamicFilter, isLast?: boolean) {\n        const [label, values] = this.getSelectedValues(filter);\n        const isNew = !!(\n            filter.presentation.newLabelModel && filter.presentation.newLabelModel.labelType\n        );\n        const newIndicatorAriaLabel =\n            filter.presentation.newLabelModel && filter.presentation.newLabelModel.labelText\n                ? filter.presentation.newLabelModel.labelText\n                : this.props.gt.gettext('New');\n\n        if (values.length > 0 && showSelectedFilterAsToggle(filter, this.props.config.isDesktop)) {\n            // Show a button style pill to clear the filter\n\n            const optionSelected = filter.options.find((o) => o.selected);\n            const url = optionSelected?.url;\n            const urlRel = optionSelected?.urlRel;\n            if (url) {\n                return (\n                    <Box as=\"li\">\n                        <TogglePill\n                            id={getId(filter, false, true)}\n                            key={getKey(filter)}\n                            label={label}\n                            isActive={true}\n                            isNew={false}\n                            newIndicatorAriaLabel={newIndicatorAriaLabel}\n                            url={url}\n                            urlRel={urlRel}\n                            ariaLabel={\n                                // # Clearing filter text, 0 is the filter label\n                                this.props.gt.gettext('Clear {0} filter', [values[0]])\n                            }\n                            disabled={!this.props.ui.enabled}\n                        />\n                    </Box>\n                );\n            } else {\n                const { gt } = this.props;\n                const screenReaderAnnounceMessage = gt.gettext(\n                    // # Job Search Filter removed\n                    'Filter {0} has been removed',\n                    [label]\n                );\n\n                return (\n                    <Box as=\"li\">\n                        <TogglePill\n                            id={getId(filter, false, false)}\n                            key={getKey(filter)}\n                            label={label}\n                            isActive={true}\n                            isNew={isNew}\n                            newIndicatorAriaLabel={newIndicatorAriaLabel}\n                            onClick={() => {\n                                this.setState({\n                                    screenReaderAnnounceMessage,\n                                    srCounter: this.state.srCounter + 1\n                                });\n                                this.handleChange(filter, filter.presentation.noOptionValue);\n                            }}\n                            ariaLabel={gt.gettext('Remove {0} filter', [label])}\n                            disabled={!this.props.ui.enabled}\n                        />\n                    </Box>\n                );\n            }\n        }\n\n        const hasUrl = filter.options.find((o) => o.url);\n        const id = getId(filter, true, true);\n\n        if (hasUrl) {\n            const options = filter.options.map((option, index) => ({\n                label: option.label,\n                value: getFilterOptionValue(option),\n                url: option.url!,\n                urlRel: option.urlRel!,\n                count: this.getCount(filter, option),\n                onMouseDown: () => {\n                    setPtkCookie('jobsearch', 'rb', filter.filterName, index + 1);\n                    this.props.logger.frontendLogger?.log('FilterPill', `filterApplied`, {\n                        ['filterSource']: id.replace('filter-', ''),\n                        ['filterOption']: option.label.replace(/ /g, '-')\n                    });\n                }\n            }));\n            return (\n                <Box as=\"li\">\n                    <DropdownPill\n                        id={id}\n                        key={getKey(filter)}\n                        ariaLabel={this.props.gt.gettext('{0}{1} filter', [\n                            isNew ? `${newIndicatorAriaLabel} ` : '',\n                            values.length > 0 ? label : filter.presentation.displayName\n                        ])}\n                        label={values.length > 0 ? label : filter.presentation.displayName}\n                        isActive={values.length > 0}\n                        isNew={isNew}\n                        newIndicatorAriaLabel={newIndicatorAriaLabel}\n                        options={options}\n                        disabled={!this.props.ui.enabled}\n                    />\n                </Box>\n            );\n        } else {\n            const options = [];\n            const notDisableArray = [\n                DynamicFilterDataType.STATIC,\n                DynamicFilterDataType.REFINEBY,\n                DynamicFilterDataType.ATTRIBUTE\n            ];\n            const hasDefault = filter.options.some((option: any) => option.default);\n            const hasNoOption = filter.options.some(\n                (option) => getFilterOptionValue(option) === filter.presentation.noOptionValue\n            );\n            if (!hasDefault && !hasNoOption && filter.presentation.noOptionPlaceholder) {\n                options.push({\n                    label: filter.presentation.noOptionPlaceholder,\n                    value: filter.presentation.noOptionValue,\n                    disabled: !notDisableArray.includes(filter.dataType)\n                });\n            }\n\n            options.push(\n                ...filter.options.map((option) => ({\n                    label: option.label,\n                    value: getFilterOptionValue(option),\n                    disabled: false,\n                    count: this.getCount(filter, option)\n                }))\n            );\n\n            return (\n                <Box as=\"li\">\n                    <SelectionPill\n                        isLast={isLast}\n                        id={getId(filter, true, false)}\n                        key={getKey(filter)}\n                        ariaLabel={this.props.gt.gettext('{0}{1} filter', [\n                            isNew ? `${newIndicatorAriaLabel} ` : '',\n                            values.length > 0 ? label : filter.presentation.displayName\n                        ])}\n                        label={values.length > 0 ? label : filter.presentation.displayName}\n                        isActive={false}\n                        isNew={isNew}\n                        newIndicatorAriaLabel={newIndicatorAriaLabel}\n                        value={values.length > 0 ? values[0] : filter.presentation.noOptionValue}\n                        options={options}\n                        disabled={!this.props.ui.enabled}\n                        onChange={(value) => this.handleChange(filter, value)}\n                    />\n                </Box>\n            );\n        }\n    }\n\n    private renderChipPill(filter: DynamicFilter) {\n        const options: ChipPillSelectionOption[] = filter.options.map((option, index) => ({\n            label: option.label,\n            value: getFilterOptionValue(option),\n            selected: option.selected,\n            index\n        }));\n        return (\n            <Box as=\"li\">\n                <ChipPill\n                    options={options}\n                    label={filter.presentation.displayName}\n                    onChange={(value: string[]) => this.handleChange(filter, ...value)}\n                />\n            </Box>\n        );\n    }\n\n    public render() {\n        const { gt } = this.props;\n        const filters = this.getSortedFilters();\n        const moveSelectedFiltersToBeginning = this.moveSelectedFiltersToBeginning(\n            this.props.config.isDesktop,\n            this.props.config.richSearchComponentModel.country\n        );\n\n        if (!filters) {\n            return null;\n        }\n\n        const sortFiltersOnRender = (dynamicFilters: DynamicFilter[]) => {\n            const sortedFilters = dynamicFilters.filter(\n                (filter) => filter.presentation.pillVisible && filter.presentation.order >= 0\n            );\n\n            if (!moveSelectedFiltersToBeginning) {\n                return sortedFilters.sort(\n                    (f1, f2) => f1.presentation.order - f2.presentation.order\n                );\n            }\n            return sortedFilters;\n        };\n\n        const displayFilters = filters.filter(\n            (filter) => filter.presentation.pillVisible && filter.presentation.order >= 0\n        );\n        const displayFiltersLenth = displayFilters.length - 1;\n        const isDesktopMultiSelectFilterTstGrp2: boolean =\n            (this.props.proctorGroups?.desktop_multiselect_filter_tst.value ?? -1) === 2;\n\n        const isYosegiSrNavHeadings: boolean =\n            (this.props.proctorGroups?.jsj_yosegi_sr_nav_headings_tst?.value ?? -1) > 0;\n\n        let suggestedFilter: DynamicFilter | undefined;\n        const indexOfSuggestedFilter = filters\n            .map((filter) => filter.filterName)\n            .indexOf('suggested');\n        if (indexOfSuggestedFilter !== -1) {\n            suggestedFilter = filters[indexOfSuggestedFilter];\n        }\n\n        const skippedFilters = ['suggested'];\n        // hardcoding this because testing only in U.S., but need to update if changed.\n        const skippedOptions = ['All jobs', 'Relevance'];\n        const activeFilterCount = filters\n            .filter((f) => !skippedFilters.includes(f.filterName ?? ''))\n            .filter((f) =>\n                f.options.some((opt) => !skippedOptions.includes(opt.label) && opt.selected)\n            ).length;\n        const FilterIconColer = activeFilterCount && suggestedFilter ? '#ffffff' : '#2D2D2D';\n\n        return (\n            <React.Fragment>\n                <div\n                    className={classnames(\n                        'yosegi-FilterPill',\n                        'yosegi-FilterPill-container',\n                        {\n                            'is-desktop': this.props.config.isDesktop\n                        },\n                        { 'not-suggestedFilter': !suggestedFilter }\n                    )}\n                >\n                    {isYosegiSrNavHeadings && (\n                        <VisuallyHidden>\n                            <Heading as=\"h2\">{this.props.gt.gettext('Filters')}</Heading>\n                        </VisuallyHidden>\n                    )}\n                    <Box\n                        as=\"ul\"\n                        className={classnames('yosegi-FilterPill-pillList', {\n                            'is-suggestedFilter': suggestedFilter\n                        })}\n                        sx={{\n                            listStyleType: 'none',\n                            minWidth: 'auto'\n                        }}\n                    >\n                        {this.props.onMoreButtonClick && (\n                            <Box as=\"li\" sx={{ minWidth: 'auto' }}>\n                                <button\n                                    id=\"pill-moreFilters\"\n                                    className={classnames(\n                                        'yosegi-FilterPill-pill yosegi-FilterPill-moreFilters',\n                                        { active: activeFilterCount && suggestedFilter }\n                                    )}\n                                    aria-label={gt.gettext('All filters')}\n                                    onClick={this.props.onMoreButtonClick}\n                                    disabled={!this.props.ui.enabled}\n                                >\n                                    <FilterIcon color={FilterIconColer} />\n                                    {suggestedFilter && activeFilterCount > 0 && (\n                                        <div\n                                            id=\"activeFiltersCoun