\n                return { query, newJobCount: null, jobCountLoading: false };\n            });\n        }\n\n        // @jballands: Otherwise, use the RQ cookie\n        const isGlobalVersion = this.rootStore.config.richSearchComponentModel.isGlobalVersion;\n        const recentSearchesInCookie = getRecentSearchesFromCookie();\n        if (isGlobalVersion) {\n            if (!this.rootStore.config.isOnHomePage && !this.rootStore.config.isSearchStartPage) {\n                recentSearchesInCookie.shift();\n            }\n        }\n\n        return getExtendedRecentSearches(recentSearchesInCookie);\n    };\n\n    @computed\n    public get hasRecentSearch() {\n        if (isBrowser()) {\n            return this.recentSearches.length > 0;\n        }\n        return this.rootStore.config.hasRecentSearch;\n    }\n\n    @computed\n    public get currentRecentSearch(): RecentSearch | null {\n        const currentRecentSearch = this.recentSearches.find((recentSearchInstate) => {\n            return (\n                this.rootStore.initialSearchInput &&\n                isSameSearchIntent(recentSearchInstate.query, this.rootStore.initialSearchInput)\n            );\n        });\n\n        return currentRecentSearch ? currentRecentSearch.query : null;\n    }\n\n    @action\n    public fetchRecentSearches = () => {\n        this.recentSearches = this.getRecentSearches();\n    };\n\n    public fetchNewJobsCountForRecentSearch = () => {\n        const {\n            ui: { isShowRecentSearch, mobtk }\n        } = this.rootStore;\n\n        if (!isShowRecentSearch) {\n            return;\n        }\n\n        this.recentSearches.forEach((rq) => {\n            const { query } = rq;\n            const url = getRecentSearchJobCountUrl(toJS(query), { mobtk });\n            const ts = query.ts;\n            rq.newJobCount = undefined;\n\n            const index = this.findIndex(ts);\n            if (index >= 0) {\n                this.recentSearches[index].jobCountLoading = true;\n            }\n\n            fetchNewJobsCount(url, ts)\n                .then((newJobCount) => {\n                    this.recentSearches = this.recentSearches.map((recentSearch) => {\n                        if (recentSearch.query.ts === ts) {\n                            return {\n                                ...recentSearch,\n                                newJobCount\n                            };\n                        } else {\n                            return recentSearch;\n                        }\n                    });\n                })\n                .catch(() => {\n                    this.recentSearches = this.recentSearches.map((recentSearch) => {\n                        if (recentSearch.query.ts === ts) {\n                            return {\n                                ...recentSearch,\n                                newJobCount: null\n                            };\n                        } else {\n                            return recentSearch;\n                        }\n                    });\n                })\n                .finally(() => {\n                    const indexAfterFinish = this.findIndex(ts);\n                    if (indexAfterFinish >= 0) {\n                        this.recentSearches[indexAfterFinish].jobCountLoading = false;\n                    }\n                });\n        });\n    };\n\n    @action\n    public deleteRecentSearch = (ts: string) => {\n        this.deletedRecentSearchesCookie = getRecentSearchesCookie();\n        deleteSearchFromCookie(ts);\n        this.recentSearches = this.recentSearches.filter(({ query }) => query.ts !== ts);\n    };\n\n    @action\n    public deleteRecentSearches = () => {\n        this.deletedRecentSearchesCookie = getRecentSearchesCookie();\n        clearRecentSearchesCookie();\n        this.recentSearches = [];\n    };\n\n    @action\n    public restoreRecentSearches = () => {\n        if (this.deletedRecentSearchesCookie) {\n            setRecentSearchesCookie(this.deletedRecentSearchesCookie);\n        }\n        this.deletedRecentSearchesCookie = null;\n        this.fetchRecentSearches();\n    };\n\n    public dispose = () => {};\n}\n","import { SavedSearchData } from '@indeed/cmi-jp-common-components/lib/types/savedSearch';\nimport { splitSavedSearchTag } from '@indeed/cmi-jp-common-components/lib/util/savedSearches/utils';\n\nimport { LineAlertFormData } from '../types/savedSearch';\n\nexport const mapLineAlertUserData = (\n    encryptedUserId: string,\n    language: string,\n    country: string\n): LineAlertFormData['user'] => {\n    return {\n        locale: `${language}_${country}`,\n        client: 'yosegi-toggle',\n        platform: 'LINE',\n        encryptedUserId\n    };\n};\n\nexport const mapLineAlertSalaryRangeData = ({\n    salarytype,\n    salarymax,\n    salarymin\n}: SavedSearchData) => {\n    if (!salarytype) {\n        return undefined;\n    }\n    return {\n        type: salarytype,\n        min: salarymin ? parseInt(salarymin, 10) : undefined,\n        max: salarymax ? parseInt(salarymax, 10) : undefined\n    };\n};\n\nexport const mapLineAlertData = (saveSearchData: SavedSearchData): LineAlertFormData['alert'] => {\n    const {\n        what = '',\n        where = '',\n        radius,\n        tags,\n        type,\n        occupation,\n        searchCriteria\n    } = saveSearchData;\n    return {\n        what,\n        where,\n        radius: radius ? parseInt(radius, 10) : undefined,\n        tags: tags ? splitSavedSearchTag(tags) : undefined,\n        salaryRange: mapLineAlertSalaryRangeData(saveSearchData),\n        jobType: type,\n        occupations: occupation ? [occupation] : undefined,\n        sc: searchCriteria\n    };\n};\n","import { SavedSearchData } from '@indeed/cmi-jp-common-components/lib/types/savedSearch';\nimport { getSavedSearchUrlQuery } from '@indeed/cmi-jp-common-components/lib/util/savedSearches/urls';\nimport { stringify } from 'qs';\n\nimport RichSearchTrackEventLogger