              {pgettext('search removed', 'Search removed')}\n                </Text>\n                <Button\n                    size=\"sm\"\n                    variant=\"text\"\n                    onClick={onUndoClick}\n                    // # a11y; aria label for 'undo delete recent search'; jobseeker\n                    aria-label={pgettext('undo delete recent search', 'undo')}\n                    ref={undoButtonRef}\n                    data-testid=\"undo-button\"\n                    type=\"button\"\n                >\n                    {pgettext('undo delete recent search', 'Undo')}\n                </Button>\n            </Flex>\n        );\n    }\n\n    const deleteRecentSearchesAriaLabel = pgettext(\n        'delete recent search, {0} is job title',\n        'delete recent search {0}',\n        [upperText]\n    );\n\n    return (\n        <Flex\n            role=\"listitem\"\n            as=\"div\"\n            data-cy={`item${index}`}\n            data-rso=\"recent-search-options\" // Locator will be the same for each option\n            className={classNames(\n                'yosegi-RecentSearchList-item',\n                {\n                    'yosegi-RecentSearchList-item-reselectv2': jsj_yosegi_reselect_v2_tst.value > 0\n                },\n                {\n                    'yosegi-RecentSearchList-selected': customTabbing?.focusedIndex === index\n                }\n            )}\n        >\n            <div\n                className={classNames('yosegi-RecentSearchList-icon', {\n                    'yosegi-RecentSearchList-icon-searchstartpage': props.isSearchStartPage\n                })}\n            >\n                <RecentSearchIcon />\n            </div>\n            <Box\n                as=\"a\"\n                sx={recentSearchItemLinkSx}\n                aria-label={linkLabel}\n                href={href}\n                onClick={(event) => {\n                    // The consumer of this can handle the actual navigation o