SearchCriteriaCompositeFilter,\n    SearchCriteriaFilter,\n    SearchCriteriaKeywordFilter,\n    SearchCriteriaSalaryFilter\n} from '../../types/searchCriteria';\n\nexport const deserializeSc = (serialized?: string) => {\n    const result = deserializeSearchCriteria(serialized);\n    if (result.status === 'SUCCESS') {\n        return result.searchCriteria;\n    }\n    return undefined;\n};\n\nexport const mergeFilters = <T>(\n    serializer: (filter: T) => string,\n    lists: Array<ReadonlyArray<T> | undefined>\n) => {\n    const existing = new Set();\n    return lists\n        .filter((f): f is T[] => !!f)\n        .flatMap((f) =>\n            f.flatMap((v) => {\n                const s = serializer(v);\n                if (!existing.has(s)) {\n                    existing.add(s);\n                    return [v];\n                }\n                return [];\n            })\n        );\n};\n\nexport const mergeSearchCriteria = (...items: Array<SearchCriteria>): SearchCriteria => ({\n    params: items.reduce((prev, curr) => ({ ...prev, ...curr.params }), {}),\n    keywordFilters: mergeFilters(\n        (v) => serializeKeywordFilters([{ ...v, keys: [...v.keys] }]),\n        items.map((v) => v.keywordFilters)\n    ),\n    booleanFilters: mergeFilters(\n        (v) => serializeBooleanFilters([v]),\n        items.map((v) => v.booleanFilters)\n    ),\n    rangeFilters: mergeFilters(\n        (v) => serializeRangeFilters([v]),\n        items.map((v) => v.rangeFilters)\n    ),\n    salaryFilter: items.reduce<SalaryFilter | undefined>(\n        (prev, curr) => (curr.salaryFilter ? curr.salaryFilter : prev),\n        undefined\n    )\n});\n\nexport const removeFiltersFromSc = (\n    searchCriteria: SearchCriteria,\n    filters: SearchCriteriaFilter[]\n) => {\n    removeKeywordFilters(\n        searchCriteria,\n        filters.filter((filter): filter is SearchCriteriaKeywordFilter => filter.type === 'keyword')\n    );\n    removeBooleanFilters(\n        searchCriteria,\n        filters.filter((filter): filter is SearchCriteriaBooleanFilter => filter.type === 'boolean')\n    );\n    if (filters.some((filter) => filter.type === 'salary')) {\n        removeSalaryFilter(searchCriteria);\n    }\n};\n\nconst removeKeywordFilters = (\n    searchCriteria: SearchCriteria,\n    filters: SearchCriteriaKeywordFilter[]\n) => {\n    const scKeywordFilters = searchCriteria.keywordFilters;\n    if (scKeywordFilters === undefined) {\n        return;\n    }\n\n    const fieldKeyMap: Map<string, string[]> = new Map<string, string[]>();\n    filters.forEach((filter) => {\n        fieldKeyMap.set(filter.field, (fieldKeyMap.get(filter.field) ?? []).concat(filter.keys));\n    });\n\n    const newFilters: KeywordFilter[] = [];\n    for (const keywordFilter of scKeywordFilters) {\n        const keysForField = fieldKeyMap.get(keywordFilter.field);\n        // Currently it only supports ANY and MUST match filters\n        const isFilterRelatedToKeys =\n            keywordFilter.matchType === 'ANY' &&\n            keywordFilter.match === 'MUST' &&\n            keysForField !== undefined &&\n            containsAnyKeys(keywordFilter.keys, keysForField);\n        if (!isFilterRelatedToKeys) {\n            newFilters.push(keywordFilter);\n        } else {\n            // Only remove specified keys for AND filters\n            if (keywordFilter.operation === 'AND') {\n                const newKeywordFilter = { ...keywordFilter };\n                newKeywordFilter.keys = keywordFilter.keys.filter(\n                    (key) => !keysForField?.includes(key)\n                );\n                if (newKeywordFilter.keys.length > 0) {\n                    newFilters.push(newKeywordFilter);\n                }\n            }\n        }\n    }\n    searchCriteria.keywordFilters = newFilters;\n};\n\nconst removeBooleanFilters = (\n    searchCriteria: SearchCriteria,\n    filters: SearchCriteriaBooleanFilter[]\n) => {\n    if (searchCriteria.booleanFilters === undefined) {\n        return;\n    }\n\n    const filterNames = filters.map((filter) => filter.field);\n    searchCriteria.booleanFilters = searchCriteria.booleanFilters.filter(\n        (filter) => !(filterNames.includes(filter.field) && filter.match === 'MUST')\n    );\n};\n\nconst removeSalaryFilter = (searchCriteria: SearchCriteria) => {\n    searchCriteria.salaryFilter = undefined;\n};\n\nexport const addMultiSelectFilters = (\n    searchCriteria: SearchCriteria,\n    filters: SearchCriteriaFilter[]\n) => {\n    addMultiSelectKeywordFilters(\n        searchCriteria,\n        filters.filter((filter): filter is SearchCriteriaKeywordFilter => filter.type === 'keyword')\n    );\n};\n\nexport const removeKeywordFilterWithValue = (searchCriteria: SearchCriteria, value: string) => {\n    searchCriteria.keywordFilters = searchCriteria.keywordFilters?.filter((filter) => {\n        return !filter.keys.includes(value);\n    });\n};\n\nconst addMultiSelectKeywordFilters = (\n    searchCriteria: SearchCriteria,\n    filters: SearchCriteriaKeywordFilter[]\n) => {\n    const fieldKeyMap: Map<string, string[]> = new Map<string, string[]>();\n    filters.forEach((filter) => {\n        fieldKeyMap.set(filter.field, (fieldKeyMap.get(filter.field) ?? []).concat(filter.keys));\n    });\n\n    for (const [field, keys] of fieldKeyMap.entries()) {\n        if (searchCriteria.keywordFilters === undefined) {\n            searchCriteria.keywordFilters = [];\n        }\n        const combinedFilter: SearchCriteriaKeywordFilter = { type: 'keyword', field, keys };\n        addKeywordFilter(searchCriteria, combinedFilter);\n    }\n};\n\nexport const addFiltersToSc = (searchCriteria: SearchCriteria, filters: SearchCriteriaFilter[]) => {\n    for (const filter of filters) {\n        switch (filter.type) {\n            case 'keyword':\n                if (searchCriteria.keywordFilters === undefined) {\n                    searchCriteria.keywordFilters = [];\n                }\n                addKeywordFilter(searchCriteria, filter);\n                break;\n            case 'salary':\n                setSalaryFilter(searchCriteria, filter);\n                break;\n            case 'boolean':\n                addBooleanFilter(searchCriteria, filter);\n                break;\n            case 'composite':\n                setCompositeFilter(searchCriteria, filter);\n                break;\n            default:\n                break;\n        }\n    }\n};\n\nconst addKeywordFilter = (searchCriteria: SearchCriteria, filter: SearchCriteriaKeywordFilter) => {\n    if (searchCriteria.keywordFilters === undefined) {\n        searchCriteria.keywordFilters = [];\n    }\n    // Check if searchCriteria already contains the filter to add\n    const existingFilters = searchCriteria.keywordFilters.filter((kf) =>\n        isTargetKeywordFilter(kf, filter)\n    );\n    if (existingFilters.length === 0) {\n        const keywordFilter = createKeywordFilter({\n            field: filter.field,\n            // @ts-ignore `createKeywordFilter` accepts mutable array, should be readonly array\n            keys: filter.keys,\n            operation: filter.keys.length === 1 ? 'AND' : 'OR'\n        });\n        if (keywordFilter.result !== undefined) {\n            searchCriteria.keywordFilters.push(keywordFilter.result);\n        }\n    }\n};\n\nconst isTargetKeywordFilter = (\n    keywordFilter: KeywordFilter,\n    searchCriteriaKeywordFilter: SearchCriteriaKeywordFilter\n) => {\n    if (keywordFilter.matchType !== 'ANY' || keywordFilter.match !== 'MUST') {\n        return false;\n    }\n    if (searchCriteriaKeywordFilter.keys.length === 0) {\n        return false;\n    } else if (searchCriteriaKeywordFilter.keys.length === 1) {\n        if (\n            keywordFilter.operation === 'AND' ||\n            (keywordFilter.operation === 'OR' && keywordFilter.keys.length === 1)\n        ) {\n            return containsAnyKeys(keywordFilter.keys, searchCriteriaKeywordFilter.keys);\n        } else {\n            return false;\n        }\n    } else {\n        if (keywordFilter.operation === 'OR') {\n            return containsSameKeys(keywordFilter.keys, searchCriteriaKeywordFilter.keys);\n        } else {\n            return false;\n        }\n    }\n};\n\n/**\n * Check if keys1 and keys2 contain same set of keys\n */\nconst containsSameKeys = (keys1: readonly string[], keys2: readonly string[]) => {\n    return containsAllKeys(keys1, keys2) && containsAllKeys(keys2, keys1);\n};\n\n/**\n * Check if keys1 contains all elements in keys2\n */\nconst containsAllKeys = (keys1: readonly string[], keys2: readonly string[]) => {\n    for (const key of keys2) {\n        if (!keys1.includes(key)) {\n            return false;\n        }\n    }\n    return true;\n};\n\n/**\n * Check if keys1 contains any elements in keys2\n */\nconst containsAnyKeys = (keys1: readonly string[], keys2: readonly string[] | undefined) => {\n    if (keys2 === undefined) {\n        return false;\n    }\n    for (const key of keys2) {\n        if (keys1.includes(key)) {\n            return true;\n        }\n    }\n    return false;\n};\n\nconst setSalaryFilter = (searchCriteria: SearchCriteria, filter: SearchCriteriaSalaryFilter) => {\n    const salaryFilter = createSalaryFilter({\n        field: 'sal', // TODO: is there a const for this?\n        min: filter.min,\n        max: filter.max,\n        type: filter.salaryType\n    });\n    if (salaryFilter.result !== undefined) {\n        searchCriteria.salaryFilter = salaryFilter.result;\n    }\n};\n\nconst addBooleanFilter = (searchCriteria: SearchCriteria, filter: SearchCriteriaBooleanFilter) => {\n    if (searchCriteria.booleanFilters === undefined) {\n        searchCriteria.booleanFilters = [];\n    }\n\n    // Filter out any boolean filter that has the same field name with the input.\n    // It does not make sense to have two boolean filters with contradicting match (MUST and MUST_NOT) or\n    // to have two boolean filters with the same match.\n    searchCriteria.booleanFilters = searchCriteria.booleanFilters.filter(\n        (bf) => bf.field !== filter.field\n    );\n    const booleanFilter = createBooleanFilter({\n        field: filter.field,\n        match: 'MUST'\n    });\n    if (booleanFilter.result !== undefined) {\n        searchCriteria.booleanFilters.push(booleanFilter.result);\n    }\n};\n\nconst setCompositeFilter = (\n    searchCriteria: SearchCriteria,\n    filter: SearchCriteriaCompositeFilter\n) => {\n    searchCriteria.params.cf = filter.value;\n};\n","/**\n * Returns the input string with dangerous HTML characters encoded.\n *\n * To be used when printing HTML directly to the page to prevent cross-site scripting attacks:\n *   - HTML contexts\n *   - HTML attribute contexts (that are surrounded by quote marks)\n *\n * This should not be used for printing to script contexts that are not interpreted as HTML!\n *\n * https://cheatsheetseries.owasp.org/cheatsheets/Cross_Site_Scripting_Prevention_Cheat_Sheet.html\n */\nexport function escapeDangerousHTML(input: string): string {\n    if (input === null || input === undefined) {\n        return '';\n    }\n\n    return input\n        .replace(/&/g, '&amp;')\n        .replace(/</g, '&lt;')\n        .replace(/>/g, '&gt;')\n        .replace(/\"/g, '&quot;')\n        .replace(/'/g, '&#x27;')\n        .replace(/\\//g, '&#x2F;');\n}\n","import React from 'react';\n\nimport { StoreContextData } from '../../context';\nimport observing from '../../hoc/observing';\nimport { escapeDangerousHTML } from '../../util/htmlEncoding';\n\nconst QueryReplace = (props: StoreContextData) => {\n    const className = props.config.isDesktop\n        ? 'yosegi-QueryReplace--isDesktop'\n        : 'yosegi-QueryReplace';\n\n    return props.config.richSearchComponentModel.queryReplaceModel ? (\n        <div className={className}>\n            <span\n                className=\"yosegi-QueryReplace-title\"\n                dangerouslySetInnerHTML={{\n                    __html: props.gt.pgettext(\n                        'Query replace',\n                        'Showing results for <b>{0}</b> jobs',\n                        escapeDangerousHTML(\n                            props.config.richSearchComponentModel.queryReplaceModel.replacedWhat\n                        )\n                    )\n                }}\n            />\n            <br />\n            <span\n                dangerouslySetInnerHTML={{\n                    __html: props.gt.pgettext(\n                        'Query replace',\n                        'Did you mean: <a class=\"yosegi-QueryReplace-link\" href={0}>{1}</a>',\n                        [\n                            escapeDangerousHTML(\n                                props.config.richSearchComponentModel.queryReplaceModel.originalLink\n                            ),\n                            escapeDangerousHTML(\n                                props.config.richSearchComponentModel.queryReplaceModel.originalWhat\n                            )\n                        ]\n                    )\n                }}\n            />\n        </div>\n    ) : null;\n};\n\nexport { QueryReplace as BaseQueryReplace };\nexport default observing(QueryReplace);\n","import React from 'react';\n\nimport { Box, Button, ButtonStyleLink, Flex } from '@indeed/ifl-components';\nimport { observer } from 'mobx-react-lite';\n\nimport { useStore } from '../../context';\nimport { useI18n } from '../../localization/useI18n';\nimport { RemoteSearchCalloutModel } from '../../types/RemoteSearchCalloutModel';\nimport { SearchInputField } from '../../types/SearchInput';\n\nimport {\n    calloutActionSx,\n    calloutContainerAtEndSx,\n    calloutContainerSx,\n    calloutHeadingSx\n} from './RemoteSearchCallout.jss';\n\ninterface Props {\n    onSearch: (event?: React.MouseEvent<HTMLElement> | React.KeyboardEvent<HTMLElement>) => void;\n    remoteSearchCalloutModel?: RemoteSearchCalloutModel;\n    isCaretAtEnd?: boolean;\n}\n\nconst FORCE_LOCATION = 'forceLocation';\n\nconst RemoteSearchCallout = observer((props: Props) => {\n    const store = useStore();\n    const gt = useI18n();\n    const {\n        search: {\n            canonical: { keyword, location, setCustomField, reset }\n        }\n    } = store;\n    const { remoteSearchCalloutModel, onSearch, isCaretAtEnd } = props;\n\n    let limitLocation;\n    let actions: JSX.Element;\n    if (remoteSearchCalloutModel) {\n        limitLocation = remoteSearchCalloutModel.limitLocation;\n\n        actions = (\n            <>\n                <ButtonStyleLink\n                    variant={'text'}\n                    sx={calloutActionSx}\n                    href={remoteSearchCalloutModel.yesHref}\n                >\n                    {gt.gettext('Yes')}\n                </ButtonStyleLink>\n                <ButtonStyleLink\n                    variant={'text'}\n                    sx={calloutActionSx}\n                    href={remoteSearchCalloutModel.noHref}\n                >\n                    {gt.gettext('No')}\n                </ButtonStyleLink>\n            </>\n        );\n    } else {\n        limitLocation = location;\n\n        const handleAction = (value: string) => (\n            event?: React.MouseEvent<HTMLElement> | React.KeyboardEvent<HTMLElement>\n        ) => {\n            setCustomField(FORCE_LOCATION, value);\n            /**\n             * If RemoteSearchCallout shows up and we select no, we remove the location\n             */\n            if (value === '0') {\n                reset(SearchInputField.LOCATION);\n            }\n            onSearch(event);\n        };\n\n        actions = (\n            <>\n                <Button\n                    sx={calloutActionSx}\n                    variant={'text'}\n                    type={'button'}\n                    onClick={handleAction('1')}\n                >\n                    {gt.gettext('Yes')}\n                </Button>\n                <Button\n            