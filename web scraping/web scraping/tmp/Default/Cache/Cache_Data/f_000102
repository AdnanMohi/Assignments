  }\n        }\n        onSuggestionsChanged(suggestions) {\n            if (this.props.onSuggestionsChanged) {\n                this.props.onSuggestionsChanged(suggestions);\n            }\n        }\n        mergePropsIntoAcOptions() {\n            const { onSelect, onLogging, onValueChanged, onSuggestionsChanged, onSessionCreated, classPrefix = this.props.config.classPrefix, // classPrefix might also be set in props.config\n            disableMatchHighlight = this.props.config.disableMatchHighlight, scrollable = this.props.config.scrollable, maxHeight = this.props.config.maxHeight, endpoint = this.props.config.endpoint, disableDefaultStyle = this.props.config.disableDefaultStyle, unfocusAfterSelection = this.props.config.unfocusAfterSelection, } = this.props;\n            return Object.assign(Object.assign({}, this.props.config), { classPrefix,\n                disableMatchHighlight,\n                disableDefaultStyle,\n                scrollable,\n                maxHeight,\n                endpoint,\n                unfocusAfterSelection, suggestionsChangeListeners: onSuggestionsChanged\n                    ? [this.onSuggestionsChanged]\n                    : this.props.config.suggestionsChangeListeners, selectListeners: onSelect ? [this.onSelect] : this.props.config.selectListeners, loggingListeners: onLogging ? [this.onLogging] : this.props.config.loggingListeners, valueChangeListeners: onValueChanged\n                    ? [this.onValueChanged]\n                    : this.props.config.valueChangeListeners, sessionCreatedListeners: onSessionCreated\n                    ? [onSessionCreated]\n                    : this.props.config.sessionCreatedListeners, merged: this.props.config.merged === undefined || this.props.config.merged === null\n                    ? true\n                    : this.props.config.merged, uiVersion: uiVersion });\n        }\n        itemSelectable(idx) {\n            const item = this.state.items[idx];\n            if (this.ac.isGrouped() &&\n                !this.ac.isParentSelectable() &&\n                item.sourceType !== 'SUB_SUGGESTION') {\n                return false;\n            }\n            return true;\n        }\n        pause() {\n            this.paused = true;\n        }\n        resume() {\n            this.paused = false;\n        }\n        isPaused() {\n            return this.paused;\n        }\n        destroy() {\n            this.paused = true;\n            this.ac.onPageUnload(false);\n        }\n        // handle events when this.state.items changes\n        handleSuggestionsChangeEvents() {\n            this.setCombinedSuggestions();\n            this.setState({ isAutocompleteOpen: this.state.items.length !== 0 });\n            this.ac.onSuggestionChanged(this.state.items);\n        }\n        handleSelectEvents(selectedItem) {\n            this.setSelectedItem(selectedItem);\n            this.ac.onSelected(selectedItem);\n        }\n        handleHoverEvents(selectedItem) {\n            this.ac.onHovered(selectedItem);\n        }\n        handleSubmit() {\n            this.ac.onPageUnload(true);\n        }\n        handleClear(e) {\n            const counter = (this.seqIdCounter += 1);\n            this.setState({ items: [], combinedSuggestions: {}, seqId: counter, chosenIndex: -1, value: '' }, this.handleSuggestionsChangeEvents);\n            if (this.inputElement.current) {\n                this.inputElement.current.focus();\n                if (e && e.preventDefault) {\n                    e.preventDefault();\n                }\n            }\n            if (this.props.onClear) {\n                this.props.onClear(e);\n            }\n        }\n        handleUnload() {\n            this.ac.onPageUnload(false);\n        }\n        handleFocus(e) {\n            try {\n                const focusAfterSelection = this.focusAfterSelection;\n                this.focusAfterSelection = false;\n                if (this.paused || !this.ac.shouldSuggestOnFocus()) {\n                    return;\n                }\n                // focusAfterSelection == true means this focus event is triggered after selection.\n                // We should not request suggestions in this case unless suggestOnSelect is enabled.\n                if (focusAfterSelection && !this.ac.shouldSuggestOnSelect()) {\n                    return;\n                }\n                this.getSuggestions();\n            }\n            finally {\n                if (this.props.onFocus) {\n                    this.props.onFocus(e);\n                }\n            }\n        }\n        // on itemClick need to reset the autocomplete for SingleTypeList\n        // to listen for blur events\n        handleClick(idx) {\n            const item = this.state.items[idx];\n            const payload = item.payload;\n            if (!this.itemSelectable(idx)) {\n                if (this.inputElement.current) {\n                    this.inputElement.current.focus();\n                }\n                return;\n            }\n            let complement;\n            if (this.ac.types()) {\n                complement = { type: item.segment };\n            }\n            if (this.ac.isGrouped()) {\n                complement = Object.assign(Object.assign({}, complement), { parentIndex: payload.pIdx, parentSuggestion: payload.suggestion, childIndex: payload.idx, childSuggestion: item.suggestion });\n            }\n            this.ac.suggestionSelected(this.originalQuery || '', payload.idx, item.suggestion, complement);\n            const suggestionToDisplay = this.props.selectedSuggestionRenderer\n                ? this.props.selectedSuggestionRenderer(item)\n                : item.suggestion;\n            this.updateInputValue(suggestionToDisplay);\n            this.setState({ items: [], chosenIndex: -1, seqId: (this.seqIdCounter += 1) }, this.handleSuggestionsChangeEvents);\n            if (this.inputElement.current &&\n                !this.compositionStart &&\n                !this.ac.shouldUnfocusAfterSelection()) {\n                this.focusAfterSelection = true;\n                this.inputElement.current.focus();\n            }\n            this.ignoreBlur = false;\n            this.blurAfterSelections = true;\n            this.originalQuery = null;\n            // highlight the tab corresponding to the clicked item\n            if (this.state.combinedSuggestions && item.payload !== undefined) {\n                this.setChosenTab(payload.segment);\n            }\n            this.handleSelectEvents(item);\n        }\n        handleMouseOut() {\n            this.setState({ chosenIndex: -1 });\n        }\n        handleMouseOver(idx) {\n            this.setState({ chosenIndex: idx });\n        }\n        updateInputValue(value) {\n            if (!uncontrolled) {\n                this.setState({ value });\n            }\n            else if (this.inputElement.current) {\n                this.inputElement.current.value = value;\n            }\n            this.ac.onValueChange(value);\n        }\n        getInputValue() {\n            if (!uncontrolled) {\n                return this.state.value;\n            }\n            else if (this.inputElement.current) {\n                return this.inputElement.current.value;\n            }\n            return '';\n        }\n        handleKeyArrowUp() {\n            this.setState((state) => {\n                let chosenIndex = state.chosenIndex;\n                for (let i = 0; i < state.items.length; i++) {\n                    chosenIndex -= 1;\n                    if (chosenIndex < 0) {\n                        chosenIndex = state.items.length - 1;\n                    }\n                    if (this.itemSelectable(chosenIndex)) {\n                        return { chosenIndex };\n                    }\n                }\n                return null;\n            }, () => {\n                const item = this.state.items[this.state.chosenIndex];\n                if (item) {\n                    const suggestionToDisplay = this.props.selectedSuggestionRenderer\n                        ? this.props.selectedSuggestionRenderer(item)\n                        : item.suggestion;\n                    this.updateInputValue(suggestionToDisplay);\n                    this.handleHoverEvents(item);\n                }\n            });\n        }\n        handleKeyArrowDown() {\n            this.setState((state) => {\n                let chosenIndex = state.chosenIndex;\n                for (let i = 0; i < state.items.length; i++) {\n                    chosenIndex += 1;\n                    if (chosenIndex >= state.items.length) {\n                        chosenIndex = 0;\n                    }\n                    if (this.itemSelectable(chosenIndex)) {\n                        return { chosenIndex };\n                    }\n                }\n                return null;\n            }, () => {\n                const item = this.state.items[this.state.chosenIndex];\n                if (item) {\n                    const suggestionToDisplay = this.props.selectedSuggestionRenderer\n                        ? this.props.selectedSuggestionRenderer(item)\n                        : item.suggestion;\n                    this.updateInputValue(suggestionToDisplay);\n                    this.handleHoverEvents(item);\n                }\n            });\n        }\n        handleKeyDown(e) {\n            try {\n                switch (e.key) {\n                    case 'ArrowUp':\n                    case 'Up': // IE\n                        if (!this.paused) {\n                            e.preventDefault();\n                            if (this.state.items.length === 0) {\n                                this.getSuggestions(this.getInputValue(), this.handleKeyArrowUp);\n                            }\n                            else {\n                                this.handleKeyArrowUp();\n                            }\n                        }\n                        break;\n                    case 'ArrowDown':\n                    case 'Down': // IE\n                        if (!this.paused) {\n                            e.preventDefault();\n                            if (this.state.items.length === 0) {\n                                this.getSuggestions(this.getInputValue(), this.handleKeyArrowDown);\n                            }\n                            else {\n                                this.handleKeyArrowDown();\n                            }\n                        }\n                        break;\n                    case 'Enter':\n                        if (!this.paused && this.state.chosenIndex >= 0) {\n                            e.preventDefault();\n                            this.handleClick(this.state.chosenIndex);\n                        }\n                        break;\n                    case 'Escape':\n                    case 'Esc': // IE\n                        if (!this.paused) {\n                            e.preventDefault();\n                            this.setState({\n                                items: [],\n                                combinedSuggestions: {},\n                                chosenIndex: -1,\n                                seqId: (this.seqIdCounter += 1),\n                            }, () => {\n                                this.handleSuggestionsChangeEvents();\n                                if (this.originalQuery) {\n                                    this.updateInputValue(this.originalQuery);\n                                    const item = {\n                                        suggestion: this.originalQuery,\n                                        matches: [],\n                                        payload: {},\n                                    };\n                                    this.handleSelectEvents(item);\n                                }\n                                this.ignoreBlur = false;\n                            });\n                        }\n                        break;\n                }\n            }\n            finally {\n                if (this.props.onKeyDown) {\n                    this.props.onKeyDown(e);\n                }\n            }\n        }\n        isTappedOnTab(e) {\n            if (this.ac.types() && e.relatedTarget) {\n                return (e.relatedTarget.className === `${this.ac.classPrefix()}-anchor`);\n            }\n            return false;\n        }\n        shouldNotRemoveCombinedResults(e) {\n            // do not remove CombinedTypeList results unless you tap on a form\n            if (this.ac.types() && e.target && !e.relatedTarget) {\n                return e.target.tagName !== 'INPUT';\n            }\n            return false;\n        }\n        handleBlur(e) {\n            try {\n                if (this.isTappedOnTab(e)) {\n                    e.target.focus();\n                    return;\n                }\n                if (this.shouldNotRemoveCombinedResults(e)) {\n                    return;\n                }\n                if (this.ignoreBlur) {\n                    this.ignoreBlur = false;\n                }\n                else if (!this.ac.shouldIgnoreBlur()) {\n                    this.setState({ items: [], combinedSuggestions: {}, seqId: (this.seqIdCounter += 1) }, this.handleSuggestionsChangeEvents);\n                    if (this.ac.isSelectLimited() && !this.blurAfterSelections) {\n                        this.updateInputValue('');\n                    }\n                    this.blurAfterSelections = false;\n                }\n                this.originalQuery = this.getInputValue();\n            }\n            finally {\n                if (this.props.onBlur) {\n                    this.props.onBlur(e);\n                }\n            }\n        }\n        handleMouseDown(e) {\n            // on mouse down, we need to stop the blur event\n            // from removing the dropdown menu\n            if (e.nativeEvent.button === 0) {\n                this.ignoreBlur = true;\n            }\n        }\n        handleInput(e) {\n            try {\n                if (this.paused || !e.target) {\n                    return;\n                }\n                const target = e.target;\n                const value = target.value;\n                if (!uncontrolled) {\n                    this.setState({ value });\n                }\n                this.getSuggestions(value);\n            }\n            finally {\n                if (this.props.onChange) {\n                    this.props.onChange(e);\n                }\n            }\n        }\n        handleInputBoxClick(e) {\n            if (!this.ac.shouldSuggestOnInputBoxClick()) {\n                return;\n            }\n            if (!this.inputElement.current) {\n                return;\n            }\n            if (isInputBoxClicked(e, this.inputElement.current)) {\n                this.getSuggestions();\n            }\n        }\n        setCombinedSuggestions() {\n            if (this.props.setCombinedSuggestions) {\n                this.props.setCombinedSuggestions(this.state.combinedSuggestions, this.originalQuery || '');\n            }\n        }\n        setSelectedItem(selectedItem) {\n            if (this.props.setSelectedItem) {\n                this.props.setSelectedItem(selectedItem);\n            }\n        }\n        setChosenTab(chosenTab) {\n            if (this.props.setChosenTab) {\n                this.props.setChosenTab(chosenTab, false);\n            }\n        }\n        getSuggestions(query = this.getInputValue(), callback = undefined) {\n            const counter = (this.seqIdCounter += 1);\n            if (this.originalQuery === query && this.ignoreBlur) {\n                // in this case, the focus is triggered by `inputElement.focus()` in\n                // `handleBlur`, which is not by user, so this fetch should be ignored.\n                return;\n            }\n            this.originalQuery = query;\n            if (!query || query.trim().length === 0) {\n                if (this.ac.shouldAllowEmptyQuery()) {\n                    query = '';\n                }\n                else {\n                    this.setState({\n                        items: [],\n                        combinedSuggestions: {},\n                        seqId: counter,\n                        chosenIndex: -1,\n                    }, this.handleSuggestionsChangeEvents);\n                    return;\n                }\n            }\n            this.ac.getSuggestions(query, counter, (seqId, suggestions, combinedSuggestions) => {\n                let items;\n                if (combinedSuggestions) {\n                    if (counter > this.state.seqId) {\n                        this.setState({ combinedSuggestions });\n                    }\n                    items = this.ac.flatten(combinedSuggestions);\n                }\n                else if (suggestions) {\n                    items = suggestions;\n                }\n                else {\n                    items = [];\n                }\n                this.setState((state) => {\n                    if (counter > state.seqId) {\n                        return {\n                            items: items,\n                            seqId: seqId,\n                            chosenIndex: -1,\n                        };\n                    }\n                    return null;\n                }, () => {\n                    if (callback) {\n                        callback();\n                    }\n                    this.handleSuggestionsChangeEvents();\n                });\n            }, () => {\n                this.setState({\n                    items: [],\n                    combinedSuggestions: {},\n                    chosenIndex: -1,\n                }, this.handleSuggestionsChangeEvents);\n            });\n        }\n        // Publish unhandledRejection to notify developers\n        disableAutoc