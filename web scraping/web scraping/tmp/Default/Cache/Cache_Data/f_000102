More && !shouldShowMore\n            ? COUNT_SHOW_LESS\n            : props.recentSearches.length;\n\n    return (\n        <div className={'yosegi-RecentSearchList'}>\n            {props.recentSearches.length > 0 && (\n                <>\n                    {toRender.length > 0 && (\n                        <RecentSearchListHeader isSearchStartPage={props.isSearchStartPage} />\n                    )}\n                    <div\n                        // eslint-disable-next-line jsx-a11y/no-noninteractive-element-to-interactive-role\n                        className={'yosegi-RecentSearchList-itemContainer'}\n                        role=\"list\"\n                        id={props.id}\n                        aria-label={`${recentSearchesCount} Recent Searches`}\n                        data-testid=\"RecentSearchesWithDelete\"\n                        ref={recentSearchListRef}\n                        // eslint-disable-next-line jsx-a11y/no-noninteractive-tabindex\n                        tabIndex={0}\n                    >\n                        {toRender.slice(0, recentSearchesCount).map((recentSearch, index) => {\n                            return (\n                                <RecentSearchListItem\n                                    href={recentSearch.href}\n                                    upperText={recentSearch.query || recentSearch.location || ''}\n                                    lowerText={getSubtitle(pgettext, recentSearch)}\n                                    onRecentSearchClick={props.onRecentSearchClick}\n                                    id={props.id}\n                                    recentSearch={recentSearch}\n                                    customTabbing={props.customTabbing}\n                                    index={index}\n                                    key={recentSearch.ts}\n                                    nifflerRecentSearches={nifflerRecentSearches}\n                                    isDesktopYosegiTestA