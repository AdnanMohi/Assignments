ring;\n    /**\n     * Auto-complete session id\n     */\n    autocompleteSessionId: string;\n    /**\n     * Callback that is executed when an auto-complete suggestion is submitted\n     */\n    submitAutocompleteSuggestion: (\n        suggestion: ExtendedAutocompleteItem,\n        index: number,\n        acmeToggleCjsTstGrp: number,\n        queryFrom: string\n    ) => void;\n    /**\n     * Callback that is executed when a custom auto-complete suggestion is submitted\n     */\n    submitCustomAutocompleteSuggestion: (\n        suggestion: string,\n        index: number,\n        queryFrom: string\n    ) => void;\n    /**\n     * Additional suggestions to prepend (e.g. jobseeker's preferences)\n     */\n    additionalWhatSuggestions?: Array<string>;\n    /**\n     * Whether draftWhatValue has changed\n     */\n    draftWhatValueHasChanged?: boolean;\n    /**\n     * Whether arrow up/down navigation is enabled\n     */\n    shouldEnableArrowUpDownNavigation?: boolean;\n    /**\n     * Related Query suggestions\n     */\n    relatedQueryWhatSuggestions?: Array<string>;\n    /**\n     * The recent searches that the job seeker has previously performed.\n     */\n    recentSearches: RecentSearch[];\n    /**\n     * Callback that is executed when a recent search item is selected. This is used to pop the history so there\n     * isn't duplicate history entries, one the original page and the second the modal being opened.\n     */\n    onClickRecentSearch?: (recentSearch: RecentSearch) => void;\n    /**\n     * If desktop yosegi test is active (jsj_hp_desktopyosegi_tst)\n     */\n    isDesktopYosegiTestActive?: boolean;\n    /**\n     * ShowAllJobsSuggestion will be true when jsj_yosegi_suggest_blank_query_tst is active and where is not empty\n     * and the input value of what is empty.\n     */\n    showAllJobsSuggestion?: boolean;\n    /**\n     * Flag to indicate if the page is search start page\n     */\n    isSearchStartPage?: boolean;\n}\n\nfunction getSubTitle(\n    suggestion: ExtendedAutocompleteItem,\n    pgettext: GettextInterface['pgettext']\n): string | undefined {\n    switch (suggestion.segment) {\n        case JOB_TITLE_WITH_OCCUPATIONS:\n            // # Subtitle shown in search input when attempting to autocomplete, 0 variable is the job occupation\n            return suggestion.concept?.label\n                ? pgettext('autocompleteItemInCategory', 'in {0}', [\n                      suggestion.concept?.label as string\n                  ])\n                : undefined;\n        case NORM_JOB_TITLE:\n            // # Subtitle shown in search input when attempting to autocomplete\n            return pgettext('autocompleteItemIsJobTitle', 'job title');\n        default:\n            return undefined;\n    }\n}\n\nfunction getSuggestionIcon(\n    suggestion: AutocompleteItem,\n    gt: GettextInterface,\n    ariaHidden: boolean,\n    acmeToggleCjsTstGrp: number,\n    companySerpIconUrl?: string\n): JSX.Element {\n    const payload = suggestion.payload as CompanyJobsearchSuggestionPayload | undefined;\n    if (isCjsSuggestion(payload, acmeToggleCjsTstGrp) && payload?.employerKey) {\n        // if it is a high confidence CJS suggestion, then show company logo\n        return (\n            <CompanyAutocompleteIcon\n                item={suggestion}\n                gt={gt}\n                className=\"yosegi-WhatWhere-suggestion-icon\"\n                ariaHidden={ariaHidden}\n            />\n        );\n    } else if (companySerpIconUrl) {\n        // if in-company SERP (i.e. employer is non-null), then add company logo\n        // to all suggestions starting with company name\n        return (\n            <AutocompleteIcon\n                iconUrl={companySerpIconUrl}\n                ariaHidden\n                className=\"yosegi-WhatWhere-suggestion-icon\"\n            />\n        );\n    }\n    return <SearchIcon />;\n}\n\nexport const WhatOverlayPaneBodyContent = (props: WhatOverlayPaneBodyContentProps) => {\n    const {\n        acme_toggle_cjs_tst,\n        jsj_yosegi_editrecentsearches_tst,\n        mosaic_mobserp_company_serp_mode_tst,\n        jsj_yosegi_reselect_v2_tst\n    } = useProctorGroups();\n\n    const gt = useI18n();\n    const { pgettext } = gt;\n\n    const showRecentSearchWithDelete = jsj_yosegi_editrecentsearches_tst.value === 1;\n\n    const shouldShowAutocompleteHeader =\n        (jsj_yosegi_reselect_v2_tst.value > 0 &&\n            (!props.draftWhatValue || !props.draftWhatValueHasChanged)) ||\n        props.isSearchStartPage;\n\n    // FROM_WHAT_AUTOCOMPLETE is when user selects autocomplete query when actively typing query\n    // FROM_SEARCH_SUGGESTIONS is when user selects suggested search with no what query present\n    const whatQueryFrom = props.draftWhatValue ? FROM_WHAT_AUTOCOMPLETE : FROM_SEARCH_SUGGESTIONS;\n\n    // SuggestionList uses relatedWhatQuerySuggestions to also generate suggestions when:\n    // 1) when user runs 'where' query only, or 2) user selects 'all jobs' search (empty query)\n    // these particular scenarios should log searchSuggestions, not relatedQueries\n    const relatedOrSuggestedQuery =\n        !props.draftWhatValue || jsj_yosegi_reselect_v2_tst.value === 0\n            ? FROM_SEARCH_SUGGESTIONS\n            : FROM_RELATED_QUERIES;\n\n    const companyLogoUrlInSuggestion = (suggestion: ExtendedAutocompleteItem) => {\n        if (props.isCompanyJobSearchClosedDesign) {\n            // grp 1 & 3 of cjs experiment\n            return props.companySerpIconUrl;\n        } else if (props.isOpenSearchMode && props.companyNameInQuery) {\n            // grp 2 of cjs experiment\n            return getCompanyLogoURLInSuggestion(\n                suggestion,\n                props.companyNameInQuery,\n                props.companySerpIconUrl,\n                acme_toggle_cjs_tst.value\n            );\n        }\n        return undefined;\n    };\n\n    /**\n     * Takes list of jobseeker preferences and converts them into Suggestion objects\n     * @param suggestions\n     * @param queryFrom Source of custom suggestion\n     */\n    const createCustomWhatSuggestions = (\n        suggestions: string[] | undefined,\n        queryFrom: string\n    ): Array<ISuggestion> | undefined => {\n        if (!suggestions) {\n            return undefined;\n        }\n        return suggestions.map(\n            (suggestion, index): ISuggestion => ({\n                label: suggestion,\n                isSelected: index === props.focusedSuggestionIndex,\n                icon: <SearchIcon />,\n                onClick: (event) => {\n                    event?.preventDefault();\n                    setAutocompleteSessionIdToCookie(\n                        props.autocompleteSessionId,\n                        AutocompleteType.WHAT\n                    );\n                    props.submitCustomAutocompleteSuggestion(suggestion, index, queryFrom);\n                }\n            })\n        );\n    };\n\n    const renderRecentSearchList = (isOnTop: boolean) => {\n        const shouldEnableShowMore = jsj_yosegi_reselect_v2_tst.value > 0 && isOnTop;\n\n        return showRecentSearchWithDelete ? (\n            <RecentSearchListWithDelete\n                id={'what-autocomplete-recent-searches'}\n                recentSearches={props.recentSearches}\n                customTabbing={{\n                    focusedIndex: props.focusedSuggestionIndex ?? null\n                }}\n                onRecentSearchClick={props.onClickRecentSearch}\n                shouldEnableShowMore={shouldEnableShowMore}\n                isDesktopYosegiTestActive={props.isDesktopYosegiTestActive}\n                isSearchStartPage={props.isSearchStartPage}\n            />\n        ) : (\n            <RecentSearchList\n                id={'what-autocomplete-recent-searches'}\n                recentSearches={props.recentSearches}\n                customTabbing={{\n                    focusedIndex: props.focusedSuggestionIndex ?? null\n                }}\n                onRecentSearchClick={props.onClickRecentSearch}\n                shouldEnableShowMore={shouldEnableShowMore}\n                isSearchStartPage={props.isSearchStartPage}\n            />\n        );\n    };\n\n    return (\n        <>\n            {props.isCompanyJobSearchClosedDesign && !props.showCompanyChip && (\n                <button\n                    className={'yosegi-WhatOverlayPane-optOutCompanySerp'}\n                    onClick={() => props.setOptedOutOfCompanySERP(true)}\n                    type=\"button\"\n                >\n                    <NewWindow aria-hidden sx={{ mx: 4 }} color=\"neutral.700\" size=\"sm\" />\n                    {// #! Menu item to opt out Company SERP mode in WhatOverlayPane\n                    pgettext('Opt out company SERP mode menu item', 'Search jobs at all companies')}\n                </button>\n            )}\n            {(!props.showSuggestions ||\n                (jsj_yosegi_reselect_v2_tst.value > 0 &&\n                    props.recentSearches?.length > 0 &&\n                    !props.draftWhatValue) ||\n                props.isSearchStartPage) &&\n                renderRecentSearchList(true)}\n            <SuggestionList\n                id={'what-autocomplete-suggestions'}\n                prefix={props.draftWhatValue}\n                ariaLabel={\n                    // #! Aria label for the what autocomplete suggestions group\n                    pgettext('What Suggestion', 'Search suggestions')\n                }\n                headerText={\n                    props.isCompanyJobSearchClosedDesign && props.employer?.name\n                        ? // #! Header shows in top of Suggestion list in company SERP mode\n                          pgettext('Suggestion list header', 'Jobs at {0}', [props.employer.name])\n                        : undefined\n                }\n                suggestions={\n                    props.showSuggestions && props.refinedSuggestions.l