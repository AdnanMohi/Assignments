verlayPillOverlaySheet = (props: OverlayPillOverlaySheetProps) => {\n    const {\n        id,\n        displayName,\n        isMultiSelect,\n        toggleOption,\n        toggleOnTop,\n        setOpen,\n        shouldCloseOnSubmit,\n        options,\n        selectedValue,\n        onChange,\n        isOpen,\n        filterName\n    } = props;\n\n    const [selections, setSelections] = useState<Set<string>>(selectedValue);\n    const { pgettext } = useI18n();\n\n    const optionRefs: Array<React.RefObject<HTMLInputElement>> = [];\n    const { log } = useLogger();\n\n    const isChecked = () => {\n        if (toggleOption && selections.has(toggleOption.value)) {\n            return selections.size === 1;\n        }\n        return selections.size === 0;\n    };\n\n    const onSubmit = (event: React.FormEvent) => {\n        event.preventDefault();\n        if (selections) {\n            const filtersApplied: string[] = getFiltersApplied(props.options, selections);\n\n            log('FilterPill', `filterApplied`, {\n                ['filterSource']: filterName,\n                ['filterApplied']: `${filtersApplied.join(',')}`\n            });\n        }\n        onChange(selections);\n        if (shouldCloseOnSubmit) {\n            setOpen(false);\n        }\n    };\n\n    const onClearSelections = () => {\n        log('FilterPill', `filterCleared`, { ['filterSource']: `${filterName}` });\n        setSelections(new Set());\n    };\n\n    const onKeyDown = (evt: React.KeyboardEvent<HTMLInputElement>) => {\n        const { keyCode, target } = evt;\n        const currentIndex: number = parseInt(\n            (target as HTMLInputElement).getAttribute('data-index') ?? '0',\n            10\n        );\n        let newIndex: number;\n        switch (keyCode) {\n            case UP_KEY:\n                newIndex = getNewIndex(currentIndex, -1, options.length);\n                break;\n            case DOWN_KEY:\n                newIndex = getNewIndex(currentIndex, 1, options.length);\n                break;\n            default:\n                return;\n        }\n        const newElement = optionRefs[newIndex];\n        newElement?.current?.parentElement?.focus();\n    };\n\n    const getToggleToSwitch = () => {\n        return toggleOnTop && toggleOption\n            ? [\n                  <MapToggleToSwitch\n                      option={toggleOption}\n                      selections={selections}\n                      setSelections={setSelections}\n                      toggleOnTop={toggleOnTop}\n                      key={id}\n                  />\n              ]\n            : [];\n    };\n\n    const getRadio = () => {\n        return !isMultiSelect\n            ? [\n                  <Radio\n                      key={-1}\n                      id={`${id}-${-1}`}\n                      label={pgettext('Single-Select filter All jobs option', 'All jobs')}\n                      name={`${id}-menu`}\n                      checked={isChecked()}\n                      onChange={() => {\n                          setSelections(\n                              toggleOption && selections.has(toggleOption.value)\n                                  ? new Set([toggleOption.value])\n                                  : new Set([])\n                          );\n                      }}\n                  />\n              ]\n            : [];\n    };\n\n    const getOptions = () => {\n        return options.map((option, index) =>\n            isMultiSelect ? (\n                <MapOptionsToCheckbox\n                    option={option}\n                    index={index}\n                    id={id}\n                    optionRefs={optionRefs}\n                    selections={selections}\n                    setSelections={setSelections}\n                    key={id}\n                />\n            ) : (\n                <MapOptionsToRadio\n                    option={option}\n                    index={index}\n                    id={id}\n                    toggleOption={toggleOption}\n                    selections={selections}\n                    setSelections={setSelections}\n                    key={id}\n                />\n            )\n        );\n    };\n\n    const fields = () => {\n        const toggleTop = getToggleToSwitch();\n        const radio = getRadio();\n        const fieldOptions = getOptions();\n        const toggleBottom =\n            !toggleOnTop && toggleOption ? (\n                <MapToggleToSwitch\n                    option={toggleOption}\n                    selections={selections}\n                    setSelections={setSelections}\n                    toggleOnTop={toggleOnTop}\n                    key={id}\n                />\n            ) : (\n                []\n            );\n\n        return [...toggleTop, ...radio, ...fieldOptions, toggleBottom];\n    };\n\n    return (\n        <FilterSelectOverlay\n            id={`${id}-menu`}\n            title={displayName}\n            isMultiSelect={isMultiSelect}\n            isOpen={isOpen}\n            onClose={() => setOpen(false)}\n            onSubmit={onSubmit}\n            onClearSelections={onClearSelections}\n            numberOfOptions={options.length}\n            isDesktop={props.isDesktop}\n        >\n            <FormField as=\"fieldset\" label={null} onKeyDown={onKeyDown}>\n                {fields()}\n            </FormField>\n        </FilterSelectOverlay>\n    );\n};\n\nexport default OverlayPillOverlaySheet;\n","import { SelectionOption } from '../OverlayPill';\n\nexport const getFiltersApplied = (\n    options: readonly SelectionOption[],\n    selections: Set<string>\n): string[] => {\n    return options\n        .filter((option) => selections.has(option.value))\n        .map((option) => option.label.replace(/ /g, '-'));\n};\n","export const UP_KEY = 38;\nexport const DOWN_KEY = 40;\n","import React, { useState } from 'react';\n\nimport { VisuallyHidden } from '@indeed/ifl-components';\nimport { Gettex