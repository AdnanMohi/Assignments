     }\n    };\n\n    const inputContainerStyle = isDesktopYosegiTestActive\n        ? styles.desktopStyles.inputContainerSx\n        : styles.defaultStyles.inputContainerSx;\n\n    const inputTextStyle = isDesktopYosegiTestActive\n        ? styles.desktopStyles.inputTextSx(showingSuggestions, id)\n        : styles.defaultStyles.inputTextSx;\n\n    const ariaActivedescendant = !!selectedSuggestionIndex\n        ? getSuggestionId(selectedSuggestionIndex)\n        : undefined;\n\n    return (\n        <Flex sx={inputContainerStyle}>\n            <InputFormField\n                invalid={isInvalid}\n                errorText={errorText}\n                sx={inputTextStyle}\n                inputProps={{\n                    id,\n                    'aria-owns': showingSuggestions ? suggestionListId : undefined,\n                    'aria-haspopup': 'listbox',\n                    enterKeyHint: 'search',\n                    contentBefore: icon,\n                    type: 'text',\n                    autoCapitalize: 'none',\n                    autoComplete: 'off',\n                    'aria-autocomplete': 'list',\n                    'aria-controls': id,\n                    'aria-activedescendant': ariaActivedescendant,\n                    sx: inputTextStyle,\n                    ref: inputRef,\n                    ...otherInputProps,\n                    contentAfter: (\n                        <AutocompleteClearButton\n                            onClear={handleClear}\n                            ariaLabel={clearAriaLabel || gettext('Clear')}\n                            ariaControls={id}\n                            isShow={!!props.value}\n                            isDesktopYosegiTestActive={isDesktopYosegiTestActive}\n                        />\n                    )\n                }}\n            />\n        </Flex>\n    );\n};\n","import e,{useRef as r,useState as t,useEffect as n,Fragment as o,createContext as s}from\"react\";import{VisuallyHidden as a}fro