: (eventType: string, data?: LogArguments): void => {\n            this.logger.logAction({\n                data: {\n                    eventType,\n                    ...data\n                }\n            });\n        }\n    };\n}\n\n/**\n * No-op logger which will be used for server side rendering and as a safe default value in the {@link LoggerContext}.\n */\nexport const noopFrontendLogger: IFrontendLogger = {\n    event: {\n        trackEvent: () => {}\n    },\n    log: (): void => {}\n};\n","import React, { createContext, PropsWithChildren } from 'react';\n\nimport { IFrontendLogger, noopFrontendLogger } from './FrontendLogger';\n\n/**\n * Context used to persist the {@link IFrontendLogger} in a context for usage by any component in the tree.\n *\n * @see {useLogger} for usage in a react functional component\n */\nexport const LoggerContext = createContext<IFrontendLogger>(noopFrontendLogger);\n\nexport const LoggerContextProvider = (\n    props: PropsWithChildren<{ logger?: IFrontendLogger }>\n): JSX.Element => (\n    <LoggerContext.Provider value={props.logger ?? noopFrontendLogger}>\n        {props.children}\n    </LoggerContext.Provider>\n);\n","import { BundleType } from '../types/bundleType';\n\nimport { IFrontendLogger, LogArguments } from './FrontendLogger';\n\nconst RICH_SEARCH_EVENT_TYPE = 'japanRichSearch';\n\nexport default class RichSearchTrackEventLogger {\n    public readonly frontendLogger: IFrontendLogger;\n\n    public constructor(frontendLogger: IFrontendLogger) {\n        this.frontendLogger = frontendLogger;\n    }\n\n    public log(source: string, field: string, value?: string | number): void {\n        const data: LogArguments = { field };\n        if (typeof value !== 'undefined') {\n            data.value = value;\n        }\n        data.source = source;\n\n        this.frontendLogger?.event?.trackEvent(RICH_SEARCH_EVENT_TYPE, data);\n    }\n\n    public logBundleType(bundleType: BundleType): void {\n        this.frontendLogger.eve