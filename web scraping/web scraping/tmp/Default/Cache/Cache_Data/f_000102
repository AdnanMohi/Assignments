NT\n            : CMP_WHAT_SEGMENT\n        : WHAT_SEGMENT;\n}\n\nfunction getAutocompleteLibraries(\n    mosaicMobserpCompanySerpModeTstValue: number,\n    acmeCmpWhatWithTopCompaniesAutocompleteTstValue: number,\n    isCompanyJobSearchClosedDesign: boolean\n) {\n    const libraries: Map<string, number> = new Map([\n        [\n            getDefaultSegment(\n                mosaicMobserpCompanySerpModeTstValue,\n                acmeCmpWhatWithTopCompaniesAutocompleteTstValue,\n                isCompanyJobSearchClosedDesign\n            ),\n            10\n        ]\n    ]);\n\n    const shouldAddJobSearchSegment =\n        !libraries.has(CMP_WHAT_WITH_TOP_COMPANIES_SEGMENT) &&\n        !libraries.has(CMP_WHAT_SEGMENT) &&\n        !isCompanyJobSearchClosedDesign;\n\n    if (shouldAddJobSearchSegment) {\n        libraries.set(COMPANY_JOB_SEARCH_SEGMENT, 1);\n    }\n\n    return libraries;\n}\n\nexport type WhatOverlayAutocomplete = Omit<\n    AutocompleteHookProps,\n    'suggestions' | 'combinedSuggestions'\n> & {\n    refinedSuggestions: ExtendedAutocompleteItem[];\n};\n\nexport const useWhatOverlayAutocomplete = (\n    draftWhatValue: string,\n    isCompanyJobSearchClosedDesign: boolean,\n    skipAutocompleteCall: boolean,\n    desktopAutocompleteOptions?: AutocompleteOptions\n): WhatOverlayAutocomplete => {\n    const {\n        mosaic_mobserp_company_serp_mode_tst,\n        acme_cmp_what_with_top_companies_autocomplete_tst\n    } = useProctorGroups();\n\n    // If we want to avoid/skip initial unused autocomplete call then enableEmptyQuery should be false and whatValue should be empty string.\n    const enableEmptyQuery = !skipAutocompleteCall;\n    const whatValue = skipAutocompleteCall ? '' : draftWhatValue;\n\n    const autocompleteOptions = useAutocompleteOptions(\n        isCompanyJobSearchClosedDesign,\n        enableEmptyQuery,\n        desktopAutocompleteOptions\n    );\n\n    const { combinedSuggestions, suggestions, ...otherAutocompleteProps } = useAuto