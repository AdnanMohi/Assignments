n\n  var walker = document.createTreeWalker(root, NodeFilter.SHOW_ELEMENT,\n  /**\n   * IE 11 does not accept an object passed to the the `filter` argument.\n   * Instead, a function must be passed. This fortunately works with other browsers\n   * as well.\n   * @see https://stackoverflow.com/questions/45595349/ie9-ie-10-ie11-createnodeiterator-exception-occurred/45609439#45609439\n   *\n   * `@ts-ignore` is added since TypeScript does allow for both interfaces.\n   * @see https://github.com/microsoft/TypeScript/issues/33462\n   *\n   * Feel free to adjust once we drop IE.\n   */\n  // @ts-ignore\n  function acceptNode(node) {\n    // Skip this node and its children if it is one of the target nodes.\n    // Also skip children of already hidden nodes, as `aria-hidden` is recursive.\n    if (visibleNodes.has(node) || hiddenNodes.has(node.parentElement)) {\n      return NodeFilter.FILTER_REJECT;\n    } // Skip this node but continue to children if one of the targets is inside the node.\n\n\n    if (targets.some(function (target) {\n      return node.contains(target);\n    })) {\n      return NodeFilter.FILTER_SKIP;\n    }\n\n    return NodeFilter.FILTER_ACCEPT;\n  }, // IE 11 requires the deprecated `expandEntityReferences` argument to be passed in.\n  // Fortunately this does not affect other browsers. Feel free to remove once we drop IE.\n  // @ts-ignore\n  false);\n\n  var hide = function hide(node) {\n    var _elementCountMap$get;\n\n    var elementCount = (_elementCountMap$get = elementCountMap.get(node)) != null ? _elementCountMap$get : 0; // If `aria-hidden` attribute existed on element initially, don't do anything.\n\n    if (node.getAttribute(\"aria-hidden\") === \"true\" && elementCount === 0) {\n      return;\n    } // If `aria-hidden` attribute did not exist on element initially and it hasn't\n    // already been hidden, hide it.\n\n\n    if (elementCount === 0) {\n      node.setAttribute(\"aria-hidden\", \"true\");\n    }\n\n    hiddenNodes.add(node);\n    elementCountMap.set(node, elementCount + 1);\n  }; // Start at the first child node of the root since the root should never have\n  // `aria-hidden` applied to it.\n\n\n  var currentNode = walker.nextNode(); // Iterate over all nodes.\n\n  while (currentNode) {\n    hide(currentNode);\n    currentNode = walker.nextNode();\n  } // Return a function that will restore hidden elements.\n\n\n  return function () {\n    for (var _iterator = _createForOfIteratorHelperLoose(hiddenNodes), _step; !(_step = _iterator()).done;) {\n      var _elementCountMap$get2;\n\n      var node = _step.value;\n      var elementCount = (_elementCountMap$get2 = elementCountMap.get(node)) != null ? _elementCountMap$get2 : 0;\n\n      if (elementCount === 1) {\n        node.removeAttribute(\"aria-hidden\");\n        elementCountMap.delete(node);\n      } else {\n        // Don't restore element if it has been hidden by another call to\n        // `accessiblyHideOutsideElements` or `useInertA11yBackground`.\n        elementCountMap.set(node, elementCount - 1);\n      }\n    }\n  };\n}\n/**\n * Accessibility hides all elements in the DOM outside the given target refs using aria-hidden.\n *\n * @param shouldHide Whether outside DOM elements should be accessibly hidden.\n * @param targets Refs representing elements that should remain accessibily visible.\n * @param root The element where DOM traversal begins.\n * @returns A function that will restore all hidden elements.\n */\n\nfunction useInertA11yBackground(shouldHide, targetRefs, root) {\n  if (root === void 0) {\n    root = document.body;\n  }\n\n  useEffect(function () {\n    // Convert refs to elements\n    var targetElements = targetRefs.reduce(function (elements, targetRef) {\n      return targetRef.current ? [].concat(elements, [targetRef.current]) : elements;\n    }, []);\n\n    if (!targetElements.length) {\n      return undefined;\n    }\n\n    var restoreElements = null; // Hide elements\n\n    if (shouldHide) {\n      restoreElements