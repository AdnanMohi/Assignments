/Textarea.css';\n\nconst Textarea = (props) => {\n    const componentName = 'icl-Textarea';\n    const {\n        decoration,\n        disabled,\n        errorText,\n        helpText,\n        id: inputId,\n        label,\n        labelId: lId,\n        name,\n        onBlur,\n        onChange,\n        rows,\n        size,\n        value,\n        inputRef,\n        customTextareaProps\n    } = props;\n    const id = inputId || `textarea-${name}`;\n    const labelId = lId || `label-${name}`;\n    const errorId = `${labelId}-error`;\n    const textareaClass = classnames('icl-Textarea-control', {\n        'is-error': !!errorText,\n        [`icl-Textarea-control--${size}`]: size\n    });\n    const labelModifiers = [size];\n    const filteredCustomAttributes = filterCustomProps(customTextareaProps);\n\n    return (\n        <FormGroup className={componentName}>\n            <div id={labelId} className=\"icl-Textarea-labelWrapper\">\n                <Label componentName={componentName} labelFor={id} modifiers={labelModifiers}>\n                    {label}\n                </Label>\n                <HelpText componentName={componentName} helpText={helpText} labelFor={id} />\n            </div>\n            {decoration && (\n                <div className=\"icl-Textarea-decorationWrapper icl-Textarea-labelWrapper\">\n                    <span className=\"icl-Textarea-decorationContent icl-Textarea-label\">\n                        {decoration}\n                    </span>\n                </div>\n            )}\n            <textarea\n                className={textareaClass}\n                aria-describedby={errorText ? errorId : null}\n                aria-invalid={!!errorText}\n                aria-labelledby={labelId}\n                disabled={disabled}\n                id={id}\n                name={name}\n                onBlur={onBlur}\n                onChange={onChange}\n                rows={rows}\n                value={value}\n                ref={(ref) => 