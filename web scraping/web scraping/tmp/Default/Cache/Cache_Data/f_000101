el: IJobModel,\n    proctorGroups: AppProctorGroups,\n    locale: Locale\n): IAQSignalProps | undefined => {\n    if (\n        !(\n            jobModel?.jobSeekerMatchSummaryModel &&\n            jobModel.jobSeekerMatchSummaryModel.sortedEntityDisplayText.length > 0\n        )\n    ) {\n        return undefined;\n    }\n\n    const trafficLight = jobModel.jobSeekerMatchSummaryModel.trafficLight;\n\n    if (trafficLight === HIGH_PRECISION_GREEN || trafficLight === HIGH_COVERAGE_GREEN) {\n        if (!isMinMatchMet(jobModel, proctorGroups)) {\n            return undefined;\n        }\n    }\n\n    if (trafficLight === HIGH_PRECISION_RED || trafficLight === HIGH_COVERAGE_RED) {\n        if (!isMinMismatchMet(jobModel, proctorGroups)) {\n            return undefined;\n        }\n    }\n\n    if (!showAQSignalPayForPerformance(jobModel, locale.countryCode, trafficLight, proctorGroups)) {\n        return undefined;\n    }\n\n    let match;\n    if (trafficLight === HIGH_PRECISION_GREEN || trafficLight === HIGH_COVERAGE_GREEN) {\n        match = true;\n    } else if (trafficLight === HIGH_PRECISION_RED || trafficLight === HIGH_COVERAGE_RED) {\n        match = false;\n    } else {\n        return undefined;\n    }\n\n    return {\n        match,\n        isBig6: getProctorBucketValue(proctorGroups, 'jcs_big6_tst') > 0,\n        isJmRedCopyTst: locale?.language === 'en',\n    };\n};\n\nexport const isRedLightAndRedLightBrandingTest = (\n    jobModel: IJobModel,\n    proctorGroups: AppProctorGroups\n): boolean => {\n    const trafficLight = jobModel.jobSeekerMatchSummaryModel?.trafficLight ?? YELLOW;\n    const isRedLight =\n        (trafficLight === HIGH_PRECISION_RED || trafficLight === HIGH_COVERAGE_RED) &&\n        isMinMismatchMet(jobModel, proctorGroups);\n    const isRedLightBrandingTest =\n        getProctorBucketValue(proctorGroups, 'jcs_red_light_branding_tst') > 0;\n\n    return isRedLight && isRedLightBrandingTest;\n};\n\nexport const isRedLightJob = (\n    jobMo