  model: IMosaicProviderJobCardsModel,\n    locale: Locale,\n    isBrandHeaderJobKey: boolean,\n    proctorGroups: AppProctorGroups\n): IBrandingHeaderProps => {\n    const brandingTestPayload: IBrandingPayload =\n        (proctorGroups?.jcs_brand_highest_bid_tst?.payload as IBrandingPayload) ?? undefined;\n\n    return {\n        companyName: jobModel.company,\n        logoUrl: jobModel.featuredCompanyAttributes.logoUrl,\n        ...jobModel.companyBrandingAttributes,\n        shouldShowBrandingHeader: shouldShowBrandingHeader(\n            jobModel,\n            hostId,\n            model,\n            locale,\n            proctorGroups\n        ),\n        showBanner: shouldShowBanner(\n            jobModel,\n            hostId,\n            model,\n            proctorGroups,\n            brandingTestPayload,\n            isBrandHeaderJobKey\n        ),\n        showLogo: shouldShowLogo(\n            jobModel,\n            hostId,\n            model,\n            proctorGroups,\n            brandingTestPayload,\n            isBrandHeaderJobKey\n        ),\n        isDesktop: model.isDesktop,\n        hostId,\n    };\n};\n\nexport const shouldShowBanner = (\n    jobModel: IJobModel,\n    hostId: string,\n    model: IMosaicProviderJobCardsModel,\n    proctorGroups: AppProctorGroups,\n    brandingTestPayload: IBrandingPayload | undefined,\n    isBrandHeaderJobKey: boolean\n): boolean => {\n    const hasBanner = jobModel.companyBrandingAttributes?.headerImageUrl ?? null;\n    const isRedLightBrandingTest = isRedLightAndRedLightBrandingTest(jobModel, proctorGroups);\n\n    if (!hasBanner || isRedLightBrandingTest) {\n        return false;\n    }\n\n    if (hostId === HostId.HOMEPAGE) {\n        if (!isBrandHeaderJobKey) {\n            return false;\n        }\n\n        if (model.isDesktop || model.isTablet) {\n            return false;\n        }\n\n        const hpBrandTstValue = getProctorBucketValue(\n            proctorGroups,\n            'jcs_hp_brand_highest_bid_tst'\n        );\n\n        return [2, 5, 6].includes(hpBrandTstValue);\n    }\n\n    if (\n        (isBrandHeaderJobKey ||\n            (getProctorBucketValue(proctorGroups, 'jcs_brand_highest_bid_tst') > 5 &&\n                jobModel.featuredEmployer &&\n                !model.shouldShowITAOverFE)) &&\n        brandingTestPayload?.json.showBanner &&\n        !model.isTablet &&\n        !model.isDesktop\n    ) {\n        return true;\n    }\n\n    return false;\n};\n\nexport const shouldShowLogo = (\n    jobModel: IJobModel,\n    hostId: string,\n    model: IMosaicProviderJobCardsModel,\n    proctorGroups: AppProctorGroups,\n    brandingTestPayload: IBrandingPayload | undefined,\n    isBrandHeaderJobKey: boolean\n): boolean => {\n    const hasLogo =\n        jobModel.companyBrandingAttributes?.logoUrl ?? jobModel.featuredCompanyAttributes.logoUrl;\n    const isRedLightBrandingTest = isRedLightAndRedLightBrandingTest(jobModel, proctorGroups);\n\n    if (!hasLogo || !isBrandHeaderJobKey || isRedLightBrandingTest) {\n        return false;\n    }\n\n    if (hostId === HostId.HOMEPAGE) {\n        if (model.isDesktop || model.isTablet) {\n            return false;\n        }\n        const hpBrandTstValue = getProctorBucketValue(\n            proctorGroups,\n            'jcs_hp_brand_highest_bid_tst'\n        );\n\n        return hpBrandTstValue > 0;\n    }\n\n    if (\n        (isBrandHeaderJobKey ||\n            (getProctorBucketValue(proctorGroups, 'jcs_brand_highest_bid_tst') >= 5 &&\n                jobModel.featuredEmployer &&\n                !model.shouldShowITAOverFE)) &&\n        brandingTestPayload?.json.showLogo\n    ) {\n        return true;\n    }\n\n    if (\n        isBrandHeaderJobKey &&\n        getProctorBucketValue(proctorGroups, 'jcs_serp_fallback_branding_tst') > 0 &&\n        hostId === HostId.MOBILE\n    ) {\n        return true;\n    }\n\n    return false;\n};\n\nexport const getBrandedResults = (\n    proctorGroups: ProctorGroups<TestSpecs> | undefined,\n    hostId: string,\n    model: IMosaicProviderJobCardsModel,\n    prevJobResultsCount: number\n): Set<string> => {\n    if (hostId === HostId.HOMEPAGE) {\n        return getBrandedResultsHomepage(proctorGroups, model, prevJobResultsCount);\n    }\n\n    const brandingTestActive =\n        getProctorBucketValue(proctorGroups, 'jcs_brand_highest_bid_tst') > 0;\n\n    if (!brandingTestActive) {\n        return getBrandedResultsIncludingFE(proctorGroups, model, prevJobResultsCount);\n    }\n\n    const brandingTestPayload = proctorGroups?.jcs_brand_highest_bid_tst\n        .payload as IBrandingPayload;\n\n    const results = model.results.slice(prevJobResultsCount);\n\n    // Sort the results by highest to lowest bid\n    const sortedResults = results\n        .sort((a, b) => {\n            const aBid = a.rankingScoresModel?.bid ?? 0;\n            const bBid = b.rankingScoresModel?.bid ?? 0;\n\n            return bBid - aBid;\n        })\n        .filter((r) => r.rankingScoresModel?.bid ?? 0 > 0);\n\n    const topBidders = sortedResults.slice(0, brandingTestPayload.json.topBidders);\n\n    const showLogo = brandingTestPayload.json.showLogo;\n    const showBanner = brandingTestPayload.json.showBanner && !model.isTablet && !model.isDesktop;\n\n    // Filter out results that don't have the needed company attributes (logo and/or banner)\n    const topBiddersWithCriteria = topBidders.filter((result) => {\n        const hasLogo =\n            result.companyBrandingAttributes?.logoUrl ?? result.featuredCompanyAttributes.logoUrl;\n        const hasBanner = result.companyBrandingAttributes?.headerImageUrl;\n\n        if (showLogo && showBanner) {\n            return hasLogo && hasBanner;\n        } else if (showLogo) {\n            return hasLogo;\n        } else if (showBanner) {\n            return hasBanner;\n        } else {\n            return false;\n        }\n    });\n\n    return new Set(\n        topBiddersWithCriteria\n            .map((result) => {\n                return result.jobkey;\n            })\n            .slice(0, brandingTestPayload.json.maxBrandings)\n    );\n};\n\nexport const getBrandedResultsIncludingFE = (\n    proctorGroups: ProctorGroups<TestSpecs> | undefined,\n    model: IMosaicProviderJobCardsModel,\n    prevJobResultsCount: number\n): Set<string> => {\n    const fallbackBrandingTstValue = getProctorBucketValue(\n        proctorGroups,\n        'jcs_serp_fallback_branding_tst'\n    );\n\n    if (fallbackBrandingTstValue < 1) {\n        return new Set();\n    }\n\n    const results = model.results.slice(prevJobResultsCount);\n\n    const feJobKey = results.find(\n        (result) => result.featuredEmployer && result.featuredCompanyAttributes.logoUrl\n    )?.jobkey;\n\n    if (fallbackBrandingTstValue === 1) {\n        return feJobKey ? new Set([feJobKey]) : new Set();\n    }\n\n    if (fallbackBrandingTstValue === 2 && feJobKey) {\n        return new Set([feJobKey]);\n    }\n\n    const sortedResults = results\n        .filter((result) => result.jobkey !== feJobKey)\n        .sort((a, b) => {\n            const aBid = a.rankingScoresModel?.bid ?? 0;\n            const bBid = b.rankingScoresModel?.bid ?? 0;\n\n            return bBid - aBid;\n        })\n        .filter((r) => r.rankingScoresModel?.bid ?? 0 > 0);\n\n    let topNBidders = fallbackBrandingTstValue === 3 ? 3 : 1;\n    topNBidders = feJobKey ? topNBidders - 1 : topNBidders;\n\n    const topBidders = sortedResults.slice(0, topNBidders);\n\n    const topBiddersJobKey = new Set(topBidders.map((result) => result.jobkey));\n\n    return feJobKey ? topBiddersJobKey.add(feJobKey) : topBiddersJobKey;\n};\n\nexport const getBrandedResultsHomepage = (\n    proctorGroups: ProctorGroups<TestSpecs> | undefined,\n    model: IMosaicProviderJobCardsModel,\n    prevJobResultsCount: number\n): Set<string> => {\n    const hpBrandTstValue = getProctorBucketValue(proctorGroups, 'jcs_hp_brand_highest_bid_tst');\n    const brandedJobKeys = new Set<string>();\n\n    if (hpBrandTstValue < 1) {\n        return brandedJobKeys;\n    }\n\n    const results = model.results.slice(prevJobResultsCount);\n\n    const feJobKey = results.find((result) => result.featuredEmployer)?.jobkey;\n\n    if (hpBrandTstValue === 1 || hpBrandTstValue === 2) {\n        if (feJobKey) {\n            brandedJobKeys.add(feJobKey);\n        }\n\n        return brandedJobKeys;\n    }\n\n    const sortedResultsByTopBids = results\n        .sort((a, b) => {\n            const aBid = a.rankingScoresModel?.bid ?? 0;\n            const bBid = b.rankingScoresModel?.bid ?? 0;\n\n            return bBid - aBid;\n        })\n        .filter((r) => r.rankingScoresModel?.bid ?? 0 > 0);\n\n    const numOfBrandedJobKeys = hpBrandTstValue === 4 || hpBrandTstValue === 6 ? 3 : 1;\n\n    sortedResultsByTopBids\n        .slice(0, numOfBrandedJobKeys)\n        .forEach((result) => brandedJobKeys.add(result.jobkey));\n\n    return brandedJobKeys;\n};\n","import { IJobModel } from '@indeed/job-cards-model';\n\nimport { appendParamsOnce } from '../../../../util/url';\n\nexport interface ICompanyOverviewLinkProps {\n    initialUrl: string;\n    finalUrl: string;\n}\n\nexport const getCompanyOverviewLinkProps = (\n    currentJobModel: IJobModel\n): ICompanyOverviewLinkProps | undefined => {\n    return !!currentJobModel.companyOverviewLink\n        ? {\n              initialUrl: currentJobModel.companyOverviewLink,\n              finalUrl: appendParamsOnce(\n                  currentJobModel.companyOverviewLink,\n                  getParams(currentJobModel)\n              ),\n          }\n        : undefined;\n};\n\nconst getParams = (currentJobModel: IJobModel): string => {\n    const campaignId = currentJobModel.companyOverviewLinkCampaignId ?? '';\n    const companyIdEncrypted = currentJobModel.companyIdEncrypted ?? '';\n\n    return `from=SERP&campaignid=${campaignId}&fromjk=${currentJobModel.jobkey}&jcid=${companyIdEncrypted}`;\n};\n","import { GettextInterface } 