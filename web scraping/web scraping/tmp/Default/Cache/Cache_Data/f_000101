           return onClick?.(e);\n        };\n    };\n}\n\nexport const getHostClickHandler = (\n    viewjobFrom: string,\n    noJsUrlOnly: boolean,\n    isViewJobDirectLinking: boolean,\n    apiSource?: IApiSource,\n    isBot?: boolean\n): JobClickHandlerFactory | undefined => {\n    if (noJsUrlOnly) {\n        return undefined;\n    }\n    if (viewjobFrom === HostId.SJPW) {\n        return getJobCardClickHandler;\n    }\n    if (viewjobFrom === HostId.COMPANY_PAGES_WEBAPP) {\n        return getCompanyPagesWebappClickHandler;\n    }\n\n    if (apiSource && apiSource === 'RJP' && viewjobFrom !== HostId.HOMEPAGE) {\n        return getNaiveClickHandler;\n    }\n\n    if (isViewJobDirectLinking && isBot) {\n        return getCanonicalUrlClickHandler;\n    }\n\n    if (viewjobFrom === HostId.JASX) {\n        return getJasxClickHandler;\n    } else if (viewjobFrom === HostId.HOMEPAGE) {\n        return getHomepageClickHandler;\n    }\n    return getMobileClickHandler;\n};\n\n/**\n * Get a mouse down handler factory that is created for each card that is executed when clicking the job card.\n *\n * If highQualityMarketplace?.match is true, we need our visits to the ViewJob page to include the\n * High Quality Marketplace (HQM/Indeed Select) token so that we can validate that it is a High Quality Match. To do\n * this we are making sure that before we navigate away to set this HQM cookie. We have to use the mousedown instead\n * of just the click handler above in the case that the job seeker opens the job in a new tab by middle clicking\n * or right clicking and opening in a new tab/window.\n */\nexport function getMouseDownHandler(\n    viewjobFrom: string,\n    highQualityMarketplace?: IHighQualityMarketplace | undefined\n): MouseDownHandlerFactory | undefined {\n    const mouseDownHandler = getHostMouseDownHandler(viewjobFrom);\n\n    return (props) => {\n        const onMouseDown = mouseDownHandler?.(props);\n\n        return (e) => {\n            updateHqmCookie(highQualityMarketplace);\n            return onMouseDown?.(e);\n        };\n    };\n}\n\nexport const getHostMouseDownHandler = (\n    viewjobFrom: string\n): MouseDownHandlerFactory | undefined => {\n    if (viewjobFrom === HostId.JASX) {\n        return getJasxMouseDownHandler;\n    } else if (viewjobFrom === HostId.HOMEPAGE) {\n        return getHomepageMouseDownHandler;\n    }\n\n    return undefined;\n};\n\nexport function getMobileClickHandler(\n    props: React.PropsWithChildren<ITapItemProps>\n): (e: React.MouseEvent | React.KeyboardEvent) => void {\n    const checkForDuplicateClickAndSetCookie = (cookieName: string, jk: string): boolean => {\n        if (Number(Cookies.get(DSANR_COOKIE))) {\n            return false;\n        }\n\n        const value = Cookies.get(cookieName) ?? '';\n        const vals = value.replace(/\"/g, '').split(':');\n        const ind = vals.indexOf(jk);\n        vals.unshift(jk);\n\n        // now check for valid keys and dedupe\n        const dedupedJobs = new Set(vals.filter((key: string) => /^[a-f0-9]{16}$/i.test(key)));\n        Cookies.set(cookieName, Array.from(dedupedJobs).slice(0, 10).join(':'), {\n            'max-age': `${60 * 60 * 24 * 30}`,\n            path: '/',\n        });\n\n        return ind > -1;\n    };\n\n    const getLink = (\n        isSponsored: boolean,\n        partialQueryParams: Record<string, string>,\n        isIpadApp: boolean,\n        link?: string,\n        hiringEventUrl?: string\n    ): string => {\n        const prevUrl = props.prevUrl ?? cleanPrevUrl();\n        const queryParams: Record<string, string> = {\n            ...partialQueryParams,\n        };\n\n        if (props.clickObj.isIndeedApplyable) {\n            queryParams.prevUrl = prevUrl;\n        }\n\n        let jobModelLink;\n        // Technically we could set hiringEventUrl into link but that would conflict with this business requirement:\n        // - If this is set, always going to this url first.\n        // And we can't do hiringEventJob && link because HE jobs without hiringEventUrl should go to /m/viewjob\n        if (hiringEventUrl) {\n            jobModelLink = hiringEventUrl;\n        } else if (isSponsored && link) {\n            jobModelLink = `${link}${link.indexOf('?') > -1 ? '&' : '?'}${new URLSearchParams(\n                queryParams\n            )}`;\n        } else if (!isSponsored) {\n            queryParams.mtk = props.mobtk;\n            jobModelLink = `/rc/clk?${new URLSearchParams(queryParams)}`;\n        } else {\n            jobModelLink = `/m/viewjob?${new URLSearchParams(queryParams)}`;\n        }\n        return isIpadApp\n            ? `/INDEED/ipadAppOpenUrl?url=${encodeURIComponent(jobModelLink)}`\n            : jobModelLink;\n    };\n\n    const cleanPrevUrl = (): string => {\n        // eslint-disable-next-line dot-notation\n        const urlToClean = window['location'].href;\n\n        const prevUrlWithAmpersandPos = urlToClean.indexOf('&prevUrl=');\n        const prevUrlWithQuestionMarkPos = urlToClean.indexOf('?prevUrl=');\n        if (prevUrlWithAmpersandPos > -1) {\n            const nextParamPos = urlToClean.indexOf('&', prevUrlWithAmpersandPos + 1);\n            if (nextParamPos > -1) {\n                return urlToClean.slice(0, prevUrlWithAmpersandPos);\n            }\n\n            return urlToClean.slice(0, prevUrlWithAmpersandPos) + urlToClean.slice(nextParamPos);\n        } else if (prevUrlWithQuestionMarkPos > -1) {\n            const nextParamPos = urlToClean.indexOf('&', prevUrlWithQuestionMarkPos + 1);\n            if (nextParamPos > -1) {\n                return urlToClean.slice(0, prevUrlWithQuestionMarkPos);\n            }\n\n            return (\n                urlToClean.slice(0, prevUrlWithQuestionMarkPos + 1) +\n                urlToClean.slice(nextParamPos + 1)\n            );\n        }\n\n        return urlToClean;\n    };\n\n    return (event: React.MouseEvent | React.KeyboardEvent): void => {\n        /**\n         * This onClick handler will erase the bound event handlers set by MOBILE, which are available to execute before JavaScript loads.\n         * When the JCS components become interactive, they will replace the previous event handlers, but they will still call the exact same function clkActionInit.\n         *\n         * In the future, we may want to consider locating the bound JS handlers as part of the HTML that is returned by JCS, in an inline script tag. MOBILE\n         * could provide the globals, but the event binding would live in JCS.\n         */\n        /* istanbul ignore if  */\n        // @ts-ignore\n        // eslint-disable-next-line dot-notation\n        if (window['clkActionInit']) {\n            // @ts-ignore\n            window['clkActionInit'](props.sponsored, props.mobtk); // eslint-disable-line\n        }\n        try {\n            if (\n                props.useMobileProvidedClickHandler &&\n                // @ts-ignore\n                // eslint-disable-next-line dot-notation\n                typeof window['jcsJobClick'] === 'function'\n            ) {\n                mobileProvidedClickHandler(\n                    props,\n                    getLink(\n                        props.sponsored,\n                        props.clickObj.queryParams as Record<string, string>,\n                        props.clickObj.isIpadApp,\n                        props.clickObj.link,\n                        props.hiringEventUrl\n                    )\n                );\n            } else {\n                const isDupe = checkForDuplicateClickAndSetCookie(\n                    props.sponsored ? 'RSJC' : 'ROJC',\n                    props.jk\n                );\n\n                const mclkVal = {\n                    ...props.clickObj.cookieParams,\n                    dupclk: isDupe ? '1' : '0',\n                };\n\n                // Set the click cookie.\n                Cookies.set('MCLK', new URLSearchParams(mclkVal), { path: '/' });\n                // Send user to the correct place.\n                // @ts-ignore\n                // eslint-disable-next-line dot-notation\n                document['location'] = getLink(\n                    props.sponsored,\n                    props.clickObj.queryParams as Record<string, string>,\n                    props.clickObj.isIpadApp,\n                    props.clickObj.link,\n                    props.hiringEventUrl\n                );\n            }\n        } catch (err) {\n            // We failed somewhere, default to our regular viewjob page even if sponsored\n            // @ts-ignore\n            // eslint-disable-next-line dot-notation\n            document['location'] = `/m/viewjob?jk=${props.jk}&from=fsSerp`;\n        }\n        event.preventDefault();\n    };\n}\n\nfunction mobileProvidedClickHandler(\n    props: React.PropsWithChildren<ITapItemProps>,\n    link: string\n): void {\n    // @ts-ignore\n    // eslint-disable-next-line dot-notation\n    window['jcsJobClick']({\n        link,\n        jobKey: props.jk,\n        from: props.clickObj.viewjobFrom,\n        // @ts-ignore\n        encryptedResultData: props.clickObj.cookieParams.rd,\n        tkName: 'mobtk',\n        tkVal: props.mobtk,\n        openNextView: true,\n        isIndeedApplyable: props.clickObj.isIndeedApplyable,\n        isSponsored: props.sponsored,\n        clickCookieParams: props.clickObj.cookieParams,\n        viewJobLink: props.viewJobLink,\n        hiringEventJob: props.hiringEventJob,\n        redirectToThirdPartySite: props.redirectToThirdPartySite,\n        partialViewJobData: props.partialViewJobData,\n    });\n}\n\nexport function getJasxClickHandler(props: React.PropsWithChildren<ITapItemProps>) {\n    return (event: React.MouseEvent | React.KeyboardEvent): void => {\n        // @ts-ignore\n        // eslint-disable-next-line dot-notation\n        window['onJobCardItemClick']?.(event, {\n            id: props.id,\n            sponsored: props.sponsored,\n            position: props.position,\n            hasSalary: props.hasSalary,\n            refineByTypes: props.refineByTypes ? props.refineByTypes : [],\n            option: props.mouseDownHandlerOption,\n            viewJobLink: props.viewJobLink,\n            url: props.clickObj.link,\n            adid: props.adId,\n            advn: props.advn,\n            ad: props.adBlob,\n            trackingUrls: props.trackingUrls,\n            jobdata: getJasxJobDataObj(props),\n            setFocusToJobDetails: true,\n            hiringEventUrl: props.hiringEventUrl,\n        });\n    };\n}\n\nexport function getHomepageClickHandler(props: React.PropsWithChildren<ITapItemProps>) {\n    return (event: React.MouseEvent | React.KeyboardEvent): void => {\n        // @ts-ignore\n        // eslint-disable-next-line dot-notation\n        window['handleJobCardClick'](event, {\n            jobKey: props.jk,\n            sponsored: props.sponsored,\n            mobtk: props.mobtk,\n            url: props.clickObj.link,\n            adid: props.adId,\n            advn: props.advn,\n            ad: props.adBlob,\n            viewJobLink: props.viewJobLink,\n            trackingUrls: props.trackingUrls,\n            hiringEventIframeUrl: props.hiringEventIframeUrl,\n        });\n    };\n}\n\nexport function getJobCardClickHandler(props: React.PropsWithChildren<ITapItemProps>) {\n    return (event: React.MouseEvent | React.KeyboardEvent): void => {\n        // @ts-ignore\n        // eslint-disable-next-line dot-notation\n        window['sponsoredJobsPresentationWebappJobCardClick'](event, {\n            jobKey: props.jk,\n            sponsored: props.sponsored,\n            mobtk: props.mobtk,\n            url: props.clickObj.link,\n            adid: props.adId,\n            viewJobLink: props.viewJobLink,\n            isApp: props.isApp,\n            isDesktop: props.isDesktop,\n        });\n    };\n}\n\nexport function getCompanyPagesWebappClickHandler(props: React.PropsWithChildren<ITapItemProps>) {\n    return (event: React.MouseEvent | React.KeyboardEvent): void => {\n        // @ts-ignore\n        // eslint-disable-next-line dot-notation\n        window['indeedHandleJobCardClick'](event, {\n            jobKey: props.jk,\n            indeedApply: props.clickObj.isIndeedApplyable,\n        });\n    };\n}\n\n/**\n * This click simply just redirects to url.\n * @param props\n */\nexport function getNaiveClickHandler(props: React.PropsWithChildren<ITapItemProps>) {\n    return (event: React.MouseEvent | React.KeyboardEvent): void => {\n        if (props.clickObj.link) {\n            // @ts-ignore\n            window['location'].assign(props.clickObj.link);\n            event.preventDefault();\n        }\n    };\n}\n\nexport function getCanonicalUrlClickHandler(props: React.PropsWithChildren<ITapItemProps>) {\n    return (event: React.MouseEvent | React.KeyboardEvent): void => {\n        if (props.href) {\n            // @ts-ignore\n            window['location'].assign(props.href);\n            event.preventDefault();\n        }\n    };\n}\n\nexport function getHomepageMouseDownHandler(props: React.PropsWithChildren<ITapItemProps>) {\n    return (event: React.MouseEvent): void => {\n        // @ts-ignore\n        // eslint-disable-next-line dot-notation\n        window['handleJobCardMouseDown']?.(event, {\n            jobKey: props.jk,\n            sponsored: props.sponsored,\n            mobtk: props.mobtk,\n            url: props.clickObj.link,\n        });\n    };\n}\n\nexport function getJasxMouseDownHandler(props: React.PropsWithChildren<ITapItemProps>) {\n    return (event: React.MouseEvent): void => {\n        // @ts-ignore\n        // eslint-disable-next-line dot-notation\n        window['onJobCardItemMouseDown']?.(event, {\n            id: props.id,\n            sponsored: props.sponsored,\n            position: props.position,\n            hasSalary: props.hasSalary,\n            option: props.mouseDownHandlerOption,\n            jobdata: getJasxJobDataObj(props),\n        });\n    };\n}\n\ninterface JasxJobDataObject {\n    jk: string;\n    rd: string;\n}\n\nfunction getJasxJobDataObj(props: React.PropsWithChildren<ITapItemProps>): JasxJobDataObject {\n    return {\n        jk: props.jk,\n        // @ts-ignore\n        rd: props.clickObj.cookieParams.rd,\n    };\n}\n","import React from 'react';\n\nimport { Box, Tag } from '@indeed/ifl-components';\nimport { expressive } from '@indeed/ifl-primitives';\n\nimport { useI18n } from '../../../hooks/useI18n';\nimport { useLogFeatureDisplay } from '../../../log/displayLog';\n\nimport { FlairOption } from './apiBridge';\n\ninterface IRedesignedFlairProps {\n    flairOption: FlairOption;\n    jobKey?: string;\n    enticerText?: string;\n}\n\nexport const RedesignedFlair: React.FC<IRedesignedFlairProps> = (props: IRedesignedFlairProps) => {\n    const { gettext, pgettext } = useI18n();\n    const logFeatureDisplay = useLogFeatureDisplay();\n    const { flairOption, jobKey, enticerText } = props;\n    const displayText = enticerText ?? getDisplayText(flairOption, gettext, pgettext);\n\n    if (!displayText) {\n        return null;\n    }\n    logFeatureDisplay(() => ({ jobKey, field: 'flair', value: displayText }));\n    return (\n        <Box>\n            <Tag\n                variant=\"light\"\n                sx={{ color: expressive.magenta[800], bg: expressive.magenta[200] }}\n            >\n                {displayText}\n            </Tag>\n        </Box>\n    );\n};\n\nconst getDisplayText = (flairOption: FlairOption, gettext: any, pgettext: any): null | string => {\n    let displayText;\n    switch (flairOption) {\n        case FlairOption.EARLY_APPLICANT:\n            displayText = pgettext('Be an early applicant label', 'Be an early applicant');\n            break;\n        case FlairOption.URGENTLY_HIRING:\n            displayText = gettext('Urgently hiring');\n            break;\n        case FlairOption.HIRING_MULTIPLE_CANDIDATES:\n            displayText = pgettext(\n                'Hiring multiple candidates label',\n                'Hiring multiple candidates'\n            );\n            break;\n        case FlairOption.NO_INTERVIEW_REQUIRED:\n            displayText = gettext('No interview required');\n            break;\n        case FlairOption.FLEXIBLE_SCHEDULE:\n            displayText = gettext('Flexible schedule');\n            break;\n        case FlairOption.OVERTIME:\n            displayText = gettext('Overtime');\n            break;\n        case FlairOption.NEW_GRADUATES_WELCOME:\n            displayText = gettext('New graduates welcome');\n            break;\n        case FlairOption.REMOTE:\n            displayText = gettext('Remote');\n            break;\n        default:\n            return null;\n    }\n    return displayText;\n};\n","import React from 'react';\n\nimport { Tag } from '@indeed/ifl-components';\nimport { Responsive } from '@indeed/ifl-icons';\nimport { expressive } from '@indeed/ifl-primitives';\n\nimport { useI18n } from '../../../hooks/useI18n';\nimport { useLogFeatureDisplay } from '../../../log/displayLog';\nimport { getProctorBucketValue } from '../../../proctor/getGroup';\nimport { useProctorContext } from '../../../proctor/proctorTest';\n\nimport { IResponsiveEmployerProps } from './apiBridge/responsiveEmployer';\n\nconst ResponsiveEmployerTag: React.FunctionComponent<IResponsiveEmployerProps> = (\n    props: IResponsiveEmployerProps\n) => {\n    const { ngettext, gettext } = useI18n();\n    const proctorGroups = useProctorContext();\n    const logFeatureDisplay = useLogFeatureDisplay();\n\n    const MAXIMUM_RESPONSE_TIME_DAYS = 14;\n    const employerResponseTime = props.employerResponseTime ?? MAXIMUM_RESPONSE_TIME_DAYS + 1;\n\n    if (!props.showResponsiveEmployerTag) {\n        return null;\n    }\n\n    logFeatureDisplay(() => ({\n        jobKey: props.jobKey,\n        field: 'responsiveEmployerTagYN',\n        value: true,\n    }));\n\n    const responsiveText = ngettext(\n        'Typical