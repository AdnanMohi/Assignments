.keys(dislikedJobMap);\n    if (keys.length > MAX_DISLIKED_JOBS) {\n        let oldestJobKey = '';\n        let oldestTimestamp = Number.MAX_VALUE;\n        keys.forEach((key) => {\n            if (dislikedJobMap[key] < oldestTimestamp) {\n                oldestJobKey = key;\n                oldestTimestamp = dislikedJobMap[key];\n            }\n        });\n        delete dislikedJobMap[oldestJobKey];\n    }\n    localStorageSetItem(DISLIKE_JOBS_KEY, JSON.stringify(dislikedJobMap));\n};\n\nexport const undoDislikeJobInLocalStorage = (jobKey: string): void => {\n    const dislikedJobMap = getDislikedJobMap();\n    if (jobKey in dislikedJobMap) {\n        delete dislikedJobMap[jobKey];\n    }\n    localStorageSetItem(DISLIKE_JOBS_KEY, JSON.stringify(dislikedJobMap));\n};\n","export const CUSTOM_JCS_LOAD_EVENT = 'JCS_LOAD_EVNT';\n\nfunction jcsOnClick(e: MouseEvent): void {\n    e.preventDefault();\n    e.stopPropagation();\n    // eslint-disable-next-line\n    var elem = e.target as HTMLElement;\n    // eslint-disable-next-line\n    var jcsIsSaving = false;\n    // eslint-disable-next-line\n    var jcsIsDisliking = false;\n    while (elem) {\n        if (elem.className?.indexOf) {\n            if (elem.className.indexOf('bookmark ') > -1) {\n                jcsIsSaving = true;\n            } else if (elem.className.indexOf('dislike-tap-target ') > -1) {\n                jcsIsDisliking = true;\n            } else if (elem.className.indexOf('result ') > -1) {\n                if (jcsIsSaving) {\n                    // @ts-ignore\n                    // eslint-disable-next-line dot-notation\n                    if (window['jcsSavedJobs'].indexOf(elem.id) < 0) {\n                        // @ts-ignore\n                        // eslint-disable-next-line dot-notation\n                        window['jcsSavedJobs'].push(elem.id);\n                    }\n                } else if (jcsIsDisliking) {\n                    // @ts-ignore\n                    // eslint-disable-nex