    .reduce((acc, val) => acc.concat(val), []) // https://stackoverflow.com/a/62657216\n        .map((configString) => {\n            const transitPayload: TransitPayload = {} as TransitPayload;\n            const keysAndValues = configString?.split(',') ?? [];\n            let payloadMta: String = '';\n\n            for (const keyValueString of keysAndValues) {\n                if (!regex.test(keyValueString)) {\n                    return null;\n                }\n                const keyValue = keyValueString.split(':');\n                const key = keyValue[0].trim();\n                const value = keyValue[1].trim();\n\n                let parsedBoolValue;\n                if ('true' === value) {\n                    parsedBoolValue = true;\n                } else if ('false' === value) {\n                    parsedBoolValue = false;\n                } else {\n                    parsedBoolValue = null;\n                }\n\n                if ('mta' === key) {\n                    payloadMta = value;\n                } else if ('nearbyLines' === key) {\n                    if (parsedBoolValue == null) {\n                        return null;\n                    }\n                    transitPayload.isNearbyLines = parsedBoolValue;\n                } else if ('nearestLines' === key) {\n                    if (parsedBoolValue == null) {\n                        return null;\n                    }\n                    transitPayload.isNearestLines = parsedBoolValue;\n                } else if ('nearestStation' === key) {\n                    if (parsedBoolValue == null) {\n                        return null;\n                    }\n                    transitPayload.isNearestStation = parsedBoolValue;\n                }\n            }\n\n            if (mta !== payloadMta) {\n                return null;\n            }\n\n            return transitPayload;\n        })\n        .filter((element) => element !== null);\n    if (mtaPayloads.length !== 1) {\n        return {} as TransitPayload;\n    } else {\n        return mtaPayloads[0];\n    }\n};\n\nconst getTransitLineModel = (transitLineName: string): TransitLineModel => {\n    return TRANSIT_LINE_MODELS.get(transitLineName)!;\n};\n\ninterface StationAndTaType {\n    key: string;\n    value: string;\n}\n\nconst splitStationAndTransitAuthority = (combined: string): StationAndTaType => {\n    const split = combined.split('~');\n    if (split.length > 1) {\n        return {\n            key: split[0],\n            value: split[1],\n        };\n    } else {\n        return {\n            key: split[0],\n            value: 'NYCTA',\n        };\n    }\n};\n","type TransitLine = {\n    name: string;\n    symbol: string;\n    transitAuth: string;\n    bulletColor: string;\n    symbolColor: string;\n    style: string;\n};\n\nexport const transitLines: TransitLine[] = [\n    {\n        name: '1',\n        symbol: '1',\n        transitAuth: 'NYCTA',\n        bulletColor: '#EE352E',\n        symbolColor: '#FFFFFF',\n        style: 'SOLID_ROUND',\n    },\n    {\n        name: '2',\n        symbol: '2',\n        transitAuth: 'NYCTA',\n        bulletColor: '#EE352E',\n        symbolColor: '#FFFFFF',\n        style: 'SOLID_ROUND',\n    },\n    {\n        name: '3',\n        symbol: '3',\n        transitAuth: 'NYCTA',\n        bulletColor: '#EE352E',\n        symbolColor: '#FFFFFF',\n        style: 'SOLID_ROUND',\n    },\n    {\n        name: '4',\n        symbol: '4',\n        transitAuth: 'NYCTA',\n        bulletColor: '#00993C',\n        symbolColor: '#FFFFFF',\n        style: 'SOLID_ROUND',\n    },\n    {\n        name: '5',\n        symbol: '5',\n        transitAuth: 'NYCTA',\n        bulletColor: '#00993C',\n        symbolColor: '#FFFFFF',\n        style: 'SOLID_ROUND',\n    },\n    {\n        name: '6',\n        symbol: '6',\n        transitAuth: 'NYCTA',\n        bulletColor: '#00993C',\n        symbolColor: '#FFFFFF',\n        style: 'SOLID_ROUND',\n    },\n    {\n        name: 