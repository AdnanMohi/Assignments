JobMetaDataProps(\n            currentJobModel,\n            model,\n            proctorGroups,\n            artifacts,\n            gt,\n            locale,\n            hostId\n        ),\n        applicationBadgeProps: getSavedApplicationBadgeProps(currentJobModel),\n        isBig6: getProctorBucketValue(proctorGroups, 'jcs_big6_tst') > 0,\n        similarJobsProps: getSimilarJobsProps(currentJobModel, hostId),\n        proctorGroups,\n    };\n};\n","import { GettextInterface } from '@indeed/indeed-gettext';\nimport { IJobModel } from '@indeed/job-cards-model';\n\nimport { AppProctorGroups } from '../../../../proctor';\nimport { getProctorBucketValue } from '../../../../proctor/getGroup';\n\nexport interface IJpMetadataSectionModel {\n    header: string;\n    contents: string[];\n    iconModel: {\n        type: string;\n        size: string;\n        title?: string;\n        color?: string;\n        position?: string;\n    };\n}\n\nexport interface IJpRatingModel {\n    countContent: string;\n    descriptionContent?: string;\n    ariaContent?: string;\n    rating: number;\n    size?: string;\n    count: string;\n    showCount: boolean;\n    showDescription: boolean;\n}\n\nexport interface IJpPartialViewJobData {\n    jobKey: string;\n    japanTopLabelModel?: {\n        newArrival?: string;\n        jobTypes?: string[];\n        expirationAlertMessage?: string;\n    };\n    title: string;\n    buttonGroup?: {\n        viewJobButtonLinkModel?: {\n            dataHref: string;\n            href: string;\n        };\n    };\n    japanCompanyModel?: {\n        name: string;\n        header: string;\n        link?: string;\n        ratingsModel?: IJpRatingModel;\n    };\n    metadataSections: IJpMetadataSectionModel[];\n    jobDescriptionModel: {\n        jobTagModel?: {\n            header: string;\n            tags: string[];\n        };\n        header: string;\n    };\n    jobDebugInfoModel?: {\n        jobKeyu: string;\n        feedId: number;\n        jobId: numb