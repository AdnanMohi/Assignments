ponsiveEmployerTagYN: 'boolean',\n    salaryYN: 'boolean',\n    jobTypeYN: 'boolean',\n    workHoursYN: 'boolean',\n    shiftYN: 'boolean',\n    commuteYN: 'boolean',\n    payNotProvidedYN: 'boolean',\n    trafficLight: 'string',\n    redLightBannerlYN: 'boolean',\n    greenLightBannerlYN: 'boolean',\n};\n\nfunction getHostId(hostId: string): HostId {\n    switch (hostId) {\n        case contextHostId.HOMEPAGE:\n            return HostId.HOMEPAGE;\n        case contextHostId.MOBILE:\n            return HostId.MOBILE;\n        default:\n            return HostId.UNKNOWN;\n    }\n}\n\nfunction getPageId(pageId: string): PageId {\n    switch (pageId) {\n        case contextPageId.VIEWJOB:\n            return PageId.VIEWJOB;\n        case contextPageId.HOMEPAGE:\n            return PageId.HOMEPAGE;\n        case contextPageId.DESKTOP_SERP:\n            return PageId.DESKTOP_SERP;\n        case contextPageId.JP_MOBILE_SERP:\n            return PageId.JP_MOBILE_SERP;\n        default:\n            return PageId.UNKNOWN;\n    }\n}\n\nexport const getFeedtk = (\n    jobKey: string,\n    jobKeyToFeedTkMapForHomepage?: { [index: string]: string }\n) => {\n    if (jobKeyToFeedTkMapForHomepage) {\n        return jobKeyToFeedTkMapForHomepage[jobKey] ?? '';\n    }\n\n    return '';\n};\n\nexport const DisplayLogContext = createContext<{\n    current: Map<string, Map<IFieldTypesKey, IFieldValue>>;\n}>({\n    current: new Map(),\n});\n\nexport const DisplayLogProvider: FC<{}> = (props) => {\n    const displayLogsRef = useRef(new Map());\n    return (\n        <DisplayLogContext.Provider value={displayLogsRef}>\n            {props.children}\n        </DisplayLogContext.Provider>\n    );\n};\n\n// Using a function for arg0 for returning params. This allows us to prevent running code on the server and trap exceptions.\nexport const useLogFeatureDisplay = () => {\n    const isActive = useProctorValue('jcs_display_log_tst') > 0;\n    const displayLogsRef = useContext(DisplayLogContext);\n\n    return (getParamsFn: () => DisplayLogParams) => {\n        // We should only log on the browser.\n        if (!isActive || isSSR()) {\n            return;\n        }\n        try {\n            const { jobKey, field, value } = getParamsFn();\n            if (!jobKey) {\n                return;\n            }\n            if (!displayLogsRef.current.has(jobKey)) {\n                displayLogsRef.current.set(jobKey, new Map());\n            }\n            const log = displayLogsRef.current.get(jobKey)!;\n            log.set(field, value);\n        } catch (e) {\n            // TODO look into logging jserr more gracefully.\n        }\n    };\n};\n\nexport const useLogJobcardDisplay = () => {\n    const isActive = useProctorValue('jcs_display_log_tst') > 0;\n    const displayLogsRef = useContext(DisplayLogContext);\n    const logTrackerRef = useRef(new Map<string, boolean>());\n\n    return (\n        jobKey: string,\n        hostId: string,\n        pageId: string,\n        mobtk: string,\n        feedtk: string,\n        ctk: string\n    ) => {\n        if (\n            !isActive ||\n            isSSR() ||\n            !displayLogsRef.current.has(jobKey) ||\n            // Only log once per job component mount\n            logTrackerRef.current.has(jobKey)\n        ) {\n            return;\n        }\n\n        logTrackerRef.current.set(jobKey, true);\n        const loggedFields = displayLogsRef.current.get(jobKey)!;\n        const fieldsToLog: { [key: string]: { [key: string]: IFieldValue } } = {};\n        for (const [fieldKey, fieldValue] of loggedFields) {\n            // Guard against potentially logging a field that is not in the schema.\n            // Types should prevent this, but being a bit more defensive\n            // in case types are loosened in the future to not send\n            // logs not meeting schema.\n            if (fieldKey in fieldTypes) {\n                const fieldType = fieldTypes[fieldKey];\n                fieldsToLog[fieldKey] = {\n                    [fieldType]: fieldValue,\n                };\n            }\n        }\n\n        const logRequest: JobcardDisplay = {\n            jobKey,\n            mobtk: {\n                string: mobtk,\n            },\n            hostId: {\n                HostId: getHostId(hostId),\n            },\n            pageId: {\n                PageId: getPageId(pageId),\n            },\n            feedtk: {\n                string: feedtk,\n            },\n            ctk: {\n                string: ctk,\n            },\n            ...fieldsToLog,\n        };\n        sendLogRequest(logRequest);\n\n        // Clear out previous fields for job.\n        displayLogsRef.current.delete(jobKey);\n    };\n};\n","import {\n    ApolloClient,\n    InMemoryCache,\n    createHttpLink,\n    NormalizedCacheObject,\n    ApolloQueryResult,\n} from '@apollo/client';\nimport Cookies from 'js-cookie';\n\nimport {\n    CreatePhoneCandidateVirtualNumberInput,\n    RecordJobSeenInput,\n    RecordJobTimeInViewportInput,\n} from '../../__generated__/globalTypes';\nimport { JobTitleSuggestionsInput } from '../__generated__/graphqlBaseTypes';\nimport { IMainProps } from '../components/Main/IMainProps';\nimport { ONE_GRAPH_HEADER, getStealthGroupsString } from '../util/stealthGroups';\n\nimport { JobTitleSuggestions } from './aiAssistant/graphql/__generated__/jobTitleSuggestions';\nimport jobTitleSuggestionsQuery from './aiAssistant/graphql/jobTitleSuggestions.graphql';\nimport { CreatePhoneCandidateVirtualNumber } from './createVirtualNumber/graphql/__generated__/createPhoneCandidateVirtualNumber';\nimport createPhoneCandidateVirtualNumber from './createVirtualNumber/graphql/createPhoneCandidateVirtualNumber.graphql';\nimport recordJobSeenMutation from './jobSeen/graphql/recordJobSeenMutation.graphql';\nimport recordJobTimeInViewportMutation from './jobTimeInViewport/graphql/recordJobTimeInViewport.graphql';\n\n// eslint-disable-next-line @typescript-eslint/no-unused-vars\nt