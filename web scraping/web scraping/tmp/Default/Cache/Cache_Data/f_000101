OMEPAGE) {\n        doLogHomepageJobSeen(\n            isMobile,\n            mosaicInitialDataLogTk,\n            jobKeyToFeedTkMapForHomepage,\n            oneGraphClient\n        );\n    } else if (pageId === PageId.VIEWJOB) {\n        doLogViewJobJobSeen(tk, oneGraphClient);\n    } else {\n        doLogJobSeen(atk, tk, isMobile, additionalParams, locale, hostId, oneGraphClient);\n    }\n\n    sponsoredJobs = [];\n    organicJobs = [];\n    lastLoggingTask = undefined;\n};\n\n/**\n * Exported to make it accessible for testing, shouldn't be used directly in production code.\n */\nexport const doLogJobSeen = (\n    atk: string,\n    tk: string,\n    isMobile: boolean,\n    additionalParams: {},\n    locale: Locale,\n    hostId?: string,\n    oneGraphClient?: OneGraphClient\n): void => {\n    const tkFieldKey = isMobile ? 'mobtk' : 'tk';\n    const query = {\n        atk,\n        [tkFieldKey]: tk,\n        m: isMobile ? '1' : '0',\n        ...additionalParams,\n    };\n\n    const logToUil = hostId !== HostId.SJPW;\n\n    if (sponsoredJobs.length > 0) {\n        if (logToUil) {\n            doLogSerpJobSeen(\n                atk,\n                tk,\n                isMobile,\n                true,\n                sponsoredJobs.map((job) => job.jk),\n                sponsoredJobs.map((job) => job.adId),\n                sponsoredJobs.map((job) => job.adBlob),\n                locale,\n                oneGraphClient\n            );\n        } else {\n            const urlParams = new URLSearchParams({\n                ...query,\n                jobType: JobSeenType.sponsored,\n            });\n            sponsoredJobs.forEach(({ jk, adId }) => {\n                urlParams.append('jk', jk);\n                urlParams.append('adId', adId);\n            });\n\n            fetch(`/tjob/rpc/jsn?${urlParams}`).catch(() => {});\n        }\n    }\n\n    if (organicJobs.length > 0) {\n        if (logToUil) {\n            doLogSerpJobSeen(\n                atk,\n                tk,\n                isMobile,\n                false,\n                organicJobs.map((job) => job.jk),\n                [],\n                organicJobs.map((job) => job.organicBlob),\n                locale,\n                oneGraphClient\n            );\n        } else {\n            const urlParams = new URLSearchParams({\n                ...query,\n                jobType: JobSeenType.organic,\n            });\n            organicJobs.forEach(({ jk }) => {\n                urlParams.append('jk', jk);\n            });\n\n            fetch(`/tjob/rpc/jsn?${urlParams}`).catch(() => {});\n        }\n    }\n};\n\n// Define the enum for ad types\nexport enum SurfaceType {\n    HOMEPAGE = 2,\n    SERP = 5,\n}\n\nexport const getVersion5UILTrackingKey = (\n    trackingKey: string,\n    jobKey: string,\n    adType: SurfaceType,\n    encodedAdData?: string\n): string => {\n    /*\n        The UIL tracking key is of the format '{version}-{origin}-${adType/surface}-${trackingKey}-${jobKey}--{encodedAdData}. We use version 5 keys since only those support encodedAdData.\n        Origin is hardcoded as unkn since it is not being used for our use-case and is required for Indeed+.\n     */\n    const baseKey = `5-unkn-${adType}-${trackingKey}-${jobKey}`;\n\n    if (encodedAdData) {\n        return `${baseKey}--${encodedAdData}`;\n    }\n\n    return baseKey;\n};\n\nconst doLogSerpJobSeen = (\n    atk: string,\n    tk: string,\n    isMobile: boolean,\n    isSponsored: boolean,\n    jobKeys: string[],\n    adIds: string[],\n    encodedAdData: string[],\n    locale: Locale,\n    oneGraphClient?: OneGraphClient\n): void => {\n    const jobResultTrackingKeys = jobKeys.map((jk, index) =>\n        getVersion5UILTrackingKey(tk, jk, SurfaceType.SERP, encodedAdData[index])\n    );\n\n    const recordJobSeenInput: RecordJobSeenInput = {\n        jobResultTrackingKeys,\n        additionalInfo: {\n            serpFields: {\n                applicationTrackingKey: atk,\n                serpTrackingKey: tk,\n                adId: adIds,\n                isMobileRequest: isMobile,\n                isSponsored: isSponsored,\n                href: window['location'].href,\n                country: locale.countryCode,\n            },\n        },\n    };\n\n    if (oneGraphClient) {\n        oneGraphClient.recordJobSeen(recordJobSeenInput);\n    }\n};\n\nconst doLogHomepageJobSeen = (\n    isMobile: boolean,\n    homepageTrackingKey: string,\n    jobKeyToFeedTkMapForHomepage?: { [index: string]: string },\n    oneGraphClient?: OneGraphClient\n): void => {\n    if (sponsoredJobs.length === 0 && organicJobs.length === 0) {\n        return;\n    }\n\n    if (oneGraphClient && jobKeyToFeedTkMapForHomepage) {\n        if (homepageTrackingKey) {\n            const organicJobKeys = organicJobs.map((organicJob) => organicJob.jk);\n            const organicJobResultTrackingKeys = organicJobs.map((organicJob) =>\n                getVersion5UILTrackingKey(\n                    homepageTrackingKey,\n                    organicJob.jk,\n                    SurfaceType.HOMEPAGE,\n                    organicJob.organicBlob\n                )\n            );\n            const sponsoredJobResultTrackingKeys = sponsoredJobs.map((sponsoredJob) =>\n                getVersion5UILTrackingKey(\n                    homepageTrackingKey,\n                    sponsoredJob.jk,\n                    SurfaceType.HOMEPAGE,\n                    sponsoredJob.adBlob\n                )\n            );\n            const jobResultTrackingKeys = sponsoredJobResultTrackingKeys.concat(\n                organicJobResultTrackingKeys\n            );\n            const sponsoredJobKeys = sponsoredJobs.map((sponsoredJob) => sponsoredJob.jk);\n            const jobKeys = sponsoredJobKeys.concat(organicJobKeys);\n            const feedTks = jobKeys.map((jk) => jobKeyToFeedTkMapForHomepage[jk]);\n            const isSponsored = sponsoredJobKeys\n                .map(() => true)\n                .concat(organicJobKeys.map(() => false));\n\n            const recordJobSeenInput = {\n                jobResultTrackingKeys,\n                additionalInfo: {\n                    homepageFields: {\n                        homepageTrackingKey,\n                        isDesktopRequest: !isMobile,\n                        feedTks,\n                        isSponsored,\n                    },\n                },\n            };\n\n            oneGraphClient.recordJobSeen(recordJobSeenInput);\n        }\n    }\n};\n\nconst doLogViewJobJobSeen = (tk: string, oneGraphClient?: OneGraphClient): void => {\n    if (sponsoredJobs.length === 0 && organicJobs.length === 0) {\n        return;\n    }\n\n    const sponsoredJobResultTrackingKeys: string[] = sponsoredJobs.map(\n        (job) => job.jobResultTrackingKey ?? `4-0-${tk}-${tk}-${job.jk}`\n    );\n    const organicJobResultTrackingKeys: string[] = organicJobs.map(\n        (job) => job.jobResultTrackingKey ?? `4-0-${tk}-${tk}-${job.jk}`\n    );\n    const jobResultTrackingKeys = sponsoredJobResultTrackingKeys.concat(\n        organicJobResultTrackingKeys\n    );\n    const recordJobSeenInput = {\n        jobResultTrackingKeys: jobResultTrackingKeys,\n    };\n\n    if (oneGraphClient) {\n        oneGraphClient.recordJobSeen(recordJobSeenInput);\n    }\n};\n\nexport const logJobSeen = (params: JobSeenParams): void => {\n    const {\n        atk,\n        tk,\n        jk,\n        mosaicInitialDataLogTk,\n        adId,\n        adBlob,\n        isSponsored,\n        isMobile,\n        additionalParams,\n        hostId,\n        pageId,\n        jobKeyToFeedTkMapForHomepage,\n        oneGraphClient,\n        locale,\n        isBatchingEnabled,\n        batchParams,\n        jobResultTrackingKey,\n        organicBlob,\n    } = params;\n    // If the keepalive flag isn't supported, we can't assume batched logging would be sent properly during a page unload\n    const isBatchingSupported = typeof Request === 'function' && 'keepalive' in new Request('');\n    // @ts-ignore\n    const isGoodConnection = (navigator['connection']?.effectiveType ?? '4g') === '4g'; // If false, don't batch logs\n    // onLine is not super reliable. A false value definitely means we're offline, but true may not mean the site is accessible\n    // Might need something better in the future\n    const isOnline = navigator.onLine;\n\n    const stopPendingBatch = () => {\n        clearTimeout(lastLoggingTask as ReturnType<typeof setTimeout>);\n        lastLoggingTask = undefined;\n    };\n\n    const sendBatchImmediately = () => {\n        stopPendingBatch();\n        sendLogsInBatch(\n            atk,\n            tk,\n            mosaicInitialDataLogTk,\n            isMobile,\n            additionalParams,\n            locale,\n            hostId,\n            pageId,\n            jobKeyToFeedTkMapForHomepage,\n            oneGraphClient\n        );\n    };\n\n    const queueBatch = () => {\n        if (isBatchingEnabled && isBatchingSupported && isGoodConnection) {\n            const maxBatchSize = batchParams?.maxBatchSize ?? -1; // maxBatchSize of -1 is unlimited\n            const jobArray = isSponsored ? sponsoredJobs : organicJobs;\n            if (maxBatchSize > 0 && maxBatchSize <= jobArray.length) {\n                sendBatchImmediately();\n            } else if (!lastLoggingTask) {\n                const batchDelay =\n                    (batchParams?.batchDelay ?? -1) >= 0 ? batchParams!.batchDelay : 30_000;\n                lastLoggingTask = setTimeout(() => {\n                    sendLogsInBatch(\n                        atk,\n                        tk,\n                        mosaicInitialDataLogTk,\n                        isMobile,\n                        additionalParams,\n                        locale,\n                        hostId,\n                        pageId,\n                        jobKeyToFeedTkMapForHomepage,\n                        oneGraphClient\n                    );\n                }, batchDelay);\n            }\n        } else {\n            stopPendingBatch();\n            lastLoggingTask = sendLogsInAnimationFrameBatch(\n                atk,\n                tk,\n                mosaicInitialDataLogTk,\n                isMobile,\n                additionalParams,\n                locale,\n                hostId,\n                pageId,\n                jobKeyToFeedTkMapForHomepage,\n                oneGraphClient\n            );\n        }\n    };\n\n    // initializing page unload event trigger for last not yet sent batch\n    if (isBatchingEnabled && isBatchingSupported && !batchEventListenerAdded) {\n        document['addEventListener']('visibilitychange', () => {\n            if (document['visibilityState'] === 'hidden') sendBatchImmediately();\n        });\n        window['addEventListener']('pagehide', sendBatchImmediately);\n        window['addEventListener']('online', sendBatchImmediately);\n        window['addEventListener']('offline', stopPendingBatch);\n        batchEventListenerAdded = true;\n    }\n\n    const jobData = { jk };\n\n    if (isSponsored) {\n        sponsoredJobs.push({\n            ...jobData,\n            adId: adId as string,\n            jobResultTrackingKey,\n            adBlob: adBlob as string,\n        });\n    } else {\n        organicJobs.push({\n            ...jobData,\n            jobResultTrackingKey,\n            organicBlob: organicBlob as string,\n        });\n    }\n\n    if (!isOnline) {\n        stopPendingBatch();\n    } else if (!isGoodConnection) {\n        sendBatchImmediately();\n    } else {\n        queueBatch();\n    }\n};\n","import { HostId } from '../contexts/MosaicInitialContext';\n\nexport type LogDetails = {\n    mobtk: string;\n    pageId: string;\n    data: object;\n};\n\nexport const postEventLog = ({ mobtk, pageId, data }: LogDetails): Promise<Response> => {\n    const route = '/m/rpc/frontendlogging';\n\n    const eventData: Record<string, string> = {\n        mobtk,\n