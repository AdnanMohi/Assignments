.jss';\n\nexport const OneMatchJobTitle: React.FC<IInnerJobTitleProps> = (props: IInnerJobTitleProps) => {\n    const { pgettext } = useI18n();\n    const { shouldHideJobTitleUnderline } = useJobTitleTextDecorationContext();\n\n    if (!props) {\n        return null;\n    }\n\n    const titleToDisplay = props.displayTitle ?? props.title;\n    const title = props.desktop ? (\n        <span id={`jobTitle-${props.jobKey}`}>{titleToDisplay}</span>\n    ) : (\n        <>{titleToDisplay}</>\n    );\n\n    return (\n        <Box className={'jobTitle'} sx={oneMatchJobTitleSx}>\n            <JobLinkWrapper\n                isNew={props.isNew}\n                isHighQualityMarketplaceMatch={props.isHighQualityMarketplaceMatch}\n                isSponsored={props.isSponsored}\n                shouldHideJobTitleUnderline={shouldHideJobTitleUnderline}\n                isTwoLineJobTitle={props.isTwoLineJobTitle}\n            >\n                <JobLink\n                    tapItem={props.tapItem}\n                    role={props.desktop ? 'button' : undefined}\n                    ariaLabel={pgettext(\n                        'aria label for job title',\n                        'full details of {0}',\n                        titleToDisplay\n                    )}\n                    onClick={props.onClick}\n                    onMouseDown={props.onMouseDown}\n                    onContextMenu={props.onMouseDown}\n                    onKeyDown={props.onKeyDown}\n                >\n                    {title}\n                </JobLink>\n            </JobLinkWrapper>\n        </Box>\n    );\n};\n","import { _ as _objectWithoutPropertiesLoose, I as Icon, a as _extends } from './Icon-b1a2a6d8.js';\nimport React, { forwardRef } from 'react';\nimport '@emotion/is-prop-valid';\nimport '@indeed/ifl-css';\nimport '@emotion/react';\n\nvar _excluded = [\"fill\"];\nvar CloseCircle = /*#__PURE__*/forwardRef(function (_ref, ref) {\n  var fill = _ref.fill,\n      props = _objectWithoutProperti