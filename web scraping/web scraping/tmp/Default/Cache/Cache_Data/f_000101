 > 0 ? showRedLightSignal : false) ||\n        manyApplicantsFilter ||\n        props.isOutOfCountry;\n\n    const isNewJobCardInfoArchitectureTestActive =\n        getProctorBucketValue(proctorGroups, 'jcs_new_jobcard_ia_tst') > 0;\n\n    const hideSalary: boolean =\n        (props.qualificationsExpTst > 0 &&\n            showRedLightSignal &&\n            [2, 3, 5, 7, 8].includes(props.qualificationsExpTst)) ||\n        manyApplicantsFilter ||\n        props.isOutOfCountry ||\n        isNewJobCardInfoArchitectureTestActive;\n\n    const isOutOfCountryMismatch: boolean = props.isOutOfCountry && !showRedLightSignal;\n\n    const showDynamicAttributes =\n        props.showDynamicAttributes &&\n        !(props.qualificationsExpTst > 0 && showRedLightSignal) &&\n        !props.isOutOfCountry;\n\n    const hideWorkingHours: boolean =\n        (props.qualificationsExpTst > 0 && showRedLightSignal) ||\n        manyApplicantsFilter ||\n        props.isOutOfCountry;\n\n    return (\n        <>\n            {responsiveEmployerUiTstActive && !isNewJobCardInfoArchitectureTestActive && (\n                <ResponsiveEmployerTag {...props.responsiveEmployer} />\n            )}\n            {props.isShowJobTagsAtTop && <JobTags {...props.jobTags} />}\n            {!props.responsiveEmployer.isOnJobCardShelf && (\n                <ResponsiveEmployer {...props.responsiveEmployer} />\n            )}\n            {!hideRedLightSignal && showRedLightSignalOnTop && props.aQSignal && (\n                <div className={'aqSignalRedLight-wrapper'}>\n                    <AQSignal {...props.aQSignal} jobKey={props.jobkey} />\n                </div>\n            )}\n            {showMetaDataContainer && !props.isMisMatchRecommendationJobs && (\n                <Box\n                    className={classnames(\n                        'heading6',\n                        'tapItem-gutter',\n                        'metadataContainer',\n                        {\n                            'metad