   const [paginationCallbacks, setPaginationCallbacks] = useState<PaginationCallback[]>([]);\n\n    useEffect(() => {\n        if (window['_jcsPaginationContext']) {\n            window['_jcsPaginationContext']['paginatedJobs'] = paginatedJobs;\n        } else {\n            window['_jcsPaginationContext'] = {\n                paginatedJobs: paginatedJobs,\n            };\n        }\n    }, [paginatedJobs]);\n\n    useEffect(() => {\n        while (paginationCallbacks.length > 0) {\n            const callback = paginationCallbacks.shift();\n            callback?.(paginatedJobs);\n        }\n        // eslint-disable-next-line react-hooks/exhaustive-deps\n    }, [paginatedJobs]);\n\n    const addPages = useCallback(\n        (models: IMosaicProviderJobCardsModel[], paginationCallback?: PaginationCallback) => {\n            let originalPaginatedJobs: IMosaicProviderJobCardsModel[] = [];\n            if (\n                window['_jcsPaginationContext']?.['paginatedJobs'] &&\n                Array.isArray(window['_jcsPaginationContext']['paginatedJobs'])\n            ) {\n                originalPaginatedJobs = window['_jcsPaginationContext']['paginatedJobs'];\n            }\n            if (paginationCallback) {\n                setPaginationCallbacks([...paginationCallbacks, paginationCallback]);\n            }\n            const updatedPaginatedJobs = [...originalPaginatedJobs, ...models];\n            setPaginatedJobs(updatedPaginatedJobs);\n            return updatedPaginatedJobs;\n        },\n        [paginationCallbacks]\n    );\n\n    const addPage = useCallback(\n        (model: IMosaicProviderJobCardsModel, paginationCallback?: PaginationCallback) => {\n            return addPages([model], paginationCallback);\n        },\n        [addPages]\n    );\n\n    const context = useMemo(\n        () => ({\n            paginatedJobs,\n            addPage,\n            addPages,\n        }),\n        [paginatedJobs, addPage, addPages]\n    );\n    return (\n        <P