ible: boolean }\n> = {};\n\n// Export for testing purposes only.\nexport const resetSeenTimes = () => {\n    SEEN_TIMES = {};\n};\n\nconst onLogTimeInViewport = (\n    oneGraphClient: OneGraphClient,\n    input: Omit<RecordJobTimeInViewportInput, 'jobInfos'>\n) => {\n    // we need to update the duration for all visible job cards to make sure timeInViewport is logged for all seen cards\n    const now = Date.now();\n\n    Object.values(SEEN_TIMES).forEach((seenInfo) => {\n        if (seenInfo.isVisible) {\n            seenInfo.duration += now - seenInfo.lastBecameVisibleTime;\n            seenInfo.lastBecameVisibleTime = now;\n        }\n    });\n\n    oneGraphClient?.recordJobTimeInViewport({\n        isMobile: input.isMobile,\n        jobInfos: Object.keys(SEEN_TIMES)\n            .map((jobKey) => ({\n                jobKey,\n                durationMilliseconds: SEEN_TIMES[jobKey].duration,\n            }))\n            .filter((job) => job.durationMilliseconds > 0),\n        searchProvider: input.searchProvider,\n        searchTrackingKey: input.searchTrackingKey,\n        surface: input.surface,\n        surfaceTrackingKey: input.surfaceTrackingKey,\n    });\n};\n\nconst onToggledVisible = (\n    jobKey: string,\n    toggledVisible: boolean,\n    isLogTimeInViewPort?: boolean\n) => {\n    if (!isLogTimeInViewPort) {\n        return;\n    }\n\n    const now = Date.now();\n    const existingSeenTimeRecord = SEEN_TIMES[jobKey] || {\n        duration: 0,\n        lastBecameVisibleTime: now,\n        isVisible: true,\n    };\n\n    // visible -> not visible\n    if (!toggledVisible) {\n        existingSeenTimeRecord.duration += now - existingSeenTimeRecord.lastBecameVisibleTime;\n        existingSeenTimeRecord.isVisible = false;\n    } else {\n        // not visible -> visible\n        existingSeenTimeRecord.lastBecameVisibleTime = now;\n        existingSeenTimeRecord.isVisible = true;\n    }\n\n    SEEN_TIMES[jobKey] = existingSeenTimeRecord;\n};\n\nconst onJobInViewport = (jobKey: string, isVisible: boolean, isLogTimeInViewPort?: boolean) => {\n    if (!isLogTimeInViewPort) {\n        return;\n    }\n\n    const initialDuration = SEEN_TIMES[jobKey] ? SEEN_TIMES[jobKey].duration : 0;\n    const lastSeenStartTime = SEEN_TIMES[jobKey] ? SEEN_TIMES[jobKey].lastBecameVisibleTime : 0;\n    const totalSeenTime = isVisible\n        ? initialDuration\n        : initialDuration + (Date.now() - lastSeenStartTime);\n    SEEN_TIMES[jobKey] = {\n        duration: totalSeenTime,\n        lastBecameVisibleTime: Date.now(),\n        isVisible: isVisible,\n    };\n};\n\n// Job Card component with visibility tracking functionality\n// Use it instead of JobCard when you want to track for job seen\nconst JobCardWithVisibilityLogging: React.FunctionComponent<IJobCardProps> = (\n    props: IJobCardProps\n) => {\n    const logJobcardDisplay = useLogJobcardDisplay();\n    const isSeen = React.useRef(false);\n    const oneGraphClient = useOneGraphClientContext();\n    const { pageId } = useMosaicInitialData();\n    const mosaicInitialDataLogTk = useMosaicInitialData().logTk;\n    const isBatchingEnabled = props.isDesktop && props.locale.countryCode !== 'JP';\n    // Keeping the grp5 batchParams from jcs_batch_jobseen_tst\n    const batchParams: Partial<BatchParams> = { batchDelay: 30000, maxBatchSize: -1 };\n    const onVisibilityChange = (isVisible: boolean): void => {\n        const { searchUID, mobtk, hostId, locale } = props;\n\n        onToggledVisible(props.jobKey, isVisible, props.isLogTimeInViewPort);\n\n        if (isVisible && !isSeen.current) {\n            onJobInViewport(props.jobKey, isVisible, props.isLogTimeInViewPort);\n\n            isSeen.current = true;\n            logJobSeen({\n                atk: searchUID,\n                tk: mobtk,\n                jk: props.jobkey,\n                mosaicInitialDataLogTk,\n                adId: props.tapItem.adId,\n                adBlob: props.tapItem.adBlob,\n                isSponsored: 