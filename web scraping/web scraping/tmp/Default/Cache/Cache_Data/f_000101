omepage or inactive/control\n    if (!props.isHomepage || props.jsj_hp_matchexplanation_tst_value <= 0) {\n        return getFromSearch(props, gt);\n    }\n\n    // empty match explanation for group 1\n    if (props.jsj_hp_matchexplanation_tst_value === 1) {\n        return null;\n    }\n\n    // otherwise, try getting testing match explanation text for grp2 and grp3\n    if (\n        props.jsj_hp_matchexplanation_tst_value === 2 ||\n        props.jsj_hp_matchexplanation_tst_value === 3\n    ) {\n        if (props.matchExplanationText.length > 0) {\n            return (\n                <span>\n                    {' '}\n                    • <b>{props.matchExplanationText}</b>\n                </span>\n            );\n        } else {\n            return null;\n        }\n    }\n\n    // default behavior\n    return getFromSearch(props, gt);\n};\n\nexport interface IManyApplicantsProps {\n    manyApplicantsString: string;\n}\n\nconst ManyApplicants: React.FunctionComponent<IManyApplicantsProps> = (\n    props: IManyApplicantsProps\n) => {\n    return (\n        <span>\n            {' '}\n            •\n            <Heading level={1} sx={{ color: 'warning.700', display: 'inline' }}>\n                <DataBar size={'xs'} sx={{ verticalAlign: 'text-bottom', display: 'inline' }} />\n                {props.manyApplicantsString}\n            </Heading>\n        </span>\n    );\n};\n\nconst MyJobsState: React.FunctionComponent<IMyJobsStateProps> = (props: IMyJobsStateProps) => {\n    const gt = useI18n();\n    const proctorGroups = useProctorContext();\n    const matchExplanation = getMatchExplanation(props, gt);\n    const shouldShowTimestamp = !shouldHideTimestamp(\n        props.isSponsored,\n        proctorGroups,\n        props.formattedRelativeTime\n    );\n\n    if (shouldShowTimestamp && (props.formattedMyJobsStateChangeTime || props.myJobsStateString)) {\n        return (\n            <Box as=\"span\" sx={myJobsStateSx} data-testId=\"myJobsState\">\n            