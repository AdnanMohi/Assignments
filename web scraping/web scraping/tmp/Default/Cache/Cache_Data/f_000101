l,\n        photoAltText: currentJobModel.photoAltText,\n        qualificationsExpTst: jcsQualsProctorVal,\n        isSponsored: currentJobModel.sponsored,\n        showDynamicAttributes: showDynamicAttributes,\n        dynamicAttributes: dynamicAttributes,\n        benefits: getDynamicAttributes(currentJobModel, BENEFITS_LABEL),\n        manyApplicantsTst: getProctorBucketValue(proctorGroups, 'jcs_many_applicants_tst'),\n        organicApplyStartCount: currentJobModel.organicApplyStartCount,\n        isOutOfCountry: model.outOfCountry && locale.countryCode !== 'JP',\n        enforceHeightLimitations,\n        enableRedlightForNonSponsorJobs:\n            getProctorBucketValue(proctorGroups, 'jcs_missing_qualification_banner_oj') > 0,\n    };\n};\n","import type { IJobModel } from '@indeed/job-cards-model';\n\nexport const getEnticers = (currentJobModel: IJobModel): string[] => {\n    return currentJobModel.enticers?.map(({ label }) => label) ?? [];\n};\n\nexport const getEnticerFlair = (currentJobModel: IJobModel): string | undefined => {\n    return currentJobModel.enticers?.[0]?.label;\n};\n","import { IJobModel, IMosaicProviderJobCardsModel } from '@indeed/job-cards-model';\n\nimport { Locale } from '../../../contexts/I18nContext';\nimport { AppProctorGroups } from '../../../proctor';\nimport { getProctorBucketValue } from '../../../proctor/getGroup';\nimport { showPayForPerformanceFeature } from '../../../util/payForPerformance';\nimport { isRedLightJob } from '../../JobMetaData/AQSignal/apiBridge';\nimport { getEnticerFlair } from '../../JobMetaData/Enticers/apiBridge';\nimport { FlairOption, getFlairOption } from '../../JobMetaData/Flair/apiBridge';\n\nexport interface IJobTitleProps {\n    isTwoLineJobTitle: boolean;\n    title: string;\n    subtitle?: string;\n    displayTitle?: string;\n    isNew: boolean;\n    linkColorClass: '' | 'jobTitle-color-purple' | 'jobTitle-color-purple-visited';\n    desktop?: boolean;\n    isHighQualityMarketplaceMatch?: boolea