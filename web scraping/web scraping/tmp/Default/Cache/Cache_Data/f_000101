rybook so that we can test the right-to-left experience even though the\n * content is English and dir=\"auto\" would default to left-to-right.\n */\nconst shouldNotUseAutoDirection =\n    typeof window !== 'undefined' && '__STORYBOOK_PREVIEW__' in window;\n\nexport enum EnumSwipeableCardStyleVariant {\n    DEFAULT,\n    JAPAN,\n}\n\nexport interface SwipeableCardProps {\n    primaryComponent: React.ReactElement;\n    secondaryComponent?: React.ReactElement;\n    isApp?: boolean;\n    ariaHideSliderSubItem?: boolean;\n    hostId?: string;\n    onSwipeLeft?: () => void;\n    onSwipeRight?: () => void;\n    swipeableCardStyleVariant: EnumSwipeableCardStyleVariant;\n    showingPrimaryComponent?: boolean;\n}\n\nconst MIN_SWIPE_LENGTH = 60;\nconst ANGLE_WINDOW = 5;\n\nexport const SwipeableCard = (props: SwipeableCardProps): JSX.Element => {\n    const [xPos, setXPos] = useState(0);\n    const [yPos, setYPos] = useState(0);\n    const [isSwiping, setIsSwiping] = useState(false);\n    const [swipeDirection, setSwipeDirection] = useState('');\n\n    const handleTouchStart = (e: React.TouchEvent<HTMLDivElement>): void => {\n        setXPos(e.changedTouches[0].pageX);\n        setYPos(e.changedTouches[0].pageY);\n        setIsSwiping(true);\n    };\n\n    const handleTouchMove = (e: React.TouchEvent<HTMLDivElement>): void => {\n        if (e.changedTouches.length > 1) {\n            setIsSwiping(false);\n        }\n    };\n\n    const handleTouchEnd = (e: React.TouchEvent<HTMLDivElement>): void => {\n        if (isSwiping) {\n            const distanceX = e.changedTouches[0].pageX - xPos;\n            const distanceY = yPos - e.changedTouches[0].pageY; // top left is 0,0\n            const swipeDistance = Math.sqrt(Math.pow(distanceX, 2) + Math.pow(distanceY, 2));\n            const { onSwipeLeft, onSwipeRight } = props;\n            setIsSwiping(false);\n\n            if (swipeDistance > MIN_SWIPE_LENGTH) {\n                const swipeAngle = Math.round((Math.atan2(distanceY, distanceX) * 180) / Math.PI); // angle in degrees\n                // swipeAngle can be negative (-180 to 180)\n                if (Math.abs(swipeAngle) < ANGLE_WINDOW) {\n                    setSwipeDirection('right');\n                    onSwipeRight?.();\n                    return;\n                } else if (Math.abs(swipeAngle) > 180 - ANGLE_WINDOW) {\n                    setSwipeDirection('left');\n                    onSwipeLeft?.();\n                    return;\n                }\n            }\n        }\n    };\n\n    const isJapan = props.swipeableCardStyleVariant === EnumSwipeableCardStyleVariant.JAPAN;\n\n    return (\n        <Box\n            onTouchStart={\n                props.isApp || props.hostId === 'homepage'\n                    ? () => {}\n                    : props.secondaryComponent && handleTouchStart\n            }\n            onTouchMove={\n                props.isApp || props.hostId === 'homepage'\n                    ? () => {}\n                    : props.secondaryComponent && handleTouchMove\n            }\n            onTouchEnd={\n                props.isApp || props.hostId === 'homepage'\n                    ? () => {}\n                    : props.secondaryComponent && handleTouchEnd\n            }\n            className=\"slider_container\"\n            data-testid=\"slider_container\"\n            sx={\n                props.swipeableCardStyleVariant === EnumSwipeableCardStyleVariant.DEFAULT\n                    ? {\n                          ...sliderContainerSx,\n                          ...swipeableCardStyleVariantDefault.sliderContainerStyle,\n                      }\n                    : sliderContainerSx\n            }\n            dir={shouldNotUseAutoDirection ? undefined : 'auto'}\n        >\n            <Box\n                className=\"slider_list\"\n                sx={\n                    props.swipeableCardStyleVariant === EnumSwipeableCardStyleVariant.JAPAN\n                        ? { ...sliderListSx, ...swipeableCardStyleVariantJapan.sliderListStyle }\n                        : sliderListSx\n                }\n            >\n                <Box\n                    data-testid=\"slider_item\"\n                    className={classnames('slider_item', {\n                        item_invisible: swipeDirection === 'left' && props.secondaryComponent,\n                    })}\n                    sx={\n                        props.swipeableCardStyleVariant === EnumSwipeableCardStyleVariant.JAPAN\n                            ? {\n                                  ...sliderItemSx,\n                                  ...swipeableCardStyleVariantJapan.sliderItemStyle,\n                                  transform: props.showingPrimaryComponent\n                                      ? 'translateX(0)'\n                                      : 'translateX(calc(-100% - 0.5rem))',\n                              }\n                            : sliderItemSx\n                    }\n                >\n                    {props.primaryComponent}\n                </Box>\n                {props.secondaryComponent && (\n                    <Box\n                        data-testid=\"slider_sub_item\"\n                        className={classnames({\n                            slider_sub_item: !isJapan,\n                            sub_item_visible: !isJapan && swipeDirection === 'left',\n                        })}\n                        aria-hidden={props.ariaHideSliderSubItem ? 'true' : undefined}\n                        sx={\n                            isJapan\n                                ? {\n                                      ...swipeableCardStyleVariantJapan.sliderSubItemStyle,\n                                      ...(swipeDirection === 'left'\n                                          ? swipeableCardStyleVariantJapan.sliderSubItemVisibleStyle\n                                          : {}),\n                                  }\n                                : {}\n                        }\n                    >\n                        {props.secondaryComponent}\n                    </Box>\n                )}\n            </Box>\n        </Box>\n    );\n};\n","import React from 'react';\n\nimport { Box, Heading, Text } from '@indeed/ifl-components';\nimport Search from '@indeed/ifl-icons/Search';\nimport classNames from 'classnames';\n\nimport { useI18n } from '../../hooks/useI18n';\nimport { AppProctorGroups } from '../../proctor';\nimport { getProctorBucketValue, isProctorBuckActiveByValue } from '../../proctor/getGroup';\nimport { getProctorBucketPayloadStringArray } from '../../proctor/getPayload';\n\nimport { getJobSectionCssClasses, IJobSectionHeaderProps, JobSection } from './apiBridge';\nimport { jobSectionsSx } from './JobSections.jss';\nimport defaultSectionIconUrl from './svg/defaultSection.svg';\nimport preferenceMatchSectionIconUrl from './svg/preferenceMatchSection.svg';\n\nconst topMatchheaderKey = 'topmatch_heading';\nconst topMatchDescriptionKey = 'topmatch_description';\nconst mainFeedHeaderKey = 'mainfeed_heading';\nconst mainFeedDescriptionKey = 'mainfeed_description';\n\nexport const JobSectionHeader: React.FunctionComponent<IJobSectionHeaderProps> = (\n    props: IJobSectionHeaderProps\n) => {\n    const { gettext, pgettext } = useI18n();\n    const showPageBreaker = isProctorBuckActiveByValue(props.groups, 'jcs_page_breaker_tst', 1);\n    const getHeadingsForTopMatch =\n        getProctorBucketValue(props.groups, 'jsj_hp_topmatch_section_header_tst') > 0;\n    let topMatchHeader;\n    if (getHeadingsForTopMatch) {\n        topMatchHeader = extractTopMatchHeadersFromProctor(props.groups);\n    }\n\n    if (props.jobSection.indexOf('page') === 0 && showPageBreaker) {\n        return (\n            <JobSectionHeaderPagination\n                headerText={pgettext(\n                    'header for appended paginated jobs where {0} indicates page number',\n                    'Page {0}',\n               