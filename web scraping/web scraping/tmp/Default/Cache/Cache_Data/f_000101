tion.NONE;\n};\n\nexport const getTopBiddersWithFlair = (\n    proctorGroups: AppProctorGroups,\n    model: IMosaicProviderJobCardsModel,\n    locale: Locale\n): Map<string, FlairOption> => {\n    const results = [...model.results];\n\n    // Sort the results by highest to lowest bid\n    const sortedResults = results\n        .sort((a, b) => {\n            const aBid = a.rankingScoresModel?.bid ?? 0;\n            const bBid = b.rankingScoresModel?.bid ?? 0;\n\n            return bBid - aBid;\n        })\n        .filter((r) => r.rankingScoresModel?.bid ?? 0 > 0);\n\n    const MAX_BIDDERS = 3;\n    const topBidders = sortedResults.slice(0, MAX_BIDDERS);\n\n    const jobKeyToFlairOptionMap: Map<string, FlairOption> = new Map();\n\n    for (const result of topBidders) {\n        const earlyAppGroupNumber = getProctorBucketValue(proctorGroups, 'jcs_early_app_flair_tst');\n        const showEarlyApplicant =\n            result.applyCount !== undefined && result.applyCount !== -1 && result.applyCount < 10;\n        const { showUrgentlyHiring } = getUrgentlyHiringProps(result, proctorGroups);\n        const { showHiringMultipleCandidates } = getHiringMultipleCandidatesProps(result, locale);\n\n        let flairOption = FlairOption.NONE;\n\n        if (earlyAppGroupNumber === 1 && showEarlyApplicant)\n            flairOption = FlairOption.EARLY_APPLICANT; // overrides any current flairOption\n        else if (earlyAppGroupNumber === 2 && showEarlyApplicant && !showUrgentlyHiring)\n            // overrides NONE and HIRING_MULTIPLE flairOptions\n            flairOption = FlairOption.EARLY_APPLICANT;\n        else if (\n            earlyAppGroupNumber === 3 &&\n            showEarlyApplicant &&\n            !showUrgentlyHiring &&\n            !showHiringMultipleCandidates\n        )\n            flairOption = FlairOption.EARLY_APPLICANT; // overrides NONE flairOption\n        else {\n            flairOption = getEnticerFlair(\n                result,\n                proctorGroups,\n                showUrgentlyHiring,\n                showHiringMultipleCandidates\n            );\n        }\n        jobKeyToFlairOptionMap.set(result.jobkey, flairOption);\n    }\n\n    return jobKeyToFlairOptionMap;\n};\n\nexport const getEnticerFlair = (\n    job: IJobModel,\n    proctorGroups: AppProctorGroups,\n    showUrgentlyHiring: boolean,\n    showHiringMultipleCandidates: boolean\n): FlairOption => {\n    const flairGroup = getProctorBucketValue(proctorGroups, 'jcs_flair_expansion_tst');\n\n    if (flairGroup < 1) {\n        if (showUrgentlyHiring) {\n            return FlairOption.URGENTLY_HIRING;\n        } else if (showHiringMultipleCandidates) {\n            return FlairOption.HIRING_MULTIPLE_CANDIDATES;\n        } else {\n            return FlairOption.NONE;\n        }\n    }\n    const taxoAttributes = parseTaxonomyAttributes(job.taxonomyAttributes);\n    if (taxoAttributes.includes('NDJ8P')) {\n        return FlairOption.NO_INTERVIEW_REQUIRED;\n    } else if (taxoAttributes.includes('WZ9TD')) {\n        return FlairOption.FLEXIBLE_SCHEDULE;\n    } else if (showUrgentlyHiring) {\n        return FlairOption.URGENTLY_HIRING;\n    } else if (showHiringMultipleCandidates) {\n        return FlairOption.HIRING_MULTIPLE_CANDIDATES;\n    } else if (taxoAttributes.includes('5WFZM')) {\n        return FlairOption.OVERTIME;\n    } else if (taxoAttributes.includes('7EQCZ')) {\n        return FlairOption.NEW_GRADUATES_WELCOME;\n    } else if (flairGroup === 1 && taxoAttributes.includes('DSQF7')) {\n        return FlairOption.REMOTE;\n    } else {\n        return FlairOption.NONE;\n    }\n};\n\nconst parseTaxonomyAttributes = (\n    taxoAttributes: ITaxonomyAttributesModel[] | undefined\n): string[] => {\n    const attributes: string[] = [];\n    taxoAttributes?.forEach((attribute) => {\n        attribute.attributes.map((attr) => attributes.push(attr.suid));\n    });\n    return attributes;\n};\n","import { IJobModel } from '@indeed/job-cards-model';\n\n