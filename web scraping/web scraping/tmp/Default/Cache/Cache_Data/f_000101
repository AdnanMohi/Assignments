nk;\n                } else {\n                    window['location'].href = `${\n                        props.myIndeedRegisterLink\n                    }&dest=${encodeURIComponent(window['location'].href)}&from=${passportFrom}`;\n                }\n            }\n        }\n    }\n};\n\nconst handleLoggedInSaveJobAction = (\n    action: 'remove' | 'save',\n    props: SaveJobStateProps,\n    vanillaStore: VanillaStore,\n    jobKey: string,\n    hqmToken: string | undefined = undefined,\n    isJpBundle: boolean | undefined = undefined,\n    isSponsored: boolean,\n    shouldLog: boolean\n) => {\n    if (action === 'save') {\n        vanillaStore.getState().saveJob(jobKey);\n        const callback = () =>\n            handleSaveJobAction(props, vanillaStore, jobKey, true, isSponsored, null);\n        windowStore.showSavedToast(callback, true);\n        windowStore.hideToast(3000);\n    } else {\n        vanillaStore.getState().unsaveJob(jobKey);\n    }\n\n    const serverSaveJobPromise = saveJobOnServer(props, action, jobKey, hqmToken, isJpBundle);\n\n    if (action === 'save') {\n        if (shouldLog) {\n            logSaveJobClick(props, jobKey, 'save');\n        }\n        windowStore.dispatchCustomEventJobSaved(jobKey);\n    } else {\n        if (isJpBundle && shouldLog) {\n            logSaveJobClick(props, jobKey, 'unsave');\n        }\n        windowStore.dispatchCustomEventJobUnsaved(jobKey);\n    }\n    return serverSaveJobPromise;\n};\n\nconst saveJobOnDevice = (\n    props: SaveJobStateProps,\n    action: string,\n    vanillaStore: VanillaStore,\n    jobKey: string,\n    isSponsored: boolean,\n    isJpBundle?: boolean\n): void => {\n    fetch('/m/rpc/log/savejobondevice', {\n        method: 'POST',\n        headers: {\n            'Content-Type': 'application/x-www-form-urlencoded',\n        },\n        body: `mobtk=${props.mobtk}&jobKey=${jobKey}&action=${action}&origin=${props.originPage}&indeedcsrftoken=${props.csrfToken}`,\n    });\n\n    if (action === 'save') {\n        const callback = () =>\n            handleSaveJobAction(props, vanillaStore, jobKey, true, isSponsored, null);\n        windowStore.showSavedToast(callback, false);\n        windowStore.hideToast(3000);\n\n        logSaveJobClick(props, jobKey, 'save');\n\n        const result = saveJob(jobKey);\n        switch (result) {\n            case SAVE_RESULT_TYPES.RESULT_SUCCESS:\n                vanillaStore.getState().saveJob(jobKey);\n                windowStore.dispatchEventJobSaved();\n                break;\n            case SAVE_RESULT_TYPES.DUPLICATE_SAVE:\n                vanillaStore.getState().handleSaveFailure({\n                    jobKey,\n                    isSaved: true,\n                    saveFailureMsg: vanillaStore\n                        .getState()\n                        .gt.pgettext('save job on device', 'This job is already saved'),\n                    isDisliked: false,\n                    shouldHideUndoDislike: true,\n                });\n                break;\n            case SAVE_RESULT_TYPES.JOB_LIST_FULL:\n                vanillaStore.getState().handleSaveFailure({\n                    jobKey,\n                    isSaved: false,\n                    saveFailureMsg: vanillaStore\n                        .getState()\n                        .gt.pgettext(\n                            'save job on device',\n                            'You reached the limit number of saved jobs'\n                        ),\n                    isDisliked: false,\n                    shouldHideUndoDislike: true,\n                });\n                if (isJpBundle) {\n                    window['location'].assign('/m/savedjoblist?r=le');\n                }\n                break;\n            default:\n                vanillaStore.getState().handleSaveFailure({\n                    jobKey,\n                    isSaved: false,\n                    saveFailureMsg: vanillaStore.getState().gt.gettext('Failed to save job'),\n                    isDisliked: false,\n                    shouldHideUndoDislike: true,\n                });\n        }\n    } else {\n        if (isJpBundle) {\n            logSaveJobClick(props, jobKey, 'unsave');\n        }\n        const result = unsaveJob(jobKey);\n        if (result === SAVE_RESULT_TYPES.RESULT_SUCCESS) {\n            vanillaStore.getState().unsaveJob(jobKey);\n            windowStore.dispatchEventJobUnsaved();\n        } else {\n            vanillaStore.getState().handleSaveFailure({\n                jobKey,\n                isSaved: true,\n                saveFailureMsg: vanillaStore.getState().gt.gettext('Failed to unsave job'),\n                isDisliked: false,\n                shouldHideUndoDislike: true,\n            });\n        }\n    }\n\n    windowStore.setSavedJobCount(vanillaStore.getState().savedJobCount);\n};\n\nconst logSaveJobClick = (\n    props: SaveJobStateProps,\n    jobKey: string,\n    action: 'save' | 'unsave'\n): void => {\n    // eslint-disable-next-line @typescript-eslint/no-floating-promises\n    fetch(\n        `/m/rpc/log/saveJobClick?mobtk=${encodeURIComponent(props.mobtk)}&pageId=${\n            props.originPage\n        }&jobKey=${encodeURIComponent(jobKey)}&action=${action}`\n    );\n};\n\nexport const getPassportFrom = (hostId: string, context: string): string => {\n    if (hostId === HostId.MOBILE) {\n        return 'jsfe-serp-jobcards-save-job';\n    } else if (hostId === HostId.HOMEPAGE) {\n        return 'jsfe-homepage-jobcards-save-job';\n    } else {\n        return `jsfe-${hostId}-${context}-jobcards`;\n    }\n};\n","import Cookies from 'js-cookie';\n\nimport { SaveJobStateProps } from '../../store/apiBridge/saveJobs';\n\nimport saveJobOnServer from './saveJobOnServer';\n\nconst SAVE_JOB_COOKIE_NAME = 'SAVED_JOB';\n\nexport default (props: SaveJobStateProps): void => {\n    const jobKey = Cookies.get(SAVE_JOB_COOKIE_NAME) ?? '';\n    if (jobKey !== '' && props.loggedIn) {\n        // eslint-disable-next-line no-void\n        void saveJobOnServer(props, 'save', jobKey, undefined, false);\n    }\n\n    Cookies.remove(SAVE_JOB_COOKIE_NAME);\n};\n\nexport const setSavedJobCookie = (jobKey: string): void => {\n    Cookies.set(SAVE_JOB_COOKIE_NAME, jobKey, { path: '/' });\n};\n","import { localStorageGetItem, localStorageSetItem, localStorageRemoveItem } from '../localStorage';\n\nconst SAVE_JOBS_LS_KEY = 'indeedmobile.savedJobs';\nconst JOB_KEYS_MAX_SIZE_ = 20;\n\nconst RESULT_SUCCESS = 'RESULT_SUCCESS';\nconst UNKNOWN_ERROR = 'UNKNOWN_ERROR';\nconst JOB_LIST_FULL = 'JOB_LIST_FULL';\nconst DUPLICATE_SAVE = 'DUPLICATE_SAVE';\n\nexport const SAVE_RESULT_TYPES = {\n    RESULT_SUCCESS,\n    UNKNOWN_ERROR,\n    JOB_LIST_FULL,\n    DUPLICATE_SAVE,\n};\n\ntype SaveJobType =\n    | typeof RESULT_SUCCESS\n    | typeof UNKNOWN_ERROR\n    | typeof JOB_LIST_FULL\n    | typeof DUPLICATE_SAVE;\ntype UnsaveJobType = typeof RESULT_SUCCESS | typeof UNKNOWN_ERROR;\n\nexport const saveJob = (jobKey: string): SaveJobType => {\n    const keys = localStorageGetItem(SAVE_JOBS_LS_KEY);\n    if (!keys) {\n        /* istanbul ignore if  */\n        return localStorageSetItem(SAVE_JOBS_LS_KEY, jobKey) ? RESULT_SUCCESS : UNKNOWN_ERROR;\n    } else if (keys.split(',').length < JOB_KEYS_MAX_SIZE_) {\n        if (keys.split(',').indexOf(jobKey) > -1) {\n            return DUPLICATE_SAVE;\n        } else {\n            /* istanbul ignore if  */\n            return localStorageSetItem(SAVE_JOBS_LS_KEY, keys.concat(',', jobKey))\n                ? RESULT_SUCCESS\n                : UNKNOWN_ERROR;\n        }\n    } else {\n        return JOB_LIST_FULL;\n    }\n};\n\nexport const unsaveJob = (jobKey: string): UnsaveJobType => {\n    const keys = localStorageGetItem(SAVE_JOBS_LS_KEY);\n    if (keys) {\n        const newKeys = keys.split(',').filter((key) => key !== jobKey);\n        if (newKeys.length > 0) {\n            /* istanbul ignore if  */\n            if (localStorageSetItem(SAVE_JOBS_LS_KEY, newKeys.join(','))) {\n                return RESULT_SUCCESS;\n            }\n        } else {\n            /* istanbul ignore if  */\n            if (localStorageRemoveItem(SAVE_JOBS_LS_KEY)) {\n                return RESULT_SUCCESS;\n            }\n        }\n    }\n\n    return UNKNOWN_ERROR;\n};\n\nexport const getSaveJobCount = (): number => {\n    const keys = getSavedJobKeys();\n    return keys.length;\n};\n\nexport const getSavedJobKeys = (): string[] => {\n    const keys = localStorageGetItem(SAVE_JOBS_LS_KEY);\n    if (keys) {\n        return keys.split(',');\n    }\n\n    return [];\n};\n\nexport const isJobSavedInLocalStorage = (jobKey: string): boolean => {\n    try {\n        // eslint-disable-next-line\n        const keys = localStorageGetItem(SAVE_JOBS_LS_KEY);\n\n        if (!keys) {\n            return false;\n        }\n\n        return keys.split(',').indexOf(jobKey) > -1;\n    } catch {}\n    return false;\n};\n","import { SaveJobStateProps } from '../../store/apiBridge/saveJobs';\n\nconst SAVED_JOB = 'SAVED_JOB';\nconst UNSAVED_JOB = 'UNSAVED_JOB';\n\nexport type SaveJobOnServerProps = Pick<\n    SaveJobStateProps,\n    'mobtk' | 'hostId' | 'originPage' | 'csrfToken' | 'searchUID'\n>;\n\nexport default async (\n    props: SaveJobOnServerProps,\n    action: 'save' | 'remove',\n    jobKey: string,\n    hqmToken: string | undefined,\n    isJpBundle: boolean | undefined\n): Promise<void> => {\n    const isDesktop =\n        !isJpBundle &&\n        // @ts-ignore\n        window['mosaic']['providerData']['mosaic-provider-jobcards'].metaData\n            .mosaicProviderJobCardsModel.isDesktop;\n    try {\n        if (isJpBundle) {\n            await saveJobJp(props, action, jobKey);\n        } else if (isDesktop) {\n            await saveJobJasx(props, action, jobKey, props.hostId, hqmToken);\n        } else {\n            await saveJobMobile(props, action, jobKey, hqmToken);\n        }\n    } catch {\n        /* istanbul ignore if  */\n        // @ts-ignore\n       