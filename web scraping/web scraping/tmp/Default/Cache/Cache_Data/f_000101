../util/payForPerformance';\nimport {\n    HIGH_COVERAGE_RED,\n    HIGH_PRECISION_RED,\n    IAQSignalProps,\n    isMinMismatchMet,\n} from '../../AQSignal/apiBridge';\n\nexport interface IJobRequirementsProps {\n    /** Returns true if the job is a red light and job seeker has missing qualifications */\n    isJCSRedLight: boolean;\n    showRequirements: boolean;\n    /** List of requirements to display on a standard job card*/\n    requirements: string[];\n    additionalRequirements?: string;\n    additionalRequirementsCount: number;\n    /** List of every requirement the job has*/\n    allRequirements: string[];\n    allMatchingRequirements: string[];\n    /** List of all the mismatching required qualifications and deal breaking screener questions */\n    allMisMatchingMustHaveRequirements: string[];\n}\n\nexport const NUM_REQUIREMENTS_TO_SHOW = 3;\n\nexport const getJobRequirementsProps = (\n    currentJobModel: IJobModel,\n    aQSignalProps: IAQSignalProps | undefined,\n    proctorGroups: AppProctorGroups,\n    countryCode: string\n): IJobRequirementsProps => {\n    const HARD_REQUIRED_SQ = currentJobModel.jobCardRequirementsModel?.requirements ?? [];\n    const MATCHING_REQUIREMENTS =\n        currentJobModel.jobSeekerMatchSummaryModel?.sortedMatchingEntityDisplayText ?? [];\n    const MISSING_AND_NEGATIVE_MATCHING_REQUIREMENTS =\n        currentJobModel.jobSeekerMatchSummaryModel?.sortedMisMatchingEntityDisplayText ?? [];\n    const MUST_HAVE_REQUIREMENTS_AND_HARD_REQUIRED_SQ =\n        new Set(\n            currentJobModel.jobCardRequirementsModel?.jobTagRequirements\n                ?.filter((requirement) => requirement.strictness === 'required')\n                .map((requirement) => requirement.label)\n        ) ?? [];\n\n    const MISSING_HARD_REQUIRED_SQ_AND_MUST_HAVE_REQUIREMENTS = HARD_REQUIRED_SQ.concat(\n        MISSING_AND_NEGATIVE_MATCHING_REQUIREMENTS\n    )\n        .filter((requirement) => MUST_HAVE_REQUIREMENTS_AND_HARD_REQUIRED_SQ.has(requirement))\n        .filter((item, pos, self) => {\n            return self.indexOf(item) === pos;\n        });\n\n    const allRequirements = MATCHING_REQUIREMENTS.concat(HARD_REQUIRED_SQ)\n        .concat(MISSING_AND_NEGATIVE_MATCHING_REQUIREMENTS)\n        .filter((item, pos, self) => {\n            return self.indexOf(item) === pos;\n        });\n\n    const screenerQuestionAdditionalRequirementsCount =\n        currentJobModel.jobCardRequirementsModel?.additionalRequirementsCount ?? 0;\n\n    const commonShowRequirementsConditions =\n        showPayForPerformanceFeature(currentJobModel, countryCode) &&\n        countryCode !== 'NL' &&\n        countryCode !== 'DE' &&\n        countryCode !== 'JP';\n\n    const isJcsQualificationsExpTstRequirementsGroup =\n        getProctorBucketValue(proctorGroups, 'jcs_qualifications_exp_tst') >= 4;\n\n    const trafficLight = currentJobModel.jobSeekerMatchSummaryModel?.trafficLight;\n    const isRedLight =\n        (trafficLight === HIGH_PRECISION_RED || trafficLight === HIGH_COVERAGE_RED) &&\n        isMinMismatchMet(currentJobModel, proctorGroups);\n\n    const showRequirementsForRedLightJobCards =\n        isJcsQualificationsExpTstRequirementsGroup &&\n        isRedLight &&\n        MISSING_HARD_REQUIRED_SQ_AND_MUST_HAVE_REQUIREMENTS.length > 0;\n\n    const showRequirements =\n        commonShowRequirementsConditions && showRequirementsForRedLightJobCards;\n\n    return {\n        isJCSRedLight: isRedLight,\n        showRequirements,\n        requirements: isJcsQualificationsExpTstRequirementsGroup\n            ? MISSING_HARD_REQUIRED_SQ_AND_MUST_HAVE_REQUIREMENTS.slice(0, NUM_REQUIREMENTS_TO_SHOW)\n            : HARD_REQUIRED_SQ,\n        additionalRequirements: currentJobModel.jobCardRequirementsModel\n            ? currentJobModel.jobCardRequirementsModel.additionalRequirementsString\n            : undefined,\n        additionalRequirementsCount: isJcsQualificationsExpTstRequirementsGroup\n            ? Math.max(\n                  MISSING_HARD_REQUIRED_SQ_AND_MUST_HAVE_REQUIREMENTS.length -\n                      NUM_REQUIREMENTS_TO_SHOW,\n                  0\n              )\n            : screenerQuestionAdditionalRequirementsCount,\n        allRequirements: allRequirements,\n        allMatchingRequirements: MATCHING_REQUIREMENTS,\n        allMisMatchingMustHaveRequirements: MISSING_HARD_REQUIRED_SQ_AND_MUST_HAVE_REQUIREMENTS,\n    };\n};\n","import { IJobModel } from '@indeed/job-cards-model';\n\ntype JobTag = string;\n\nexport interface IJobTagsProps {\n    showJobTags: boolean;\n    jobTags: JobTag[];\n    showAll?: boolean;\n}\n\nexport const getJobTagsProps = (currentJobModel: IJobModel, country: string): IJobTagsProps => {\n    return {\n        showJobTags: country === 'JP',\n        jobTags: currentJobModel.translatedCmiJobTags || [],\n    };\n};\n","import { ReactElement } from 'react';\n\nimport {\n    IJobModel,\n    IStringMatchedPreference,\n    ITaxonomyAttribute,\n    ITaxonomyAttributesModel,\n} from '@indeed/job-cards-model/dist/JobCardsModel';\n\nimport { Locale } from '../../../../contexts/I18nContext';\nimport { AppProctorGroups } from '../../../../proctor';\nimport { isPrefMatchEnabled } from '../../../../util/isPrefMatchEnabled';\n\nexport interface IAttributeMatchProps {\n    isMatching: boolean;\n    matchingValues: string[];\n    allValues: string[];\n    taglineElement?: ReactElement;\n}\n\nexport interface IMatchingAttributes {\n    allAttributes: string[];\n    matchingAttributes: string[];\n    hasMatches: boolean;\n}\n\nfunction isDefined<T>(v: T | undefined | null): v is T {\n    return typeof v !== 'undefined' && v !== null;\n}\n\nfunction isAttrPriorityEnabled(locale: Locale, isMobile: boolean) {\n    return !(locale.countryCode.match('JP|DE|NL') && isMobile);\n}\n\nexport const getMatchingAttributes = (\n    currentJobModel: IJobModel,\n    isDesktop: boolean,\n    proctorGroups: AppProctorGroups,\n    attributeLabel: string,\n    matchedPreferenceLabel: string,\n    prioritizedAttributeSuids: string[],\n    locale: Locale,\n    filteredSuid?: string | string[]\n): IMatchingAttributes => {\n    // Get the group of attributes corresponding to the attribute label\n    const attributeGroup: ITaxonomyAttributesModel | undefined =\n        currentJobModel.taxonomyAttributes?.filter(\n            (taxonomyAttribute) => taxonomyAttribute.label === attributeLabel\n        )[0];\n\n    const isfilteredSuid = (suid: string) => {\n        return typeof filteredSuid === 'string'\n            ? suid === filteredSuid\n            : filteredSuid?.includes(suid);\n    };\n\n    function sortFunc(a: ITaxonomyAttribute, b: ITaxonomyAttribute): number {\n        if (!isDefined(filteredSuid)) {\n            return 0;\n        }\n        if (isfilteredSuid(a.suid)) {\n            return -1;\n        }\n        if (isfilteredSuid(b.suid)) {\n            return 1;\n        }\n        return 0;\n    }\n\n    function prioritizedAttributeSortFn(a: ITaxonomyAttribute, b: ITaxonomyAttribute): number {\n        // If the SUID of the attribute matches the filteredSuid, prioritize it at the top\n        if (isfilteredSuid(a.suid)) {\n            return -1;\n        }\n        if (isfilteredSuid(b.suid)) {\n            return 1;\n        }\n\n        const suidAIndex = prioritizedAttributeSuids.indexOf(a.suid);\n        const suidBIndex = prioritizedAttributeSuids.indexOf(b.suid);\n        // Otherwise, if the SUID of attribute 'a' is at a lower index or the SUID of attribute 'b'\n        // doesn't exist in the prioritizedAttributeSuids list, prioritize 'a' at the top\n        // Similarly, if the SUID of attribute 'b' is at a lower index or the SUID of attribute 'a'\n        // doesn't exist in the prioritizedAttributeSuids list, prioritize 'b' at the top\n        if (suidAIndex !== -1 && (suidAIndex < suidBIndex || suidBIndex === -1)) {\n            return -1;\n        }\n        if (suidBIndex !== -1 && (suidAIndex > suidBIndex || suidAIndex === -1)) {\n            return 1;\n        }\n\n        // Otherwise, if the SUIDs for both attributes aren't present in the prioritizedAttributeSuids list, prioritize alphabetically\n        if (suidAIndex === -1 && suidBIndex === -1) {\n            return a.label!.localeCompare(b.label!);\n        }\n\n        return 0;\n    }\n\n    if (!attributeGroup?.attributes) {\n        return {\n            allAttributes: [],\n            matchingAttributes: [],\n            hasMatches: false,\n        };\n    }\n\n    const allAttributeLabels: string[] = isAttrPriorityEnabled(locale, !isDesktop)\n        ? attributeGroup.attributes\n              .filter((attr) => isDefined(attr.label))\n              .sort(prioritizedAttributeSortFn)\n              .map((attr) => attr.label!)\n        : attributeGroup.attributes\n              .filter((attr) => isDefined(attr.label))\n              .sort(sortFunc)\n              .map((attr) => attr.label!);\n\n    // Get the SUIDs from the matched preferences\n    const matchedPreferences: IStringMatchedPreference[] =\n        currentJobModel.tier?.matchedPreferences?.stringMatchedPreferences.filter(\n            (matchedPref) => matchedPref.label === matchedPreferenceLabel\n        ) ?? [];\n\n    const matchedPrefSuids: string[] = matchedPreferences[0]?.values || [];\n    const matchingAttributeLabels: string[] =\n        matchedPrefSuids.length > 0\n            ? isAttrPriorityEnabled(locale, !isDesktop)\n                ? attributeGroup.attributes\n                      .filter(\n                          (attr) => matchedPrefSuids.includes(attr.suid) && isDefined(attr.label)\n                      )\n                      .sort(prioritizedAttributeSortFn)\n                      .map((attr) => attr.label!)\n                : attributeGroup.attributes\n                      .filter(\n                          (attr) => matchedPrefSuids.includes(attr.suid) && isDefined(attr.label)\n                      )\n                      .sort(sortFunc)\n                      .map((attr) => attr.label!)\n            : [];\n    const showMatches: boolean = isPrefMatchEnabled(locale.countryCode, !isDesktop, proctorGroups);\n\n    // Matching labels are the intersection of the taxonomyAttributes and stringMatchedPreferences\n    return {\n        allAttributes: allAttributeLabels,\n        matchingAttributes: matchingAttributeLabels,\n        hasMatches: showMatches && matchingAttributeLabels.length > 0,\n    };\n};\n","import { IJobModel } from '@indeed/job-cards-model';\n\nimport { Locale } from '../../../../contexts/I18nContext';\nimport { AppProctorGroups } from '../../../../proctor';\nimport { getProctorBucketValue } from '../../../../proctor/getGroup';\nimport {\n    JOB_TYPE_ATTRIBUTE_LABEL,\n    JOB_TYPE_CC_ATTRIBUTE_LABEL,\n} from '../../../../taxonomy/taxonomyGroups';\nimport { getMatchingAttributes } from '../../AttributeMatch/apiBridge';\n\nconst JOB_TYPE_MATCHED_PREFERENCE_LABEL = 'jobtype';\n\nexport interface IJobTypeProps {\n    showJobType: boolean;\n    jobTypes: string[];\n    matchingJobTypes: string[];\n    isBig6Style: boolean;\n    isWrapStyle: boolean;\n    hasMatchingPreference: boolean;\n    jobKey?: string;\n}\n\n// Provides a mapping from the jobTypeFilterRefineBy value to the taxonomy SUID\nconst jobTypeSuidMap = new Map<string, string>([\n    ['fulltime', 'CF3CP'],\n    ['parttime', '75GKK'],\n    ['contract', 'NJXCK'],\n    ['temporary', '4HKF7'],\n    ['internship', 'VDTG7'],\n]);\n\n// Job type attribute SUIDs ordered by display priority i.e. suid at index 0 gets the highest priority\nconst prioritizedJobTypeSuids = [\n    'CF3CP',\n    '75GKK',\n    'NJXCK',\n    '4HKF7',\n    'VDTG7',\n    '5QWDV',\n    '7EQCZ',\n    'T9BXE',\n    'CPAHG',\n    'CJWTS',\n    'ZG59D',\n    'UXQZ8',\n    '7SBAT',\n    'HS8UZ',\n    '9SYVT',\n];\n\nexport const getJobTypeProps = (\n    currentJobModel: IJobModel,\n    isDesktop: boolean,\n    proctorGroups: AppProctorGroups,\n    locale: Locale,\n    appliedFilter?: string\n): IJobTypeProps => {\n    if (getProctorBucketValue(proctorGroups, 'jcs_big6_tst') <= 1) {\n        // Keep legacy behavior for job type display\n        const isWrapStyle: boolean =\n            (currentJobModel.taxoAttributes && currentJobModel.taxoAttributes.length > 0) || false;\n\n        return {\n            showJobType: currentJobModel.showJobType,\n            jobTypes: currentJobModel.jobTypes || [],\n            matchingJobTypes: [],\n            isBig6Style: false,\n            isWrapStyle: !!isWrapStyle,\n            hasMatchingPreference: false,\n        };\n    }\n\n    const getAppliedFilterSuid = (appliedFilterSuid?: string) => {\n        if (appliedFilterSuid && jobTypeSuidMap.has(appliedFilterSuid)) {\n            return jobTypeSuidMap.get(appliedFilterSuid)!;\n        }\n\n        return;\n    };\n\n    const { allAttributes, matchingAttributes, hasMatches } = getMatchingAttributes(\n        currentJobModel,\n        isDesktop,\n        proctorGroups,\n        getProctorBucketValue(proctorGroups, 'jcs_big6_tst') === 5\n            ? JOB_TYPE_CC_ATTRIBUTE_LABEL\n            : JOB_TYPE_ATTRIBUTE_LABEL,\n        JOB_TYPE_MATCHED_PREFERENCE_LABEL,\n        prioritizedJobTypeSuids,\n        locale,\n        getAppliedFilterSuid(appliedFilter)\n    );\n\n    if (allAttributes.length === 0) {\n        return {\n            showJobType: false,\n            jobTypes: currentJobModel.jobTypes || [],\n            matchingJobTypes: [],\n            isBig6Style: true,\n            isWrapStyle: false,\n            hasMatchingPreference: false,\n        };\n    }\n\n    return {\n        showJobType: true,\n        jobTypes: allAttributes,\n        matchingJobTypes: hasMatches ? matchingAttributes : [],\n        isBig6Style: true,\n        isWrapStyle: false,\n        hasMatchingPreference: hasMatches,\n    };\n};\n","import { IJobModel } from '@indeed/job-cards-model';\n\nexport interface IRecommendationStatementProps {\n