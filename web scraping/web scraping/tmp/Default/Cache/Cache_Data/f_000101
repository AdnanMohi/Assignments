SingleLine: getProctorBucketValue(proctorGroups, 'jcs_new_jobcard_ia_tst') > 0,\n        shelfItemList: getShelfItemList(\n            props,\n            model,\n            isRedLightJob(jobModel, proctorGroups, locale) &&\n                getProctorBucketValue(proctorGroups, 'jcs_qualifications_exp_tst') === 8,\n            manyApplicantsBadgeFilter\n        ),\n    };\n};\n\nexport const getShelfItemList = (\n    props: IJobCardShelfProps,\n    model: IMosaicProviderJobCardsModel,\n    hideForQualificationsExpTst: boolean,\n    manyApplicantsBadgeFilter: boolean\n): Array<string | ShelfItemType> => {\n    if (!manyApplicantsBadgeFilter && (props.shelfItemList.length > 0 || !model.overrideShelf)) {\n        return props.shelfItemList;\n    }\n\n    // Eventually we don't need to check in JobCardShelf.tsx if text exists or not - that check should live here.\n    const shelfItems: ShelfItemType[] = [];\n\n    if (manyApplicantsBadgeFilter) {\n        return shelfItems;\n    }\n\n    if (hideForQualificationsExpTst) {\n        return shelfItems;\n    }\n\n    if (props.attainabilityBadge?.showAttainabilityBadge) {\n        shelfItems.push(ShelfItemType.ATTAINABILITY_BADGE);\n    }\n\n    if (\n        (props.indeedApplyText.iaText || props.indeedApplyText.iaTextOverride) &&\n        !props.openInterviews.openInterviewsText\n    ) {\n        shelfItems.push(ShelfItemType.INDEED_APPLY);\n    }\n\n    if (props.hiringEvents.iheIAText) {\n        // TODO this relies on reading an artifact for a list of job ids - JCS needs to read an artifact.\n        shelfItems.push(ShelfItemType.HIRING_EVENT);\n    }\n\n    if (props.openInterviews.openInterviewsText) {\n        shelfItems.push(ShelfItemType.OPEN_INTERVIEWS);\n    }\n\n    if (props.responsiveEmployer.showResponsiveEmployerBadge) {\n        shelfItems.push(ShelfItemType.RESPONSIVE_EMPLOYER);\n    }\n\n    if (props.employerRecentlyActive.showEmployerRecentlyActive) {\n        shelfItems.push(ShelfItemType.EMPLOYER_REC