udes(rqLightResetGrp)) {\n        return isAQRedLight;\n    }\n    return isAQRedLight && isMinMismatchMet(jobModel, proctorGroups);\n};\n","import { GettextInterface } from '@indeed/indeed-gettext';\nimport { IJobModel } from '@indeed/job-cards-model';\n\nimport { AppProctorGroups } from '../../../../proctor';\nimport { getProctorBucketValue } from '../../../../proctor/getGroup';\n\nexport enum CategoryIconType {\n    NONE,\n    JOB,\n    CLOCK,\n    SALARY,\n    LANGUAGE,\n    CONTACT_METHOD,\n}\n\nconst CUSTOM_CLASS_UUID_JOB_TYPE = '2b08da1b-fe62-43ee-adbe-7f48c9061d39';\nconst CUSTOM_CLASS_UUID_SCHEDULE = 'f9dde7ac-b536-493d-ac13-2914188757f3';\nconst CUSTOM_CLASS_UUID_PAY_TYPE = '0c4377f7-0db4-4581-9c4b-f78a259aa306';\nconst CUSTOM_CLASS_UUID_LANGUAGE_SKILL = '4eaef874-1a0f-448b-b5e8-78798a854766';\nconst CUSTOM_CLASS_UUID_APPLY_METHOD = '018023b6-ea2a-4f4c-bab6-cf3543783814';\n\nconst CATEGORY_KEY_TO_ICON_TYPE: { [key: string]: CategoryIconType } = {\n    [CUSTOM_CLASS_UUID_JOB_TYPE]: CategoryIconType.JOB, // Job type\n    [CUSTOM_CLASS_UUID_SCHEDULE]: CategoryIconType.CLOCK, // Schedule\n    [CUSTOM_CLASS_UUID_PAY_TYPE]: CategoryIconType.SALARY, // Pay type\n    [CUSTOM_CLASS_UUID_LANGUAGE_SKILL]: CategoryIconType.LANGUAGE, // Language skill\n    [CUSTOM_CLASS_UUID_APPLY_METHOD]: CategoryIconType.CONTACT_METHOD, // Apply method\n};\n\nexport interface ICategorizedAttributesProps {\n    showCategorizedAttributes: boolean;\n    attributeCategories: IAttributeCategory[];\n}\n\nexport interface IAttributeCategory {\n    categoryIconType: CategoryIconType;\n    categoryName: string;\n    attributeDisplayNames: string[];\n}\n\nexport const getCategorizedAttributesProps = (\n    currentJobModel: IJobModel,\n    proctorGroups: AppProctorGroups,\n    gt: GettextInterface\n): ICategorizedAttributesProps => {\n    const EMPTY_RESULT = {\n        showCategorizedAttributes: false,\n        attributeCategories: [],\n    };\n\n    if (\n        !currentJobModel.categorizedAttributesModel ||\n        getProctorBucketValue(proctorGroups, 'jcs_big6_tst') === 5\n    ) {\n        return EMPTY_RESULT;\n    }\n\n    const attributeCategories: IAttributeCategory[] =\n        currentJobModel.categorizedAttributesModel.attributeCategories\n            .map((attributeCategory) => ({\n                categoryIconType: CATEGORY_KEY_TO_ICON_TYPE[attributeCategory.categoryKey],\n                categoryName: getCategoryDisplayName(\n                    attributeCategory.categoryKey,\n                    gt,\n                    attributeCategory.categoryDisplayName\n                ),\n                attributeDisplayNames: attributeCategory.attributeDisplayNames,\n            }))\n            .filter(\n                (cat): cat is IAttributeCategory => !!cat.categoryIconType && !!cat.categoryName\n            );\n\n    if (attributeCategories.length === 0) {\n        return EMPTY_RESULT;\n    }\n\n    return {\n        showCategorizedAttributes: true,\n        attributeCategories,\n    };\n};\n\n// visible for testing\nexport const getCategoryDisplayName = (\n    categoryKey: string,\n    gt: GettextInterface,\n    categoryDisplayName?: string\n): string | null => {\n    if (!!categoryDisplayName) {\n        return categoryDisplayName;\n    }\n\n    switch (categoryKey) {\n        case CUSTOM_CLASS_UUID_JOB_TYPE:\n            return gt.pgettext('Categorized attribute category label', 'Job Type');\n        case CUSTOM_CLASS_UUID_SCHEDULE:\n            return gt.pgettext('Categorized attribute category label', 'Schedule');\n        case CUSTOM_CLASS_UUID_PAY_TYPE:\n            return gt.pgettext('Categorized attribute category label', 'Pay Type');\n        case CUSTOM_CLASS_UUID_LANGUAGE_SKILL:\n            return gt.pgettext('Categorized attribute category label', 'Language Skill');\n        case CUSTOM_CLASS_UUID_APPLY_METHOD:\n            return gt.pgettext('Categorized attribute category label', 'Apply Method');\n        default:\n            re