 { hasError: false };\n    }\n\n    public static getDerivedStateFromError(): { hasError: boolean } {\n        return { hasError: true };\n    }\n\n    public componentDidCatch(error: Error, errorInfo: ErrorInfo): void {\n        if (this.props.errorLog) {\n            this.props.errorLog(error, errorInfo);\n        }\n    }\n\n    public render(): ReactNode {\n        if (this.state.hasError) {\n            return this.props.fallback;\n        }\n\n        return this.props.children;\n    }\n}\n\nexport default ErrorBoundary;\n","import React from 'react';\n\nimport { Box } from '@indeed/ifl-components';\n\nimport { IJobSnippet } from './apiBridge';\nimport { jobSnippetSx } from './JobSnippet.jss';\n\nconst JobSnippet: React.FunctionComponent<IJobSnippet> = (props: IJobSnippet) => {\n    if (!props.showSnippetOnJobCard || !props.snippet) {\n        return null;\n    }\n\n    return (\n        <Box\n            sx={[\n                jobSnippetSx,\n                {\n                    display: '-webkit-box',\n                    '-webkit-line-clamp': '4',\n                    '-webkit-box-orient': 'vertical',\n                },\n            ]}\n            dangerouslySetInnerHTML={{ __html: props.snippet }}\n        />\n    );\n};\n\nexport { JobSnippet };\n","import { _ as _objectWithoutPropertiesLoose, I as Icon, a as _extends } from './Icon-b1a2a6d8.js';\nimport React, { forwardRef } from 'react';\nimport '@emotion/is-prop-valid';\nimport '@indeed/ifl-css';\nimport '@emotion/react';\n\nvar _excluded = [\"fill\"];\nvar Interview = /*#__PURE__*/forwardRef(function (_ref, ref) {\n  var fill = _ref.fill,\n      props = _objectWithoutPropertiesLoose(_ref, _excluded);\n\n  var paths = {\n    md: /*#__PURE__*/React.createElement(\"path\", {\n      fillRule: \"evenodd\",\n      d: \"M2 4.5a.5.5 0 01.5-.5h19a.5.5 0 01.5.5v13a.5.5 0 01-.5.5H5.594l-3.172 2.948a.25.25 0 01-.42-.183L2.001 18 2 4.5zm6 9.868c0-1.402 2.667-2.105 4-2.105 1.333 0 4 .704 4 2.107v.493a.135.135 0 