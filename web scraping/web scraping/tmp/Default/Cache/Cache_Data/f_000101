anguage },\n                            contextId: jobKey,\n                            scenarioData,\n                        },\n                        () => {},\n                        undefined,\n                        sourceName\n                    )\n                    .catch(fallback);\n\n                shouldFallback = false;\n            }\n        } catch (e) {\n            shouldFallback = true;\n        }\n\n        if (shouldFallback) {\n            fallback();\n        }\n    };\n\n    MosaicApi.renderProviderOnce('mosaic-provider-salary-feedback').then(\n        tryUseProviderForSurvey,\n        tryUseProviderForSurvey\n    );\n};\n","import React from 'react';\n\nimport { Tag, Text } from '@indeed/ifl-components';\nimport { Check } from '@indeed/ifl-icons';\nimport classnames from 'classnames';\n\nimport { useI18n } from '../../../hooks/useI18n';\nimport { TagsList } from '../../JobMetaData/JobInfoArchitecture/TagsList';\nimport { attributeVariantA, attributeVariantB } from '../OneMatchJobCard.jss';\n\nexport enum OneMatchAttributeVariant {\n    A,\n    B,\n}\n\nexport interface IOneMatchAttributeProps {\n    attributeList: string[];\n    matchingList: string[];\n    variant: OneMatchAttributeVariant;\n    minDisplayedAttributes?: number;\n}\n\nexport const PrimaryAttributes: React.FunctionComponent<IOneMatchAttributeProps> = (\n    props: IOneMatchAttributeProps\n) => {\n    const { gettext } = useI18n();\n\n    if (props.attributeList.length < 1) {\n        return null;\n    }\n\n    const primaryAttributes = props.attributeList;\n\n    if (props.attributeList.length < 1) {\n        return null;\n    }\n\n    return (\n        <TagsList\n            tagsList={primaryAttributes.map((attribute) => {\n                const isMatching = props.matchingList.includes(attribute);\n\n                return props.variant === OneMatchAttributeVariant.A ? (\n                    <Tag\n                        status={isMatching ? 'success' : 'neutral'}\n  