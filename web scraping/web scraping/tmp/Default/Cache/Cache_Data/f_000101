-explicit-any\n    mouseDownHandlerOption?: { [index: string]: any };\n    prevUrl?: string;\n    noJsUrlOnly: boolean;\n    viewjobFrom: string;\n    useMobileProvidedClickHandler: boolean;\n    useJCSMouseDownHandler: boolean;\n    linkTargetAttribute?: string;\n    saveButton: ISaveButton;\n    hideSaveButton?: boolean;\n    dislikeButton: DislikeJavascriptProps;\n    hideDislikeButton?: boolean;\n    reportButton: IReportButton;\n    isBranded: boolean;\n    apiSource?: IApiSource; // see https://go.indeed.com/GLY2PH3C2\n    advn?: string;\n    viewJobLink?: string;\n    adBlob?: string;\n    organicBlob?: string;\n    adSignature?: string;\n    trackingUrls?: string;\n    enableNationwideSection: boolean;\n    enableHomepageJobSections: boolean;\n    jobSection: JobSection;\n    highQualityMarketplace: IJobModel['highQualityMarketplace'];\n    partialViewJobData: IJpPartialViewJobData | null;\n    kebabMenuEnabled?: boolean;\n    tabIndex?: number;\n    redirectToThirdPartySite: boolean;\n    useLocationRollup?: boolean;\n}\n\nexport const getHref = (\n    jobModel: IJobModel,\n    model: IMosaicProviderJobCardsModel,\n    hostId: HostId,\n    proctorGroups: AppProctorGroups\n): string => {\n    const isFullViewjobUrl = hostId === HostId.HOMEPAGE || hostId === HostId.PRR_WEBAPP;\n    const testVJUrl = jobModel?.link ? jobModel.link.includes('from=rjp_mob.vjSimilarJobs') : false;\n    const isViewJobDirectLinking =\n        getProctorBucketValue(proctorGroups, 'mobg4jvjdirectlinkingtst') > 0;\n\n    if (isViewJobDirectLinking && hostId === HostId.MOBILE && !model.isDesktop) {\n        return '/viewjob?jk=' + jobModel.jobkey;\n    }\n\n    if (model.isDesktop || isFullViewjobUrl || testVJUrl) {\n        return jobModel.link!;\n    } else {\n        if (jobModel.hiringEventUrl) {\n            return jobModel.hiringEventUrl;\n        }\n        if (jobModel.noJsUrl) {\n            return jobModel.noJsUrl;\n        }\n\n        return '#';\n    }\n};\n\nexport const getTapItemProps = (\n    locale: Locale,\n    jobModel: IJobModel,\n    model: IMosaicProviderJobCardsModel,\n    proctorGroups: AppProctorGroups,\n    position: number,\n    isApp: boolean,\n    enableNationwideSection: boolean,\n    enableHomepageJobSections: boolean,\n    hostId: HostId,\n    gt: GettextInterface,\n    isAndroid: boolean,\n    isJpBundle: boolean,\n    isDsanr: boolean,\n    pageNum?: number\n): ITapItemProps => {\n    const isDesktop = model.isDesktop;\n    const hasSalary =\n        jobModel.salarySnippet &&\n        (!!jobModel.salarySnippet.text || !!jobModel.salarySnippet.source);\n    let jobSection = enableHomepageJobSections\n        ? convertSectionIdToHomepageJobSection(jobModel.homepageJobFeedSectionId)\n        : convertTierTypeToJobSection(jobModel.tier?.type);\n    const pageNumNormalized = pageNum ?? 0;\n\n    if (jobSection === 'default' && pageNumNormalized > 0) {\n        // we use this alternation to keep sections grouped together for the sake of preserving JobSection logic\n        jobSection = pageNumNormalized % 2 === 0 ? 'pageEven' : 'pageOdd';\n    }\n\n    return {\n        id: `${jobModel.sponsored ? 'sj' : 'job'}_${jobModel.jobkey}`,\n        jk: jobModel.jobkey,\n        sponsored: jobModel.sponsored,\n        mobtk: jobModel.mobtk,\n        searchUID: model.searchUID,\n        href: getHref(jobModel, model, hostId, proctorGroups),\n        hiringEventJob: jobModel.hiringEventJob,\n        hiringEventUrl: jobModel.hiringEventUrl,\n        hiringEventIframeUrl: jobModel.hiringEventIframeUrl,\n        isVisitedOrSaved:\n            (jobModel.myApiJobState && jobModel.myApiJobState.stateString === 'VISITED') ||\n            jobModel.saved,\n        isWithShelf: shouldShowJobCardShelf(jobModel, model, locale.countryCode),\n        clickObj: getClickObj(jobModel, model, hasSalary, proctorGroups),\n        adId: jobModel.adId,\n        advn: jobModel.advn,\n        viewJobLink: jobModel.viewJobLink,\n        trackingUrls: j