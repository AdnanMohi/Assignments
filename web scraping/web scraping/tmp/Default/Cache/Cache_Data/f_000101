,\n              }\n            : undefined;\n\n    // TODO: add other sections: 'commute', 'jobCategory', 'workSchedule'\n    const potentialMetadataSections: Array<IJpMetadataSectionModel | undefined> = [\n        {\n            header: gt.gettext('Job Location'),\n            contents: [jobModel.formattedLocation ?? ''],\n            iconModel: {\n                type: 'location-filled',\n                size: 'sm',\n                color: 'inheritColor',\n            },\n        },\n        jobModel.salarySnippet.text\n            ? {\n                  header: gt.gettext('Salary'),\n                  contents: [jobModel.salarySnippet.text],\n                  iconModel: {\n                      type: 'yen-filled',\n                      size: 'sm',\n                      color: 'inheritColor',\n                  },\n              }\n            : undefined,\n    ];\n    const metadataSections = potentialMetadataSections.filter(\n        (v): v is IJpMetadataSectionModel => !!v\n    );\n\n    return {\n        jobKey: jobModel.jobkey,\n        japanTopLabelModel: jobModel.jobTypes && {\n            newArrival: jobModel.newJob ? gt.pgettext('TopLabels: new top label', 'new') : '',\n            jobTypes: jobModel.jobTypes,\n        },\n        title: jobModel.title,\n        japanCompanyModel: {\n            header: gt.pgettext('Company section header for japan MVJ', 'Company'),\n            name: jobModel.company,\n            ratingsModel,\n        },\n        metadataSections,\n        jobDescriptionModel: {\n            header: gt.pgettext('Header for job description section', 'About this Job'),\n            jobTagModel: undefined, // TODO: add job tags\n        },\n        placeholderPhotoUrl: jobModel.photoUrl,\n        mosaicPlatformModel: {},\n        isAndroid,\n        isAppDesign,\n        japanJobSubtitleModel: jobModel.subtitle ? { subtitle: jobModel.subtitle } : undefined,\n        isRedirectToThirdPartySite: jobModel.redirectToThirdPartySite,\n     