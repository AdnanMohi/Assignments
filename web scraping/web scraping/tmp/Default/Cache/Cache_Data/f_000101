: Map<string, string>;\n    manyApplicantsTst: number;\n    organicApplyStartCount?: number;\n    isOutOfCountry: boolean;\n    enforceHeightLimitations?: boolean;\n    isMisMatchRecommendationJobs?: boolean;\n    enableRedlightForNonSponsorJobs?: boolean;\n}\n\nexport const getJobMetaDataProps = (\n    currentJobModel: IJobModel,\n    model: IMosaicProviderJobCardsModel,\n    proctorGroups: AppProctorGroups,\n    artifacts: IArtifactDataMap,\n    gt: GettextInterface,\n    locale: Locale,\n    hostId: string,\n    enforceHeightLimitations?: boolean\n): IJobMetaDataProps => {\n    const aQSignal = getAQSignalProps(currentJobModel, proctorGroups, locale);\n    const estimatedSalary = getEstimatedSalaryProps(gt, locale, currentJobModel);\n    const jcsQualsProctorVal = getProctorBucketValue(proctorGroups, 'jcs_qualifications_exp_tst');\n    const showDynamicAttributes = shouldShowDynamicAttributes(\n        currentJobModel,\n        model.isDesktop,\n        locale.countryCode\n    );\n    const dynamicAttributes = getDynamicAttributes(currentJobModel);\n    return {\n        hideMetaData: currentJobModel.hideMetaData,\n        jobkey: currentJobModel.jobkey,\n        featuredCompany: getFeaturedCompanyProps(\n            currentJobModel,\n            model,\n            hostId as HostId,\n            locale,\n            proctorGroups\n        ),\n        indeedApplyText: getIndeedApplyTextProps(\n            currentJobModel,\n            model,\n            proctorGroups,\n            artifacts,\n            hostId,\n            gt,\n            locale.countryCode\n        ),\n        jobCardFooter: getJobCardFooterProps(\n            currentJobModel,\n            model,\n            proctorGroups,\n            artifacts,\n            gt,\n            hostId,\n            locale\n        ),\n        jobRequirements: getJobRequirementsProps(\n            currentJobModel,\n            aQSignal,\n            proctorGroups,\n            locale.countryCode\n        ),\n 