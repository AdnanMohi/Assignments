3:\n            // match %\n            const matchPercentage = getMatchPercentage(trafficLight);\n\n            return matchPercentage === ''\n                ? ''\n                : gt.pgettext(\n                      'explains match percentage based on resume and likelihood of employer engagement',\n                      '{0}% Match',\n                      matchPercentage\n                  );\n        default:\n            return '';\n    }\n};\n\nconst isValidTrafficLight = (trafficLight: string | null): boolean => {\n    return (\n        trafficLight === YELLOW ||\n        trafficLight === HIGH_COVERAGE_GREEN ||\n        trafficLight === HIGH_PRECISION_GREEN\n    );\n};\n\nconst getMatchScale = (gt: GettextInterface, trafficLight: string): string => {\n    if (trafficLight === HIGH_PRECISION_GREEN) {\n        return gt.pgettext(\n            'explains match rating based on resume and likelihood of employer engagement',\n            'Strong match'\n        );\n    } else if (trafficLight === HIGH_COVERAGE_GREEN) {\n        return gt.pgettext(\n            'explains match rating based on resume and likelihood of employer engagement',\n            'Good match'\n        );\n    } else if (trafficLight === YELLOW) {\n        return gt.pgettext(\n            'explains match rating based on resume and likelihood of employer engagement',\n            'Fair match'\n        );\n    } else {\n        return '';\n    }\n};\n\n// we will determine the % number by randomly selecting a value within the range\nconst getMatchPercentage = (trafficLight: string): string => {\n    let num;\n\n    if (trafficLight === HIGH_PRECISION_GREEN) {\n        // 90 - 99\n        num = Math.floor(Math.random() * 10) + 90;\n    } else if (trafficLight === HIGH_COVERAGE_GREEN) {\n        // 80 - 89\n        num = Math.floor(Math.random() * 10) + 80;\n    } else if (trafficLight === YELLOW) {\n        // 70 - 79\n        num = Math.floor(Math.random() * 10) + 70;\n    } else {\n        return '';\n    }\n\n    return num.toString();\n};\n","import { GettextInterface } from '@indeed/indeed-gettext';\nimport { IJobModel, IMosaicProviderJobCardsModel, IMatchReason } from '@indeed/job-cards-model';\n\nimport { Locale } from '../../../../../contexts/I18nContext';\nimport { HostId } from '../../../../../contexts/MosaicInitialContext';\nimport { AppProctorGroups } from '../../../../../proctor';\nimport { getProctorBucketValue } from '../../../../../proctor/getGroup';\nimport { getMatchExplanationText } from '../../../../../util/getMatchExplanationText';\nimport { showPayForPerformanceFeature } from '../../../../../util/payForPerformance';\nimport { getSearchParamsTuple } from '../../../../JobCard/SearchFooter/apiBridge';\n\nexport interface IMyJobsStateProps {\n    formattedMyJobsStateChangeTime?: string;\n    isExpired: boolean;\n    recurringHire: RecurringHireStyling | null;\n    formattedActivityDate?: string;\n    formattedRelativeTime?: string;\n    locale: Locale;\n    myJobsStateString?: string;\n    myJobsStateRelativeTime?: string;\n    fromSearchText: (string | undefined)[];\n    matchExplanationText: string;\n    matchReason?: IMatchReason;\n    trafficLight?: string;\n    isDesktop: boolean;\n    isSponsored: boolean;\n    isHomepage: boolean;\n    jsj_hp_matchexplanation_tst_value: number;\n    manyApplicantsString?: string | null;\n}\n\nexport type RecurringHireStyling = 'always' | 'ongoing';\n\nconst getIsRecurringHire = (\n    currentJobModel: IJobModel,\n    countryCode: string\n): RecurringHireStyling | null => {\n    if (!showPayForPerformanceFeature(currentJobModel, countryCode)) {\n        return null;\n    }\n\n    if (!currentJobModel.hiringMultipleCandidatesModel) {\n        return null;\n    }\n\n    if (\n        currentJobModel.hiringMultipleCandidatesModel.hiresNeeded !== 'RECURRING_HIRE' &&\n        currentJobModel.hiringMultipleCandidatesModel.hiresNeededExact !== 'RECURRING_HIRE'\n    ) {\n        return null;\n    }\n\n    ret