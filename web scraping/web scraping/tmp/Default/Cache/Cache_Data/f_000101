Ago, 10);\n    if (isSponsored) {\n        const testGroup: number = getProctorBucketValue(\n            proctorGroups,\n            'jcs_hide_spons_timestamp_tst'\n        );\n\n        const hideSponsTimestamp =\n            (testGroup === 1 && daysAgoNum >= 15) || (testGroup === 2 && daysAgoNum >= 30);\n\n        return hideSponsTimestamp;\n    }\n\n    return false;\n};\n\nconst shouldReplaceTimestamp = (\n    isSponsored: boolean,\n    proctorGroups: AppProctorGroups,\n    formattedRelativeTime: string | undefined\n): boolean => {\n    if (!formattedRelativeTime) {\n        return false;\n    }\n\n    if (!isSponsored) {\n        return false;\n    }\n\n    const testGroup = getProctorBucketValue(proctorGroups, 'jcs_new_spons_timestamp_old_jobs_tst');\n\n    if (testGroup < 1 || testGroup >= 4) {\n        return false;\n    }\n\n    const daysAgo: string = extractDaysAgo(formattedRelativeTime);\n    const daysAgoNum: number = parseInt(daysAgo, 10);\n\n    if (daysAgoNum < 15) {\n        return false;\n    }\n\n    return true;\n};\n\nconst getReplacementTimestampText = (\n    proctorGroups: AppProctorGroups,\n    gt: GettextInterface\n): string => {\n    const testGroup = getProctorBucketValue(proctorGroups, 'jcs_new_spons_timestamp_old_jobs_tst');\n    switch (testGroup) {\n        case 1:\n            return gt.gettext('Actively hiring');\n        case 2:\n            return gt.gettext('Accepting applications');\n        case 3:\n            return gt.gettext('Still hiring');\n        default:\n            return '';\n    }\n};\n\nexport {\n    MyJobsState,\n    getFormattedMyJobsStateChangeTime,\n    getFormattedRelativeTime,\n    shouldHideTimestamp,\n    shouldReplaceTimestamp,\n};\n","import React, { useState } from 'react';\n\nimport { Box } from '@indeed/ifl-components';\n\nimport { useMosaicInitialData } from '../../../hooks/useMosaicInitialContext';\nimport { JobSnippet } from '../../JobSnippet/JobSnippet';\n\nimport { IJobCardFooterProps } from './ap