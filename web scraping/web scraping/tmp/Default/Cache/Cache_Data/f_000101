bModelsCache();\n\n    const { paginatedJobs: mosaicProviderJobCardsModelOfAllPages } = usePaginationContext();\n\n    if (cache.computedMaxPageNum < mosaicProviderJobCardsModelOfAllPages.length) {\n        updateJobModelsCache(mosaicProviderJobCardsModelOfAllPages, cache);\n    }\n\n    return cache;\n};\n\nfunction updateJobModelsCache(\n    mosaicProviderJobCardsModelOfAllPages: IMosaicProviderJobCardsModel[],\n    cache: IJobModelsCache\n): void {\n    const { computedMaxPageNum } = cache;\n    const maxPageNumToBeComputed = mosaicProviderJobCardsModelOfAllPages.length;\n    let currentPositionInAllPages = cache.computedPositionInAllPages;\n    // Write this code imperatively since we are in a hot spot for page load performance.\n    for (let pageNum = computedMaxPageNum; pageNum < maxPageNumToBeComputed; pageNum++) {\n        const jobModelsOfThePage = mosaicProviderJobCardsModelOfAllPages[pageNum]?.results;\n        const jobModelsNumOfThePage = jobModelsOfThePage.length;\n        for (\n            let positionInItsOwnPage = 0;\n            positionInItsOwnPage < jobModelsNumOfThePage;\n            positionInItsOwnPage++\n        ) {\n            const jobModel = jobModelsOfThePage[positionInItsOwnPage];\n            cache.jobKeysOfAllPages.push(jobModel.jobkey);\n            cache.jobKeysOfAllPagesWithPosition.set(jobModel.jobkey, {\n                pageNum,\n                positionInItsOwnPage,\n                positionInAllPages: currentPositionInAllPages,\n            });\n            currentPositionInAllPages += 1;\n        }\n    }\n\n    cache.computedMaxPageNum = mosaicProviderJobCardsModelOfAllPages.length;\n    cache.computedPositionInAllPages = currentPositionInAllPages;\n}\n","import { useMemo } from 'react';\n\nimport { GettextInterface } from '@indeed/indeed-gettext';\n\nimport { Locale } from '../../../contexts/I18nContext';\nimport { HostId } from '../../../contexts/MosaicInitialContext';\nimport { useInitialMosaicProviderJobCardsModel } from