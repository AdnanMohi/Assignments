xtended fields used for the Explicit Dislike Feedback (Feedback V2)\n    jobTypes: ITaxonomyAttribute[] | undefined;\n\n    location:\n        | {\n              label: string;\n              country: string;\n              city?: string;\n              admin1?: string;\n              postalCode?: string;\n          }\n        | undefined;\n    remote: ITaxonomyAttribute[] | undefined;\n\n    salary:\n        | {\n              label: string;\n              amount: number;\n              currency: string;\n              salaryType: JobSeekerProfileStructuredDataSalaryType;\n          }\n        | undefined;\n    onHideCloseModal?: () => void;\n}\n\nexport const getDislikeMosaicFeedbackProps = (\n    currentJobModel: IJobModel,\n    model: IMosaicProviderJobCardsModel,\n    enableNationwideSection: boolean,\n    enableHomepageJobSections: boolean,\n    locale: Locale\n): DislikeMosaicFeedbackProps => {\n    const [what, where] = getSearchParamsTuple(model, currentJobModel);\n    const jobSection = enableHomepageJobSections\n        ? convertSectionIdToHomepageJobSection(currentJobModel.homepageJobFeedSectionId)\n        : convertTierTypeToJobSection(currentJobModel.tier?.type);\n    return {\n        jobKey: currentJobModel.jobkey,\n        csrfToken: model.csrfToken!,\n        // should probably update for overlap with mobpreciseloctst in company location\n        commute: currentJobModel.formattedLocation,\n        jobTitle: currentJobModel.title,\n        normTitle: currentJobModel.normTitle,\n        companyName: currentJobModel.company,\n        salarySnippet: currentJobModel.salarySnippet.text,\n        encryptedFccompanyId: currentJobModel.encryptedFccompanyId,\n        fccompanyId: currentJobModel.fccompanyId,\n        refLocation: where,\n        refQuery: what,\n        jobSection: jobSection,\n        enableNationwideSection,\n        enableHomepageJobSections,\n        jobTypes: getJobTypes(currentJobModel),\n        location: getLocation(currentJobModel, locale),\n        remote: getRemote(currentJobModel),\n        salary: getSalary(currentJobModel),\n    };\n};\n\nconst getLocation = (\n    job: IJobModel,\n    locale: Locale\n): DislikeMosaicFeedbackProps['location'] | undefined => {\n    if (\n        job.formattedLocation &&\n        (job.jobLocationCity || job.jobLocationState || job.jobLocationPostal)\n    ) {\n        return {\n            label: job.formattedLocation,\n            country: locale.countryCode,\n            city: job.jobLocationCity,\n            admin1: job.jobLocationState,\n            postalCode: job.jobLocationPostal,\n        };\n    }\n\n    return undefined;\n};\n\nconst getJobTypes = (job: IJobModel): DislikeMosaicFeedbackProps['jobTypes'] | undefined => {\n    return (job.taxonomyAttributes ?? [])\n        .filter((attributes) => attributes.label === JOB_TYPE_ATTRIBUTE_LABEL)\n        .map((attributes) => attributes.attributes)[0];\n};\n\nconst getRemote = (job: IJobModel): DislikeMosaicFeedbackProps['remote'] | undefined => {\n    return (job.taxonomyAttributes ?? [])\n        .filter((attributes) => attributes.label === REMOTE_ATTRIBUTE_LABEL)\n        .map((attributes) => attributes.attributes)[0];\n};\n\nconst getSalary = (job: IJobModel): DislikeMosaicFeedbackProps['salary'] | undefined => {\n    if (!job.salarySnippet.text || !job.extractedSalary || !job.extractedSalary.type) {\n        return undefined;\n    }\n\n    const amount = job.extractedSalary.min ?? job.extractedSalary.max;\n    if (!amount) {\n        return undefined;\n    }\n\n    return {\n        label: job.salarySnippet.text.replace(/ \\- .*\\d/, ''),\n        amount: amount,\n        currency: job.salarySnippet?.currency ?? 'USD',\n        salaryType: job.extractedSalary.type as JobSeekerProfileStructuredDataSalaryType,\n    };\n};\n","import { IJobModel } from '@indeed/job-cards-model';\n\nexport interface ITopLabelProps {\n    isNew: boolean;\n    jobTypes: string[];\n    daysToExpiration: number;\n}\n\ncon