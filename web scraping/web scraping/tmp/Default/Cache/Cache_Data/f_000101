      // eslint-disable-next-line dot-notation\n                window['jcsClickedJob'] = null;\n                if (jcsJobElem) {\n                    // eslint-disable-next-line dot-notation\n                    const clickEvent = document['createEvent']('MouseEvents');\n                    clickEvent.initEvent('mousedown', true, true);\n                    jcsJobElem.dispatchEvent(clickEvent);\n                    // @ts-ignore\n                    jcsJobElem.click();\n                }\n            }\n        }\n    } catch {}\n    // do nothing in catch right now.\n};\n","const TEMP_CHECK_KEY = '__JOBCARDS_LS_TEMP_CHECK_KEY_';\n\nconst isLocalStorageSupported = (): boolean => {\n    try {\n        /* istanbul ignore if  */\n        // eslint-disable-next-line\n        if (window['localStorage']) {\n            // eslint-disable-next-line\n            window['localStorage'].setItem(TEMP_CHECK_KEY, TEMP_CHECK_KEY);\n            // eslint-disable-next-line\n            window['localStorage'].removeItem(TEMP_CHECK_KEY);\n            return true;\n        }\n    } catch {}\n    return false;\n};\n\nexport const localStorageGetItem = (key: string): string => {\n    /* istanbul ignore if  */\n    // eslint-disable-next-line\n    return (isLocalStorageSupported() && window['localStorage'].getItem(key)) || '';\n};\n\nexport const localStorageSetItem = (key: string, val: string): boolean => {\n    /* istanbul ignore if  */\n    if (isLocalStorageSupported()) {\n        try {\n            // eslint-disable-next-line\n            window['localStorage'].setItem(key, val);\n            return true;\n        } catch {}\n    }\n    return false;\n};\n\nexport const localStorageRemoveItem = (key: string): boolean => {\n    /* istanbul ignore if  */\n    if (isLocalStorageSupported()) {\n        try {\n            // eslint-disable-next-line\n            window['localStorage'].removeItem(key);\n            return true;\n        } catch {}\n    }\n    return false;\n};\n","import