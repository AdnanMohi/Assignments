      {props.unit}\n        </Text>\n    );\n};\n\ninterface IBenefits {\n    benefitsList?: string[];\n}\nconst Benefits = (props: IBenefits): JSX.Element | null => {\n    if (props.benefitsList && props.benefitsList.length < 1) {\n        return null;\n    }\n\n    return (\n        <Text\n            level={2}\n            sx={{\n                pt: 0.5,\n                overflow: 'hidden',\n                textOverflow: 'ellipsis',\n                ...{\n                    display: '-webkit-box',\n                    '-webkit-line-clamp': '1',\n                    '-webkit-box-orient': 'vertical',\n                },\n            }}\n        >\n            {props.benefitsList?.join(' · ')}\n        </Text>\n    );\n};\n\nexport const PayInformationNotProvided = () => {\n    const { gettext } = useI18n();\n\n    return (\n        <Tag\n            status=\"neutral\"\n            variant=\"light\"\n            sx={{\n                fontSize: 2,\n                lineHeight: 'normal',\n                fontWeight: 'normal',\n            }}\n        >\n            {gettext('Pay information not provided')}\n        </Tag>\n    );\n};\n\ninterface ISalaryComponent {\n    salarySnippetProps?: ISalarySnippetProps;\n    benefitsList?: string[];\n    isSponsored: boolean;\n    jobKey?: string;\n}\nexport const SalaryComponent = (props: ISalaryComponent): JSX.Element | null => {\n    const gt = useI18n();\n    const proctorGroups = useProctorContext();\n    const logFeatureDisplay = useLogFeatureDisplay();\n    const infoArchitectureGroup = getProctorBucketValue(proctorGroups, 'jcs_new_jobcard_ia_tst');\n\n    if (infoArchitectureGroup <= 0) {\n        return null;\n    }\n\n    if (!props.salarySnippetProps?.text) {\n        logFeatureDisplay(() => ({ jobKey: props.jobKey, field: 'payNotProvidedYN', value: true }));\n\n        return (\n            <Box sx={{ pb: 1 }}>\n                <PayInformationNotProvided key=\"payInformationNotProvided\" />\n            </Box>\n        );\n    }\n\n    const { amount, unit } = extractSalaryDetails(gt, props.salarySnippetProps?.text);\n\n    logFeatureDisplay(() => ({ jobKey: props.jobKey, field: 'salaryYN', value: true }));\n\n    return (\n        <Box sx={infoArchitectureGroup < 3 ? { pt: 2, pb: 5 } : { pb: 4 }}>\n            <Flex sx={{ alignItems: 'baseline', flexDirection: 'row', flexWrap: 'wrap' }}>\n                <PayAmount amount={amount} infoArchitectureGroup={infoArchitectureGroup} />\n                <Unit unit={unit} infoArchitectureGroup={infoArchitectureGroup} />\n            </Flex>\n            {props.benefitsList &&\n                (([1, 3, 4, 5].includes(infoArchitectureGroup) && props.isSponsored) ||\n                    infoArchitectureGroup === 2) && <Benefits benefitsList={props.benefitsList} />}\n        </Box>\n    );\n};\n","import { GettextInterface } from '@indeed/indeed-gettext';\n\nexport const extractSalaryDetails = (gt: GettextInterface, salaryString: string | undefined) => {\n    if (!salaryString) {\n        return {\n            amount: '',\n            unit: '',\n        };\n    }\n\n    const amountRegex = /\\$(\\d+(?:,\\d+)*(?:\\.\\d+)?)/g;\n    const unitRegex = /(a day|an hour|a week|a month|a year|a quarter|biweekly)/g;\n\n    const amount = salaryString.match(amountRegex) ?? [''];\n    const unit = salaryString.match(unitRegex) ?? [''];\n\n    const containsFromOrUpToRegex = /(From|Up to)/g ?? [''];\n\n    const containsFromOrUpTo = salaryString.match(containsFromOrUpToRegex);\n\n    if (containsFromOrUpTo) {\n        return {\n            amount: gt.pgettext('Pay information', '{0} {1}', [containsFromOrUpTo[0], amount[0]]),\n            unit: gt.pgettext('Interval of pay', '{0}', [unit[0]]),\n        };\n    }\n\n    if (amount.length > 1) {\n        return {\n            amount: gt.pgettext('Pay information', '{0} - {1}', [amount[0], amount[1]]),\n            unit: gt.pgettext('Interval of pay', '{0}', [unit[0]]),\n        };\n    }\n\n    return {\n        amount: gt.pgettext('Pay information', '{0}', [amount[0]]),\n        unit: gt.pgettext('Interval of pay', '{0}', [unit[0]]),\n    };\n};\n","import React from 'react';\n\nimport { Box } from '@indeed/ifl-components';\nimport { SystemStyleObject } from '@indeed/ifl-css';\nimport classnames from 'classnames';\n\nimport { PageId } from '../../contexts/MosaicInitialContext';\nimport { useMosaicInitialData } from '../../hooks/useMosaicInitialContext';\nimport { getProctorBucketValue } from '../../proctor/getGroup';\nimport { useProctorContext } from '../../proctor/proctorTest';\nimport { SavedApplicationBadge } from '../ApplicationBadge';\nimport { AppliedSnippet } from '../AppliedSnippet';\nimport { CompanyInfo } from '../CompanyInfo';\nimport { JobCardMenu } from '../JobCardMenu/JobCardMenuLazy';\nimport { JobMetaData } from '../JobMetaData';\nimport { getReorderedBenefitsList } from '../JobMetaData/JobInfoArchitecture/helpers/reorderBenefitsList';\nimport { SalaryComponent } from '../JobMetaData/JobInfoArchitecture/SalaryComponent';\nimport { ResponsiveEmployerTag } from '../JobMetaData/ResponsiveEmployer/ResponsiveEmployerTag';\nimport { shouldHideSalary } from '../JobMetaData/utils/shouldHideSalary';\nimport { JobTitle } from '../JobTitle';\nimport { style } from '../KebabMenu/KebabMenu.jss';\n\nimport { IMainContentProps } from './apiBridge/MainContent';\nimport { ITapItemProps } from './apiBridge/TapItem';\n\nimport '../common/promos.microcontent.css';\n\nconst mainContentSx: SystemStyleObject = {\n    '.tapItem.result.resultWithShelf .slider_container &': {\n        padding: '1rem 1rem 0 1rem !important',\n    },\n    '.tapItem.result .slider_container &': {\n        padding: '1rem !important',\n        borderCollapse: 'separate',\n    },\n    '.result &': {\n        tableLayout: 'unset',\n        width: 'unset !important',\n    },\n};\n\nconst resultContentSx: SystemStyleObject = {\n    '.tapItem.result &': {\n        width: '100%',\n    },\n};\n\nin