iGroup = getProctorBucketValue(proctorGroups, 'jcs_one_match_ui_tst') ?? -1;\n    const activeOnHomePage =\n        host === HostId.HOMEPAGE && (oneMatchIsActive || oneMatchUiIsActive > 0);\n    const activeOnSerp = host === HostId.MOBILE && oneMatchUiGroup === 5;\n\n    return activeOnHomePage || activeOnSerp;\n};\n\nexport const getJobCardProps = (\n    currentJobModel: IJobModel,\n    model: IMosaicProviderJobCardsModel,\n    proctorGroups: AppProctorGroups,\n    position: number,\n    artifacts: IArtifactDataMap,\n    gt: GettextInterface,\n    locale: Locale,\n    isApp: boolean,\n    hostId: string,\n    enableNationwideSection: boolean,\n    enableHomepageJobSections: boolean,\n    isAndroid: boolean,\n    isDsanr: boolean,\n    isJpBundle: boolean,\n    isBlurred?: boolean | null,\n    pageNum?: number,\n    isBrandHeaderJobKey?: boolean,\n    isBot?: boolean,\n    isMisMatchRecommendationJobs?: boolean,\n    enforceHeightLimitations?: boolean\n): IJobCardProps => {\n    const brandingProps = getBrandingHeaderProps(\n        currentJobModel,\n        hostId,\n        model,\n        locale,\n        isBrandHeaderJobKey ?? false,\n        proctorGroups\n    );\n\n    return {\n        isHighQualityMarketplaceMatch: isHighQualityMarketplaceMatch(\n            currentJobModel,\n            proctorGroups\n        ),\n        jobkey: currentJobModel.jobkey,\n        featuredCompany: getFeaturedCompanyProps(\n            currentJobModel,\n            model,\n            hostId as HostId,\n            locale,\n            proctorGroups\n        ),\n        jobSnippet: getJobSnippetProps(currentJobModel, model),\n        tooltip: getTooltipProps(\n            currentJobModel,\n            model,\n            proctorGroups,\n            hostId as HostId,\n            locale,\n            isDsanr\n        ),\n        jobCardShelfContainer: getJobCardShelfContainerProps(\n            currentJobModel,\n            model,\n            proctorGroups,\n            artifact